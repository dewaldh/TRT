#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management ETAFIC0 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:00:30"
A_USER  = "ADMIN"
A_STAMP = "20180828000030"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask ETAFIC0 [ETF0]
Gosub HORODAT
Local Inpbox "ETAFIC0" From GFONCTION At A_STAMP With A_WINDPREV Mask [ETF0] As [SIM_]
Grizo [ETF0]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [ETF0]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=112 : Y1(0)=1 : Y2(0)=2
Return

$ON_DOSSIER
Onevent CONTROL Call C_DOSSIER(zc) From W1ETAFIC0
Onevent AFTER_CHANGE Gosub AMM_DOSSIER From W1ETAFIC0
Onevent BEFORE_INPUT Call AS_DOSSIER(zc) From W1ETAFIC0
Onevent INIT_AREA Call D_DOSSIER(zc) From W1ETAFIC0
Return

Subprog D_DOSSIER(VALEUR)
Variable Char    VALEUR()
If varinit([M]DOSSIER,indice)=0
 VALEUR = nomap
Endif
Gosub _AFF
End

Subprog AS_DOSSIER(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_DOSSIER(zc) From W1ETAFIC0
Gosub _AFF
End

Subprog C_DOSSIER(VALEUR)
Variable Char    VALEUR()
## Action CONTDOS
Call C_DOSSIER(VALEUR) From DOSCONT
If mkstat : GERR = 1 : GZONE = "DOSSIER" : GZNUM = 10035 : Endif
If !mkstat
 ## Action ACCESDOS
 Call CTL_DROIT_DOSSIER(VALEUR,"") From DOSCONT
 If mkstat : GERR = 1 : GZONE = "DOSSIER" : GZNUM = 10035 : Endif
 If !mkstat
  ## Action STD
  Call C_DOSSIER(VALEUR) From ETAFIC
  If mkstat : GERR = 1 : GZONE = "DOSSIER" : GZNUM = 10035 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_DOSSIER
Call AM_DOSSIER(zc)
If !mkstat & GOBJET
 zoncou = "DOSSIER" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_DOSSIER(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action APASSWD
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "APASSWD"
 PARMSK(1) = VALEUR
 PARMSK(2) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"APASSWD","APASSWD","SPEPASSWD") From GSAISIE
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DOSSIER" : GZNUM = 10035 : Endif
Endif
Gosub _AFF
End

Subprog S_DOSSIER(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELOBJSTD
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELOBJSTD"
 PARMSK(1) = "ADS"
 PARMSK(2) = ""
 PARMSK(3) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJSTD","SELOBJ","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

$CHG_STY
Return


