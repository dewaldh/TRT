Local Char T0(30)(1..)
Local Char T1(30)(1..)
Local Integer NBPRO : NBPRO=0
NBPRO += 1 : T1(NBPRO) = "SRHNUM" : T0(NBPRO) = "[F:SRD]SRHNUM"
