#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CHGITG 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_ZTSICODEND
Onevent BEFORE_AREA Gosub AV_ZTSICODEND From W2CHGITG
Return

$AV_ZTSICODEND
Call LECTEXTRA(zc,"ATABDIV","LNGDES",num$(indice+20),num$([M]TSICODEND(indice))) From ATEXTRA
Gosub _AFF
Return

$ON_SELFOR
Onevent CONTROL Call C_SELFOR(zc) From W2CHGITG
Onevent BEFORE_INPUT Call AS_SELFOR(zc) From W2CHGITG
Onevent AFTER_CHANGE Gosub AMM_SELFOR From W2CHGITG
Return

Subprog AS_SELFOR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(4574,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_SELFOR(zc) From W2CHGITG
Onkey   2 Titled GBOUTS Call S_SELFOR(zc) From W2CHGITG
Onkey 144 Titled GBOUTP Call P_SELFOR(zc) From W2CHGITG
Onkey 146 Titled GBOUTA Call SA_SELFOR(zc) From W2CHGITG
Gosub _AFF
End

Subprog C_SELFOR(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("TFO","!",VALEUR,"30",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "SELFOR" : GZNUM = 4574 : Endif
Gosub _AFF
End

$AMM_SELFOR
Call AM_SELFOR(zc)
If !mkstat & GOBJET
 zoncou = "SELFOR" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_SELFOR(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZSELFOR)")>indice
 [M]SELFOR(indice) = VALEUR
 Affzo ZSELFOR(indice)
Endif
Gosub _AFF
End

Subprog S_SELFOR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "TFO"
PARMSK(2) = "!"
PARMSK(3) = "30"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_SELFOR(VALEUR)
Variable Char    VALEUR()
Call OBJET("TFO",VALEUR,"30") From GOBJET
Gosub _AFF
End

Subprog SA_SELFOR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TFO"
PARMSK(2) = "30"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_SELFOR(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "30"
Call VISU("TFO",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZSELFOR
Onevent BEFORE_AREA Gosub AV_ZSELFOR From W2CHGITG
Return

$AV_ZSELFOR
Call LECTEXTRA(zc,"TABFOR","DES","30",num$([M]SELFOR(indice))) From ATEXTRA
Gosub _AFF
Return

$ON_TRACE
Onevent INIT_AREA Call D_TRACE(zc) From W2CHGITG
Onevent BEFORE_INPUT Call AS_TRACE(zc) From W2CHGITG
Return

Subprog D_TRACE(VALEUR)
Variable Integer VALEUR
If varinit([M]TRACE,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_TRACE(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$CHG_STY
Return


