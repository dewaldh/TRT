# Supervisor
## CRUD-CLASSE ## AUTILISINI
# 2018-08-27 13:03:51 601


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(AUTILISINI,ASTATUS)
Variable Instance AUTILISINI Using R_AUTILISINI
Variable Integer ASTATUS

If AUTILISINI = null : End : Endif

Gosub AVARIABLES From WMR00AUTILISINI
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_AUTILISINI
Else
 Gosub AUPDATE_AUTILISINI
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = AUTILISINI.freeSnapshot
AUTILISINI.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
AUTILISINI.ASTALIN = [V]CST_AUPD
AUTILISINI.AUS.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = AUTILISINI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = AUTILISINI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet AUTILISINI._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet AUTILISINI._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet AUTILISINI._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If AUTILISINI.AUS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
AUTILISINI.AUS.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(AUTILISINI.AUS.ASTALIN)<>""
 [L]A_ASTALIN = AUTILISINI.AUS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.AUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(AUTILISINI.AUS.FNCCOL)
 If AUTILISINI.AUS.FNCCOL([L]A_I)<>null
  [L]A_ASTALIN = AUTILISINI.AUS.FNCCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.AUS.FNCCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If AUTILISINI.AUS.BPA(A_N2) <> null
& and AUTILISINI.AUS.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(AUTILISINI.AUS.BID) : # BID -- 3
 If AUTILISINI.AUS.BID(A_N2) <> null
& and AUTILISINI.AUS.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
A_N2=1
If AUTILISINI.AUS.BPA(A_N2) <> null
& and AUTILISINI.AUS.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(AUTILISINI.AUS.BID) : # BID -- 3
 If AUTILISINI.AUS.BID(A_N2) <> null
& and AUTILISINI.AUS.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(AUTILISINI.AUS.FNCCOL)
 If AUTILISINI.AUS.FNCCOL([L]A_I)<>null
  [L]A_ASTALIN = AUTILISINI.AUS.FNCCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.AUS.FNCCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(AUTILISINI.AUS.ASTALIN)<>""
 [L]A_ASTALIN = AUTILISINI.AUS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.AUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_AUTILISINI
Trbegin [F:AUS]
 Gosub AUPDATE_AUTILISINI
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(AUTILISINI.AUS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 AUTILISINI.AUS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(AUTILISINI.AUS.FNCCOL)
 If AUTILISINI.AUS.FNCCOL([L]A_I)<>null
  If AUTILISINI.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ADEL or AUTILISINI.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AUTILISINI.AUS.FNCCOL([L]A_I)
   AUTILISINI.AUS.FNCCOL([L]A_I) = null
  Elsif AUTILISINI.AUS.FNCCOL([L]A_I).ASTALIN <> [V]CST_ALL
   AUTILISINI.AUS.FNCCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
A_N2=1
If AUTILISINI.AUS.BPA(A_N2) <> null
 If AUTILISINI.AUS.BPA(A_N2).ASTALIN=[V]CST_ADEL or AUTILISINI.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance AUTILISINI.AUS.BPA(A_N2)
  AUTILISINI.AUS.BPA(A_N2) = null
 Elsif AUTILISINI.AUS.BPA(A_N2).ASTALIN <> [V]CST_ALL
  AUTILISINI.AUS.BPA(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(AUTILISINI.AUS.BPA(A_N2).COLTEL)
   If AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I)
     AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I) = null
    Elsif AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
     AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  For [L]A_I=1 To maxtab(AUTILISINI.AUS.BPA(A_N2).COLWEB)
   If AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I)
     AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I) = null
    Elsif AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
     AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(AUTILISINI.AUS.BID)
 If AUTILISINI.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet AUTILISINI.AUS.BID(A_N2)._AUPDATE_AFTER
  If AUTILISINI.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or AUTILISINI.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AUTILISINI.AUS.BID(A_N2)
   AUTILISINI.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_AUTILISINI
[L]A_ASTALIN = AUTILISINI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.AUS._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = AUTILISINI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet AUTILISINI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If AUTILISINI.snapshotEnabled=1
 [L]A_RETIK = AUTILISINI.revertUpdTick
Endif
Local Integer ASTATUS2
A_N2=1
If AUTILISINI.AUS.BPA(A_N2) <> null
 If AUTILISINI.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  AUTILISINI.AUS.BPA(A_N2).ASTALIN = [V]CST_ANEW
  For [L]A_I=1 To maxtab(AUTILISINI.AUS.BPA(A_N2).COLTEL)
   If AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     AUTILISINI.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ANEW
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  For [L]A_I=1 To maxtab(AUTILISINI.AUS.BPA(A_N2).COLWEB)
   If AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     AUTILISINI.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ANEW
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(AUTILISINI.AUS.BID)
 If AUTILISINI.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet AUTILISINI.AUS.BID(A_N2)._AUPDATE_ABORT
  If AUTILISINI.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or AUTILISINI.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AUTILISINI.AUS.BID(A_N2)
   AUTILISINI.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet AUTILISINI.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



