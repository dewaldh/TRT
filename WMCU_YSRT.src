############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(YSRT,ASTATUS)
Variable Instance YSRT Using C_YSRT
Variable Integer ASTATUS

If YSRT = null : End : Endif

Gosub _AVARIABLES From WMC00YSRT

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_YSRT

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YSRT,ASTATUS)
Variable Instance YSRT Using C_YSRT
Variable Integer ASTATUS

If YSRT = null : End : Endif

Gosub AVARIABLES From WMC00YSRT
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
YSRT.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_YSRT
Else
 Gosub AUPDATE_YSRT
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YSRT.freeSnapshot
YSRT.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YSRT._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YSRT._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YSRT._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If YSRT.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YSRT.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(YSRT.ASTALIN)<>""
 [L]A_ASTALIN = YSRT.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YSRT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

If [V]CST_ASTALIN(YSRT.ASTALIN)<>""
 [L]A_ASTALIN = YSRT.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YSRT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_YSRT
Trbegin [F:YRTP]
 Gosub AUPDATE_YSRT
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_YSRT
[L]A_ASTALIN = YSRT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YSRT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YSRT._MAJSQL_YSETRULTYP(YSRT))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif


[L]A_ASTALIN = YSRT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YSRT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If YSRT.snapshotEnabled=1
 [L]A_RETIK = YSRT.revertUpdTick
Endif

[L]ASTATUS = max(fmet YSRT.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(YSRT.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YSRT.ASTALIN = [V]CST_ALL
Endif
Return



