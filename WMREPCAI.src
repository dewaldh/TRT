#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1CAI
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "CAI"
REP.CLA = "CAI"
REP.ICL = "CAI"
REP.AFCRIGHT = "GESCAI"
REP.NCL = 7643 : # Petty cash
REP.DES = 7643 : # Petty cash
REP.TIT = "DES"
REP.ANX = "BAN0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Code
     REP.CLE(1).PAR = 'BAN' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'BAN' : REP.CLE(1).VPR= 'CAI.BAN' : REP.CLE(1).DES = 5296
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'CAI_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 1 : # Detail
REP.AENAFAC(2) = 1 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "HEADER" : REP.SEC(1).NSC = 7643 : # Petty cash
     REP.SEC(1).BLC(1)="B1_1" : REP.SEC(1).NBC(1)=7643 : # Petty cash
Return

#-------------------------------------------------------#
# Liens de la représentation
