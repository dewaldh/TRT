     Call ASET_AREPLNK(REP.LNK(1),'$create',4,'GESCAI','',1,'',12414,4,'','',1,1,2,1,1) From ASYRSUBREP
     REP.LNK(1).CLE(1)  = NewInstance C_AREPPAR AllocGroup REP
          Call ASET_AREPPAR(REP.LNK(1).CLE(1),'BAN',7,'CAI.BAN','BAN',0,0,0,0,'',1,1) From ASYRSUBREP

REP.LNK(2) = NewInstance C_AREPLNK AllocGroup REP : # Edit
     Call ASET_AREPLNK(REP.LNK(2),'$edit',4,'GESCAI','',3,'',12964,5,'','',1,1,2,1,1) From ASYRSUBREP
