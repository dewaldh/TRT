$LEC_DERLU
Read [AFF]AFF0 = [F:ICM]SALFCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Read [AXX]AXX0="ITMCOMP";"CPPITMDESAXX";GLANGUE;[F:ICM]NUM;""
If fstat : Raz [F:AXX] : Endif
Return

$CHARGE_DERLU
If clalev([F:ICM9])>0
 Read [ICM9] = [F:ICM9]A_TABI
 [L]CLE = [F:ICM9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:ICM9])>0
 Filter [ICM9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [CCC1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:ICM9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:ICM]NUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:ICM9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:ICM9]
 Endif
Endif
Return

