$BOUTON
STDBOUT = "O"
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "M" : LBOUT(NBTEX) = 1101 : Call TEXTE(845,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "R" : LBOUT(NBTEX) = 1802 : Call TEXTE(12424,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "E" : LBOUT(NBTEX) = 1803 : Call TEXTE(13717,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "M"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "[M:PHYM],[M:PHYM1],[M:ASL]"
  PARBOUT(2) = "trf"
  PARBOUT(3) = "1"
  FLGEXE = 1
  GACTION = "MEMO"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"GMEMO","GMEMO","XWGMEMO") From GSAISIE
 When "R"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "[M:PHYM],[M:PHYM1],[M:ASL]"
  PARBOUT(2) = "trf"
  PARBOUT(3) = "2"
  FLGEXE = 1
  GACTION = "MEMO"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"GMEMO","GMEMO","XWGMEMO") From GSAISIE
 When "E"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "[M:PHYM],[M:ASL]"
  PARBOUT(2) = "trf"
  PARBOUT(3) = "3"
  FLGEXE = 1
  GACTION = "MEMO"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"GMEMO","GMEMO","XWGMEMO") From GSAISIE
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


