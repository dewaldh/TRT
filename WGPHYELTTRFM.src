$DEFVAR
Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "PHYELTTRFM"   : ABMSK(0)  = "PHYM"     : AMA(0) = 1
NOMMSK(1) = "PHYELTTRFM1"  : ABMSK(1)  = "PHYM1"    : AMA(1) = 1
NOMMSK(2) = "ASELOBJ"      : ABMSK(2)  = "ASL"      : AMA(2) = 1
NBMASK = 2
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 0 : If clalev([M:PHYM])>0 : Default Mask [PHYM] : Endif
 When 1 : If clalev([M:PHYM1])>0 : Default Mask [PHYM1] : Endif
 When 2 : If clalev([M:ASL])>0 : Default Mask [ASL] : Endif
Endcase
Return

$TITMASK
Case MA
 When 0 : If clalev([M:PHYM])>0 : Gosub TITRES From W1PHYELTTRFM : Endif
 When 1 : If clalev([M:PHYM1])>0 : Gosub TITRES From W1PHYELTTRFM1 : Endif
 When 2 : If clalev([M:ASL])>0 : Gosub TITRES From W1ASELOBJ : Endif
Endcase
Return

$FERME_MASK
If clalev([M:PHYM])>0 : Close Local Mask [PHYM] : Endif
If clalev([M:PHYM1])>0 : Close Local Mask [PHYM1] : Endif
If clalev([M:ASL])>0 : Close Local Mask [ASL] : Endif
Return

$RAZMASK
Case MA
 When 0 : If clalev([M:PHYM])>0 : Raz [M:PHYM] : Endif
 When 1 : If clalev([M:PHYM1])>0 : Raz [M:PHYM1] : Endif
 When 2 : If clalev([M:ASL])>0 : Raz [M:ASL] : Endif
Endcase
Return

$AFFMASK
