####################################
IMPTBL = 0

## Zone YRCPAMT
mkstat = 0
zoncou = "YRCPAMT"
indice = 0
If varmode([M]YRCPAMT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YRCPAMT" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YRCPAMT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YRCPAMT" : GZNUM = 100531 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YRPTDAT
