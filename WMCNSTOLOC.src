$AINIT
Gosub DECLARE_VAR From WMC0STOLOC
Local Integer ALINE,TBLINE,ASTASNAPSHOT,ALLSTAT
If this.APARENT=null & this.snapshotEnabled=0
 this.snapshotEnabled = 1
Elsif this.APARENT<>null
 this.snapshotEnabled = this.APARENT.snapshotEnabled
Endif
If this.ASTALIN=[V]CST_ALL : this.ASTALIN = [V]CST_ANEW : Endif
Gosub INIT_STOFCY From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_WRH From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_LOC From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_OCPCOD From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_LOCTYP From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_LOCCAT From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_LOCNUMFMT From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_PPSSEQ From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_MONITMFLG From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_DEDFLG From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_REAFLG From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_FRGMGTMOD From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_TEMLTI From WMC1STOLOC : Gosub TESTALLSTAT
Gosub INIT_FILMGTFLG From WMC1STOLOC : Gosub TESTALLSTAT
