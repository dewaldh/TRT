Funprog IS_IMPLEMENTED()
End 1
#---------------------------------------------------------------------------------------------------#
# List of tables to open
#---------------------------------------------------------------------------------------------------#
$ASEARCH_OPEN
If filinfo(filpath("FIL","SHIPMENTD","fde",-1),1) <0  : ERROR=[V]CST_AERROR : Return : Endif 
Local File SHIPMENTD [SHD]
Local File SHIPMENTD [SHD_]
Local Char KEY_SHD(120)(1..3)
ACTION = "ASEARCH_OPEN_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# List of tables to close
#---------------------------------------------------------------------------------------------------#
$ASEARCH_CLOSE
LogicClose File [SHD]
LogicClose File [SHD_]
ACTION = "ASEARCH_CLOSE_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# List of tables to join
#---------------------------------------------------------------------------------------------------#
$ASEARCH_JOIN
Link [SHD] With
& [SHD_]SHD0=[SHD]SHIPNUM;[SHD]SHIPLIN
& As [LNK_]
& Where (1=1
& )
ACTION = "ASEARCH_JOIN_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# List of tables to join
#---------------------------------------------------------------------------------------------------#
$ASEARCH_JOIN_INC
Link [SHD] With
& [SHD_]SHD0=[SHD]SHIPNUM;[SHD]SHIPLIN
& As [LNK_]
& Where (UPDDATTIM > gdatetime$(QUPDDATTIM)
& )
ACTION = "ASEARCH_JOIN_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# Create an instance of
#---------------------------------------------------------------------------------------------------#
$ASEARCH_CREATEINSTANCE
Local Integer ASTATUS
Local Integer FINS
#Create a new instance
INS = NewInstance C_SHIPMENTD(GACTX,UIDINS,NUMLIN,null,"") AllocGroup null
INS.setaccessorEnabled = 0
Return

#---------------------------------------------------------------------------------------------------#
# Set an instance
#---------------------------------------------------------------------------------------------------#
$ASEARCH_SETINSTANCE

SetInstance INS With [F:SHD]
FINS = 1
[L]ASTATUS = fmet INS.ACTION("ASEARCH_TRANS_AFTER")
Gosub ASEARCH_TO_JSON
Return

#---------------------------------------------------------------------------------------------------#
# Serialize the entity in a JSON format
#---------------------------------------------------------------------------------------------------#
$ASEARCH_TO_JSON
If FINS=0 : Return : Endif
QCOUNT += 1
Onerrgo ERROR_VALUE
Append ASND.DATA ,SEPINS1
SEPINS1 = ','+chr$(10)
Append ASND.DATA ,'{'
Append ASND.DATA ,'"SHIPNUM":'         +'"'+escJson(INS.SHIPNUM)+'"'
Append ASND.DATA ,',"SHIPLIN":'         + num$(INS.SHIPLIN)
Append ASND.DATA ,',"CTRNUM":'         +'"'+escJson(INS.CTRNUM)+'"'
Append ASND.DATA ,',"POHNUM":'         +'"'+escJson(INS.POHNUM)+'"'
Append ASND.DATA ,',"BPSNUM":'         +'"'+escJson(INS.BPSNUM)+'"'
Append ASND.DATA ,',"ITMREF":'         +'"'+escJson(INS.ITMREF)+'"'
Append ASND.DATA ,',"UOM":'            +'"'+escJson(INS.UOM)+'"'
Append ASND.DATA ,',"PUU":'            +'"'+escJson(INS.PUU)+'"'
Append ASND.DATA ,',"STU":'            +'"'+escJson(INS.STU)+'"'
Append ASND.DATA ,',"NETCUR":'         +'"'+escJson(INS.NETCUR)+'"'
Append ASND.DATA ,',"LINVOU":'         +'"'+escJson(INS.LINVOU)+'"'
Append ASND.DATA ,',"LINWEU":'         +'"'+escJson(INS.LINWEU)+'"'
Append ASND.DATA ,'}'
Onerrgo
FINS = 0
Return

#---------------------------------------------------------------------------------------------------#
# Errors
#---------------------------------------------------------------------------------------------------#
$ERROR_VALUE
SVALUE ='"<error'-errmes$(errn)+'>"'
Resume
#---------------------------------------------------------------------------------------------------#
# Link standard key
#---------------------------------------------------------------------------------------------------#
$AQUERY_PRIMARYKEYS
Local Char ASTDKEY(250)
ASTDKEY = ""
ASTDKEY += "[F:SHD]SHIPNUM"
ASTDKEY += ";[F:SHD]SHIPLIN"
ACTION = "ASEARCH_PRIMARYKEYS_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# Actions
#---------------------------------------------------------------------------------------------------#
$ASEARCH_ACTION
Local Char CURPRO(250),CURPTH(250),AMETHOD(50),AEVENT(50)
[L]AEVENT = [L]ACTION
Gosub METHODS From SHIPMENTD_CSTD
Return

