$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(10)(0..GNBDERNIER)
Local Char     A_BUF2(80)(0..GNBDERNIER)
Local Char     A_BUF3(3)(0..GNBDERNIER)
Local Char     A_BUF4(80)(0..GNBDERNIER)
Local Libelle  A_BUF5(0..GNBDERNIER)
If clalev([F:ATV9])>0 : Close Local File [ATV9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(10),
& Char     A_TAB2(80),
& Char     A_TAB3(3),
& Char     A_TAB4(80),
& Libelle  A_TAB5
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5
& As [ATV9]
Return

$LEC_DERLU
Return

$CHARGE_DERLU
If clalev([F:ATV9])>0
 Read [ATV9] = [F:ATV9]A_TABI
 [L]CLE = [F:ATV9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:ATV9])>0
 Filter [ATV9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [ATV1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:ATV9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:ATV]RAPORT+"~"
 A_CLEDERLU += [F:ATV]ABM+"~"
 A_CLEDERLU += [F:ATV]LANTRA+"~"
 A_CLEDERLU += [F:ATV]TEXTE+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:ATV9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:ATV9]
 Endif
Endif
Return

