# Supervisor
# Mask management PPF1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone PLI
mkstat = 0
zoncou = "PLI"
indice = 0
If varmode([M]PLI,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PLI" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("PPC","!",[M]PLI,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "PLI" : GZNUM = 2652 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone PLICRD
mkstat = 0
zoncou = "PLICRD"
indice = 0
If varmode([M]PLICRD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PLICRD" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone PLISTRDAT
mkstat = 0
zoncou = "PLISTRDAT"
indice = 0
If varmode([M]PLISTRDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PLISTRDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone PLIENDDAT
mkstat = 0
zoncou = "PLIENDDAT"
indice = 0
If varmode([M]PLIENDDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PLIENDDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "PPF1"
Return

