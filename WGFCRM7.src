$BOUTON
STDBOUT = "O"
NBTEX  = 0
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
NBMEN += 1
NMEN(NBMEN) = 10 : Call TEXTE(1042,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : Call TEXTE(12966,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : CMEN(NBMEN) = ")" : LMEN(NBMEN) = 2001 : Call TEXTE(14423,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : CMEN(NBMEN) = "(" : LMEN(NBMEN) = 2002 : Call TEXTE(13606,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : CMEN(NBMEN) = "£" : LMEN(NBMEN) = 2003 : Call TEXTE(1195,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : Call TEXTE(12413,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : CMEN(NBMEN) = "}" : LMEN(NBMEN) = 2004 : Call TEXTE(14423,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : CMEN(NBMEN) = "{" : LMEN(NBMEN) = 2005 : Call TEXTE(13606,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : CMEN(NBMEN) = "~" : LMEN(NBMEN) = 2006 : Call TEXTE(1195,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : CMEN(NBMEN) = "!" : LMEN(NBMEN) = 2007 : Call TEXTE(18260,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 11 : CMEN(NBMEN) = "%" : LMEN(NBMEN) = 2008 : Call TEXTE(14603,TMEN(NBMEN)) From OBJDIV
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When ")"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GBIDC3
  PARBOUT(2) = "BAP"
  PARBOUT(3) = ""
  FLGEXE = 1
  GACTION = "GOBJETC1"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  GBIDC3 = PARBOUT(1)
 When "("
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GBIDC3
  PARBOUT(2) = "CLL"
  PARBOUT(3) = ""
  FLGEXE = 1
  GACTION = "GOBJETC2"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  GBIDC3 = PARBOUT(1)
 When "£"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GBIDC3
  PARBOUT(2) = "TSK"
  PARBOUT(3) = ""
  FLGEXE = 1
  GACTION = "GOBJETC3"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  GBIDC3 = PARBOUT(1)
 When "}"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GBIDC3
  PARBOUT(2) = "BAP"
  PARBOUT(3) = ""
  FLGEXE = 1
  GACTION = "GOBJETC1"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  GBIDC3 = PARBOUT(1)
 When "{"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GBIDC3
  PARBOUT(2) = "CLL"
  PARBOUT(3) = ""
  FLGEXE = 1
  GACTION = "GOBJETC2"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  GBIDC3 = PARBOUT(1)
 When "~"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GBIDC3
  PARBOUT(2) = "TSK"
  PARBOUT(3) = ""
  FLGEXE = 1
  GACTION = "GOBJETC3"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  GBIDC3 = PARBOUT(1)
 When "!"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GBIDC3
  PARBOUT(2) = "OPP"
  PARBOUT(3) = ""
  FLGEXE = 1
  GACTION = "GOBJETC4"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  GBIDC3 = PARBOUT(1)
 When "%"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GBIDC3
  PARBOUT(2) = "SRE"
  PARBOUT(3) = ""
  FLGEXE = 1
  GACTION = "GOBJETC1"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  GBIDC3 = PARBOUT(1)
Endcase
GACTION = SAVACT
Return

