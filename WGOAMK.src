$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "V"
  FLGEXE = 1
  GACTION = "STD"
 When "2"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "AMK"
  PARBOUT(2) = [M]CODMSK
  PARBOUT(3) = ""
  PARBOUT(4) = ""
  PARBOUT(5) = ""
  FLGEXE = 1
  GACTION = "ACOPIE"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ACOPIE","ACOPIE",3,"XYACOPIE") From GTRAITE
 When "3"
  FLGEXE = 1
  GACTION = "STD"
 When "6"
  FLGEXE = 1
  GACTION = "AVISUVT"
  Call VISUVT([M:AMK0]CODMSK) From SUBAMK
 When "7"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "AICONE"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"AICONE","AICONE","") From GSAISIE
 When "8"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "PYRELT"
  PARBOUT(2) = "ASB"
  PARBOUT(3) = "MIS"
  FLGEXE = 1
  GACTION = "GOBJETC0"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
 When "4"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = nomap
  FLGEXE = 1
  GACTION = "ACTLPARMSK"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ACTLPARMSK","",1,"") From GTRAITE
Endcase
GACTION = SAVACT
Return

