# Supervisor
## BI-DIMENSION ## Synchronization rule ACCLIN

$OUVRE
Local Char FILTRE(250)
OUVTAB = GDOSSIER+".GACCENTRYD" : If clalev([F:DAE])=0 : Local File =OUVTAB [DAE] : Endif
OUVTAB = GDOSSIER+".GACCENTRYD" : If clalev([F:A_])=0 : Local File =OUVTAB [A_] : Endif
OUVTAB = "Link [DAE]"
Link [DAE] With
& [A_]DAE0=[DAE]TYP;[DAE]NUM;[DAE]LIN;[DAE]LEDTYP
& As [LNK_]
& Order By Key CLE = [DAE]TYP;[DAE]NUM;[DAE]LIN;[DAE]LEDTYP
FILTRE = ""
Return

$FERME
If clalev([F:DAE]) : Close local File [DAE] : Endif
If clalev([F:A_]) : Close local File [A_] : Endif
Return

$DCLE
Local Char    DCLE(GLONAVA)(1..4)
Local Integer DTYP(1..4)
Local Integer NBDCLE
NBDCLE = 4
DCLE(1) = "TYP"
DTYP(1) =  7
DCLE(2) = "NUM"
DTYP(2) =  7
DCLE(3) = "LEDTYP"
DTYP(3) =  1
DCLE(4) = "NUMLIG"
DTYP(4) =  7
Return

$CHARGE_CLE
If dim([F:AAA_]TYP)>0
 [F:AAA_]TYP = [F:DAE]TYP
Endif
If dim([F:AAA_]NUM)>0
 [F:AAA_]NUM = [F:DAE]NUM
Endif
If dim([F:AAA_]LEDTYP)>0
 [F:AAA_]LEDTYP = [F:DAE]LEDTYP
Endif
If dim([F:AAA_]NUMLIG)>0
 [F:AAA_]NUMLIG = [F:DAE]LIN
Endif
Return

########################################################################
# Return the filter necessary for incremental delete in synchroonization
Subprog GET_FLTDELINC(PFLT)
Variable Clbfile PFLT
PFLT = "1=1"
Append PFLT, " & [F:AAA_]TYP=[F:DAE]TYP"
Append PFLT, " & [F:AAA_]NUM=[F:DAE]NUM"
Append PFLT, " & [F:AAA_]NUMLIG=[F:DAE]LIN"
Append PFLT, " & [F:AAA_]LEDTYP=[F:DAE]LEDTYP"

End

