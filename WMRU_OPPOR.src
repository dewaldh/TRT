############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(OPPOR,ASTATUS)
Variable Instance OPPOR Using R_OPPOR
Variable Integer ASTATUS

If OPPOR = null : End : Endif

Gosub AVARIABLES From WMR00OPPOR
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet OPPOR.ACTX.AGETAFCRIGHT(OPPOR,"GESOPP",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet OPPOR.ACTX.AGETAFCRIGHTU(OPPOR,"GESOPP",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_OPPOR
Else
 Gosub AUPDATE_OPPOR
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = OPPOR.freeSnapshot
OPPOR.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
OPPOR.ASTALIN = [V]CST_AUPD
OPPOR.OPP.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = OPPOR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet OPPOR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = OPPOR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet OPPOR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet OPPOR._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet OPPOR._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet OPPOR._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

