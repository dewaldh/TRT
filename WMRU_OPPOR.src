$_AUPDATE_AFTER
If find(OPPOR.OPP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 OPPOR.OPP.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
A_N2=1
