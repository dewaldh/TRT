    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.SBB(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.SBB(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If OPPOR.OPP.PIMPL(A_N2) <> null
& and OPPOR.OPP.PIMPL(A_N2).ASTALIN<>[V]CST_ANEWDEL
 If OPPOR.OPP.PIMPL(A_N2).ASTALIN<>[V]CST_ANEW & OPPOR.OPP.ASTALIN=[V]CST_ADEL
  OPPOR.OPP.PIMPL(A_N2).ASTALIN = [V]CST_ADEL
 Endif
 If OPPOR.OPP.PIMPL(A_N2).ASTALIN=[V]CST_ANEW & find(OPPOR.OPP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
  OPPOR.OPP.PIMPL(A_N2).ASTALIN = [V]CST_ANEWDEL
 Endif
 If [V]CST_ASTALIN(OPPOR.OPP.PIMPL(A_N2).ASTALIN)<>""
  Case [V]CST_ASTALIN(OPPOR.OPP.PIMPL(A_N2).ASTALIN)
   When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.PIMPL(A_N2)._AINSERT_CONTROL_BEFORE)
   When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.PIMPL(A_N2)._AUPDATE_CONTROL_BEFORE)
   When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.PIMPL(A_N2)._ADELETE_CONTROL_BEFORE)
  Endcase
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If OPPOR.OPP.PIMPL(A_N2).ASTALIN<>[V]CST_ADEL & OPPOR.OPP.PIMPL(A_N2).ASTALIN<>[V]CST_ANEWDEL
