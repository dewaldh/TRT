 If OPPOR.OPP.CRM(A_N2).ASTALIN=[V]CST_ADEL or OPPOR.OPP.CRM(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance OPPOR.OPP.CRM(A_N2)
  OPPOR.OPP.CRM(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
