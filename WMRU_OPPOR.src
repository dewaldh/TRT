   When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.PIMPL(A_N2)._ADELETE_CONTROL_AFTER)
  Endcase
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
