$_AUPDATE_CONTROL_BEFORE
If OPPOR.OPP.ASTALIN = [V]CST_ANEWDEL : Return : Endif
OPPOR.OPP.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(OPPOR.OPP.ASTALIN)<>""
 [L]A_ASTALIN = OPPOR.OPP.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
A_N2=1
If OPPOR.OPP.CRM(A_N2) <> null
& and OPPOR.OPP.CRM(A_N2).ASTALIN<>[V]CST_ANEWDEL
 If OPPOR.OPP.CRM(A_N2).ASTALIN<>[V]CST_ANEW & OPPOR.OPP.ASTALIN=[V]CST_ADEL
  OPPOR.OPP.CRM(A_N2).ASTALIN = [V]CST_ADEL
 Endif
 If OPPOR.OPP.CRM(A_N2).ASTALIN=[V]CST_ANEW & find(OPPOR.OPP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
  OPPOR.OPP.CRM(A_N2).ASTALIN = [V]CST_ANEWDEL
 Endif
 If [V]CST_ASTALIN(OPPOR.OPP.CRM(A_N2).ASTALIN)<>""
  Case [V]CST_ASTALIN(OPPOR.OPP.CRM(A_N2).ASTALIN)
   When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.CRM(A_N2)._AINSERT_CONTROL_BEFORE)
   When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.CRM(A_N2)._AUPDATE_CONTROL_BEFORE)
   When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.CRM(A_N2)._ADELETE_CONTROL_BEFORE)
  Endcase
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(OPPOR.OPP.CPP) : # OPPORCPP -- 3
 If OPPOR.OPP.CPP(A_N2) <> null
& and OPPOR.OPP.CPP(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If OPPOR.OPP.CPP(A_N2).ASTALIN<>[V]CST_ANEW & OPPOR.OPP.ASTALIN=[V]CST_ADEL
