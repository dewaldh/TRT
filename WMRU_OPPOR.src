    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.CPP(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.CPP(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
