  OPPOR.OPP.PJM(A_N2).ASTALIN = [V]CST_ANEWDEL
 Endif
 If [V]CST_ASTALIN(OPPOR.OPP.PJM(A_N2).ASTALIN)<>""
  Case [V]CST_ASTALIN(OPPOR.OPP.PJM(A_N2).ASTALIN)
   When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.PJM(A_N2)._AINSERT_CONTROL_BEFORE)
   When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.PJM(A_N2)._AUPDATE_CONTROL_BEFORE)
   When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.PJM(A_N2)._ADELETE_CONTROL_BEFORE)
  Endcase
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
 For [L]A_I=1 To maxtab(OPPOR.OPP.PJM(A_N2).CDIE)
  If OPPOR.OPP.PJM(A_N2).CDIE([L]A_I)<>null
   [L]A_ASTALIN = OPPOR.OPP.PJM(A_N2).CDIE([L]A_I).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet OPPOR.OPP.PJM(A_N2).CDIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Next
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

