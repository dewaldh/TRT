  If OPPOR.OPP.CPP(A_N2).ASTALIN=[V]CST_ANEW & find(OPPOR.OPP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   OPPOR.OPP.CPP(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(OPPOR.OPP.CPP(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPOR.OPP.CPP(A_N2).ASTALIN)
