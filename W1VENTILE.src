Subprog AM_DSP(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZDSP)")>indice
 [M]DSP(indice) = VALEUR
 Affzo ZDSP(indice)
Endif
Gosub _AFF
End

Subprog S_DSP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
