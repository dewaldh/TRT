Subprog AS_DSP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(4805,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_DSP(VALEUR) From SAIVENTILE
If mkstat=2
 [M]DSP(INDIC) = VALEUR
 Affzo DSP(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_DSP(zc) From W1VENTILE
Onkey   2 Titled GBOUTS Call S_DSP(zc) From W1VENTILE
Onkey 144 Titled GBOUTP Call P_DSP(zc) From W1VENTILE
Onkey 146 Titled GBOUTA Call SA_DSP(zc) From W1VENTILE
Gosub _AFF
End

Subprog C_DSP(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("DSP","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 3022 : Endif
If !mkstat
 ## Action STD
 Call C_DSP(VALEUR) From SAIVENTILE
 If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 3022 : Endif
Endif
Gosub _AFF
End

Subprog AP_DSP(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AP_DSP(VALEUR) From SAIVENTILE
If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 3022 : Endif
Gosub _AFF
End

