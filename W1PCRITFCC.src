 SAVACT = GACTION : GACTION = "SELTRPFACC"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELTRPFACC","TRTACHSEL1","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_WTRNNUM(VALEUR)
Variable Char    VALEUR()
Call OBJET("TRNP",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_WTRNNUM(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TRNP"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_WTRNNUM(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TRNP",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_WITMREF
Onevent AFTER_CHANGE Gosub AMM_WITMREF From W1PCRITFCC
Onevent BEFORE_INPUT Call AS_WITMREF(zc) From W1PCRITFCC
Onevent BEFORE_AREA Gosub AV_WITMREF From W1PCRITFCC
Return

Subprog IB_WITMREF(VALEUR)
Variable Char    VALEUR()
## Action STD
Call IB_WITMREF From SUBPCF1
Gosub _AFF
End

$AV_WITMREF
If "K:"+num$(GLONITM)+"X"<>""
 Chgfmt [M]WITMREF(indice) With "K:"+num$(GLONITM)+"X"
Else
 Chgfmt [M]WITMREF(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog AS_WITMREF(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(735,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call IB_WITMREF(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_WITMREF(zc) From W1PCRITFCC
Onkey   2 Titled GBOUTS Call S_WITMREF(zc) From W1PCRITFCC
Gosub _AFF
End

$AMM_WITMREF
Call AM_WITMREF(zc)
If !mkstat & GOBJET
 zoncou = "WITMREF" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_WITMREF(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_WITMREF(VALEUR) From SUBPCF1
If mkstat : GERR = 1 : GZONE = "WITMREF" : GZNUM = 3651 : Endif
Gosub _AFF
End

Subprog S_WITMREF(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELITM
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELITM"
 PARMSK(1) = ""
 PARMSK(2) = "!"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELITM","TRTACHSEL1","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_WITMREF(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action GOBJET
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GOBJET"
 Call OBJET_CHAR(VALEUR,"ITM","") From GOBJET
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$CHG_STY
Return


