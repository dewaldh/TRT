Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone TYPGRA
mkstat = 0
zoncou = "TYPGRA"
indice = 0
IMPENT = [M]TYPGRA
If varinit([M]TYPGRA,indice)=0
 IMPENT = 1
Endif
If varmode([M]TYPGRA,indice)=0
 Call INICOMBO(2932) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_TYPGRA(IMPENT) From SUBALH
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2932,1))))
 Endif
 [M]TYPGRA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYPGRA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TYPGRA(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "TYPGRA" : GZNUM = 680 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call AP_TYPGRA(IMPENT) From SUBALH
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "TYPGRA" : GZNUM = 680 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]TYPGRA = IMPENT

