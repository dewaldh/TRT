############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(TIMEADJUST,ASTATUS)
Variable Instance TIMEADJUST Using R_TIMEADJUST
Variable Integer ASTATUS

If TIMEADJUST = null : End : Endif

Gosub AVARIABLES From WMR00TIMEADJUST
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_TIMEADJUST
Else
 Gosub AUPDATE_TIMEADJUST
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = TIMEADJUST.freeSnapshot
TIMEADJUST.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
TIMEADJUST.ASTALIN = [V]CST_AUPD
TIMEADJUST.TAD.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = TIMEADJUST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet TIMEADJUST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = TIMEADJUST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet TIMEADJUST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet TIMEADJUST._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet TIMEADJUST._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet TIMEADJUST._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If TIMEADJUST.TAD.ASTALIN = [V]CST_ANEWDEL : Return : Endif
TIMEADJUST.TAD.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(TIMEADJUST.TAD.ASTALIN)<>""
 [L]A_ASTALIN = TIMEADJUST.TAD.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet TIMEADJUST.TAD.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
A_N2=1
If TIMEADJUST.TAD.TTRSTR(A_N2) <> null
& and TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN<>[V]CST_ADEL & TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(TIMEADJUST.TAD,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"TIMETRANS:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
A_N2=1
If TIMEADJUST.TAD.TTREND(A_N2) <> null
& and TIMEADJUST.TAD.TTREND(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If TIMEADJUST.TAD.TTREND(A_N2).ASTALIN<>[V]CST_ADEL & TIMEADJUST.TAD.TTREND(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(TIMEADJUST.TAD,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"TIMETRANS:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
A_N2=1
If TIMEADJUST.TAD.TTRSTR(A_N2) <> null
& and TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN<>[V]CST_ADEL & TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(TIMEADJUST.TAD,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"TIMETRANS:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
A_N2=1
If TIMEADJUST.TAD.TTREND(A_N2) <> null
& and TIMEADJUST.TAD.TTREND(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If TIMEADJUST.TAD.TTREND(A_N2).ASTALIN<>[V]CST_ADEL & TIMEADJUST.TAD.TTREND(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(TIMEADJUST.TAD,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"TIMETRANS:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

If [V]CST_ASTALIN(TIMEADJUST.TAD.ASTALIN)<>""
 [L]A_ASTALIN = TIMEADJUST.TAD.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet TIMEADJUST.TAD.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_TIMEADJUST
Trbegin [F:TAD]
 Gosub AUPDATE_TIMEADJUST
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(TIMEADJUST.TAD.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 TIMEADJUST.TAD.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
A_N2=1
If TIMEADJUST.TAD.TTRSTR(A_N2) <> null
 [L]ASTATUS2 = fmet TIMEADJUST.TAD.TTRSTR(A_N2)._AUPDATE_AFTER
 If TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN=[V]CST_ADEL or TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance TIMEADJUST.TAD.TTRSTR(A_N2)
  TIMEADJUST.TAD.TTRSTR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
A_N2=1
If TIMEADJUST.TAD.TTREND(A_N2) <> null
 [L]ASTATUS2 = fmet TIMEADJUST.TAD.TTREND(A_N2)._AUPDATE_AFTER
 If TIMEADJUST.TAD.TTREND(A_N2).ASTALIN=[V]CST_ADEL or TIMEADJUST.TAD.TTREND(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance TIMEADJUST.TAD.TTREND(A_N2)
  TIMEADJUST.TAD.TTREND(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TIMEADJUST
[L]A_ASTALIN = TIMEADJUST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet TIMEADJUST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet TIMEADJUST.TAD._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = TIMEADJUST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet TIMEADJUST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If TIMEADJUST.snapshotEnabled=1
 [L]A_RETIK = TIMEADJUST.revertUpdTick
Endif
Local Integer ASTATUS2
A_N2=1
If TIMEADJUST.TAD.TTRSTR(A_N2) <> null
 [L]ASTATUS2 = fmet TIMEADJUST.TAD.TTRSTR(A_N2)._AUPDATE_ABORT
 If TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN=[V]CST_ADEL or TIMEADJUST.TAD.TTRSTR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance TIMEADJUST.TAD.TTRSTR(A_N2)
  TIMEADJUST.TAD.TTRSTR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
A_N2=1
If TIMEADJUST.TAD.TTREND(A_N2) <> null
 [L]ASTATUS2 = fmet TIMEADJUST.TAD.TTREND(A_N2)._AUPDATE_ABORT
 If TIMEADJUST.TAD.TTREND(A_N2).ASTALIN=[V]CST_ADEL or TIMEADJUST.TAD.TTREND(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance TIMEADJUST.TAD.TTREND(A_N2)
  TIMEADJUST.TAD.TTREND(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif

[L]ASTATUS = max(fmet TIMEADJUST.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



