$BOUTON
STDBOUT = "O"
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "B" : LBOUT(NBTEX) = 1101 : Call TEXTE(18703,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "C" : LBOUT(NBTEX) = 1102 : Call TEXTE(19076,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "B"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:MGG0]QURNUM
  PARBOUT(2) = [M:MGG0]TGR
  PARBOUT(3) = "Y"
  PARBOUT(4) = [M:MGG0]TGPNUM
  FLGEXE = 1
  GACTION = "SAIMGC1"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"FMGC1","TRTMGC1","XWFMGC1") From GSAISIE
 When "C"
  FLGEXE = 1
  GACTION = "STD"
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


