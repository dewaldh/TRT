############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(FACILITY,ASTATUS)
Variable Instance FACILITY Using R_FACILITY
Variable Integer ASTATUS

If FACILITY = null : End : Endif

Gosub AVARIABLES From WMR00FACILITY
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet FACILITY.ACTX.AGETAFCRIGHT(FACILITY,"GESFCY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet FACILITY.ACTX.AGETAFCRIGHTU(FACILITY,"GESFCY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_FACILITY
Else
 Gosub AUPDATE_FACILITY
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = FACILITY.freeSnapshot
FACILITY.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
FACILITY.ASTALIN = [V]CST_AUPD
FACILITY.FCY.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = FACILITY.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet FACILITY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = FACILITY.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet FACILITY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet FACILITY._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet FACILITY._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet FACILITY._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If FACILITY.FCY.ASTALIN = [V]CST_ANEWDEL : Return : Endif
FACILITY.FCY.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(FACILITY.FCY.ASTALIN)<>""
 [L]A_ASTALIN = FACILITY.FCY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(FACILITY.FCY.BPA) : # BPADDRESS -- 2
 If FACILITY.FCY.BPA(A_N2) <> null
& and FACILITY.FCY.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If FACILITY.FCY.BPA(A_N2).ASTALIN<>[V]CST_ANEW & FACILITY.FCY.ASTALIN=[V]CST_ADEL
   FACILITY.FCY.BPA(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If FACILITY.FCY.BPA(A_N2).ASTALIN=[V]CST_ANEW & find(FACILITY.FCY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   FACILITY.FCY.BPA(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(FACILITY.FCY.BPA(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = FACILITY.FCY.BPA(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BPA(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
  For [L]A_I=1 To maxtab(FACILITY.FCY.BPA(A_N2).COLTEL)
   If FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I)<>null
    [L]A_ASTALIN = FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
  For [L]A_I=1 To maxtab(FACILITY.FCY.BPA(A_N2).COLWEB)
   If FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I)<>null
    [L]A_ASTALIN = FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(FACILITY.FCY.BID) : # BID -- 3
 If FACILITY.FCY.BID(A_N2) <> null
& and FACILITY.FCY.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If FACILITY.FCY.BID(A_N2).ASTALIN<>[V]CST_ANEW & FACILITY.FCY.ASTALIN=[V]CST_ADEL
   FACILITY.FCY.BID(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If FACILITY.FCY.BID(A_N2).ASTALIN=[V]CST_ANEW & find(FACILITY.FCY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   FACILITY.FCY.BID(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(FACILITY.FCY.BID(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(FACILITY.FCY.BID(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BID(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BID(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BID(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(FACILITY.FCY.BPA) : # BPADDRESS -- 2
 If FACILITY.FCY.BPA(A_N2) <> null
& and FACILITY.FCY.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(FACILITY.FCY.BPA(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = FACILITY.FCY.BPA(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BPA(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
  For [L]A_I=1 To maxtab(FACILITY.FCY.BPA(A_N2).COLTEL)
   If FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I)<>null
    [L]A_ASTALIN = FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
  For [L]A_I=1 To maxtab(FACILITY.FCY.BPA(A_N2).COLWEB)
   If FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I)<>null
    [L]A_ASTALIN = FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(FACILITY.FCY.BID) : # BID -- 3
 If FACILITY.FCY.BID(A_N2) <> null
& and FACILITY.FCY.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(FACILITY.FCY.BID(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(FACILITY.FCY.BID(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BID(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BID(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.BID(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(FACILITY.FCY.ASTALIN)<>""
 [L]A_ASTALIN = FACILITY.FCY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_FACILITY
Trbegin [F:FCY]
 Gosub AUPDATE_FACILITY
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(FACILITY.FCY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 FACILITY.FCY.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(FACILITY.FCY.BPA)
 If FACILITY.FCY.BPA(A_N2) <> null
  If FACILITY.FCY.BPA(A_N2).ASTALIN=[V]CST_ADEL or FACILITY.FCY.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance FACILITY.FCY.BPA(A_N2)
   FACILITY.FCY.BPA(A_N2) = null
  Elsif FACILITY.FCY.BPA(A_N2).ASTALIN <> [V]CST_ALL
   FACILITY.FCY.BPA(A_N2).ASTALIN = [V]CST_ALL
   For [L]A_I=1 To maxtab(FACILITY.FCY.BPA(A_N2).COLTEL)
    If FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I)<>null
     If FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
      FreeInstance FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I)
      FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I) = null
     Elsif FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
      FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
     Endif
    Endif
   Next
   If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
   For [L]A_I=1 To maxtab(FACILITY.FCY.BPA(A_N2).COLWEB)
    If FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I)<>null
     If FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
      FreeInstance FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I)
      FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I) = null
     Elsif FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
      FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
     Endif
    Endif
   Next
   If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(FACILITY.FCY.BID)
 If FACILITY.FCY.BID(A_N2) <> null
  [L]ASTATUS2 = fmet FACILITY.FCY.BID(A_N2)._AUPDATE_AFTER
  If FACILITY.FCY.BID(A_N2).ASTALIN=[V]CST_ADEL or FACILITY.FCY.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance FACILITY.FCY.BID(A_N2)
   FACILITY.FCY.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_FACILITY
[L]A_ASTALIN = FACILITY.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet FACILITY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet FACILITY.FCY._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = FACILITY.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet FACILITY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If FACILITY.snapshotEnabled=1
 [L]A_RETIK = FACILITY.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(FACILITY.FCY.BPA)
 If FACILITY.FCY.BPA(A_N2) <> null
  If FACILITY.FCY.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FACILITY.FCY.BPA(A_N2).ASTALIN = [V]CST_ANEW
   For [L]A_I=1 To maxtab(FACILITY.FCY.BPA(A_N2).COLTEL)
    If FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I)<>null
     If FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
      FACILITY.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ANEW
     Endif
    Endif
   Next
   If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
   For [L]A_I=1 To maxtab(FACILITY.FCY.BPA(A_N2).COLWEB)
    If FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I)<>null
     If FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
      FACILITY.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ANEW
     Endif
    Endif
   Next
   If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(FACILITY.FCY.BID)
 If FACILITY.FCY.BID(A_N2) <> null
  [L]ASTATUS2 = fmet FACILITY.FCY.BID(A_N2)._AUPDATE_ABORT
  If FACILITY.FCY.BID(A_N2).ASTALIN=[V]CST_ADEL or FACILITY.FCY.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance FACILITY.FCY.BID(A_N2)
   FACILITY.FCY.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet FACILITY.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



