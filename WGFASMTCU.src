$SETCUR
Return

$DEFVAR
Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "FASMTC0"      : ABMSK(0)  = "OAC0"     : AMA(0) = 1
NOMMSK(1) = "FASMTC"       : ABMSK(1)  = "FASM"     : AMA(1) = 1
NBMASK = 1
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 0 : If clalev([M:OAC0])>0 : Default Mask [OAC0] : Endif
 When 1 : If clalev([M:FASM])>0 : Default Mask [FASM] : Endif
Endcase
Return

$TITMASK
Case MA
 When 0 : If clalev([M:OAC0])>0 : Gosub TITRES From W1FASMTC0 : Endif
 When 1 : If clalev([M:FASM])>0 : Gosub TITRES From W1FASMTC : Endif
Endcase
Return

$FERME_MASK
If clalev([M:OAC0])>0 : Close Local Mask [OAC0] : Endif
If clalev([M:FASM])>0 : Close Local Mask [FASM] : Endif
Return

$RAZMASK
Case MA
 When 0 : If clalev([M:OAC0])>0 : Raz [M:OAC0] : Endif
 When 1 : If clalev([M:FASM])>0 : Raz [M:FASM] : Endif
Endcase
Return

$AFFMASK
Case MA
 When 0 : If clalev([M:OAC0])>0 : Affzo [M:OAC0] : Endif
 When 1 : If clalev([M:FASM])>0 : Affzo [M:FASM] : Endif
Endcase
Return

$OUVMSKCMP
Return

