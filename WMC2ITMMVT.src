####################################################################
# 31 : SCCALL Allocated ( Field QTY 0)
####################################################################

$SET_SCCALL
If [L]_AOLDVAL<>this.SCCALL
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_SCCALL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.SCCALL
 Gosub PROPAGATE_SCCALL
Endif
Return

$INIT_SCCALL
Gosub DECLARE_VAR From WMC0ITMMVT
[L]CURPRO = "SCCALL"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_SCCALL
Gosub DECLARE_VAR From WMC0ITMMVT
[L]CURPRO = "SCCALL"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_SCCALL                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_SCCALL                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_SCCALL
[L]CURPRO = "SCCALL"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.SCCALL=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_SCCALL
[L]CURPRO = "SCCALL"
Return

$_AOTHERS_SCCALL
[L]CURPRO = "SCCALL"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_SCCALL
[L]CURPRO = "SCCALL"
Return

$PROPAGATE_SCCALL
this.isReadonly = 0
[L]CURPRO = "SCCALL"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_SCCALL
Gosub DECLARE_VAR From WMC0ITMMVT
[L]CURPRO = "SCCALL"
$_LNKPRO_SCCALL
Return


