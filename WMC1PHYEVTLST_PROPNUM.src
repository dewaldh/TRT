####################################################################
# 1 : CODEN Code ( Field A 20)
####################################################################

$SET_CODEN
If [L]_AOLDVAL<>this.CODEN
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_CODEN
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.CODEN
 Gosub PROPAGATE_CODEN
Endif
Return

$INIT_CODEN
Gosub DECLARE_VAR From WMC0PHYEVTLST_PROPNUM
[L]CURPRO = "CODEN"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_CODEN
Gosub DECLARE_VAR From WMC0PHYEVTLST_PROPNUM
[L]CURPRO = "CODEN"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_CODEN                 : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_CODEN                 : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_CODEN
[L]CURPRO = "CODEN"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.CODEN,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_CODEN
[L]CURPRO = "CODEN"
Return

$_AOTHERS_CODEN
[L]CURPRO = "CODEN"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_CODEN
[L]CURPRO = "CODEN"
Return

$PROPAGATE_CODEN
this.isReadonly = 0
[L]CURPRO = "CODEN"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_CODEN
Gosub DECLARE_VAR From WMC0PHYEVTLST_PROPNUM
[L]CURPRO = "CODEN"
$_LNKPRO_CODEN
Return


