$AV_DATDEB
## Action ADATRELI
Call INIDAT(zc,[M:CBAN]DATFRMD) From ADATREL
Chgfmt [M]DATDEB(indice) With "DzZ:"+GFMDAT
Gosub _AFF
Return

Subprog AS_DATDEB(VALEUR)
Variable Date    VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(32514,GBOUT1) From OBJDIV
## Action STD
Call AS_DATDEB(VALEUR) From CNSBANSTD
If mkstat=2
 [M]DATDEB(INDIC) = VALEUR
 Affzo DATDEB(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_DATDEB(zc) From W1CONSBAN1
Gosub _AFF
End

$AMM_DATDEB
Call AM_DATDEB(zc)
If !mkstat & GOBJET
 zoncou = "DATDEB" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_DATDEB(VALEUR)
Variable Date    VALEUR
## Action ADATRELM
Call MODIFDAT(VALEUR,[M:CBAN]DATFRMD) From ADATREL
If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
Gosub _AFF
End

Subprog B1_DATDEB(VALEUR)
Variable Date    VALEUR
If !GWEBSERV
 ## Action ADATREL
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ADATREL"
 PARMSK(1) = [M:CBAN]DATFRMD
 Call TRAITE_DAT(VALEUR,PARMSK,"","ADATREL","ADATREL",3,"") From GTRAITE
 [M:CBAN]DATFRMD = PARMSK(1)
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

