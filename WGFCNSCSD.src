$DEFVAR
Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "CONSCSD1"     : ABMSK(0)  = "CSD1"     : AMA(0) = 1
NOMMSK(1) = "CONSCSD2"     : ABMSK(1)  = "CSD2"     : AMA(1) = 1
NBMASK = 1
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 0 : If clalev([M:CSD1])>0 : Default Mask [CSD1] : Endif
 When 1 : If clalev([M:CSD2])>0 : Default Mask [CSD2] : Endif
Endcase
Return

$TITMASK
Case MA
 When 0 : If clalev([M:CSD1])>0 : Gosub TITRES From W1CONSCSD1 : Endif
 When 1 : If clalev([M:CSD2])>0 : Gosub TITRES From W1CONSCSD2 : Endif
Endcase
Return

$FERME_MASK
If clalev([M:CSD1])>0 : Close Local Mask [CSD1] : Endif
If clalev([M:CSD2])>0 : Close Local Mask [CSD2] : Endif
Return

$RAZMASK
Case MA
 When 0 : If clalev([M:CSD1])>0 : Raz [M:CSD1] : Endif
 When 1 : If clalev([M:CSD2])>0 : Raz [M:CSD2] : Endif
Endcase
Return

$AFFMASK
Case MA
 When 0 : If clalev([M:CSD1])>0 : Affzo [M:CSD1] : Endif
 When 1 : If clalev([M:CSD2])>0 : Affzo [M:CSD2] : Endif
Endcase
Return

