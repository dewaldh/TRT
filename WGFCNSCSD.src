$BOUTON
STDBOUT = "RC-+D"
NBTEX  = 0
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
NBMEN += 1
NMEN(NBMEN) = 10 : Call TEXTE(30750,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "B" : LMEN(NBMEN) = 2001 : Call TEXTE(31208,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "C" : LMEN(NBMEN) = 2002 : Call TEXTE(31225,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "D" : LMEN(NBMEN) = 2003 : Call TEXTE(31237,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "E" : LMEN(NBMEN) = 2004 : Call TEXTE(31743,TMEN(NBMEN)) From OBJDIV
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "B"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "CCSD"
  PARBOUT(2) = num$(1)
  PARBOUT(3) = [M:CCSD]DLVDATDEB
  PARBOUT(4) = [M:CCSD]DLVDATFIN
  PARBOUT(5) = num$("0")
  PARBOUT(6) = num$("0")
  FLGEXE = 1
  GACTION = "VISUGRHSD1"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRHSDH1","FUNGRHSDH","XYFUNGRHSDH") From GSAISIE
  [M:CCSD]DLVDATDEB = PARBOUT(3)
  [M:CCSD]DLVDATFIN = PARBOUT(4)
 When "C"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "CCSD"
  PARBOUT(2) = num$(2)
  PARBOUT(3) = [M:CCSD]DLVDATDEB
  PARBOUT(4) = [M:CCSD]DLVDATFIN
  PARBOUT(5) = num$("0")
  PARBOUT(6) = num$("0")
  FLGEXE = 1
  GACTION = "VISUGRHSD3"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRHSDH3","FUNGRHSDH","XYFUNGRHSDH") From GSAISIE
  [M:CCSD]DLVDATDEB = PARBOUT(3)
  [M:CCSD]DLVDATFIN = PARBOUT(4)
 When "D"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "CCSD"
  PARBOUT(2) = num$(3)
  PARBOUT(3) = [M:CCSD]DLVDATDEB
  PARBOUT(4) = [M:CCSD]DLVDATFIN
  PARBOUT(5) = num$("0")
  PARBOUT(6) = num$("0")
  FLGEXE = 1
  GACTION = "VISUGRHSD3"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRHSDH3","FUNGRHSDH","XYFUNGRHSDH") From GSAISIE
  [M:CCSD]DLVDATDEB = PARBOUT(3)
  [M:CCSD]DLVDATFIN = PARBOUT(4)
 When "E"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "CCSD"
  PARBOUT(2) = num$(1)
  PARBOUT(3) = [M:CCSD]DLVDATDEB
  PARBOUT(4) = [M:CCSD]DLVDATFIN
  FLGEXE = 1
  GACTION = "VISUGRHSD2"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRHSDH2","FUNGRHSDH","XYFUNGRHSDH") From GSAISIE
  [M:CCSD]DLVDATDEB = PARBOUT(3)
  [M:CCSD]DLVDATFIN = PARBOUT(4)
Endcase
GACTION = SAVACT
Return

