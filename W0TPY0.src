#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management TPY0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone PAYTYP
mkstat = 0
zoncou = "PAYTYP"
indice = 0
If varmode([M]PAYTYP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PAYTYP" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]PAYTYP,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "PAYTYP" : GZNUM = 7981 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTTPY"
  Call CONTTPY([M]PAYTYP,"","") From SUBTPY
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "PAYTYP" : GZNUM = 7981 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTOBJN"
   Local Char    VALEUR(250)
   VALEUR = [M]PAYTYP
   Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJN","CONTOBJ","") From GACTION
   [M]PAYTYP = VALEUR
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "PAYTYP" : GZNUM = 7981 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "CONTTPYCHR"
    Call CONTTPYCHR([M]PAYTYP) From SUBTPY
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "PAYTYP" : GZNUM = 7981 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTOBJW"
     PARAM(1) = "TPY"
     Local Char    VALEUR(250)
     VALEUR = [M]PAYTYP
     Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
     [M]PAYTYP = VALEUR
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "PAYTYP" : GZNUM = 7981 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone LEG
mkstat = 0
zoncou = "LEG"
indice = 0
If varinit([M]LEG,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]LEG,"909","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]LEG,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "LEG" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]LEG,"909","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!",[M]LEG,"909",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_LEG([M]LEG) From SUBTPY
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone DESTRA
mkstat = 0
zoncou = "DESTRA"
indice = 0
If varinit([M]DESTRA,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DESTRA,"TABPAYTYP","DESTRA",[M:TPY0]PAYTYP,GCURLEG) From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DESTRA,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DESTRA" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]DESTRA,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "DESTRA" : GZNUM = 8 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone SHOTRA
mkstat = 0
zoncou = "SHOTRA"
indice = 0
If varinit([M]SHOTRA,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]SHOTRA,"TABPAYTYP","SHOTRA",[M:TPY0]PAYTYP,GCURLEG) From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]SHOTRA,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "SHOTRA" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]SHOTRA,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "SHOTRA" : GZNUM = 4010 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone GFY
mkstat = 0
zoncou = "GFY"
indice = 0
If varmode([M]GFY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "GFY" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("AGF","!",[M]GFY,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "GFY" : GZNUM = 616 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_GFY([M]GFY) From SUBTPY
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "GFY" : GZNUM = 616 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "TPY0"
Return

