#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:SOH])=0 : Local File SORDER [SOH] : Endif
If clalev([F:A_])=0 : Local File SORDER [A_] : Endif
If clalev([F:AFF])=0 : Local File AFCTFCY [AFF] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [SOH]
LogicClose File [A_]
LogicClose File [AFF]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"SOH","SOH",ACRITROLE,[V]CST_AFALSE)
Link [SOH] With
& [AFF]AFF0~= [F:SOH]SALFCY;GACTX.APRFCOD;[L]_AFCRIGHT
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
& and ([F:SOH]SOHCAT<>4 and [F:SOH]ORDSTA=1 and [F:SOH]UNL=[V]CST_ANO)
&  )
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"SOH","SOH",ACRITROLE,[V]CST_AFALSE)
Link [SOH] With
& [AFF]AFF0~= [F:SOH]SALFCY;GACTX.APRFCOD;[L]_AFCRIGHT
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
& and ([F:SOH]SOHCAT<>4 and [F:SOH]ORDSTA=1 and [F:SOH]UNL=[V]CST_ANO)
&  )
Return
 
#-------------------------------------------------------------------#
# Default filter for the query
#-------------------------------------------------------------------#
$AQUERY_FILTER_DEF
Return
 
#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# SOH --- SORDER(SOH)
QRY(NBREP) = NewInstance R_SOHEXCCDTM AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:SOH]AUUID)>0 & [F:SOH]AUUID=NullUuid
 [F:SOH]AUUID = getUuid
Endif
SetInstance QRY(NBREP).SOH With [F:SOH]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# SOH --- SORDER(SOH)
QRY = NewInstance R_SOHEXCCDTM AllocGroup PQRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:SOH]AUUID)>0 & [F:SOH]AUUID=NullUuid
 [F:SOH]AUUID = getUuid
Endif
SetInstance QRY.SOH With [F:SOH]
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

