Subprog AP_YBUSBAT(VALEUR)
Variable Integer VALEUR
#Check if default journal is a business batch
  If [M]DEFJOU <> "" Then
    Call C_YJOU([M]DEFJOU) From YBBSELJOU
  Endif
End

Subprog C_DEFJOU(VALEUR)
Variable Char    VALEUR()
#Check if Flag as Business batch, if yes, then check if default journal is also a business batch
  If [M]YBUSBAT = 2 Then
    Call C_YJOU([M]DEFJOU) From YBBSELJOU
  Endif
End


