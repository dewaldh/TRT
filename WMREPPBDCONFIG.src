REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "PBDCONFIG"
REP.CLA = "PBDCONFIG"
REP.ICL = "PBDCNF"
REP.AFCRIGHT = "GESPBDECO"
REP.NCL = 52640 : # Payment balance configuration
REP.DES = 52640 : # Payment balance configuration
REP.TIT = ""
REP.ANX = "PBDCNF0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Code
     REP.CLE(1).PAR = 'CODE' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'CODE' : REP.CLE(1).VPR= 'PBDCNF.CODE' : REP.CLE(1).DES = 5296
REP.CLE(2) = NewInstance C_AREPPAR AllocGroup REP : # Legislation
