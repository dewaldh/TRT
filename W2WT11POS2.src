# Supervisor
# Mask management WT11POS2 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_PCU
Return

$ON_STU
Return

$ON_QTY
Onevent BEFORE_AREA Gosub AV_QTY From W2WT11POS2
Onevent CONTROL Call C_QTY(zc) From W2WT11POS2
Onevent AFTER_CHANGE Gosub AMM_QTY From W2WT11POS2
Return

$AV_QTY
If !GWEBSERV
 ## Action FMTQTY
 GUNTFMT="15F" : GUNTFMT = GUNTFMT1(max(find([M]SAU(nolign-1),GUNITE(1..GNBUNT)),1))
Endif
Chgfmt [M]QTY(indice) With "N+z:"+GUNTFMT
Gosub _AFF
Return

Subprog C_QTY(VALEUR)
Variable Decimal VALEUR
## Action STD
Call C_QTY(VALEUR) From TRTPKP2
If mkstat : GERR = 1 : GZONE = "QTY" : GZNUM = 19856 : Endif
Gosub _AFF
End

$AMM_QTY
Call AM_QTY(zc)
If !mkstat & GOBJET
 zoncou = "QTY" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_QTY(VALEUR)
Variable Decimal VALEUR
## Action STD
Call AM_QTY(VALEUR) From TRTPKP2
If mkstat : GERR = 1 : GZONE = "QTY" : GZNUM = 19856 : Endif
Gosub _AFF
End

$ON_QTYSTU
Onevent BEFORE_AREA Gosub AV_QTYSTU From W2WT11POS2
Return

$AV_QTYSTU
If !GWEBSERV
 ## Action FMTQTY
 GUNTFMT="15F" : GUNTFMT = GUNTFMT1(max(find([M]STU(nolign-1),GUNITE(1..GNBUNT)),1))
Endif
Chgfmt [M]QTYSTU(indice) With "NPz:"+GUNTFMT
Gosub _AFF
Return

$ON_NETWEI
Onevent BEFORE_INPUT Call AS_NETWEI(zc) From W2WT11POS2
Onevent AFTER_CHANGE Gosub AMM_NETWEI From W2WT11POS2
Onevent CONTROL Call C_NETWEI(zc) From W2WT11POS2
Return

Subprog AS_NETWEI(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action STD
Call AS_NETWEI(VALEUR) From TRTPKP2
If mkstat=2
 [M]NETWEI(INDIC) = VALEUR
 Affzo NETWEI(INDIC) : mkstat = 2
Endif
Gosub _AFF
End

Subprog C_NETWEI(VALEUR)
Variable Decimal VALEUR
Gosub _AFF
End

$AMM_NETWEI
Call AM_NETWEI(zc)
If !mkstat & GOBJET
 zoncou = "NETWEI" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_NETWEI(VALEUR)
Variable Decimal VALEUR
## Action STD
Call AM_NETWEI(VALEUR) From TRTPKP2
If mkstat : GERR = 1 : GZONE = "NETWEI" : GZNUM = 3634 : Endif
Gosub _AFF
End

$ON_LIGWEU
Return

$ON_WSTOCOU
Onevent INIT_AREA Call D_WSTOCOU(zc) From W2WT11POS2
Return

Subprog D_WSTOCOU(VALEUR)
Variable Integer VALEUR
If varinit([M]WSTOCOU,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_SAU
Return

$ON_GROWEI
Onevent CONTROL Call C_GROWEI(zc) From W2WT11POS2
Return

Subprog C_GROWEI(VALEUR)
Variable Decimal VALEUR
Gosub _AFF
End

$ON_GROWEU
Return

$ON_ZGROWEU
Onevent BEFORE_AREA Gosub AV_ZGROWEU From W2WT11POS2
Return

$AV_ZGROWEU
Call LECTEXTRA(zc,"TABUNIT","DES",num$([M]GROWEU(indice)),"") From ATEXTRA
Gosub _AFF
Return

$CHG_STY
Return


