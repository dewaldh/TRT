$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(20)(0..GNBDERNIER)
Local Integer  A_BUF2(0..GNBDERNIER)
Local Char     A_BUF3(15)(0..GNBDERNIER)
Local Char     A_BUF4(5)(0..GNBDERNIER)
Local Char     A_BUF5(20)(0..GNBDERNIER)
Local Char     A_BUF6(20)(0..GNBDERNIER)
If clalev([F:SOC9])>0 : Close Local File [SOC9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(20),
& Integer  A_TAB2,
& Char     A_TAB3(15),
& Char     A_TAB4(5),
& Char     A_TAB5(20),
& Char     A_TAB6(20)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5,A_BUF6
& As [SOC9]
Return

$LEC_DERLU
Read [AFF]AFF0 = [F:SOC]SALFCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Return

$CHARGE_DERLU
If clalev([F:SOC9])>0
 Read [SOC9] = [F:SOC9]A_TABI
 [L]CLE = [F:SOC9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:SOC9])>0
 Filter [SOC9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [SOQ1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:SOC9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = num$([F:SOC]SOPLIN)+"~"
 A_CLEDERLU += [F:SOC]SOHNUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:SOC9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:SOC9]
 Endif
Endif
Return

