####################################################################
# 31 : LASCHGTYP Rate type ( Field Local menu number 202)
####################################################################

$SET_LASCHGTYP
If [L]_AOLDVAL<>this.LASCHGTYP
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_LASCHGTYP
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.LASCHGTYP
 Gosub PROPAGATE_LASCHGTYP
Endif
Return

$INIT_LASCHGTYP
Gosub DECLARE_VAR From WMC0SINCENT
[L]CURPRO = "LASCHGTYP"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If this.LASCHGTYP=0 and fmet this.AGETATTRIBUTE("LASCHGTYP","$isMandatory")="true"
 this.LASCHGTYP = 1
Endif
Return
 
$CONTROL_LASCHGTYP
Gosub DECLARE_VAR From WMC0SINCENT
[L]CURPRO = "LASCHGTYP"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_LASCHGTYP             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_LASCHGTYP             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_LASCHGTYP
[L]CURPRO = "LASCHGTYP"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.LASCHGTYP=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_LASCHGTYP
[L]CURPRO = "LASCHGTYP"
If this.LASCHGTYP<0 or this.LASCHGTYP>len(mess(0,202,1))
 # Pop / Invalid option
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(4,11,1)-"202 ->"-num$(this.LASCHGTYP)-mess(99,123,1)):  Return : # Local menu "202 -> Invalid option
Endif
Return

$_AOTHERS_LASCHGTYP
[L]CURPRO = "LASCHGTYP"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_LASCHGTYP
[L]CURPRO = "LASCHGTYP"
Return

$PROPAGATE_LASCHGTYP
this.isReadonly = 0
[L]CURPRO = "LASCHGTYP"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_LASCHGTYP
Gosub DECLARE_VAR From WMC0SINCENT
[L]CURPRO = "LASCHGTYP"
$_LNKPRO_LASCHGTYP
Return


