$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "A"
  FLGEXE = 1
  GACTION = "TRTSELSSL"
  Call ACT_SELSSL From TRTSSLA
 When "I"
  FLGEXE = 1
  GACTION = "TRTINCSSL"
  Call ACT_INCSSL From TRTSSLA
 When "J"
  FLGEXE = 1
  GACTION = "TRTEXCSSL"
  Call ACT_EXCSSL From TRTSSLA
 When "D"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "TRTCRISSL"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISSLC","TRTSSL","XWISSLC") From GSAISIE
 When "C"
  FLGEXE = 1
  GACTION = "TRTLISSSL"
  Call ACT_LISSSL From TRTSSLA
 When "E"
  FLGEXE = 1
  GACTION = "TRTENRSSL"
  Call ACT_ENRSSL From TRTSSLA
 When "G"
  FLGEXE = 1
  GACTION = "TRTTRISSL"
  Call ACT_TRISSL From TRTSSLA
 When "F"
  FLGEXE = 1
  GACTION = "TRTVALSSL"
  Call ACT_VALSSL From TRTSSLA
 When "H"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GSSLNUM
  FLGEXE = 1
  GACTION = "VISUSRG"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISRG","SELTRS","XWISRT") From GSAISIE
Endcase
GACTION = SAVACT
Return

