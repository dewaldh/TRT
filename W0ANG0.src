#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management ANG0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone COD
mkstat = 0
zoncou = "COD"
indice = 0
If varmode([M]COD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]COD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 5296 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "ANG"
  Local Char    VALEUR(250)
  VALEUR = [M]COD
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]COD = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 5296 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone ZDES
mkstat = 0
zoncou = "ZDES"
indice = 0
If varmode([M]ZDES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ZDES" : Gosub ACTION From =IMPTRT
  IMPMOD = 0 : IMPZON = "DES" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone FNC
mkstat = 0
zoncou = "FNC"
indice = 0
If varmode([M]FNC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FNC" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]FNC,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "FNC" : GZNUM = 17492 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("AFC","T",[M]FNC,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "FNC" : GZNUM = 17492 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone OBJ
mkstat = 0
zoncou = "OBJ"
indice = 0
If varmode([M]OBJ,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "OBJ" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]OBJ,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "OBJ" : GZNUM = 22181 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("AOB","",[M]OBJ,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "OBJ" : GZNUM = 22181 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_OBJ([M]OBJ) From SUBANG
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "OBJ" : GZNUM = 22181 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone MODULE
mkstat = 0
zoncou = "MODULE"
indice = 0
IMPENT = [M]MODULE
If varinit([M]MODULE,indice)=0
 IMPENT = 1
Endif
If varmode([M]MODULE,indice)=0
 [M]MODULE(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "MODULE" : Gosub ACTION From =IMPTRT
  IMPENT = [M]MODULE(indice)
 Endif
Endif
[M]MODULE = IMPENT

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
If varinit([M]ENAFLG,indice)=0
 IMPENT = 2
Endif
If varmode([M]ENAFLG,indice)=0
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
Endif
[M]ENAFLG = IMPENT

## Zone CODACT
mkstat = 0
zoncou = "CODACT"
indice = 0
If varmode([M]CODACT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CODACT" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ACV","!",[M]CODACT,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CODACT" : GZNUM = 4 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ANG0"
Return

