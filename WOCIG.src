$CHARGE_DERLU
If clalev([F:CIG9])>0
 Read [CIG9] = [F:CIG9]A_TABI
 [L]CLE = [F:CIG9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:CIG9])>0
 Filter [CIG9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [CIR1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:CIG9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:CIG]CIGCOD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:CIG9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:CIG9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 1
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:CIG]CIGCOD"
Call TEXTE(1371,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:10X"
SELFOR(NBSEL) = "K:10c"
SELMAJ(NBSEL) = 1
NBTRI += 1
TRIZON(NBTRI) = "[F:CIG]CIGCOD"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:CIG]DES"
Call TEXTE(282,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:60X"
SELFOR(NBSEL) = "K:60X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:CIG]DES"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:CIG_])=0
 Gosub OUVFIC_LISTE
Endif
Return

