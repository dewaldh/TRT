    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "LOCTYPDES" : GZNUM = 3197 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call C_LOCTYPDES([M]LOCTYPDES(indice)) From TRTSTOSMP
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "LOCTYPDES" : GZNUM = 3197 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

