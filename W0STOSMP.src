If varmode([M]WRHDES,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_WRHDES([M]WRHDES(indice)) From TRTSTOSMP
 GACTION = SAVACT
 If mkstat=1 : Raz [M]WRHDES(indice) : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "WRHDES" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "WRHCTL"
  Call WRHCTL([M]WRHDES(indice),[M:SMP]STOFCY,1) From STKWRH
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "WRHDES" : GZNUM = 281 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("WRH","!",[M]WRHDES(indice),"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "WRHDES" : GZNUM = 281 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_WRHDES([M]WRHDES(indice)) From TRTSTOSMP
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "WRHDES" : GZNUM = 281 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     If WCHAR<>[M]WRHDES(indice) or IMPMOD
      SAVACT = GACTION : GACTION = "STD"
      Call AM_WRHDES([M]WRHDES(indice)) From TRTSTOSMP
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "WRHDES" : GZNUM = 281 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

