$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Libelle  A_BUF1(0..GNBDERNIER)
Local Char     A_BUF2(10)(0..GNBDERNIER)
Local Char     A_BUF3(5)(0..GNBDERNIER)
Local Char     A_BUF4(80)(0..GNBDERNIER)
Local Libelle  A_BUF5(0..GNBDERNIER)
If clalev([F:APR9])>0 : Close Local File [APR9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Libelle  A_TAB1,
& Char     A_TAB2(10),
& Char     A_TAB3(5),
& Char     A_TAB4(80),
& Libelle  A_TAB5
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5
& As [APR9]
Return

$LEC_DERLU
Read [ACC]CODACC = GUSER ; [F:APR]CODACC
If fstat : Raz [F:ACC] : Endif
Read [AXX]AXX0="APROCESSUS";"INTIT1";GLANGUE;[F:APR]CODPRO;""
If fstat : Raz [F:AXX] : Endif
Return

$CHARGE_DERLU
If clalev([F:APR9])>0
 Read [APR9] = [F:APR9]A_TABI
 [L]CLE = [F:APR9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:APR9])>0
 Filter [APR9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [APR1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:APR9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:APR]CODPRO+"~"
 A_CLEDERLU += [F:APR]CODLEG+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:APR9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:APR9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 1
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:APR]TYPEP"
Call TEXTE(680,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 1
SELFMT(NBSEL) = "LA07855:15X"
SELFOR(NBSEL) = ""
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:APR]TYPEP"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:APR]CODPRO"
Call TEXTE(6446,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:10X"
SELFOR(NBSEL) = "K:10c"
SELMAJ(NBSEL) = 1
NBTRI += 1
TRIZON(NBTRI) = "[F:APR]CODPRO"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(3)"
SELEXP(NBSEL) = "string$([F:APR]CODLEG<>'',[F:APR]CODLEG)+string$([F:APR]CODLEG='',mess(236,143,1))"
Call TEXTE(26213,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:5X"
SELFOR(NBSEL) = "K:5X"
SELMAJ(NBSEL) = 0
NBSEL += 1
SELZON(NBSEL) = "[F:AXX]TEXTE"
Call TEXTE(8,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:80X"
SELFOR(NBSEL) = "K:80X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:AXX]TEXTE"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:APR]MODULE"
Call TEXTE(943,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 1
SELFMT(NBSEL) = "LA00014:20X"
SELFOR(NBSEL) = ""
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:APR]MODULE"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:APR_])=0
 Gosub OUVFIC_LISTE
Endif
Return

