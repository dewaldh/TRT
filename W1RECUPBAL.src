Subprog D_SOCIETE(VALEUR)
Variable Char    VALEUR()
If varinit([M]SOCIETE,indice)=0
 VALEUR = GDEFSOC
Endif
Gosub _AFF
End

Subprog AS_SOCIETE(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action INICPY
Call INICPY(VALEUR) From EXEFNC
If mkstat=2
 [M]SOCIETE(INDIC) = VALEUR
 Affzo SOCIETE(INDIC) : mkstat = 2
Endif
Onkey   2 Titled GBOUTS Call S_SOCIETE(zc) From W1RECUPBAL
Onkey 144 Titled GBOUTP Call P_SOCIETE(zc) From W1RECUPBAL
Onkey 146 Titled GBOUTA Call SA_SOCIETE(zc) From W1RECUPBAL
Gosub _AFF
End

Subprog C_SOCIETE(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "SOCIETE" : GZNUM = 1343 : Endif
If !mkstat
 ## Action ACONTCPY
 Call CONTSOC(VALEUR) From SUBAGF
 If mkstat : GERR = 1 : GZONE = "SOCIETE" : GZNUM = 1343 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("CPY","A",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "SOCIETE" : GZNUM = 1343 : Endif
  If !mkstat
   ## Action CONTCPY
   Call CONTCPY(VALEUR) From EXEFNC
   If mkstat : GERR = 1 : GZONE = "SOCIETE" : GZNUM = 1343 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

$AMM_SOCIETE
Call AM_SOCIETE(zc)
If !mkstat & GOBJET
 zoncou = "SOCIETE" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

