# Supervisor
# Mask management TIT0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
indice = 0
If varmode([M]ITMREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREF" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]ITMREF,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "VXADCUPC"
  Call ADCCTRUPC([M]ITMREF) From VXADCUPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ITM","",[M]ITMREF,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone INTDES1
mkstat = 0
zoncou = "INTDES1"
indice = 0
If varinit([M]INTDES1,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]INTDES1,"TITMMASTER","INTDES1",[M]ITMREF,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]INTDES1,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "INTDES1" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
If varinit([M]ENAFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]ENAFLG,indice)=0
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
Endif
[M]ENAFLG = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "TIT0"
Return

