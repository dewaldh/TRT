# Supervisor
# Mask management WMSARSTD 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone QTYTYP
mkstat = 0
zoncou = "QTYTYP"
IMPENT = [M]QTYTYP(indice)
If varinit([M]QTYTYP,indice)=0
 IMPENT = 1
Endif
[M]QTYTYP(indice) = IMPENT

## Zone DES1AXX
mkstat = 0
zoncou = "DES1AXX"
If varinit([M]DES1AXX,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DES1AXX(indice),"ITMMASTER","DES1AXX",[M:SAR2]ITMREF(nolign-1),"") From ATEXTRA
 GACTION = SAVACT
Endif

## Zone TSICOD1
mkstat = 0
zoncou = "TSICOD1"
If varinit([M]TSICOD1,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TSICOD1(indice),"20","") From SUBADI
 GACTION = SAVACT
Endif

## Zone TSICOD2
mkstat = 0
zoncou = "TSICOD2"
If varinit([M]TSICOD2,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TSICOD2(indice),"21","") From SUBADI
 GACTION = SAVACT
Endif

## Zone TSICOD3
mkstat = 0
zoncou = "TSICOD3"
If varinit([M]TSICOD3,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TSICOD3(indice),"22","") From SUBADI
 GACTION = SAVACT
Endif

## Zone TSICOD4
mkstat = 0
zoncou = "TSICOD4"
If varinit([M]TSICOD4,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TSICOD4(indice),"23","") From SUBADI
 GACTION = SAVACT
Endif

## Zone TSICOD5
mkstat = 0
zoncou = "TSICOD5"
If varinit([M]TSICOD5,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TSICOD5(indice),"24","") From SUBADI
 GACTION = SAVACT
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
