Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NB
mkstat = 0
zoncou = "NB"
indice = 0
status = 1
zoncou = "NB"
Gosub SETDEV From TRTDIV
GDEV1(1) = [M]CURAFF
If varmode([M]NB,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NB" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NB
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NB = nolign
   status = 75
   Call SAI_NB(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone LIGRND
mkstat = 0
zoncou = "LIGRND"
indice = 0
IMPENT = [M]LIGRND
If varinit([M]LIGRND,indice)=0
 IMPENT = 1
Endif
[M]LIGRND = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 1
