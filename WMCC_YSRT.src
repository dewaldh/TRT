############################################################################################
### Subprog _AINSERT
############################################################################################
Subprog _AINSERT(YSRT,ASTATUS)
Variable Instance YSRT Using C_YSRT
Variable Integer ASTATUS

If YSRT = null : End : Endif

Gosub _AVARIABLES From WMC00YSRT

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AINSERT_YSRT

End

#-----------------------------------------------------#

############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(YSRT,ASTATUS)
Variable Instance YSRT Using C_YSRT
Variable Integer ASTATUS

If YSRT = null : End : Endif

Gosub AVARIABLES From WMC00YSRT
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If !adxlog
 Gosub AINSERT_TR_YSRT
Else
 Gosub AINSERT_YSRT
Endif

Gosub _AINSERT_AFTER
[L]A_I = YSRT.freeSnapshot
YSRT.snapshotEnabled = 1
$AINSERT_END
End

#-----------------------------------------------------#

$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YSRT._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YSRT._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YSRT._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If YSRT.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YSRT.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet YSRT.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER

[L]ASTATUS = max([L]ASTATUS,fmet YSRT.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_YSRT
Trbegin [F:YRTP]
 Gosub AINSERT_YSRT
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AINSERT_YSRT
Raz [F:YRTP]
[L]ASTATUS = max([L]ASTATUS,fmet YSRT.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YSRT._MAJSQL_YSETRULTYP(YSRT))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet YSRT.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If YSRT.snapshotEnabled=1
 [L]A_RETIK = YSRT.revertUpdTick
Endif
[L]ASTATUS = max(fmet YSRT.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AINSERT_AFTER
If find(YSRT.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YSRT.ASTALIN = [V]CST_ALL
Endif
Return



