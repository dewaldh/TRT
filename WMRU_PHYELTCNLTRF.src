############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PHYELTCNLTRF,ASTATUS)
Variable Instance PHYELTCNLTRF Using R_PHYELTCNLTRF
Variable Integer ASTATUS

If PHYELTCNLTRF = null : End : Endif

Gosub AVARIABLES From WMR00PHYELTCNLTRF
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PHYELTCNLTRF.ACTX.AGETAFCRIGHT(PHYELTCNLTRF,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PHYELTCNLTRF.ACTX.AGETAFCRIGHTU(PHYELTCNLTRF,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PHYELTCNLTRF
Else
 Gosub AUPDATE_PHYELTCNLTRF
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PHYELTCNLTRF.freeSnapshot
PHYELTCNLTRF.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PHYELTCNLTRF.ASTALIN = [V]CST_AUPD
PHYELTCNLTRF.CNLTRF.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PHYELTCNLTRF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTCNLTRF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTCNLTRF._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTCNLTRF._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTCNLTRF._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PHYELTCNLTRF.CNLTRF.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PHYELTCNLTRF.CNLTRF.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PHYELTCNLTRF.CNLTRF.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTCNLTRF.CNLTRF.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PHYELTCNLTRF.CNLTRF.PHYMVT) : # PHYMVT -- 2
 If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) <> null
& and PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEW & PHYELTCNLTRF.CNLTRF.ASTALIN=[V]CST_ADEL
   PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ANEW & find(PHYELTCNLTRF.CNLTRF.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PHYELTCNLTRF.CNLTRF.PHYMVT) : # PHYMVT -- 2
 If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) <> null
& and PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PHYELTCNLTRF.CNLTRF.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTCNLTRF.CNLTRF.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PHYELTCNLTRF
Trbegin [F:PHY]
 Gosub AUPDATE_PHYELTCNLTRF
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PHYELTCNLTRF.CNLTRF.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PHYELTCNLTRF.CNLTRF.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTCNLTRF.CNLTRF.PHYMVT)
 If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AUPDATE_AFTER
  If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)
   PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_PHYELTCNLTRF
[L]A_ASTALIN = PHYELTCNLTRF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTCNLTRF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PHYELTCNLTRF.snapshotEnabled=1
 [L]A_RETIK = PHYELTCNLTRF.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTCNLTRF.CNLTRF.PHYMVT)
 If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AUPDATE_ABORT
  If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)
   PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PHYELTCNLTRF.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



