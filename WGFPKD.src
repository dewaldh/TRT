AFOLDEF = 1
ABRENA = ""
BOX1 = "GAU_CHE1"
OBJLIS(1) = "SP1"
CHGLIS(1) = 2
BROLIS(1) = 1
TRELIS(1) = 1
DESLIS(1) = "VCRTYP+VCRNUM+PACNUM+PACSEQ"
OBJLIS(2) = "PR2"
CHGLIS(2) = 2
BROLIS(2) = 1
TRELIS(2) = 5
DESLIS(2) = "PRHNUM+PRELIN"
OBJLIS(3) = "SD3"
