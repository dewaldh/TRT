   Call CONTROLE("PS2","",[M]COD,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 5296 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_COD([M]COD) From CNSSTASTD
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 5296 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
