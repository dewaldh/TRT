#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CONSSMQ1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
WCHAR = [M]FCY
If varmode([M]FCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FCY([M]FCY) From CNSSMQSTD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STKASFCY"
  Call STKASFCY([M]FCY) From STKACT
  GACTION = SAVACT
  If mkstat=1 : Raz [M]FCY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   If vireblc([M]FCY,2)=""
    mkstat = 1 : GMESSAGE = mess(10,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "ACONTFCY"
    Call CONTSITE([M]FCY) From SUBAGF
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     Call CONTROLE("FCY","",[M]FCY,"",mkstat) From CONTOBJ
     If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "STKCTRFCY"
      Call STKCTRFCY([M]FCY) From STKACT
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       If WCHAR<>[M]FCY or IMPMOD
        SAVACT = GACTION : GACTION = "STD"
        Call AM_FCY([M]FCY) From CNSSMQSTD
        GACTION = SAVACT
        If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
        Gosub AFF From GSAISIE
        If mkstat : End : Endif
       Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone ITMREFSTR
mkstat = 0
zoncou = "ITMREFSTR"
indice = 0
WCHAR = [M]ITMREFSTR
If varmode([M]ITMREFSTR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREFSTR" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]ITMREFSTR,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "VXADCUPC"
  Call ADCCTRUPC([M]ITMREFSTR) From VXADCUPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ITM","",[M]ITMREFSTR,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "CONTITF"
    Call CONTITF([M]ITMREFSTR,[M]FCY) From STKLIB
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call AP_ITMREFSTR([M]ITMREFSTR) From CNSSMQSTD
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]ITMREFSTR or IMPMOD
       SAVACT = GACTION : GACTION = "STD"
       Call AM_ITMREFSTR([M]ITMREFSTR) From CNSSMQSTD
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone ITMREFEND
mkstat = 0
zoncou = "ITMREFEND"
indice = 0
WCHAR = [M]ITMREFEND
If varmode([M]ITMREFEND,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_ITMREFEND([M]ITMREFEND) From CNSSMQSTD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]ITMREFEND : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREFEND" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "VXADCUPC"
  Call ADCCTRUPC([M]ITMREFEND) From VXADCUPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ITM","!",[M]ITMREFEND,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_ITMREFEND([M]ITMREFEND) From CNSSMQSTD
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     If WCHAR<>[M]ITMREFEND or IMPMOD
      SAVACT = GACTION : GACTION = "STD"
      Call AM_ITMREFEND([M]ITMREFEND) From CNSSMQSTD
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
indice = 0
IMPENT = [M]VCRTYP
WNUM = IMPENT
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]VCRTYP,indice)=0
 Call INICOMBO(725) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_VCRTYP(IMPENT) From CNSSMQSTD
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,725,1))))
 Endif
 [M]VCRTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "VCRTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]VCRTYP(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "VCRTYP" : GZNUM = 4969 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WNUM<>IMPENT or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_VCRTYP(IMPENT) From CNSSMQSTD
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "VCRTYP" : GZNUM = 4969 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif
[M]VCRTYP = IMPENT

## Zone VCRNUM
mkstat = 0
zoncou = "VCRNUM"
indice = 0
WCHAR = [M]VCRNUM
If varmode([M]VCRNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "VCRNUM" : Gosub ACTION From =IMPTRT
 Endif
 If WCHAR<>[M]VCRNUM or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_VCRNUM([M]VCRNUM) From CNSSMQSTD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 3170 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CONSSMQ1"
Return

