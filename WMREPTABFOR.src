#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1TABFOR
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "TABFOR"
REP.CLA = "TABFOR"
REP.ICL = "TFO"
REP.AFCRIGHT = "GESTFO"
REP.NCL = 7062 : # Formula table  
REP.DES = 7062 : # Formula table  
REP.TIT = "DES"
REP.ANX = "TFO0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Formula type
     REP.CLE(1).PAR = 'FORTYP' : REP.CLE(1).TYP = 10 : REP.CLE(1).PRO= 'FORTYP' : REP.CLE(1).VPR= 'TFO.FORTYP' : REP.CLE(1).DES = 6991
REP.CLE(2) = NewInstance C_AREPPAR AllocGroup REP : # Formula code
     REP.CLE(2).PAR = 'FORCOD' : REP.CLE(2).TYP = 7 : REP.CLE(2).PRO= 'FORCOD' : REP.CLE(2).VPR= 'TFO.FORCOD' : REP.CLE(2).DES = 6993
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'TABFOR_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 2 : # Detail
REP.AENAFAC(2) = 2 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
