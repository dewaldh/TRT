####################################################################
# 1 : LIBPRIX Description ( Field A 12)
####################################################################

$SET_LIBPRIX
If [L]_AOLDVAL<>this.LIBPRIX
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_LIBPRIX
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.LIBPRIX
 Gosub PROPAGATE_LIBPRIX
Endif
Return

$INIT_LIBPRIX
Gosub DECLARE_VAR From WMC0PPRICESEARCH_C_PRICE
[L]CURPRO = "LIBPRIX"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_LIBPRIX
Gosub DECLARE_VAR From WMC0PPRICESEARCH_C_PRICE
[L]CURPRO = "LIBPRIX"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_LIBPRIX               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_LIBPRIX               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_LIBPRIX
[L]CURPRO = "LIBPRIX"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.LIBPRIX,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_LIBPRIX
[L]CURPRO = "LIBPRIX"
Return

$_AOTHERS_LIBPRIX
[L]CURPRO = "LIBPRIX"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_LIBPRIX
[L]CURPRO = "LIBPRIX"
Return

$PROPAGATE_LIBPRIX
this.isReadonly = 0
[L]CURPRO = "LIBPRIX"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_LIBPRIX
Gosub DECLARE_VAR From WMC0PPRICESEARCH_C_PRICE
[L]CURPRO = "LIBPRIX"
$_LNKPRO_LIBPRIX
Return


