# Supervisor
# Mask management RELANU 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CHOIX
mkstat = 0
zoncou = "CHOIX"
indice = 0
IMPENT = [M]CHOIX
If varinit([M]CHOIX,indice)=0
 IMPENT = 1
Endif
If varmode([M]CHOIX,indice)=0
 [M]CHOIX(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "CHOIX" : Gosub ACTION From =IMPTRT
  IMPENT = [M]CHOIX(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CHOIX" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]CHOIX = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "RELANU"
Return

