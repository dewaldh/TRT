####################################################################
# 31 : YCANREQDAT Cancellation Req. Date ( Field D)
####################################################################

$SET_YCANREQDAT
If [L]_AOLDVAL<>this.YCANREQDAT
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_YCANREQDAT
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.YCANREQDAT
 Gosub PROPAGATE_YCANREQDAT
Endif
Return

$INIT_YCANREQDAT
Gosub DECLARE_VAR From WMC0YSWF
[L]CURPRO = "YCANREQDAT"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_YCANREQDAT
Gosub DECLARE_VAR From WMC0YSWF
[L]CURPRO = "YCANREQDAT"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_YCANREQDAT            : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_YCANREQDAT            : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_YCANREQDAT
[L]CURPRO = "YCANREQDAT"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.YCANREQDAT=[0/0/0]
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_YCANREQDAT
[L]CURPRO = "YCANREQDAT"
Return

$_AOTHERS_YCANREQDAT
[L]CURPRO = "YCANREQDAT"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_YCANREQDAT
[L]CURPRO = "YCANREQDAT"
Return

$PROPAGATE_YCANREQDAT
this.isReadonly = 0
[L]CURPRO = "YCANREQDAT"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_YCANREQDAT
Gosub DECLARE_VAR From WMC0YSWF
[L]CURPRO = "YCANREQDAT"
$_LNKPRO_YCANREQDAT
Return


