####################################################################
# 31 : YSTAGSTA Stage Title Status ( Field A 50)
####################################################################

$SET_YSTAGSTA
If [L]_AOLDVAL<>this.YSTAGSTA
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_YSTAGSTA
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.YSTAGSTA
 Gosub PROPAGATE_YSTAGSTA
Endif
Return

$INIT_YSTAGSTA
Gosub DECLARE_VAR From WMC0YSWF
[L]CURPRO = "YSTAGSTA"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_YSTAGSTA
Gosub DECLARE_VAR From WMC0YSWF
[L]CURPRO = "YSTAGSTA"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_YSTAGSTA              : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_YSTAGSTA              : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_YSTAGSTA
[L]CURPRO = "YSTAGSTA"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.YSTAGSTA,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_YSTAGSTA
[L]CURPRO = "YSTAGSTA"
Return

$_AOTHERS_YSTAGSTA
[L]CURPRO = "YSTAGSTA"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_YSTAGSTA
[L]CURPRO = "YSTAGSTA"
Return

$PROPAGATE_YSTAGSTA
this.isReadonly = 0
[L]CURPRO = "YSTAGSTA"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_YSTAGSTA
Gosub DECLARE_VAR From WMC0YSWF
[L]CURPRO = "YSTAGSTA"
$_LNKPRO_YSTAGSTA
Return


