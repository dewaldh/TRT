# Supervisor
# Mask management AFT9 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone CRITERE
mkstat = 0
zoncou = "CRITERE"
indice = 0
If varmode([M]CRITERE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CRITERE" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone OPTION
mkstat = 0
zoncou = "OPTION"
indice = 0
IMPENT = [M]OPTION
If varinit([M]OPTION,indice)=0
 IMPENT = 1
Endif
If varmode([M]OPTION,indice)=0
 [M]OPTION(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "OPTION" : Gosub ACTION From =IMPTRT
  IMPENT = [M]OPTION(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "OPTION" : GZNUM = 8720 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]OPTION = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "AFT9"
Return

