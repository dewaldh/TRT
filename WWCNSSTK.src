Subprog ACTRUN(INFO)
Value Char INFO
Local Char    BOUT(3)
Local Integer ONOL
ONOL    = nolign
BOUT    = func AFNC.PART(INFO,1)
nolign  = val(func AFNC.PART(INFO,2))
indice  = max(0,nolign-1)
Gosub NOMFIC
For MA=0 to dim(NOMMSK)-1
 Gosub OUVRE_MSK
Next MA
For MA=0 to dim(NOMMSK)-1
 Gosub SETMASK
Next MA
Case BOUT
 When "B2"
  Onerrgo RIEN
  Assign "[M]ITMREF(nolign-1)" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Onerrgo
  Call B2_NBLIG From ="W1CONSSTK5"
 When "B3"
  Onerrgo RIEN
  Assign "WCTLDEM" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Onerrgo
  Call B3_NBLIG From ="W1CONSSTK5"
 When "B4"
  Onerrgo RIEN
  Onerrgo
  Call B4_NBLIG From ="W1CONSSTK5"
 When "B5"
  Onerrgo RIEN
  Onerrgo
  Call B5_NBLIG From ="W1CONSSTK5"
 When "B6"
  Onerrgo RIEN
  Assign "[M]STOFCY(nolign-1)" With ctrans(func AFNC.PART(INFO,4),"¨","~")
  Assign "[M]ITMREF(nolign-1)" With ctrans(func AFNC.PART(INFO,5),"¨","~")
  Assign "[M]LOT(nolign-1)" With ctrans(func AFNC.PART(INFO,6),"¨","~")
  Onerrgo
  Call B6_NBLIG From ="W1CONSSTK5"
 When "B7"
  Onerrgo RIEN
  Assign "[M]STOFCY(nolign-1)" With ctrans(func AFNC.PART(INFO,4),"¨","~")
  Assign "[M]ITMREF(nolign-1)" With ctrans(func AFNC.PART(INFO,5),"¨","~")
  Onerrgo
  Call B7_NBLIG From ="W1CONSSTK5"
 When "B8"
  Onerrgo RIEN
  Assign "[M]STOFCY(nolign-1)" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Assign "[M]ITMREF(nolign-1)" With ctrans(func AFNC.PART(INFO,4),"¨","~")
  Onerrgo
  Call B8_NBLIG From ="W1CONSSTK5"
Endcase
nolign = ONOL
indice = max(0,nolign-1)
End

$RIEN
Resume


