$_AMANDATORY
Gosub DECLARE_VAR From WMC0BUDPAR
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_BUD From WMC1BUDPAR : Gosub TESTALLSTAT :  Endif
