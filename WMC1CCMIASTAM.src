####################################################################
# 1 : COL1 Request ID ( Field A 20)
####################################################################

$SET_COL1
If [L]_AOLDVAL<>this.COL1
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_COL1
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.COL1
 Gosub PROPAGATE_COL1
Endif
Return

$INIT_COL1
Gosub DECLARE_VAR From WMC0CCMIASTAM
[L]CURPRO = "COL1"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_COL1
Gosub DECLARE_VAR From WMC0CCMIASTAM
[L]CURPRO = "COL1"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_COL1                  : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_COL1                  : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_COL1
[L]CURPRO = "COL1"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.COL1,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_COL1
[L]CURPRO = "COL1"
Return

$_AOTHERS_COL1
[L]CURPRO = "COL1"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_COL1
[L]CURPRO = "COL1"
Return

$PROPAGATE_COL1
this.isReadonly = 0
[L]CURPRO = "COL1"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_COL1
Gosub DECLARE_VAR From WMC0CCMIASTAM
[L]CURPRO = "COL1"
$_LNKPRO_COL1
Return


