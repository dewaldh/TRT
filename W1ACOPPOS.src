#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management ACOPPOS 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:47:14"
A_USER  = "ADMIN"
A_STAMP = "20180827234714"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask ACOPPOS [ACOP]
Gosub HORODAT
Local Inpbox "ACOPPOS" From GFONCTION At A_STAMP With A_WINDPREV Mask [ACOP] As [SIM_]
Grizo [ACOP]10
Grizo [ACOP]20
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [ACOP]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=29 : X2(0)=83 : Y1(0)=12 : Y2(0)=14
X1(1)=29 : X2(1)=83 : Y1(1)=15 : Y2(1)=16
Return

$ON_DOSORG
Onevent CONTROL Call C_DOSORG(zc) From W1ACOPPOS
Onevent AFTER_CHANGE Gosub AMM_DOSORG From W1ACOPPOS
Onevent INIT_AREA Call D_DOSORG(zc) From W1ACOPPOS
Onevent BEFORE_INPUT Call AS_DOSORG(zc) From W1ACOPPOS
Return

Subprog D_DOSORG(VALEUR)
Variable Char    VALEUR()
If varinit([M]DOSORG,indice)=0
 ## Action STD
 Call D_DOSORG(VALEUR) From ACOPPOS
Endif
Gosub _AFF
End

Subprog AS_DOSORG(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
## Action STD
Call AS_DOSORG(VALEUR) From ACOPPOS
If mkstat=2
 [M]DOSORG(INDIC) = VALEUR
 Affzo DOSORG(INDIC) : mkstat = 2
Endif
Onkey   2 Titled GBOUTS Call S_DOSORG(zc) From W1ACOPPOS
Gosub _AFF
End

Subprog C_DOSORG(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1090 : Endif
If !mkstat
 ## Action CONTDOS
 Call C_DOSSIER(VALEUR) From DOSCONT
 If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1090 : Endif
 If !mkstat
  ## Action ACCESDOS
  Call CTL_DROIT_DOSSIER(VALEUR,"") From DOSCONT
  If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1090 : Endif
  If !mkstat
   ## Action STD
   Call C_DOSORG(VALEUR) From ACOPPOS
   If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1090 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

$AMM_DOSORG
Call AM_DOSORG(zc)
If !mkstat & GOBJET
 zoncou = "DOSORG" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_DOSORG(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action APASSWD
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "APASSWD"
 PARMSK(1) = VALEUR
 PARMSK(2) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"APASSWD","APASSWD","SPEPASSWD") From GSAISIE
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1090 : Endif
Endif
Gosub _AFF
End

Subprog S_DOSORG(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADS
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADS"
 Call S_LIS_CHAR(VALEUR,PARMSK,"SELADS","SUBADS","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

