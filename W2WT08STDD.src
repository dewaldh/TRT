Onevent BEFORE_INPUT Call AS_PCUDES(zc) From W2WT08STDD
Return

Subprog AS_PCUDES(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3612,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_PCUDES(zc) From W2WT08STDD
Onkey   2 Titled GBOUTS Call S_PCUDES(zc) From W2WT08STDD
Onkey 144 Titled GBOUTP Call P_PCUDES(zc) From W2WT08STDD
Onkey 146 Titled GBOUTA Call SA_PCUDES(zc) From W2WT08STDD
Gosub _AFF
End

Subprog C_PCUDES(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("TUN","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "PCUDES" : GZNUM = 5201 : Endif
If !mkstat
 ## Action STD
 Call C_PCUDES(VALEUR) From TRTSTOSMP1
 If mkstat : GERR = 1 : GZONE = "PCUDES" : GZNUM = 5201 : Endif
Endif
Gosub _AFF
End

Subprog S_PCUDES(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action STKSELUOMN
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "STKSELUOMN"
 PARMSK(1) = "1"
 PARMSK(2) = "1"
 Call S_LIS_CHAR(VALEUR,PARMSK,"STKSELUOMN","STKSEL","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_PCUDES(VALEUR)
Variable Char    VALEUR()
Call OBJET("TUN",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_PCUDES(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TUN"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_PCUDES(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TUN",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

