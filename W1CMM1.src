Onkey   2 Titled GBOUTS Call S_ACC10(zc) From W1CMM1
Onkey 144 Titled GBOUTP Call P_ACC10(zc) From W1CMM1
Onkey 146 Titled GBOUTA Call SA_ACC10(zc) From W1CMM1
Gosub _AFF
End

Subprog C_ACC10(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action GACCHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GACCHOICE"
 PARMSK(1) = num$([M]COA(9))
 Call S_TAB_CHAR(VALEUR,PARMSK,"GACCHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ACC10" : GZNUM = 0 : Endif
Endif
If !GWEBSERV
 If !mkstat
  ## Action CONTCPT
  Call CONTCPT(VALEUR,num$([M]COA(9))) From CPTSUB
  If mkstat : GERR = 1 : GZONE = "ACC10" : GZNUM = 0 : Endif
 Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("GAC","!",VALEUR,num$([M]COA(9)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "ACC10" : GZNUM = 0 : Endif
 If !mkstat
  ## Action STD
  Call C_ACC10(VALEUR) From SUBCMM
  If mkstat : GERR = 1 : GZONE = "ACC10" : GZNUM = 0 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_ACC10(VALEUR)
