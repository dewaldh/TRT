############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YLNDPUR,ASTATUS)
Variable Instance YLNDPUR Using R_YLNDPUR
Variable Integer ASTATUS

If YLNDPUR = null : End : Endif

Gosub AVARIABLES From WMR00YLNDPUR
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet YLNDPUR.ACTX.AGETAFCRIGHT(YLNDPUR,"GESYLDP",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet YLNDPUR.ACTX.AGETAFCRIGHTU(YLNDPUR,"GESYLDP",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_YLNDPUR
Else
 Gosub AUPDATE_YLNDPUR
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YLNDPUR.freeSnapshot
YLNDPUR.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
YLNDPUR.ASTALIN = [V]CST_AUPD
YLNDPUR.YLDP.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = YLNDPUR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = YLNDPUR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YLNDPUR._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YLNDPUR._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YLNDPUR._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If YLNDPUR.YLDP.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YLNDPUR.YLDP.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(YLNDPUR.YLDP.ASTALIN)<>""
 [L]A_ASTALIN = YLNDPUR.YLDP.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(YLNDPUR.YLDP.YLDS) : # YLDS -- 2
 If YLNDPUR.YLDP.YLDS(A_N2) <> null
& and YLNDPUR.YLDP.YLDS(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YLNDPUR.YLDP.YLDS(A_N2).ASTALIN<>[V]CST_ANEW & YLNDPUR.YLDP.ASTALIN=[V]CST_ADEL
   YLNDPUR.YLDP.YLDS(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YLNDPUR.YLDP.YLDS(A_N2).ASTALIN=[V]CST_ANEW & find(YLNDPUR.YLDP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YLNDPUR.YLDP.YLDS(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YLNDPUR.YLDP.YLDS(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDP.YLDS(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDS(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDS(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDS(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLNDPUR.YLDP.YLDM) : # YLDM -- 3
 If YLNDPUR.YLDP.YLDM(A_N2) <> null
& and YLNDPUR.YLDP.YLDM(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YLNDPUR.YLDP.YLDM(A_N2).ASTALIN<>[V]CST_ANEW & YLNDPUR.YLDP.ASTALIN=[V]CST_ADEL
   YLNDPUR.YLDP.YLDM(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YLNDPUR.YLDP.YLDM(A_N2).ASTALIN=[V]CST_ANEW & find(YLNDPUR.YLDP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YLNDPUR.YLDP.YLDM(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YLNDPUR.YLDP.YLDM(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDP.YLDM(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDM(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDM(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDM(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLNDPUR.YLDP.YRMA) : # YRMA -- 4
 If YLNDPUR.YLDP.YRMA(A_N2) <> null
& and YLNDPUR.YLDP.YRMA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YLNDPUR.YLDP.YRMA(A_N2).ASTALIN<>[V]CST_ANEW & YLNDPUR.YLDP.ASTALIN=[V]CST_ADEL
   YLNDPUR.YLDP.YRMA(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YLNDPUR.YLDP.YRMA(A_N2).ASTALIN=[V]CST_ANEW & find(YLNDPUR.YLDP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YLNDPUR.YLDP.YRMA(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YLNDPUR.YLDP.YRMA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDP.YRMA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YRMA(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YRMA(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YRMA(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(YLNDPUR.YLDP.YLDS) : # YLDS -- 2
 If YLNDPUR.YLDP.YLDS(A_N2) <> null
& and YLNDPUR.YLDP.YLDS(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YLNDPUR.YLDP.YLDS(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDP.YLDS(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDS(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDS(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDS(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLNDPUR.YLDP.YLDM) : # YLDM -- 3
 If YLNDPUR.YLDP.YLDM(A_N2) <> null
& and YLNDPUR.YLDP.YLDM(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YLNDPUR.YLDP.YLDM(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDP.YLDM(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDM(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDM(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YLDM(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLNDPUR.YLDP.YRMA) : # YRMA -- 4
 If YLNDPUR.YLDP.YRMA(A_N2) <> null
& and YLNDPUR.YLDP.YRMA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YLNDPUR.YLDP.YRMA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDP.YRMA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YRMA(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YRMA(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.YRMA(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(YLNDPUR.YLDP.ASTALIN)<>""
 [L]A_ASTALIN = YLNDPUR.YLDP.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_YLNDPUR
Trbegin [F:YLDP]
 Gosub AUPDATE_YLNDPUR
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(YLNDPUR.YLDP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YLNDPUR.YLDP.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDP.YLDS)
 If YLNDPUR.YLDP.YLDS(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDP.YLDS(A_N2)._AUPDATE_AFTER
  If YLNDPUR.YLDP.YLDS(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDP.YLDS(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDP.YLDS(A_N2)
   YLNDPUR.YLDP.YLDS(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDP.YLDM)
 If YLNDPUR.YLDP.YLDM(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDP.YLDM(A_N2)._AUPDATE_AFTER
  If YLNDPUR.YLDP.YLDM(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDP.YLDM(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDP.YLDM(A_N2)
   YLNDPUR.YLDP.YLDM(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDP.YRMA)
 If YLNDPUR.YLDP.YRMA(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDP.YRMA(A_N2)._AUPDATE_AFTER
  If YLNDPUR.YLDP.YRMA(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDP.YRMA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDP.YRMA(A_N2)
   YLNDPUR.YLDP.YRMA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_YLNDPUR
[L]A_ASTALIN = YLNDPUR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDP._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = YLNDPUR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If YLNDPUR.snapshotEnabled=1
 [L]A_RETIK = YLNDPUR.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDP.YLDS)
 If YLNDPUR.YLDP.YLDS(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDP.YLDS(A_N2)._AUPDATE_ABORT
  If YLNDPUR.YLDP.YLDS(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDP.YLDS(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDP.YLDS(A_N2)
   YLNDPUR.YLDP.YLDS(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDP.YLDM)
 If YLNDPUR.YLDP.YLDM(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDP.YLDM(A_N2)._AUPDATE_ABORT
  If YLNDPUR.YLDP.YLDM(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDP.YLDM(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDP.YLDM(A_N2)
   YLNDPUR.YLDP.YLDM(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDP.YRMA)
 If YLNDPUR.YLDP.YRMA(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDP.YRMA(A_N2)._AUPDATE_ABORT
  If YLNDPUR.YLDP.YRMA(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDP.YRMA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDP.YRMA(A_N2)
   YLNDPUR.YLDP.YRMA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet YLNDPUR.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



