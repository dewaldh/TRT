#-------------------------------------------------------#
$TEST2
Local Instance SQHACT Using R_SQHACT
SQHACT = NewInstance R_SQHACT AllocGroup null
Call ALOAD_LINKS_DET_PAGE(SQHACT)
Infbox 'OK'
FreeGroup SQHACT
End

#-------------------------------------------------------#
$TEST
Local Instance REP Using C_AREPENT
Call DESCRIPTION_REPRESENTATION(REP,"TEST")
Infbox 'OK'
FreeGroup REP
End

#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1SQHACT
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "SQHACT"
REP.CLA = "SQUOTE"
REP.ICL = "SQH"
REP.AFCRIGHT = "GESSQH"
REP.NCL = 3135 : # Quote header
REP.DES = 50673 : # Active quotes
REP.TIT = ""
REP.ANX = "SQH0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Quote no.
     REP.CLE(1).PAR = 'SQHNUM' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'SQHNUM' : REP.CLE(1).VPR= 'SQH.SQHNUM' : REP.CLE(1).DES = 3088
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'SQHACTM_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 2 : # Detail
REP.AENAFAC(2) = 1 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "HEAD" : REP.SEC(1).NSC = 1506 : # Header
     REP.SEC(1).BLC(1)="HEADER" : REP.SEC(1).NBC(1)=1506 : # Header
Return

