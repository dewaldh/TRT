# Supervisor
# Mask management STOK 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
indice = 0
IMPENT = [M]VCRTYP
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP = IMPENT

## Zone ISSMGTCOD
mkstat = 0
zoncou = "ISSMGTCOD"
indice = 0
IMPENT = [M]ISSMGTCOD
If varinit([M]ISSMGTCOD,indice)=0
 IMPENT = 1
Endif
[M]ISSMGTCOD = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone STOMGTCOD
mkstat = 0
zoncou = "STOMGTCOD"
indice = 0
IMPENT = [M]STOMGTCOD
If varinit([M]STOMGTCOD,indice)=0
 IMPENT = 1
Endif
[M]STOMGTCOD = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"
indice = 0
status = 1
zoncou = "BASPAG"
If varmode([M]BASPAG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BASPAG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]BASPAG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]BASPAG = nolign
   status = 75
   Call SAI_BASPAG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 4
####################################
Subprog SAI_BASPAG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "STOK"
Return

