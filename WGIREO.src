CBOUT(NBTEX) = "A" : LBOUT(NBTEX) = 1101 : Call TEXTE(5910,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "I" : LBOUT(NBTEX) = 1802 : Call TEXTE(20395,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "J" : LBOUT(NBTEX) = 1803 : Call TEXTE(20396,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "D" : LBOUT(NBTEX) = 1804 : Call TEXTE(6425,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "C" : LBOUT(NBTEX) = 1805 : Call TEXTE(19822,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "E" : LBOUT(NBTEX) = 1806 : Call TEXTE(12413,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "G" : LBOUT(NBTEX) = 1807 : Call TEXTE(147,TEXTE(NBTEX)) From OBJDIV
