$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "A"
  FLGEXE = 1
  GACTION = "TRTSELREO"
  Call ACT_SELREO From TRTREOA
 When "I"
  FLGEXE = 1
  GACTION = "TRTINCREO"
  Call ACT_INCREO From TRTREOA
 When "J"
  FLGEXE = 1
  GACTION = "TRTEXCREO"
  Call ACT_EXCREO From TRTREOA
 When "D"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "TRTCRIREO"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IREOC","TRTREO","XWIREOC") From GSAISIE
 When "C"
  FLGEXE = 1
  GACTION = "TRTLISREO"
  Call ACT_LISREO From TRTREOA
 When "E"
  FLGEXE = 1
  GACTION = "TRTENRREO"
  Call ACT_ENRREO From TRTREOA
 When "G"
  FLGEXE = 1
  GACTION = "TRTTRIREO"
  Call ACT_TRIREO From TRTREOA
 When "F"
  FLGEXE = 1
  GACTION = "TRTVALREO"
  Call ACT_VALREO From TRTREOA
 When "H"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GREONUM
  FLGEXE = 1
  GACTION = "VISUREO"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISRO","SELTRS","XWISRT") From GSAISIE
Endcase
GACTION = SAVACT
Return

