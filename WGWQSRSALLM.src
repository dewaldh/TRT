$BOUTON
STDBOUT = "EA<>"
NBTEX  = 0
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
NBMEN += 1
NMEN(NBMEN) = 10 : Call TEXTE(34,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "M" : LMEN(NBMEN) = 2001 : Call TEXTE(3531,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 20 : Call TEXTE(6084,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 20 : CMEN(NBMEN) = "N" : LMEN(NBMEN) = 2002 : Call TEXTE(12817,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 20 : CMEN(NBMEN) = "O" : LMEN(NBMEN) = 2003 : Call TEXTE(23197,TMEN(NBMEN)) From OBJDIV
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "M"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M]STOFCY
  PARBOUT(2) = num$([M]VCRTYP)
  PARBOUT(3) = [M]VCRNUM
  PARBOUT(4) = num$([M]VCRLIN)
  PARBOUT(5) = [M]ITMREF
  FLGEXE = 1
  GACTION = "SAISTOANAM"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISTOANAM","TRTSTOANAM","XYTRTSTOANAM") From GSAISIE
  [M]STOFCY = PARBOUT(1)
  [M]VCRTYP = val(PARBOUT(2))
  [M]VCRNUM = PARBOUT(3)
  [M]VCRLIN = val(PARBOUT(4))
  [M]ITMREF = PARBOUT(5)
 When "N"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$(1)
  PARBOUT(2) = [M:ENM]STOFCY
  PARBOUT(3) = [M:ENM]ITMREF
  PARBOUT(4) = ""
  FLGEXE = 1
  GACTION = "CONSSFC"
  Call CONSULT_CHAR(VALBOUT,"SFC",PARBOUT) From GCONSULT
 When "O"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:ENM]STOFCY
  PARBOUT(2) = [M:ENM]ITMREF
  PARBOUT(3) = num$([M:ENM]TRSTYP)
  PARBOUT(4) = [M:ENM]TRSCOD
  FLGEXE = 1
  GACTION = "VISUSRUV"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISRUV","VISUSRUV","XYVISUSRUV") From GSAISIE
  [M:ENM]STOFCY = PARBOUT(1)
  [M:ENM]ITMREF = PARBOUT(2)
  [M:ENM]TRSTYP = val(PARBOUT(3))
  [M:ENM]TRSCOD = PARBOUT(4)
Endcase
GACTION = SAVACT
Return

$EXEACT
