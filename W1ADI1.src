Subprog IB_CODE(VALEUR)
Variable Char    VALEUR()
## Action STD
Call IB_CODE From SUBADI
Gosub _AFF
End

$AV_CODE
If "K:"+num$([M]LNG)+"B"<>""
 Chgfmt [M]CODE(indice) With "K:"+num$([M]LNG)+"B"
Else
 Chgfmt [M]CODE(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog D_CODE(VALEUR)
Variable Char    VALEUR()
If varinit([M]CODE,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_CODE(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_CODE(VALEUR)
## Action STD
Call AS_CODE(VALEUR) From SUBADI
If mkstat=2
 [M]CODE(INDIC) = VALEUR
 Affzo CODE(INDIC) : mkstat = 2
Endif
Onkey   2 Titled GBOUTS Call S_CODE(zc) From W1ADI1
Onkey 144 Titled GBOUTP Call P_CODE(zc) From W1ADI1
Onkey 146 Titled GBOUTA Call SA_CODE(zc) From W1ADI1
Gosub _AFF
End

Subprog C_CODE(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "CODE" : GZNUM = 5296 : Endif
If !mkstat
 ## Action CONTADI
 Call CONTADI(VALEUR,"","") From SUBADI
 If mkstat : GERR = 1 : GZONE = "CODE" : GZNUM = 5296 : Endif
 If !mkstat
  ## Action STD
  Call C_CODE(VALEUR) From SUBADI
  If mkstat : GERR = 1 : GZONE = "CODE" : GZNUM = 5296 : Endif
  If !mkstat
   ## Action CONTOBJN
   Local Char    PARMSK(250)(1..20) , SAVACT(20)
   SAVACT = GACTION : GACTION = "CONTOBJN"
   Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJN","CONTOBJ","") From GACTION
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CODE" : GZNUM = 5296 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_CODE(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = ""
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog SA_CODE(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(3) = "$"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CODE(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

