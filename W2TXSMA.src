#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management TXSMA 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_AMTVAL31
Onevent BEFORE_INPUT Call AS_AMTVAL31(zc) From W2TXSMA
Onevent AFTER_CHANGE Gosub AMM_AMTVAL31 From W2TXSMA
Return

Subprog AS_AMTVAL31(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(5562,GBOUT1) From OBJDIV
Call TEXTE(20064,GBOUT2) From OBJDIV
Call TEXTE(24414,GBOUT3) From OBJDIV
Call TEXTE(12888,GBOUT5) From OBJDIV
## Action STD
Call AS_AMTVAL31(VALEUR) From SUBTXWM
If mkstat=2
 [M]AMTVAL31(INDIC) = VALEUR
 Affzo AMTVAL31(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_AMTVAL31(zc) From W2TXSMA
Onkey 130 Titled GBOUT2 Call B2_AMTVAL31(zc) From W2TXSMA
Onkey 131 Titled GBOUT3 Call B3_AMTVAL31(zc) From W2TXSMA
Onkey 133 Titled GBOUT5 Call B5_AMTVAL31(zc) From W2TXSMA
Gosub _AFF
End

$AMM_AMTVAL31
Call AM_AMTVAL31(zc)
If !mkstat & GOBJET
 zoncou = "AMTVAL31" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_AMTVAL31(VALEUR)
Variable Decimal VALEUR
## Action STD
Call AM_AMTVAL31(VALEUR) From SUBTXWM
If mkstat : GERR = 1 : GZONE = "AMTVAL31" : GZNUM = 0 : Endif
Gosub _AFF
End

Subprog B1_AMTVAL31(VALEUR)
Variable Decimal VALEUR
If !GWEBSERV
 ## Action TXSCNS
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "TXSCNS"
 Call SAISIE_NUM(VALEUR,PARMSK,"DTXSCNS","TXSCNS","XWDTXSCNS") From GSAISIE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

