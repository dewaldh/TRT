# Supervisor
## CRUD-CLASSE ## CCMAUSAPP
# 2018-08-27 13:09:30 019


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(CCMAUSAPP,ASTATUS)
Variable Instance CCMAUSAPP Using R_CCMAUSAPP
Variable Integer ASTATUS

If CCMAUSAPP = null : End : Endif

Gosub AVARIABLES From WMR00CCMAUSAPP
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet CCMAUSAPP.ACTX.AGETAFCRIGHT(CCMAUSAPP,"GESAUS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet CCMAUSAPP.ACTX.AGETAFCRIGHTU(CCMAUSAPP,"GESAUS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif
If fmet CCMAUSAPP.ACTX.AGETACSRIGHTM(CCMAUSAPP,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AUPDATE_END
  Endif
  
  Gosub AREPCONTROL_AUPDATE
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  
If !adxlog
 Gosub AUPDATE_TR_CCMAUSAPP
Else
 Gosub AUPDATE_CCMAUSAPP
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = CCMAUSAPP.freeSnapshot
CCMAUSAPP.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
CCMAUSAPP.ASTALIN = [V]CST_AUPD
CCMAUSAPP.AUS.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = CCMAUSAPP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = CCMAUSAPP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSAPP._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSAPP._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSAPP._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If CCMAUSAPP.AUS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
CCMAUSAPP.AUS.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(CCMAUSAPP.AUS.ASTALIN)<>""
 [L]A_ASTALIN = CCMAUSAPP.AUS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.AUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(CCMAUSAPP.AUS.FNCCOL)
 If CCMAUSAPP.AUS.FNCCOL([L]A_I)<>null
  [L]A_ASTALIN = CCMAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.AUS.FNCCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If CCMAUSAPP.AUS.BPA(A_N2) <> null
& and CCMAUSAPP.AUS.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUSAPP.AUS.BID) : # BID -- 3
 If CCMAUSAPP.AUS.BID(A_N2) <> null
& and CCMAUSAPP.AUS.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
A_N2=1
If CCMAUSAPP.AUS.BPA(A_N2) <> null
& and CCMAUSAPP.AUS.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUSAPP.AUS.BID) : # BID -- 3
 If CCMAUSAPP.AUS.BID(A_N2) <> null
& and CCMAUSAPP.AUS.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(CCMAUSAPP.AUS.FNCCOL)
 If CCMAUSAPP.AUS.FNCCOL([L]A_I)<>null
  [L]A_ASTALIN = CCMAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.AUS.FNCCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(CCMAUSAPP.AUS.ASTALIN)<>""
 [L]A_ASTALIN = CCMAUSAPP.AUS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.AUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_CCMAUSAPP
Trbegin [F:AUS]
 Gosub AUPDATE_CCMAUSAPP
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(CCMAUSAPP.AUS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 CCMAUSAPP.AUS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(CCMAUSAPP.AUS.FNCCOL)
 If CCMAUSAPP.AUS.FNCCOL([L]A_I)<>null
  If CCMAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSAPP.AUS.FNCCOL([L]A_I)
   CCMAUSAPP.AUS.FNCCOL([L]A_I) = null
  Elsif CCMAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN <> [V]CST_ALL
   CCMAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
A_N2=1
If CCMAUSAPP.AUS.BPA(A_N2) <> null
 If CCMAUSAPP.AUS.BPA(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSAPP.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance CCMAUSAPP.AUS.BPA(A_N2)
  CCMAUSAPP.AUS.BPA(A_N2) = null
 Elsif CCMAUSAPP.AUS.BPA(A_N2).ASTALIN <> [V]CST_ALL
  CCMAUSAPP.AUS.BPA(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(CCMAUSAPP.AUS.BPA(A_N2).COLTEL)
   If CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I)
     CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I) = null
    Elsif CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  For [L]A_I=1 To maxtab(CCMAUSAPP.AUS.BPA(A_N2).COLWEB)
   If CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I)
     CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I) = null
    Elsif CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(CCMAUSAPP.AUS.BID)
 If CCMAUSAPP.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet CCMAUSAPP.AUS.BID(A_N2)._AUPDATE_AFTER
  If CCMAUSAPP.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSAPP.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSAPP.AUS.BID(A_N2)
   CCMAUSAPP.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_CCMAUSAPP
[L]A_ASTALIN = CCMAUSAPP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.AUS._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = CCMAUSAPP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSAPP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If CCMAUSAPP.snapshotEnabled=1
 [L]A_RETIK = CCMAUSAPP.revertUpdTick
Endif
Local Integer ASTATUS2
A_N2=1
If CCMAUSAPP.AUS.BPA(A_N2) <> null
 If CCMAUSAPP.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  CCMAUSAPP.AUS.BPA(A_N2).ASTALIN = [V]CST_ANEW
  For [L]A_I=1 To maxtab(CCMAUSAPP.AUS.BPA(A_N2).COLTEL)
   If CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     CCMAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ANEW
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  For [L]A_I=1 To maxtab(CCMAUSAPP.AUS.BPA(A_N2).COLWEB)
   If CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     CCMAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ANEW
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(CCMAUSAPP.AUS.BID)
 If CCMAUSAPP.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet CCMAUSAPP.AUS.BID(A_N2)._AUPDATE_ABORT
  If CCMAUSAPP.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSAPP.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSAPP.AUS.BID(A_N2)
   CCMAUSAPP.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet CCMAUSAPP.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



