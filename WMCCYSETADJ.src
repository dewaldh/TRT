If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDATTIM From WMC1YSETADJ : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDATTIM From WMC1YSETADJ : Gosub TESTALLSTAT :  Endif
