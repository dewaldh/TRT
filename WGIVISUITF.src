$BOUTON
STDBOUT = ""
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "a" : LBOUT(NBTEX) = 1801 : Call TEXTE(3612,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "b" : LBOUT(NBTEX) = 1802 : Call TEXTE(11651,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "c" : LBOUT(NBTEX) = 1803 : Call TEXTE(13005,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "d" : LBOUT(NBTEX) = 1804 : Call TEXTE(13622,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "e" : LBOUT(NBTEX) = 1805 : Call TEXTE(12935,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "a"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M]ITMREF
  FLGEXE = 1
  GACTION = "VISUUNI"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IVISUUNI","VISUITF","XWVISUITF") From GSAISIE
 When "b"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M]FCY
  PARBOUT(2) = [M]ITMREF
  PARBOUT(3) = ""
  PARBOUT(4) = ""
  FLGEXE = 1
  GACTION = "VISUUOM"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IVISUUOM","VISUUOM","XWIVISUUOM") From GSAISIE
 When "c"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M]FCY
  PARBOUT(2) = [M]ITMREF
  PARBOUT(3) = ""
  PARBOUT(4) = ""
  FLGEXE = 1
  GACTION = "VISUSTA"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IVISUSTA","VISUSTA","XWIVISUSTA") From GSAISIE
 When "e"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M]FCY
  PARBOUT(2) = [M]CUNLISNUM
  PARBOUT(3) = [M]ITMREF
  PARBOUT(4) = ""
  FLGEXE = 1
  GACTION = "VISUINV"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IVISUINV","VISUINV","XWIVISUINV") From GSAISIE
Endcase
GACTION = SAVACT
Return

