####################################################################
# Traitments
####################################################################
$_ADDTRT
Return

####################################################################
#Declaration of variables
####################################################################

$DECLARE_VAR
Local Char    ACTION(50) , CURPRO(250) , VALPRO(250) , CURQRY(50) , CURPTH(250)
Local Char    AMETHOD(50) ,AOPERATION(50),AEVENT(50),ARULE(50)
Local Integer ALLSTAT,AISMET,ASTATUS
Return


####################################################################
# Global methods & standards
####################################################################
$AINIT
Gosub DECLARE_VAR
[L]AEVENT = "AINIT" : [L]ACTION = "AINIT" : [L]CURPRO = "" : Gosub ACTION From ASYRSUB
Return

####################################################################
# AERRORS
####################################################################

$_AERROR
Gosub DECLARE_VAR From WMC0GACCOUNTSI
Local Integer ALLSTAT
Call ALLERRORS(this,_CTX,_SUPP) From ASYRSUB
If this.CLSCOA<>null
 [L]ASTATUS = fmet this.CLSCOA._AERROR(_CTX,_SUPP)
Endif
If this.CLSACC<>null
 [L]ASTATUS = fmet this.CLSACC._AERROR(_CTX,_SUPP)
Endif
Return

####################################################################
# Delete all errors
####################################################################

$_ADELERRORS
Gosub DECLARE_VAR From WMC0GACCOUNTSI
Local Integer ALLSTAT
[L]ASTATUS = fmet this.ADELETEERROR([L]_APARAM)
If this.CLSCOA<>null
 [L]ASTATUS = fmet this.CLSCOA._ADELERRORS(_APARAM)
Endif
If this.CLSACC<>null
 [L]ASTATUS = fmet this.CLSACC._ADELERRORS(_APARAM)
Endif
Return

####################################################################
# _ASEARCHINFOS
####################################################################

$_ASEARCHINFOS
Gosub DECLARE_VAR From WMC0GACCOUNTSI
ASTATUS= func ASYRSUB.ASEARCHINFOS(this,[L]AINS_ORI,[L]ERR_TYPE,[L]PROPERTY,[L]ASTAT,[L]SELSTAT,[L]PATH)
If [L]ERR_TYPE=[V]CST_ACURRENT : Return : Endif
If this.CLSCOA<>null
 [L]ASTATUS = fmet this.CLSCOA._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'CLSCOA')
Endif
If this.CLSACC<>null
 [L]ASTATUS = fmet this.CLSACC._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'CLSACC')
Endif
Return

####################################################################
# AGETERRORSFROM
####################################################################

$_AGETERRORSFROM
Gosub DECLARE_VAR From WMC0GACCOUNTSI
If AINS_FROM<>null
 [L]ASTATUS = fmet AINS_FROM.ASEARCHINFOS( [V]CST_ALL, "", [V]CST_AFATAL, 0)
Endif
Call AGETERRORSFROM(AINS_FROM,this,ENVIRONNEMENT) From ASYRSUB
Return

####################################################################
$ACONTROL
####################################################################
Gosub DECLARE_VAR
[L]AEVENT = "ACONTROL" : [L]ACTION = "ACONTROL" : [L]CURPRO = "" : Gosub ACTION From ASYRSUB
Return

####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "CLSACC"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"CLSACC",this)
 When "CLSCOA"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"CLSCOA",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# ADDLINE
####################################################################
$ADDLINE
Case _ACOLLECTION
 When "CLSACC"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"CLSACC","C_GACCCLS",this)
 When "CLSCOA"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"CLSCOA","C_GCOA",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# AGETDIM
####################################################################
$AGETDIM
Case _ACOLLECTION
 When "CLSACC"
  [L]_ALINE = 1
 When "CLSCOA"
  [L]_ALINE = 1
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# AGETMIN
####################################################################
$AGETMIN
Case _ACOLLECTION
 When "CLSACC"
  [L]_ALINE = 2
 When "CLSCOA"
  [L]_ALINE = 2
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# _ALOAD_KEYS
####################################################################
$_ALOAD_KEYS
this.AKEYS(1) = "COA"
this.AKEYS(2) = "ACC"
Return

####################################################################
# _ADELINSTANCE
####################################################################
$_ADELINSTANCE
[L]ASTATUS = func ASYRSUBTAB._ADELINSTANCE(this)
Return

####################################################################
# ADDINSTANCE_BOUND
####################################################################
$ADDINSTANCE_BOUND
this.CLSACC(1) = NewInstance C_GACCCLS(this.ACTX,NullUuid,1,this,"CLSACC") AllocGroup this
this.CLSACC(1).ASTALIN = [V]CST_ANEW
[L]ASTATUS = fmet this.CLSACC(1).ACTION("ADDLINE_AFTER")
If [L]ASTATUS>=[V]CST_AERROR : [L]ASTATUS = [V]CST_ANOTDEFINED : Return : Endif
this.CLSCOA(1) = NewInstance C_GCOA(this.ACTX,NullUuid,1,this,"CLSCOA") AllocGroup this
this.CLSCOA(1).ASTALIN = [V]CST_ANEW
[L]ASTATUS = fmet this.CLSCOA(1).ACTION("ADDLINE_AFTER")
If [L]ASTATUS>=[V]CST_AERROR : [L]ASTATUS = [V]CST_ANOTDEFINED : Return : Endif
Return



