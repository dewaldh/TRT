# Supervisor
# Mask management YSLC1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone YCUSTYP
mkstat = 0
zoncou = "YCUSTYP"
IMPENT = [M]YCUSTYP(indice)
If varinit([M]YCUSTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]YCUSTYP,indice)=0
 [M]YCUSTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCUSTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YCUSTYP(indice)
 Endif
Endif
[M]YCUSTYP(indice) = IMPENT

## Zone YCUS
mkstat = 0
zoncou = "YCUS"
If varmode([M]YCUS,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCUS" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YSLNAM
mkstat = 0
zoncou = "YSLNAM"
If varmode([M]YSLNAM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSLNAM" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YSLEML
mkstat = 0
zoncou = "YSLEML"
If varmode([M]YSLEML,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSLEML" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YSLTEL
mkstat = 0
zoncou = "YSLTEL"
WCHAR = [M]YSLTEL(indice)
SAVACT = GACTION : GACTION = "FMTTELPAY"
Call FMTTELPAY("AUS") From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]YSLTEL(indice) : Endif
If !mkstat
 If varmode([M]YSLTEL,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "YSLTEL" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "ACNTTEL"
  Call CONTTEL([M]YSLTEL(indice),"AUS") From CONTNUM
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "YSLTEL" : GZNUM = 12781 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WCHAR<>[M]YSLTEL(indice) or IMPMOD
    SAVACT = GACTION : GACTION = "ACNTTELMOD"
    Call CONTTELMOD([M]YSLTEL(indice),"AUS") From CONTNUM
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "YSLTEL" : GZNUM = 12781 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone YFIRB
mkstat = 0
zoncou = "YFIRB"
IMPENT = [M]YFIRB(indice)
If varinit([M]YFIRB,indice)=0
 IMPENT = 1
Endif
If varmode([M]YFIRB,indice)=0
 [M]YFIRB(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YFIRB" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YFIRB(indice)
 Endif
Endif
[M]YFIRB(indice) = IMPENT

## Zone YNOMSTA
mkstat = 0
zoncou = "YNOMSTA"
IMPENT = [M]YNOMSTA(indice)
If varinit([M]YNOMSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YNOMSTA,indice)=0
 [M]YNOMSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YNOMSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YNOMSTA(indice)
 Endif
Endif
[M]YNOMSTA(indice) = IMPENT

## Zone YNOMDAT
mkstat = 0
zoncou = "YNOMDAT"
If varmode([M]YNOMDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YNOMDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YNOMACT
mkstat = 0
zoncou = "YNOMACT"
If varmode([M]YNOMACT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YNOMACT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YRESDAT
mkstat = 0
zoncou = "YRESDAT"
If varmode([M]YRESDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YRESDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YSLCMT
mkstat = 0
zoncou = "YSLCMT"
If varmode([M]YSLCMT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSLCMT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
