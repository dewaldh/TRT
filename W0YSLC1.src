 Endif
Endif

## Zone YSLTEL
mkstat = 0
zoncou = "YSLTEL"
WCHAR = [M]YSLTEL(indice)
SAVACT = GACTION : GACTION = "FMTTELPAY"
Call FMTTELPAY("AUS") From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]YSLTEL(indice) : Endif
If !mkstat
 If varmode([M]YSLTEL,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "YSLTEL" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "ACNTTEL"
  Call CONTTEL([M]YSLTEL(indice),"AUS") From CONTNUM
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "YSLTEL" : GZNUM = 12781 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WCHAR<>[M]YSLTEL(indice) or IMPMOD
    SAVACT = GACTION : GACTION = "ACNTTELMOD"
    Call CONTTELMOD([M]YSLTEL(indice),"AUS") From CONTNUM
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "YSLTEL" : GZNUM = 12781 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

