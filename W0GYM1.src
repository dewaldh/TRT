# Supervisor
# Mask management GYM1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone SHOTRA
mkstat = 0
zoncou = "SHOTRA"
indice = 0
If varinit([M]SHOTRA,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]SHOTRA,"GACCPYM","SHOTRA",[M]PYM,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]SHOTRA,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "SHOTRA" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone COA
mkstat = 0
zoncou = "COA"
indice = 0
If varmode([M]COA,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COA" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]COA,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "COA" : GZNUM = 38357 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("COA","",[M]COA,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "COA" : GZNUM = 38357 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone ACS
mkstat = 0
zoncou = "ACS"
indice = 0
If varmode([M]ACS,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ACS" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ACS","!",[M]ACS,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "ACS" : GZNUM = 71 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GYM1"
Return

