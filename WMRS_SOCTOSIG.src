$METHODS
$EVENTS
Case [L]AEVENT
 When "AREAD_AFTER"
  Case [L]CURPTH
   When "AWS" : Gosub ADDACS1
   When "" : Gosub ADDACSREP
  Endcase
 When "AINIT"
  Case [L]CURPTH
   When "AWS" : Gosub ADDACS1
   When "" : Gosub ADDACSREP
  Endcase
 When Default
Endcase
Return

