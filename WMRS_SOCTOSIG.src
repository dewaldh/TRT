$PROPERTIES
Case [L]ARULE
 When "CONTROL"
  Case [L]CURPRO
   When "AWS.CPY"
    If this.CPY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTCPY(this,"GESSOR",this.CPY,[V]CST_ATRUE)
    Endif
   When Default
  Endcase
 When "PROPAGATE"
  Case [L]CURPRO
   When Default
  Endcase
 Endcase
 Return
 
