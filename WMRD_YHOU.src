  YHOU.YHOU.YHJC(A_N2).ASTALIN = [V]CST_ADEL
  [L]ASTATUS = max([L]ASTATUS,fmet YHOU.YHOU.YHJC(A_N2)._ADELETE_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If YHOU.YHOU.YHJP(A_N2) <> null
& and YHOU.YHOU.YHJP(A_N2).ASTALIN<>[V]CST_ANEWDEL
 If YHOU.YHOU.YHJP(A_N2).ASTALIN<>[V]CST_ANEW
  YHOU.YHOU.YHJP(A_N2).ASTALIN = [V]CST_ADEL
  [L]ASTATUS = max([L]ASTATUS,fmet YHOU.YHOU.YHJP(A_N2)._ADELETE_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If YHOU.YHOU.YHDH(A_N2) <> null
& and YHOU.YHOU.YHDH(A_N2).ASTALIN<>[V]CST_ANEWDEL
 If YHOU.YHOU.YHDH(A_N2).ASTALIN<>[V]CST_ANEW
  YHOU.YHOU.YHDH(A_N2).ASTALIN = [V]CST_ADEL
  [L]ASTATUS = max([L]ASTATUS,fmet YHOU.YHOU.YHDH(A_N2)._ADELETE_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If YHOU.YHOU.YHJF(A_N2) <> null
& and YHOU.YHOU.YHJF(A_N2).ASTALIN<>[V]CST_ANEWDEL
 If YHOU.YHOU.YHJF(A_N2).ASTALIN<>[V]CST_ANEW
  YHOU.YHOU.YHJF(A_N2).ASTALIN = [V]CST_ADEL
  [L]ASTATUS = max([L]ASTATUS,fmet YHOU.YHOU.YHJF(A_N2)._ADELETE_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YHOU.YHOU.YRMA) : # YRMA -- 8
 If YHOU.YHOU.YRMA(A_N2) <> null
& and YHOU.YHOU.YRMA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YHOU.YHOU.YRMA(A_N2).ASTALIN<>[V]CST_ANEW
   YHOU.YHOU.YRMA(A_N2).ASTALIN = [V]CST_ADEL
   [L]ASTATUS = max([L]ASTATUS,fmet YHOU.YHOU.YRMA(A_N2)._ADELETE_CONTROL_BEFORE)
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

