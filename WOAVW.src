$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(12)(0..GNBDERNIER)
Local Char     A_BUF2(80)(0..GNBDERNIER)
Local Char     A_BUF3(8)(0..GNBDERNIER)
Local Libelle  A_BUF4(0..GNBDERNIER)
If clalev([F:AVW9])>0 : Close Local File [AVW9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(12),
& Char     A_TAB2(80),
& Char     A_TAB3(8),
& Libelle  A_TAB4
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4
& As [AVW9]
Return

$LEC_DERLU
Read [ATX]NUMERO = GLANGUE ; [F:AVW]INTIT
If fstat : Raz [F:ATX] : Endif
Return

$CHARGE_DERLU
If clalev([F:AVW9])>0
 Read [AVW9] = [F:AVW9]A_TABI
 [L]CLE = [F:AVW9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:AVW9])>0
 Filter [AVW9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AVW1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AVW9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AVW]CODVUE+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AVW9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AVW9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
