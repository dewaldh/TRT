# Supervisor
# Mask management GPCCAB0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone CPTBATICO
mkstat = 0
zoncou = "CPTBATICO"
indice = 0
If varmode([M]CPTBATICO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPTBATICO" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone LEDTYP
mkstat = 0
zoncou = "LEDTYP"
indice = 0
IMPENT = [M]LEDTYP
WNUM = IMPENT
If varinit([M]LEDTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]LEDTYP,indice)=0
 Call INICOMBO(2644) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_LEDTYP(IMPENT) From TRTGPCCAB
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2644,1))))
 Endif
 [M]LEDTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LEDTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LEDTYP(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27517 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
    mkstat = 1 : GMESSAGE = mess(99,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27517 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WNUM<>IMPENT or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_LEDTYP(IMPENT) From TRTGPCCAB
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27517 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif
[M]LEDTYP = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GPCCAB0"
Return

