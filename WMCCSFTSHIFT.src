Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_SHFCOD From WMC1SFTSHIFT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_SHFCOD From WMC1SFTSHIFT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_SHFDES From WMC1SFTSHIFT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_SHFDES From WMC1SFTSHIFT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_SHFSTR From WMC1SFTSHIFT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_SHFSTR From WMC1SFTSHIFT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_SHFEND From WMC1SFTSHIFT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_SHFEND From WMC1SFTSHIFT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_SHFDUR From WMC1SFTSHIFT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_SHFDUR From WMC1SFTSHIFT : Gosub TESTALLSTAT :  Endif
For AINDCLA=1 To 10
