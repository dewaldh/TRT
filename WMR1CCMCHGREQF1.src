####################################################################
# 1 : BOMCODE BOM code ( Field C 4)
####################################################################
 
$SET_BOMCODE
If [L]_AOLDVAL<>this.BOMCODE
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_BOMCODE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.BOMCODE
 Gosub PROPAGATE_BOMCODE
Endif
Return

$INIT_BOMCODE
Gosub DECLARE_VAR From WMR0CCMCHGREQF1
[L]CURPRO = "BOMCODE"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_BOMCODE
Gosub DECLARE_VAR From WMR0CCMCHGREQF1
[L]CURPRO = "BOMCODE"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_BOMCODE               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_BOMCODE               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_BOMCODE
[L]CURPRO = "BOMCODE"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.BOMCODE=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_BOMCODE
[L]CURPRO = "BOMCODE"
Return

$_AOTHERS_BOMCODE
[L]CURPRO = "BOMCODE"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_BOMCODE
[L]CURPRO = "BOMCODE"
Return

$PROPAGATE_BOMCODE
this.isReadonly = 0
[L]CURPRO = "BOMCODE"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_BOMCODE
Gosub DECLARE_VAR From WMR0CCMCHGREQF1
[L]CURPRO = "BOMCODE"
$_LNKPRO_BOMCODE
Return


