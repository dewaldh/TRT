Return

Subprog D_RCPVCRTYP(VALEUR)
Variable Integer VALEUR
If varinit([M]RCPVCRTYP,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_RCPVCRNUM
Return

