 Call S_AFF_NUM(BASPAG,PARMSK,"CONSPAY1","SUBPAY","") From GACTION
 GACTION = SAVACT
Endif
If !GWEBSERV
 ## Action CONSCPT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONSCPT"
 PARMSK(1) = [M]CPY
 PARMSK(2) = [M]FCY
 PARMSK(3) = num$(GTYPGEN)
