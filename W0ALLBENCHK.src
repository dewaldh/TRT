# Supervisor
# Mask management ALLBENCHK 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

indice = 0
status = 1
zoncou = "NBLIG"
If !mkstat
 NBLIG = [M]NBLIG
 For nolign=1 To NBLIG
  indice = nolign-1
  [M]NBLIG = nolign
  status = 75
  Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1


## Zone PECFLG
mkstat = 0
zoncou = "PECFLG"
IMPENT = [M]PECFLG(indice)
If varinit([M]PECFLG,indice)=0
 IMPENT = 1
Endif
[M]PECFLG(indice) = IMPENT

## Zone XPECFLG
mkstat = 0
zoncou = "XPECFLG"
IMPENT = [M]XPECFLG(indice)
If varinit([M]XPECFLG,indice)=0
 IMPENT = 1
Endif
[M]XPECFLG(indice) = IMPENT


End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ALLBENCHK"
Return

