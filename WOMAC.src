Endif
Return

$CLE_DERLU
If clalev([F:MAC9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:MAC]MACNUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:MAC9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:MAC9]
 Endif
Endif
Return

