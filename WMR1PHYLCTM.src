####################################################################
# Labels of the main class and methods 
####################################################################
 
####################################################################
# 1 : PHYELTIVY Stock to count ( Field L 4)
####################################################################
 
$GET_PHYELTIVY
Gosub DECLARE_VAR From WMR0PHYLCTM
[L]CURPRO = "PHYELTIVY"
[L]ARULE = "GET" : [L]ACTION = "GET" : Gosub ACTION From ASYRSUB
Return

$SET_PHYELTIVY
If [L]_AOLDVAL<>this.PHYELTIVY
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_PHYELTIVY
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.PHYELTIVY
 Gosub PROPAGATE_PHYELTIVY
Endif
Return

$INIT_PHYELTIVY
Gosub DECLARE_VAR From WMR0PHYLCTM
[L]CURPRO = "PHYELTIVY"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_PHYELTIVY
Gosub DECLARE_VAR From WMR0PHYLCTM
[L]CURPRO = "PHYELTIVY"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_PHYELTIVY             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_PHYELTIVY             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_PHYELTIVY
[L]CURPRO = "PHYELTIVY"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.PHYELTIVY=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_PHYELTIVY
[L]CURPRO = "PHYELTIVY"
Return

$_AOTHERS_PHYELTIVY
[L]CURPRO = "PHYELTIVY"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_PHYELTIVY
[L]CURPRO = "PHYELTIVY"
Return

$PROPAGATE_PHYELTIVY
this.isReadonly = 0
[L]CURPRO = "PHYELTIVY"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_PHYELTIVY
Gosub DECLARE_VAR From WMR0PHYLCTM
[L]CURPRO = "PHYELTIVY"
$_LNKPRO_PHYELTIVY
Return


