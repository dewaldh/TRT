Append WCLOB,',"$facility":"{STOFCY}"'
Append WCLOB,',"$properties":{'
Append WCLOB,'"$resources":{'
Append WCLOB,'"$type":"application/x-array"'
Append WCLOB,',"$item":{'
Append WCLOB,'"$title":"'+func GET_RES_STRING(ACTX.LAN,8437)+' {$key}"'
Append WCLOB,',"$key":"{SRHNUM}"'
Append WCLOB,',"$url":"{$baseUrl}/SRETURNH1('+"'"+'{$key}'+"'"+')'+PSYR.URLREP+'SRETURNH1.searchItem"'
Append WCLOB,',"$properties":{'
Append WCLOB,'"STOFCY":{'
