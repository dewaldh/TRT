   If mkstat : GERR = 1 : GZONE = "MFGTRKNUM" : GZNUM = 10780 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_MFGTRKNUM([M]MFGTRKNUM) From SUBMTK
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "MFGTRKNUM" : GZNUM = 10780 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone MFGNUM
mkstat = 0
zoncou = "MFGNUM"
indice = 0
WCHAR = [M]MFGNUM
If varmode([M]MFGNUM,indice)=0
