  Call C_MFGTRKDAT([M]MFGTRKDAT) From SUBMTK
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "MFGTRKDAT" : GZNUM = 10781 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

