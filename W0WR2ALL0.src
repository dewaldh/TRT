# Sage X3 supervisor
# Mask management WR2ALL0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone MFGFCY
mkstat = 0
zoncou = "MFGFCY"
indice = 0
WCHAR = [M]MFGFCY
If varmode([M]MFGFCY,indice)=0
 If !(GREP = "C")
  mkstat = 2
 Endif
 If mkstat=1 : Raz [M]MFGFCY : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call AS_MFGFCY([M]MFGFCY) From SUBMTK
  GACTION = SAVACT
  If mkstat=1 : Raz [M]MFGFCY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "MFGFCY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   If vireblc([M]MFGFCY,2)=""
    mkstat = 1 : GMESSAGE = mess(10,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "ACONTFCY"
    Call CONTSITE([M]MFGFCY) From SUBAGF
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     Call CONTROLE("FCY","E",[M]MFGFCY,"",mkstat) From CONTOBJ
     If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "CONTFCY"
      Call CONTFCY([M]MFGFCY) From EXEFNC
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       If WCHAR<>[M]MFGFCY or IMPMOD
        SAVACT = GACTION : GACTION = "STD"
        Call AM_MFGFCY([M]MFGFCY) From SUBMTK
        GACTION = SAVACT
        If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
        Gosub AFF From GSAISIE
        If mkstat : End : Endif
       Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone MFGTRKNUM
mkstat = 0
zoncou = "MFGTRKNUM"
indice = 0
If varmode([M]MFGTRKNUM,indice)=0
 If !((GREP <> "C" & GREP <> "D") | GMANCOU = 2)
  mkstat = 2
 Endif
 If mkstat=1 : Raz [M]MFGTRKNUM : Endif
