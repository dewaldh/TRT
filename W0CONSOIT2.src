# Supervisor
# Mask management CONSOIT2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone WIPTYPSTA
mkstat = 0
zoncou = "WIPTYPSTA"
SAVACT = GACTION : GACTION = "STD"
Call AV_WIPTYPSTA([M]WIPTYPSTA(indice)) From CNSOITSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]WIPTYPSTA(indice) : Endif

## Zone WIPTYP
mkstat = 0
zoncou = "WIPTYP"
IMPENT = [M]WIPTYP(indice)
If varinit([M]WIPTYP,indice)=0
 IMPENT = 1
Endif
[M]WIPTYP(indice) = IMPENT

## Zone WIPSTA
mkstat = 0
zoncou = "WIPSTA"
IMPENT = [M]WIPSTA(indice)
If varinit([M]WIPSTA,indice)=0
 IMPENT = 1
Endif
[M]WIPSTA(indice) = IMPENT

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
IMPENT = [M]VCRTYP(indice)
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP(indice) = IMPENT

## Zone FMI
mkstat = 0
zoncou = "FMI"
IMPENT = [M]FMI(indice)
If varinit([M]FMI,indice)=0
 IMPENT = 1
Endif
[M]FMI(indice) = IMPENT

## Zone MRPMES
mkstat = 0
zoncou = "MRPMES"
IMPENT = [M]MRPMES(indice)
If varinit([M]MRPMES,indice)=0
 IMPENT = 1
Endif
[M]MRPMES(indice) = IMPENT

## Zone ORI
mkstat = 0
zoncou = "ORI"
IMPENT = [M]ORI(indice)
If varinit([M]ORI,indice)=0
 IMPENT = 1
Endif
[M]ORI(indice) = IMPENT

## Zone VCRTYPORI
mkstat = 0
zoncou = "VCRTYPORI"
IMPENT = [M]VCRTYPORI(indice)
If varinit([M]VCRTYPORI,indice)=0
 IMPENT = 1
Endif
[M]VCRTYPORI(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CONSOIT2"
Return

