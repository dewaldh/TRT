# Supervisor
# Mask management OPP5 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBOPP5
mkstat = 0
zoncou = "NBOPP5"
indice = 0
status = 1
zoncou = "NBOPP5"
If varmode([M]NBOPP5,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBOPP5" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBOPP5
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBOPP5 = nolign
   status = 75
   Call SAI_NBOPP5(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  SAVACT = GACTION : GACTION = "STD"
  Call C_NBOPP5 From SUBOPP
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NBOPP5" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBOPP5(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBOPP5
mkstat = 0
zoncou = "NBOPP5"
If !mkstat
 SAVACT = GACTION : GACTION = "STD"
 Call C_NBOPP5 From SUBOPP
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "NBOPP5" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone SBBPJT
mkstat = 0
zoncou = "SBBPJT"
WCHAR = [M]SBBPJT(indice)
If varmode([M]SBBPJT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SBBPJT" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]SBBPJT(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "SBBPJT" : GZNUM = 5296 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("PIM","E",[M]SBBPJT(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "SBBPJT" : GZNUM = 5296 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WCHAR<>[M]SBBPJT(indice) or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_SBBPJT([M]SBBPJT(indice)) From SUBOPP
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "SBBPJT" : GZNUM = 5296 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone NBOPP5
mkstat = 0
zoncou = "NBOPP5"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "OPP5"
Return

