# Supervisor
# Mask management UTILEG 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ACTION
mkstat = 0
zoncou = "ACTION"
indice = 0
IMPENT = [M]ACTION
If varinit([M]ACTION,indice)=0
 IMPENT = 1
Endif
If varmode([M]ACTION,indice)=0
 [M]ACTION(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ACTION" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ACTION(indice)
 Endif
Endif
[M]ACTION = IMPENT

## Zone MODLEG
mkstat = 0
zoncou = "MODLEG"
indice = 0
If varinit([M]MODLEG,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]MODLEG,"909","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]MODLEG,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "MODLEG" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]MODLEG,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "MODLEG" : GZNUM = 26213 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTADI"
   Call CONTADI([M]MODLEG,"909","") From SUBADI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "MODLEG" : GZNUM = 26213 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("ADI","",[M]MODLEG,"909",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "MODLEG" : GZNUM = 26213 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "UTILEG"
Return

