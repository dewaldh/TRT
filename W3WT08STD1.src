# Sage X3 supervisor
# Mask management WT08STD1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_WSTJQTY
Onevent BEFORE_AREA Gosub AV_WSTJQTY From W3WT08STD1
Return

$AV_WSTJQTY
If !GWEBSERV
 ## Action FMTQTY
 GUNTFMT="15F" : GUNTFMT = GUNTFMT1(max(find([M:STQ1]STU,GUNITE(1..GNBUNT)),1))
Endif
Gosub _AFF
Return

$ON_CRDFLG
Onevent INIT_AREA Call D_CRDFLG(zc) From W3WT08STD1
Return

Subprog D_CRDFLG(VALEUR)
Variable Integer VALEUR
If varinit([M]CRDFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_MVTFLG
Onevent INIT_AREA Call D_MVTFLG(zc) From W3WT08STD1
Return

Subprog D_MVTFLG(VALEUR)
Variable Integer VALEUR
If varinit([M]MVTFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_RENSMP
Onevent INIT_AREA Call D_RENSMP(zc) From W3WT08STD1
Return

Subprog D_RENSMP(VALEUR)
Variable Char    VALEUR()
If varinit([M]RENSMP,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"104","") From SUBADI
Endif
Gosub _AFF
End

$CHG_STY
Return


