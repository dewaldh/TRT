############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(OPPORCRM,ASTATUS)
Variable Instance OPPORCRM Using C_OPPORCRM
Variable Integer ASTATUS

If OPPORCRM = null : End : Endif

Gosub _AVARIABLES From WMC00OPPORCRM

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_OPPORCRM

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(OPPORCRM,ASTATUS)
Variable Instance OPPORCRM Using C_OPPORCRM
Variable Integer ASTATUS

If OPPORCRM = null : End : Endif

Gosub AVARIABLES From WMC00OPPORCRM
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
OPPORCRM.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_OPPORCRM
Else
 Gosub AUPDATE_OPPORCRM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = OPPORCRM.freeSnapshot
OPPORCRM.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet OPPORCRM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet OPPORCRM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet OPPORCRM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If OPPORCRM.ASTALIN = [V]CST_ANEWDEL : Return : Endif
OPPORCRM.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(OPPORCRM.ASTALIN)<>""
 [L]A_ASTALIN = OPPORCRM.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(OPPORCRM.STPSTB) : # OPPORSTB -- 2
 If OPPORCRM.STPSTB(A_N2) <> null
& and OPPORCRM.STPSTB(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If OPPORCRM.STPSTB(A_N2).ASTALIN<>[V]CST_ANEW & OPPORCRM.ASTALIN=[V]CST_ADEL
   OPPORCRM.STPSTB(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If OPPORCRM.STPSTB(A_N2).ASTALIN=[V]CST_ANEW & find(OPPORCRM.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   OPPORCRM.STPSTB(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(OPPORCRM.STPSTB(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPORCRM.STPSTB(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(OPPORCRM.STPSTA) : # OPPORSTA -- 3
 If OPPORCRM.STPSTA(A_N2) <> null
& and OPPORCRM.STPSTA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If OPPORCRM.STPSTA(A_N2).ASTALIN<>[V]CST_ANEW & OPPORCRM.ASTALIN=[V]CST_ADEL
   OPPORCRM.STPSTA(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If OPPORCRM.STPSTA(A_N2).ASTALIN=[V]CST_ANEW & find(OPPORCRM.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   OPPORCRM.STPSTA(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(OPPORCRM.STPSTA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPORCRM.STPSTA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(OPPORCRM.STPSTB) : # OPPORSTB -- 2
 If OPPORCRM.STPSTB(A_N2) <> null
& and OPPORCRM.STPSTB(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(OPPORCRM.STPSTB(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPORCRM.STPSTB(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(OPPORCRM.STPSTA) : # OPPORSTA -- 3
 If OPPORCRM.STPSTA(A_N2) <> null
& and OPPORCRM.STPSTA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(OPPORCRM.STPSTA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPORCRM.STPSTA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(OPPORCRM.ASTALIN)<>""
 [L]A_ASTALIN = OPPORCRM.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_OPPORCRM
Trbegin [F:OPPCRM]
 Gosub AUPDATE_OPPORCRM
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_OPPORCRM
[L]A_ASTALIN = OPPORCRM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00OPPORCRM._MAJSQL_OPPORCRM(OPPORCRM))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 For A_N2=1 To maxtab(OPPORCRM.STPSTB)
  If OPPORCRM.STPSTB(A_N2) <> null and
& (OPPORCRM.STPSTB(A_N2).ASTALIN=[L]A_TYPMAJ or
& OPPORCRM.STPSTB(A_N2).ASTALIN=[V]CST_ALL)
   If OPPORCRM.STPSTB(A_N2).ASTALIN=[L]A_TYPMAJ
    Case OPPORCRM.STPSTB(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTB(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(OPPORCRM.STPSTA)
  If OPPORCRM.STPSTA(A_N2) <> null and
& (OPPORCRM.STPSTA(A_N2).ASTALIN=[L]A_TYPMAJ or
& OPPORCRM.STPSTA(A_N2).ASTALIN=[V]CST_ALL)
   If OPPORCRM.STPSTA(A_N2).ASTALIN=[L]A_TYPMAJ
    Case OPPORCRM.STPSTA(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.STPSTA(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = OPPORCRM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet OPPORCRM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If OPPORCRM.snapshotEnabled=1
 [L]A_RETIK = OPPORCRM.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(OPPORCRM.STPSTB)
 If OPPORCRM.STPSTB(A_N2) <> null
  [L]ASTATUS2 = fmet OPPORCRM.STPSTB(A_N2)._AUPDATE_ABORT
  If OPPORCRM.STPSTB(A_N2).ASTALIN=[V]CST_ADEL or OPPORCRM.STPSTB(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance OPPORCRM.STPSTB(A_N2)
   OPPORCRM.STPSTB(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(OPPORCRM.STPSTA)
 If OPPORCRM.STPSTA(A_N2) <> null
  [L]ASTATUS2 = fmet OPPORCRM.STPSTA(A_N2)._AUPDATE_ABORT
  If OPPORCRM.STPSTA(A_N2).ASTALIN=[V]CST_ADEL or OPPORCRM.STPSTA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance OPPORCRM.STPSTA(A_N2)
   OPPORCRM.STPSTA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet OPPORCRM.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(OPPORCRM.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 OPPORCRM.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(OPPORCRM.STPSTB)
 If OPPORCRM.STPSTB(A_N2) <> null
  [L]ASTATUS2 = fmet OPPORCRM.STPSTB(A_N2)._AUPDATE_AFTER
  If OPPORCRM.STPSTB(A_N2).ASTALIN=[V]CST_ADEL or OPPORCRM.STPSTB(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance OPPORCRM.STPSTB(A_N2)
   OPPORCRM.STPSTB(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(OPPORCRM.STPSTA)
 If OPPORCRM.STPSTA(A_N2) <> null
  [L]ASTATUS2 = fmet OPPORCRM.STPSTA(A_N2)._AUPDATE_AFTER
  If OPPORCRM.STPSTA(A_N2).ASTALIN=[V]CST_ADEL or OPPORCRM.STPSTA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance OPPORCRM.STPSTA(A_N2)
   OPPORCRM.STPSTA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



