If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_FLGMODACE From WMC1GDIE : Gosub TESTALLSTAT :  Endif
For AINDCLA=1 To 10
 If this.MISCCE(AINDCLA)<>null& find(this.MISCCE(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.MISCCE(AINDCLA)._AMANDATORY : Gosub TESTALLSTAT
