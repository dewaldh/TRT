#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management FXDLGT1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:01:49"
A_USER  = "ADMIN"
A_STAMP = "20180828000149"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBCRIT"
  If [M]NBCRIT=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask FXDLGT1 [LGT1]
Gosub HORODAT
Local Inpbox "FXDLGT1" From GFONCTION At A_STAMP With A_WINDPREV Mask [LGT1] As [SIM_]
Grizo [LGT1]10
Grizo [LGT1]15
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [LGT1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=13
X1(1)=1 : X2(1)=74 : Y1(1)=14 : Y2(1)=15
Return

$ON_NBCRIT
Onevent BEFORE_AREA Call AV_NBCRIT From W1FXDLGT1
Return

Subprog AV_NBCRIT
Local Decimal BASPAG
## Action STD
Call AV_NBCRIT From SUBLGT
Gosub _AFF
End

$ON_CODZON
Onevent CONTROL Call C_CODZON(zc) From W1FXDLGT1
Onevent BEFORE_INPUT Call AS_CODZON(zc) From W1FXDLGT1
Return

Subprog AS_CODZON(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_CODZON(zc) From W1FXDLGT1
Gosub _AFF
End

Subprog C_CODZON(VALEUR)
Variable Char    VALEUR()
## Action STD
Call C_CODZON(VALEUR) From SUBLGT
If mkstat : GERR = 1 : GZONE = "CODZON" : GZNUM = 20 : Endif
Gosub _AFF
End

Subprog S_CODZON(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELAWILIK1
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELAWILIK1"
 PARMSK(1) = "OLOF"
 PARMSK(2) = num$(GVML_OMDEPEN)
 PARMSK(3) = [M]CPY
 PARMSK(4) = ""
 Call S_LIS_CHAR(VALEUR,PARMSK,"SELAWILIK1","TRTBIPLIK","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

$ON_PRILOF
Onevent INIT_AREA Call D_PRILOF(zc) From W1FXDLGT1
Onevent BEFORE_INPUT Call AS_PRILOF(zc) From W1FXDLGT1
Return

Subprog D_PRILOF(VALEUR)
Variable Integer VALEUR
If varinit([M]PRILOF,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_PRILOF(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$CHG_STY
Return


