#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management ALT0 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:48:48"
A_USER  = "ADMIN"
A_STAMP = "20180827234848"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask ALT0 [ALT0]
Gosub HORODAT
Local Inpbox "ALT0" From GFONCTION At A_STAMP With A_WINDPREV Mask [ALT0] As [SIM_]
Grizo [ALT0]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [ALT0]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=80 : Y1(0)=1 : Y2(0)=2
Return

$ON_COD
Onevent CONTROL Call C_COD(zc) From W1ALT0
Onevent BEFORE_INPUT Call AS_COD(zc) From W1ALT0
Return

Subprog AS_COD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_COD(zc) From W1ALT0
Onkey 144 Titled GBOUTP Call P_COD(zc) From W1ALT0
Onkey 146 Titled GBOUTA Call SA_COD(zc) From W1ALT0
Gosub _AFF
End

Subprog C_COD(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 5296 : Endif
If !mkstat
 ## Action CONTOBJW
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONTOBJW"
 PARMSK(1) = "ALT"
 Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJW","CONTOBJ","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 5296 : Endif
Endif
Gosub _AFF
End

Subprog S_COD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELOBJSTD
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELOBJSTD"
 PARMSK(1) = "ALT"
 PARMSK(2) = ""
 PARMSK(3) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJSTD","SELOBJ","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog SA_COD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ALH"
PARMSK(3) = "$"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_COD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ALH",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_INTIT
Onevent INIT_AREA Call D_INTIT(zc) From W1ALT0
Onevent BEFORE_INPUT Call AS_INTIT(zc) From W1ALT0
Return

Subprog IB_INTIT(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action BOUTTEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "BOUTTEXTRA"
 Call S_AFF_CHAR(VALEUR,PARMSK,"","ATEXTRA","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog D_INTIT(VALEUR)
Variable Char    VALEUR()
If varinit([M]INTIT,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"ALISTEH","INTIT",[M]COD,"") From ATEXTRA
Endif
Gosub _AFF
End

Subprog AS_INTIT(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(9637,GBOUT1) From OBJDIV
Call IB_INTIT(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_INTIT(zc) From W1ALT0
Gosub _AFF
End

Subprog B1_INTIT(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ATEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ATEXTRA"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_INTIT" : PROGSUB = "SUBALT" : PROGSPE = "SPEALT" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "ALISTEH"
 PARMSK(2) = "INTIT"
 PARMSK(3) = [M]COD
 PARMSK(4) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"ATEXTRA","ATEXTRA","") From GSAISIE
 ACTION = "BB1_INTIT" : PROGSUB = "SUBALT" : PROGSPE = "SPEALT" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

