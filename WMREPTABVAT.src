#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1TABVAT
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "TABVAT"
REP.CLA = "TABVAT"
REP.ICL = "TVT"
REP.AFCRIGHT = "GESTVT"
REP.NCL = 3505 : # Tax code table
REP.DES = 5342 : # Taxes
REP.TIT = "DESAXX"
REP.ANX = "TVT0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Tax
     REP.CLE(1).PAR = 'VAT' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'VAT' : REP.CLE(1).VPR= 'TVT.VAT' : REP.CLE(1).DES = 2554
REP.CLE(2) = NewInstance C_AREPPAR AllocGroup REP : # Legislation
     REP.CLE(2).PAR = 'LEG' : REP.CLE(2).TYP = 7 : REP.CLE(2).PRO= 'LEG' : REP.CLE(2).VPR= 'TVT.LEG' : REP.CLE(2).DES = 26213
# Facets
REP.AENAFAC(1) = 2 : # Detail
REP.AENAFAC(2) = 1 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "HEAD" : REP.SEC(1).NSC = 5042 : # Header
     REP.SEC(1).BLC(1)="HEAD" : REP.SEC(1).NBC(1)=5042 : # Header
Return

