# Supervisor
# Mask management AIU1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:48:36"
A_USER  = "ADMIN"
A_STAMP = "20180827234836"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask AIU1 [AIU1]
Gosub HORODAT
Local Inpbox "AIU1" From GFONCTION At A_STAMP With A_WINDPREV Mask [AIU1] As [SIM_]
Grizo [AIU1]5
Grizo [AIU1]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [AIU1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=2
X1(1)=1 : X2(1)=74 : Y1(1)=3 : Y2(1)=28
Return

$ON_PRF
Onevent CONTROL Call C_PRF(zc) From W1AIU1
Onevent BEFORE_INPUT Call AS_PRF(zc) From W1AIU1
Return

Subprog AS_PRF(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_PRF(zc) From W1AIU1
Onkey 144 Titled GBOUTP Call P_PRF(zc) From W1AIU1
Onkey 146 Titled GBOUTA Call SA_PRF(zc) From W1AIU1
Gosub _AFF
End

Subprog C_PRF(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "PRF" : GZNUM = 1469 : Endif
If !mkstat
 ## Action CONTOBJ
 Call C_CLE("AIU") From GOBJET
 If mkstat : GERR = 1 : GZONE = "PRF" : GZNUM = 1469 : Endif
Endif
Gosub _AFF
End

Subprog S_PRF(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "AIU"
PARMSK(2) = "$"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_PRF(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AIU"
PARMSK(3) = "$"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_PRF(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AIU",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_INTUSR
Onevent INIT_AREA Call D_INTUSR(zc) From W1AIU1
Onevent BEFORE_INPUT Call AS_INTUSR(zc) From W1AIU1
Return

Subprog IB_INTUSR(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action BOUTTEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "BOUTTEXTRA"
 Call S_AFF_CHAR(VALEUR,PARMSK,"","ATEXTRA","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog D_INTUSR(VALEUR)
Variable Char    VALEUR()
If varinit([M]INTUSR,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"ABIPRFUSR","INTUSR",[M]PRF,"") From ATEXTRA
Endif
Gosub _AFF
End

Subprog AS_INTUSR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(9637,GBOUT1) From OBJDIV
Call IB_INTUSR(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_INTUSR(zc) From W1AIU1
Gosub _AFF
End

Subprog B1_INTUSR(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ATEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ATEXTRA"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_INTUSR" : PROGSUB = "SUBAIU" : PROGSPE = "SPEAIU" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "ABIPRFUSR"
 PARMSK(2) = "INTUSR"
 PARMSK(3) = [M]PRF
 PARMSK(4) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"ATEXTRA","ATEXTRA","") From GSAISIE
 ACTION = "BB1_INTUSR" : PROGSUB = "SUBAIU" : PROGSPE = "SPEAIU" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_MODE
Onevent BEFORE_INPUT Call AS_MODE(zc) From W1AIU1
Onevent INIT_AREA Call D_MODE(zc) From W1AIU1
Onevent CONTROL Call C_MODE(zc) From W1AIU1
Return

Subprog D_MODE(VALEUR)
Variable Integer VALEUR
If varinit([M]MODE,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_MODE(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(7835) From COMBOS
## Action STD
Call AS_MODE(VALEUR) From SUBAIU
If mkstat=2
 [M]MODE(INDIC) = VALEUR
 Affzo MODE(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,7835,1))))
Endif
If !mkstat
 Discombo 0 From MODE(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From MODE(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_MODE(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
Gosub _AFF
End

$CHG_STY
Return


