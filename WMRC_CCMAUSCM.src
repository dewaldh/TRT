############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(CCMAUSCM,ASTATUS)
Variable Instance CCMAUSCM Using R_CCMAUSCM
Variable Integer ASTATUS

If CCMAUSCM = null : End : Endif

Gosub AVARIABLES From WMR00CCMAUSCM
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If fmet CCMAUSCM.ACTX.AGETAFCRIGHT(CCMAUSCM,"GESAUS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END : Endif
If fmet CCMAUSCM.ACTX.AGETAFCRIGHTC(CCMAUSCM,"GESAUS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END
Endif
If fmet CCMAUSCM.ACTX.AGETACSRIGHTM(CCMAUSCM,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AINSERT_END
  Endif
  
  Gosub AREPCONTROL_AINSERT
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
  
If !adxlog
 Gosub AINSERT_TR_CCMAUSCM
Else
 Gosub AINSERT_CCMAUSCM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_END : Return : Endif

Gosub _AINSERT_AFTER
$AINSERT_END
[L]A_I = CCMAUSCM.freeSnapshot
CCMAUSCM.snapshotEnabled = 1
End

#-----------------------------------------------------#
$AREPCONTROL_AINSERT
CCMAUSCM.ASTALIN = [V]CST_ANEW
CCMAUSCM.AUS.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSCM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSCM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSCM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If CCMAUSCM.AUS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
CCMAUSCM.AUS.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.AUS.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(CCMAUSCM.AUS.FNCCOL)
 If CCMAUSCM.AUS.FNCCOL([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.AUS.FNCCOL([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If CCMAUSCM.AUS.BPA(A_N2) <> null
& and !find(CCMAUSCM.AUS.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUSCM.AUS.BID) : # BID -- 3
 If CCMAUSCM.AUS.BID(A_N2) <> null
& and !find(CCMAUSCM.AUS.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
A_N2=1
If CCMAUSCM.AUS.BPA(A_N2) <> null
& and !find(CCMAUSCM.AUS.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUSCM.AUS.BID) : # BID -- 3
 If CCMAUSCM.AUS.BID(A_N2) <> null
& and !find(CCMAUSCM.AUS.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(CCMAUSCM.AUS.FNCCOL)
 If CCMAUSCM.AUS.FNCCOL([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.AUS.FNCCOL([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.AUS.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_CCMAUSCM
Trbegin [F:AUS]
 Gosub AINSERT_CCMAUSCM
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AINSERT_AFTER
If find(CCMAUSCM.AUS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 CCMAUSCM.AUS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(CCMAUSCM.AUS.FNCCOL)
 If CCMAUSCM.AUS.FNCCOL([L]A_I)<>null
  If CCMAUSCM.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSCM.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSCM.AUS.FNCCOL([L]A_I)
   CCMAUSCM.AUS.FNCCOL([L]A_I) = null
  Elsif CCMAUSCM.AUS.FNCCOL([L]A_I).ASTALIN <> [V]CST_ALL
   CCMAUSCM.AUS.FNCCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
Local Integer ASTATUS2
A_N2=1
If CCMAUSCM.AUS.BPA(A_N2) <> null
 If CCMAUSCM.AUS.BPA(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSCM.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance CCMAUSCM.AUS.BPA(A_N2)
  CCMAUSCM.AUS.BPA(A_N2) = null
 Elsif CCMAUSCM.AUS.BPA(A_N2).ASTALIN <> [V]CST_ALL
  CCMAUSCM.AUS.BPA(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(CCMAUSCM.AUS.BPA(A_N2).COLTEL)
   If CCMAUSCM.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If CCMAUSCM.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSCM.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUSCM.AUS.BPA(A_N2).COLTEL([L]A_I)
     CCMAUSCM.AUS.BPA(A_N2).COLTEL([L]A_I) = null
    Elsif CCMAUSCM.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUSCM.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
  For [L]A_I=1 To maxtab(CCMAUSCM.AUS.BPA(A_N2).COLWEB)
   If CCMAUSCM.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If CCMAUSCM.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSCM.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUSCM.AUS.BPA(A_N2).COLWEB([L]A_I)
     CCMAUSCM.AUS.BPA(A_N2).COLWEB([L]A_I) = null
    Elsif CCMAUSCM.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUSCM.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(CCMAUSCM.AUS.BID)
 If CCMAUSCM.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet CCMAUSCM.AUS.BID(A_N2)._AINSERT_AFTER
  If CCMAUSCM.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSCM.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSCM.AUS.BID(A_N2)
   CCMAUSCM.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AINSERT_CCMAUSCM
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.AUS._AINSERT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSCM.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If CCMAUSCM.snapshotEnabled=1
 [L]A_RETIK = CCMAUSCM.revertUpdTick
Endif
[L]ASTATUS = max(fmet CCMAUSCM.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



