# Supervisor
# Mask management ADOCFIC 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone TYPEXP
mkstat = 0
zoncou = "TYPEXP"
indice = 0
IMPENT = [M]TYPEXP
WNUM = IMPENT
If varinit([M]TYPEXP,indice)=0
 IMPENT = 1
Endif
If varmode([M]TYPEXP,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_TYPEXP(IMPENT) From ADOCPIC
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]TYPEXP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYPEXP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TYPEXP(indice)
 Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_TYPEXP(IMPENT) From ADOCPIC
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "TYPEXP" : GZNUM = 32516 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]TYPEXP = IMPENT

## Zone FICHIER
mkstat = 0
zoncou = "FICHIER"
indice = 0
If varmode([M]FICHIER,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FICHIER" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_FICHIER([M]FICHIER) From ADOCPIC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "FICHIER" : GZNUM = 799 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ADOCFIC"
Return

