# Supervisor
# Mask management GYM0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone PYM
mkstat = 0
zoncou = "PYM"
indice = 0
If varmode([M]PYM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PYM" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]PYM,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "PYM" : GZNUM = 11308 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "GYM"
  Local Char    VALEUR(250)
  VALEUR = [M]PYM
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]PYM = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "PYM" : GZNUM = 11308 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DESTRA
mkstat = 0
zoncou = "DESTRA"
indice = 0
If varinit([M]DESTRA,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DESTRA,"GACCPYM","DESTRA",[M]PYM,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DESTRA,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DESTRA" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GYM0"
Return

