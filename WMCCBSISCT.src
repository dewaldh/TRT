Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CODSCT From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CODSCT From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_DESAXX From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_DESAXX From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LNGMAX From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LNGMAX From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_FILTYP From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_FILTYP From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
