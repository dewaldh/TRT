 If this.LINES(AINDCLA)<>null& find(this.LINES(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.LINES(AINDCLA)._AOTHERS : Gosub TESTALLSTAT
 Endif
Next AINDCLA
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREDATTIM From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREDATTIM From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDATTIM From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDATTIM From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AUUID From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AUUID From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREUSR From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREUSR From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDUSR From WMC1BSISCT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDUSR From WMC1BSISCT : Gosub TESTALLSTAT :  Endif
[L]ASTATUS = fmet this.ACONTROL
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



