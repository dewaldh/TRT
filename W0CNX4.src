# Supervisor
# Mask management CNX4 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBUPD
mkstat = 0
zoncou = "NBUPD"
indice = 0
status = 1
zoncou = "NBUPD"
If varmode([M]NBUPD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBUPD" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBUPD
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBUPD = nolign
   status = 75
   Call SAI_NBUPD(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBUPD(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBUPD
mkstat = 0
zoncou = "NBUPD"

## Zone NBUPD
mkstat = 0
zoncou = "NBUPD"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CNX4"
Return

