#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management AOBJTXTA 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:49:18"
A_USER  = "ADMIN"
A_STAMP = "20180827234918"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask AOBJTXTA [DIA]
Gosub HORODAT
Local Inpbox "AOBJTXTA" From GFONCTION At A_STAMP With A_WINDPREV Mask [DIA] As [SIM_]
Grizo [DIA]5
Grizo [DIA]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [DIA]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=37 : X2(0)=76 : Y1(0)=13 : Y2(0)=14
X1(1)=37 : X2(1)=76 : Y1(1)=15 : Y2(1)=16
Return

$ON_MOTCLE
Onevent BEFORE_INPUT Call AS_MOTCLE(zc) From W1AOBJTXTA
Return

Subprog AS_MOTCLE(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_MOTCLE(zc) From W1AOBJTXTA
Gosub _AFF
End

Subprog S_MOTCLE(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELMOTCLE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELMOTCLE"
 Call S_LIS_CHAR(VALEUR,PARMSK,"SELMOTCLE","SELOBJTXT","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

$ON_ALLTYP
Onevent INIT_AREA Call D_ALLTYP(zc) From W1AOBJTXTA
Onevent BEFORE_INPUT Call AS_ALLTYP(zc) From W1AOBJTXTA
Return

Subprog D_ALLTYP(VALEUR)
Variable Integer VALEUR
If varinit([M]ALLTYP,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_ALLTYP(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_TYPDOC
Onevent INIT_AREA Call D_TYPDOC(zc) From W1AOBJTXTA
Onevent CONTROL Call C_TYPDOC(zc) From W1AOBJTXTA
Onevent BEFORE_INPUT Call AS_TYPDOC(zc) From W1AOBJTXTA
Return

Subprog D_TYPDOC(VALEUR)
Variable Char    VALEUR()
If varinit([M]TYPDOC,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"902","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_TYPDOC(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"902") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
If !GWEBSERV
 ## Action STD
 Call AS_TYPDOC(VALEUR) From SELOBJTXT
 If mkstat=2
  [M]TYPDOC(INDIC) = VALEUR
  Affzo TYPDOC(INDIC) : mkstat = 2
 Endif
Endif
Onkey 129 Titled GBOUT1 Call B1_TYPDOC(zc) From W1AOBJTXTA
Onkey   2 Titled GBOUTS Call S_TYPDOC(zc) From W1AOBJTXTA
Onkey 144 Titled GBOUTP Call P_TYPDOC(zc) From W1AOBJTXTA
Onkey 146 Titled GBOUTA Call SA_TYPDOC(zc) From W1AOBJTXTA
Gosub _AFF
End

Subprog C_TYPDOC(VALEUR)
Variable Char    VALEUR()
## Action CONTADI
Call CONTADI(VALEUR,"902","") From SUBADI
If mkstat : GERR = 1 : GZONE = "TYPDOC" : GZNUM = 865 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("ADI","!",VALEUR,"902",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "TYPDOC" : GZNUM = 865 : Endif
Endif
Gosub _AFF
End

Subprog S_TYPDOC(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = "902"
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_TYPDOC(VALEUR)
Variable Char    VALEUR()
Call OBJET("ADI",VALEUR,"902") From GOBJET
Gosub _AFF
End

Subprog SA_TYPDOC(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(2) = "902"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_TYPDOC(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "902"
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$CHG_STY
Return


