$LEC_DERLU
Read [AYI]AYI0=[F:AYE]FCYCOD;[F:AYE]INTCOD
If fstat : Raz [F:AYI] : Endif
Read [AX3]AXX0="AYTENT";"INTIT";GLANGUE;FCYCOD;ENTCOD
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:AYE9])>0
 Read [AYE9] = [F:AYE9]A_TABI
 [L]CLE = [F:AYE9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:AYE9])>0
 Filter [AYE9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AYE1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AYE9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AYE]ENTCOD+"~"
 A_CLEDERLU += [F:AYE]FCYCOD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AYE9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AYE9]
 Endif
Endif
Return

