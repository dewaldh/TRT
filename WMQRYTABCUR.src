#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:TCU])=0 : Local File TABCUR [TCU] : Endif
If clalev([F:WAXX1])=0 : Local File ATEXTRA [WAXX1] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [TCU]
LogicClose File [WAXX1]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"TCU","TCU",ACRITROLE,[V]CST_AFALSE)
Link [TCU] With
& [F:WAXX1]AXX0 = "TABCUR";"INTDES";GACTX.LAN;[F:TCU]CUR;""
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"TCU","TCU",ACRITROLE,[V]CST_AFALSE)
Link [TCU] With
& [F:WAXX1]AXX0 = "TABCUR";"INTDES";GACTX.LAN;[F:TCU]CUR;""
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
Return
 
#-------------------------------------------------------------------#
# Default filter for the query
#-------------------------------------------------------------------#
$AQUERY_FILTER_DEF
Return
 
#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# TCU --- TABCUR(TCU)
QRY(NBREP) = NewInstance R_TABCUR AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:TCU]AUUID)>0 & [F:TCU]AUUID=NullUuid
 [F:TCU]AUUID = getUuid
Endif
SetInstance QRY(NBREP).TCU With [F:TCU]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
Call ASETTEXTRA(QRY(NBREP).TCU.AXX,"QRY","INTDES",[F:WAXX1]TEXTE,[F:WAXX1]LANGUE) From ASYRSUB
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# TCU --- TABCUR(TCU)
QRY = NewInstance R_TABCUR AllocGroup PQRY
QRY.TCU.AXX(1) = NewInstance C_ATXTTRA AllocGroup QRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:TCU]AUUID)>0 & [F:TCU]AUUID=NullUuid
 [F:TCU]AUUID = getUuid
Endif
SetInstance QRY.TCU With [F:TCU]
QRY.TCU.AXX(1).PRO = "INTDES"
QRY.TCU.AXX(1).LAN = [F:WAXX1]LANGUE
QRY.TCU.AXX(1).TEX = [F:WAXX1]TEXTE
QRY.TCU.AXX(1).FLG = 1
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

#-------------------------------------------------------------------#
# Additionnal criteria of query
#-------------------------------------------------------------------#
$AQUERY_CRITERIA
Gosub DECLARE_VAR From WMC0TABCUR
CURQRY = "R_TABCUR"
ACTION = "AQUERY_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# LNKPRO of query 
#-------------------------------------------------------------------#
$AQUERY_LNKPRO
Callmet QRY(NBREP).TCU.CUR.LNKPRO
Return

#-------------------------------------------------------------------#
# LNKPRO of query INSTANCE
#-------------------------------------------------------------------#
$AQUERY_LNKPRO_INSTANCE
Callmet QRY.TCU.CUR.LNKPRO
Return

#-------------------------------------------------------------------#
# LOAD_PROPERTIES of query
#-------------------------------------------------------------------#
$AQUERY_DECODE_CRITERIA
NBPRO = 0
NBPRO += 1 : TBPRO(NBPRO) = "CUR" : TBFIL(NBPRO) = "[F:TCU]CUR"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "TCU.CUR"
NBPRO += 1 : TBPRO(NBPRO) = "INTDES" : TBFIL(NBPRO) = "[F:WAXX1]TEXTE"
           : TBTYP(NBPRO) = 26 : TBPTH(NBPRO) = "TCU.INTDES"
NBPRO += 1 : TBPRO(NBPRO) = "ISOCOD" : TBFIL(NBPRO) = "[F:TCU]ISOCOD"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "TCU.ISOCOD"
NBPRO += 1 : TBPRO(NBPRO) = "CURSYM" : TBFIL(NBPRO) = "[F:TCU]CURSYM"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "TCU.CURSYM"
ACTION = "AQUERY_DECODE_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

