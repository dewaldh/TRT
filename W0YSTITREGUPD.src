# Supervisor
# Mask management YSTITREGUPD 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YTITREGDAT
mkstat = 0
zoncou = "YTITREGDAT"
indice = 0
If varmode([M]YTITREGDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YTITREGDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YREAREG
mkstat = 0
zoncou = "YREAREG"
indice = 0
If varmode([M]YREAREG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YREAREG" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YOLDSTA
mkstat = 0
zoncou = "YOLDSTA"
indice = 0
IMPENT = [M]YOLDSTA
If varinit([M]YOLDSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YOLDSTA,indice)=0
 [M]YOLDSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YOLDSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YOLDSTA(indice)
 Endif
Endif
[M]YOLDSTA = IMPENT

## Zone YUSRREG
mkstat = 0
zoncou = "YUSRREG"
indice = 0
If varmode([M]YUSRREG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YUSRREG" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("AUS","!",[M]YUSRREG,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YUSRREG" : GZNUM = 3976 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YSTITREGUPD"
Return

