$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(20)(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
Local Libelle  A_BUF3(0..GNBDERNIER)
If clalev([F:ADLV9])>0 : Close Local File [ADLV9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(20),
& Char     A_TAB2(30),
& Libelle  A_TAB3
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3
& As [ADLV9]
Return

$LEC_DERLU
Read [AX3]AXX0="ADELIVER";"DLVDES";GLANGUE;COD;""
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:ADLV9])>0
 Read [ADLV9] = [F:ADLV9]A_TABI
 [L]CLE = [F:ADLV9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:ADLV9])>0
 Filter [ADLV9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [ADLV4] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:ADLV9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:ADLV]COD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:ADLV9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:ADLV9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 0
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:ADLV]COD"
Call TEXTE(5296,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:20X"
SELFOR(NBSEL) = "K:20X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:ADLV]COD"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(2)"
SELEXP(NBSEL) = "[F:AX3]TEXTE"
Call TEXTE(0,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = "K:30X"
SELMAJ(NBSEL) = 0
NBSEL += 1
SELZON(NBSEL) = "[F:ADLV]ENAFLG"
Call TEXTE(188,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 1
SELFMT(NBSEL) = "LA00001:4X"
SELFOR(NBSEL) = ""
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:ADLV]ENAFLG"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:ADLV_])=0
 Gosub OUVFIC_LISTE
Endif
Return

