Subprog AS_FCY(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_FCY(zc) From W1MFASTRFR
Onkey 144 Titled GBOUTP Call P_FCY(zc) From W1MFASTRFR
Onkey 146 Titled GBOUTA Call SA_FCY(zc) From W1MFASTRFR
Gosub _AFF
End

Subprog C_FCY(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
If !mkstat
 ## Action ACONTFCY
 Call CONTSITE(VALEUR) From SUBAGF
 If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("FCY","CI",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
  If !mkstat
   ## Action CONTFCYCPY
   Call CONTFCYCPY(VALEUR,[M:MFS0]CPY(nolign-1)) From EXEFNC
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
   If !mkstat
    ## Action CONTFCY
    Call CONTFCY(VALEUR) From EXEFNC
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
   Endif
  Endif
 Endif
Endif
Gosub _AFF
End

