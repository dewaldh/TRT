   SAVACT = GACTION : GACTION = "ADATRELM"
   Call MODIFDAT([M]MFGDATDEB,[M:CMKM]MFGDATDEBF) From ADATREL
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "MFGDATDEB" : GZNUM = 17859 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call AM_MFGDATDEB([M]MFGDATDEB) From CNSMKMSTD
