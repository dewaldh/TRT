If varinit([M]PRVFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_PRVFLG(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

