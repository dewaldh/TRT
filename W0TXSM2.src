# Supervisor
# Mask management TXSM2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBPAR
mkstat = 0
zoncou = "NBPAR"
indice = 0
status = 1
zoncou = "NBPAR"
If varmode([M]NBPAR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBPAR" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBPAR
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBPAR = nolign
   status = 75
   Call SAI_NBPAR(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBVAR
mkstat = 0
zoncou = "NBVAR"
indice = 0
status = 1
zoncou = "NBVAR"
If varmode([M]NBVAR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBVAR" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBVAR
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBVAR = nolign
   status = 75
   Call SAI_NBVAR(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBPAR(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBPAR
mkstat = 0
zoncou = "NBPAR"

## Zone NBPAR
mkstat = 0
zoncou = "NBPAR"

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBVAR(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBVAR
mkstat = 0
zoncou = "NBVAR"

## Zone NBVAR
mkstat = 0
zoncou = "NBVAR"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "TXSM2"
Return

