####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "C_GRP"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_GRP",this)
 When "C_SATTAX"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_SATTAX",this)
 When "C_TSICOD"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_TSICOD",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# ADDLINE
####################################################################
$ADDLINE
Case _ACOLLECTION
 When "C_GRP"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_GRP","C_TAXLINK_C_GRP",this)
 When "C_SATTAX"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_SATTAX","C_TAXLINK_C_SATTAX",this)
 When "C_TSICOD"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_TSICOD","C_TAXLINK_C_TSICOD",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

