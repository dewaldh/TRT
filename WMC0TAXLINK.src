####################################################################
# AERRORS
####################################################################

$_AERROR
Gosub DECLARE_VAR From WMC0TAXLINK
Local Integer ALLSTAT
Call ALLERRORS(this,_CTX,_SUPP) From ASYRSUB
For AINDCLA=1 To maxtab(this.C_GRP)
 If this.C_GRP(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_GRP(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
For AINDCLA=1 To maxtab(this.C_SATTAX)
 If this.C_SATTAX(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_SATTAX(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_TSICOD(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_TSICOD(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
Return

####################################################################
# Delete all errors
####################################################################

$_ADELERRORS
Gosub DECLARE_VAR From WMC0TAXLINK
Local Integer ALLSTAT
[L]ASTATUS = fmet this.ADELETEERROR([L]_APARAM)
For AINDCLA=1 To maxtab(this.C_GRP)
 If this.C_GRP(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_GRP(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
For AINDCLA=1 To maxtab(this.C_SATTAX)
 If this.C_SATTAX(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_SATTAX(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_TSICOD(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_TSICOD(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
Return

####################################################################
# _ASEARCHINFOS
####################################################################

$_ASEARCHINFOS
Gosub DECLARE_VAR From WMC0TAXLINK
ASTATUS= func ASYRSUB.ASEARCHINFOS(this,[L]AINS_ORI,[L]ERR_TYPE,[L]PROPERTY,[L]ASTAT,[L]SELSTAT,[L]PATH)
If [L]ERR_TYPE=[V]CST_ACURRENT : Return : Endif
For AINDCLA=1 To maxtab(this.C_GRP)
 If this.C_GRP(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_GRP(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_GRP('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
For AINDCLA=1 To maxtab(this.C_SATTAX)
 If this.C_SATTAX(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_SATTAX(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_SATTAX('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_TSICOD(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_TSICOD(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_TSICOD('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
Return

