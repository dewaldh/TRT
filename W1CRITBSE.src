Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:58:08"
A_USER  = "ADMIN"
A_STAMP = "20180827235808"
Return

$TITRES
