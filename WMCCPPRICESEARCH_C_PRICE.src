####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0PPRICESEARCH_C_PRICE
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LIBPRIX From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LIBPRIX From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_GROPRI From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_GROPRI From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_NETPRI From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_NETPRI From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AUTRE From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AUTRE From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_TOTAL From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_TOTAL From WMC1PPRICESEARCH_C_PRICE : Gosub TESTALLSTAT :  Endif
[L]ASTATUS = fmet this.ACONTROL
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



