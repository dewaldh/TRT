Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

indice = 0
status = 1
zoncou = "NBRLNK"
If !mkstat
 NBLIG = [M]NBRLNK
 For nolign=1 To NBLIG
  indice = nolign-1
  [M]NBRLNK = nolign
  status = 75
  Call SAI_NBRLNK(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

mkstat = 0

End

