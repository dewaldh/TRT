#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management GCU1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone GRP
mkstat = 0
zoncou = "GRP"
indice = 0
If varmode([M]GRP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "GRP" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]GRP,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "GRP" : GZNUM = 616 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "GCU"
  Local Char    VALEUR(250)
  VALEUR = [M]GRP
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]GRP = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "GRP" : GZNUM = 616 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DESTRA
mkstat = 0
zoncou = "DESTRA"
indice = 0
If varinit([M]DESTRA,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DESTRA,"GRPCUR","DESTRA",[M]GRP,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DESTRA,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DESTRA" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone SHOTRA
mkstat = 0
zoncou = "SHOTRA"
indice = 0
If varinit([M]SHOTRA,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]SHOTRA,"GRPCUR","SHOTRA",[M]GRP,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]SHOTRA,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "SHOTRA" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBRCUR
mkstat = 0
zoncou = "NBRCUR"
indice = 0
status = 1
zoncou = "NBRCUR"
If varmode([M]NBRCUR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRCUR" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRCUR
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRCUR = nolign
   status = 75
   Call SAI_NBRCUR(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBRCUR(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBRCUR
mkstat = 0
zoncou = "NBRCUR"

## Zone NUMLIG
mkstat = 0
zoncou = "NUMLIG"
[M]NUMLIG(indice) = nolign

## Zone CUR
mkstat = 0
zoncou = "CUR"
If varmode([M]CUR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CUR" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CUR(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CUR" : GZNUM = 323 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("TCU","",[M]CUR(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CUR" : GZNUM = 323 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_CUR([M]CUR(indice)) From SUBGCU
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CUR" : GZNUM = 323 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone NBRCUR
mkstat = 0
zoncou = "NBRCUR"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GCU1"
Return

