# Supervisor
# Mask management CSFEXT 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ALLCOD
mkstat = 0
zoncou = "ALLCOD"
indice = 0
IMPENT = [M]ALLCOD
If varinit([M]ALLCOD,indice)=0
 IMPENT = 2
Endif
If varmode([M]ALLCOD,indice)=0
 [M]ALLCOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLCOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLCOD(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_ALLCOD(IMPENT) From CSFEXT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ALLCOD" : GZNUM = 27400 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]ALLCOD = IMPENT

