## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
WCHAR = [M]CPY
If varmode([M]CPY,indice)=0
 SAVACT = GACTION : GACTION = "INICPY"
 Call INICPY([M]CPY) From EXEFNC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CPY : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call AS_CPY([M]CPY) From CSFEXT
  GACTION = SAVACT
  If mkstat=1 : Raz [M]CPY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   If vireblc([M]CPY,2)=""
    mkstat = 1 : GMESSAGE = mess(10,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "ACONTCPY"
    Call CONTSOC([M]CPY) From SUBAGF
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     Call CONTROLE("CPY","",[M]CPY,"",mkstat) From CONTOBJ
     If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "CONTCPY"
      Call CONTCPY([M]CPY) From EXEFNC
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       SAVACT = GACTION : GACTION = "STD"
       Call C_CPY([M]CPY) From CSFEXT
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
       If !mkstat
        If WCHAR<>[M]CPY or IMPMOD
         SAVACT = GACTION : GACTION = "STD"
         Call AM_CPY([M]CPY) From CSFEXT
         GACTION = SAVACT
         If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
         Gosub AFF From GSAISIE
         If mkstat : End : Endif
        Endif
       Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

