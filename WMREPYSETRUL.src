#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1YSETRUL
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "YSETRUL"
REP.CLA = "YSETRUL"
REP.ICL = "YSTR"
REP.AFCRIGHT = "GESYSETRUL"
REP.NCL = 101129 : # Settlement Rule
REP.DES = 101129 : # Settlement Rule
REP.TIT = ""
REP.ANX = "YSTR0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Settlement Rule ID
     REP.CLE(1).PAR = 'YRULID' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'YRULID' : REP.CLE(1).VPR= 'YSTR.YRULID' : REP.CLE(1).DES = 101130
# Facets
REP.AENAFAC(1) = 1 : # Detail
REP.AENAFAC(2) = 2 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "HEADER" : REP.SEC(1).NSC = 5042 : # Header
     REP.SEC(1).BLC(1)="DETAIL" : REP.SEC(1).NBC(1)=101126 : # Settlement rule
REP.SEC(2) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(2).SEC = "DETAIL" : REP.SEC(2).NSC = 5562 : # Detail
     REP.SEC(2).BLC(1)="IDENTITY" : REP.SEC(2).NBC(1)=6253 : # Identity
Return

#-------------------------------------------------------#
# Liens de la représentation
#-------------------------------------------------------#
$LINK_REP
REP.LNK(1) = NewInstance C_AREPLNK AllocGroup REP : # Save
     Call ASET_AREPLNK(REP.LNK(1),'$save',1,'','',17,'YSETRUL',12413,4,'','',1,2,1,1,1) From ASYRSUBREP
     REP.LNK(1).ATT(1)= NewInstance C_AREPATT AllocGroup REP
          REP.LNK(1).ATT(1).COD = "$isHidden" : REP.LNK(1).ATT(1).ATT = "true"
     REP.LNK(1).ATT(2)= NewInstance C_AREPATT AllocGroup REP
          REP.LNK(1).ATT(2).COD = "$isDisabled" : REP.LNK(1).ATT(2).ATT = "false"
     REP.LNK(1).LNK(1) = NewInstance C_AREPLNK AllocGroup REP : # List
          Call ASET_AREPLNK(REP.LNK(1).LNK(1),'$query',1,'','YSETRUL',5,'YSETRUL',49616,4,'','',1,2,1,1,1) From ASYRSUBREP
          REP.LNK(1).LNK(1).PAR(1)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(1).LNK(1).PAR(1),'YRULID',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP
          REP.LNK(1).LNK(1).PAR(2)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(1).LNK(1).PAR(2),'YRULTYPCOD',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP
          REP.LNK(1).LNK(1).PAR(3)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(1).LNK(1).PAR(3),'YLOT',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP
     

REP.LNK(2) = NewInstance C_AREPLNK AllocGroup REP : # Cancel
     Call ASET_AREPLNK(REP.LNK(2),'$abort',1,'','',18,'YSETRUL',12409,4,'','',1,2,1,1,1) From ASYRSUBREP
     REP.LNK(2).ATT(1)= NewInstance C_AREPATT AllocGroup REP
          REP.LNK(2).ATT(1).COD = "$isHidden" : REP.LNK(2).ATT(1).ATT = "true"
     REP.LNK(2).ATT(2)= NewInstance C_AREPATT AllocGroup REP
          REP.LNK(2).ATT(2).COD = "$isDisabled" : REP.LNK(2).ATT(2).ATT = "false"
     REP.LNK(2).LNK(1) = NewInstance C_AREPLNK AllocGroup REP : # List
          Call ASET_AREPLNK(REP.LNK(2).LNK(1),'$query',1,'','YSETRUL',5,'YSETRUL',49616,4,'','',1,2,1,1,1) From ASYRSUBREP
          REP.LNK(2).LNK(1).PAR(1)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(2).LNK(1).PAR(1),'YRULID',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP
          REP.LNK(2).LNK(1).PAR(2)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(2).LNK(1).PAR(2),'YRULTYPCOD',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP
          REP.LNK(2).LNK(1).PAR(3)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(2).LNK(1).PAR(3),'YLOT',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP
     

REP.LNK(3) = NewInstance C_AREPLNK AllocGroup REP : # Create
     Call ASET_AREPLNK(REP.LNK(3),'$create',4,'GESYSETRUL','',1,'',12414,4,'','',1,1,2,1,1) From ASYRSUBREP
     REP.LNK(3).CLE(1)  = NewInstance C_AREPPAR AllocGroup REP
          Call ASET_AREPPAR(REP.LNK(3).CLE(1),'YRULID',7,'YSTR.YRULID','YRULID',0,0,0,0,'',1,1) From ASYRSUBREP

REP.LNK(4) = NewInstance C_AREPLNK AllocGroup REP : # List
     Call ASET_AREPLNK(REP.LNK(4),'$query',1,'','YSETRUL',5,'YSETRUL',49616,4,'','',1,1,1,1,1) From ASYRSUBREP
     REP.LNK(4).PAR(1)  = NewInstance C_AREPPAR AllocGroup REP
          Call ASET_AREPPAR(REP.LNK(4).PAR(1),'YRULID',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP
     REP.LNK(4).PAR(2)  = NewInstance C_AREPPAR AllocGroup REP
          Call ASET_AREPPAR(REP.LNK(4).PAR(2),'YRULTYPCOD',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP
     REP.LNK(4).PAR(3)  = NewInstance C_AREPPAR AllocGroup REP
          Call ASET_AREPPAR(REP.LNK(4).PAR(3),'YLOT',7,'','',0,0,0,1,'',2,1) From ASYRSUBREP

REP.LNK(5) = NewInstance C_AREPLNK AllocGroup REP : # Help
     Call ASET_AREPLNK(REP.LNK(5),'$help',1,'','',22,'YSETRUL',78,4,'','',1,2,2,1,1) From ASYRSUBREP

REP.LNK(6) = NewInstance C_AREPLNK AllocGroup REP : # Excel
     Call ASET_AREPLNK(REP.LNK(6),'$excel',1,'','YSETRUL',7,'YSETRUL',27137,5,'','',1,1,1,1,1) From ASYRSUBREP
     REP.LNK(6).CLE(1)  = NewInstance C_AREPPAR AllocGroup REP
          Call ASET_AREPPAR(REP.LNK(6).CLE(1),'YRULID',7,'YSTR.YRULID','YRULID',0,0,0,0,'YRULID',1,1) From ASYRSUBREP

REP.LNK(7) = NewInstance C_AREPLNK AllocGroup REP : # Excel
     Call ASET_AREPLNK(REP.LNK(7),'$excel',1,'','YSETRUL',8,'YSETRUL',27137,4,'','',1,1,2,1,1) From ASYRSUBREP

Return

