Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone INTSHO
mkstat = 0
zoncou = "INTSHO"
indice = 0
If varinit([M]INTSHO,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]INTSHO,"AWRKTRND","INTSHO",[M]WRKTRN,right$(maskcou,4)) From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]INTSHO,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "INTSHO" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone INTLNG
mkstat = 0
zoncou = "INTLNG"
indice = 0
If varinit([M]INTLNG,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]INTLNG,"AWRKTRND","INTLNG",[M]WRKTRN,right$(maskcou,4)) From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]INTLNG,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "INTLNG" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone SELWRK
mkstat = 0
zoncou = "SELWRK"
indice = 0
If varmode([M]SELWRK,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SELWRK" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_SELWRK([M]SELWRK) From SUBAWW
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "SELWRK" : GZNUM = 10104 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBRFLD
mkstat = 0
zoncou = "NBRFLD"
indice = 0
status = 1
zoncou = "NBRFLD"
SAVACT = GACTION : GACTION = "STD"
Call AV_NBRFLD From SUBAWW
GACTION = SAVACT
If mkstat=1 : Raz [M]NBRFLD : Endif
If !mkstat
 If varmode([M]NBRFLD,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "NBRFLD" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   NBLIG = [M]NBRFLD
   For nolign=1 To NBLIG
    indice = nolign-1
    [M]NBRFLD = nolign
    status = 75
    Call SAI_NBRFLD(IMPFIC,ACTION,IMPTRT)
    If mkstat : Break : Endif
   Next nolign
   If mkstat : End : Endif
   indice = 0
   SAVACT = GACTION : GACTION = "STD"
   Call C_NBRFLD From SUBAWW
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NBRFLD" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBRSTY
mkstat = 0
zoncou = "NBRSTY"
indice = 0
status = 1
zoncou = "NBRSTY"
If varmode([M]NBRSTY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRSTY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRSTY
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRSTY = nolign
   status = 75
   Call SAI_NBRSTY(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

## Zone NBRCLE
mkstat = 0
zoncou = "NBRCLE"
indice = 0
status = 1
zoncou = "NBRCLE"
If varmode([M]NBRCLE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRCLE" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRCLE
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRCLE = nolign
   status = 75
   Call SAI_NBRCLE(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 5
####################################
IMPTBL = 0

## Zone ABRFLD
mkstat = 0
zoncou = "ABRFLD"
indice = 0
[M]ABRFLD = "AWS"

## Zone TBLFLD
mkstat = 0
zoncou = "TBLFLD"
indice = 0
[M]TBLFLD = "AWRKHISSUI"

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBRFLD(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBRFLD
mkstat = 0
zoncou = "NBRFLD"
SAVACT = GACTION : GACTION = "STD"
Call C_NBRFLD From SUBAWW
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "NBRFLD" : GZNUM = 0 : Endif
Gosub AFF From GSAISIE
If mkstat : End : Endif

## Zone FLDORD
mkstat = 0
zoncou = "FLDORD"
If varmode([M]FLDORD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLDORD" : Gosub ACTION From =IMPTRT
 Endif
 If [M]FLDORD(indice)<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "FLDORD" : GZNUM = 5626 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone FLDINT
mkstat = 0
zoncou = "FLDINT"
If varinit([M]FLDINT,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]FLDINT(indice),"","","","") From ATEXTRA
 GACTION = SAVACT
Endif

## Zone NBRFLD
mkstat = 0
zoncou = "NBRFLD"

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBRSTY(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBRSTY
mkstat = 0
zoncou = "NBRSTY"

## Zone STYLES
mkstat = 0
zoncou = "STYLES"
If varmode([M]STYLES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "STYLES" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]STYLES(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "STYLES" : GZNUM = 19324 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("ASY","",[M]STYLES(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "STYLES" : GZNUM = 19324 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone CRISTY
mkstat = 0
zoncou = "CRISTY"
If varmode([M]CRISTY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CRISTY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CRISTY(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CRISTY" : GZNUM = 1028 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone NBRSTY
mkstat = 0
zoncou = "NBRSTY"

End

####################################
## Bloc tableau 4
####################################
Subprog SAI_NBRCLE(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBRCLE
mkstat = 0
zoncou = "NBRCLE"

## Zone CLEFLD
mkstat = 0
zoncou = "CLEFLD"
If varmode([M]CLEFLD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CLEFLD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CLEFLD(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CLEFLD" : GZNUM = 1261 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_CLEFLD([M]CLEFLD(indice)) From SUBAWW
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CLEFLD" : GZNUM = 1261 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone CLESNS
mkstat = 0
zoncou = "CLESNS"
IMPENT = [M]CLESNS(indice)
If varinit([M]CLESNS,indice)=0
 IMPENT = 1
 SAVACT = GACTION : GACTION = "STD"
 Call D_CLESNS(IMPENT) From SUBAWW
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]CLESNS,indice)=0
  Call INICOMBO(10) From COMBOS
  SAVACT = GACTION : GACTION = "STD"
  Call AS_CLESNS(IMPENT) From SUBAWW
  GACTION = SAVACT
  If mkstat=1 : Raz IMPENT : Endif
  If IMPENT<1 | GMENLOC(IMPENT)=1
   IMPENT = find(0,GMENLOC(1..len(mess(0,10,1))))
  Endif
  [M]CLESNS(indice) = IMPENT
  If !mkstat
   IMPMOD = 0 : IMPZON = "CLESNS" : Gosub ACTION From =IMPTRT
   IMPENT = [M]CLESNS(indice)
  Endif
  If !mkstat
   If IMPENT=0
    mkstat = 1 : GMESSAGE = mess(10,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "CLESNS" : GZNUM = 36876 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
     mkstat = 1 : GMESSAGE = mess(99,123,1)
    Endif
    If mkstat : GERR = 1 : GZONE = "CLESNS" : GZNUM = 36876 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif
[M]CLESNS(indice) = IMPENT

## Zone NBRCLE
mkstat = 0
zoncou = "NBRCLE"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "AWW1"
Return

