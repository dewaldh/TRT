####################################################################
# 1 : YSTAID Stage ID ( Field YSTA 8)
####################################################################

$SET_YSTAID
If [L]_AOLDVAL<>this.YSTAID
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_YSTAID
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.YSTAID
 Gosub PROPAGATE_YSTAID
Endif
Return

$INIT_YSTAID
Gosub DECLARE_VAR From WMC0YSLD
[L]CURPRO = "YSTAID"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_YSTAID
Gosub DECLARE_VAR From WMC0YSLD
[L]CURPRO = "YSTAID"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_YSTAID                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_YSTAID                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_YSTAID
[L]CURPRO = "YSTAID"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.YSTAID,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_YSTAID
[L]CURPRO = "YSTAID"
If vireblc(this.YSTAID,2)<>vireblc(format$("K:8X",this.YSTAID),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_YSTAID
[L]CURPRO = "YSTAID"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_YSTAID
[L]CURPRO = "YSTAID"
Return

$PROPAGATE_YSTAID
this.isReadonly = 0
[L]CURPRO = "YSTAID"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_YSTAID
Gosub DECLARE_VAR From WMC0YSLD
[L]CURPRO = "YSTAID"
$_LNKPRO_YSTAID
Return

$_ALOADATTRIBUTE_YSTAID
[L]ASTATUS = fmet this.ASETATTRIBUTE("YSTAID","$isMandatory","true")
Return


