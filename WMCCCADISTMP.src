If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDAT From WMC1CADISTMP : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDAT From WMC1CADISTMP : Gosub TESTALLSTAT :  Endif
