# Supervisor
# Mask management WMBANSTD 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone TOTBLCVIR
mkstat = 0
zoncou = "TOTBLCVIR"
indice = 0
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:CBA1]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]TOTBLCVIR : Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone COLOR
mkstat = 0
zoncou = "COLOR"
SAVACT = GACTION : GACTION = "STD"
Call AV_COLOR([M]COLOR(indice)) From CNSBANSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]COLOR(indice) : Endif

## Zone AMTBAN
mkstat = 0
zoncou = "AMTBAN"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]AMTBAN(indice) : Endif

## Zone AMTBLCVIR
mkstat = 0
zoncou = "AMTBLCVIR"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:CBA1]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]AMTBLCVIR(indice) : Endif

## Zone AMTCUR
mkstat = 0
zoncou = "AMTCUR"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]AMTCUR(indice) : Endif

## Zone STA
mkstat = 0
zoncou = "STA"
IMPENT = [M]STA(indice)
If varinit([M]STA,indice)=0
 IMPENT = 1
Endif
[M]STA(indice) = IMPENT

## Zone SNS
mkstat = 0
zoncou = "SNS"
IMPENT = [M]SNS(indice)
If varinit([M]SNS,indice)=0
 IMPENT = 1
Endif
[M]SNS(indice) = IMPENT

## Zone BIDCRY
mkstat = 0
zoncou = "BIDCRY"
If varinit([M]BIDCRY,indice)=0
 [M]BIDCRY(indice) = GPAYS
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "WMBANSTD"
Return

