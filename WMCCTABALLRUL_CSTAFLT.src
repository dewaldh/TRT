If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LOCFLT From WMC1TABALLRUL_CSTAFLT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LOCFLT From WMC1TABALLRUL_CSTAFLT : Gosub TESTALLSTAT :  Endif
