####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0TABALLRUL_CSTAFLT
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_STAFLT From WMC1TABALLRUL_CSTAFLT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_STAFLT From WMC1TABALLRUL_CSTAFLT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_DOCFLT From WMC1TABALLRUL_CSTAFLT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_DOCFLT From WMC1TABALLRUL_CSTAFLT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_STUFLT From WMC1TABALLRUL_CSTAFLT : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_STUFLT From WMC1TABALLRUL_CSTAFLT : Gosub TESTALLSTAT :  Endif
