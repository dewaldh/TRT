############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(SPRICESEARCH,ASTATUS)
Variable Instance SPRICESEARCH Using R_SPRICESEARCH
Variable Integer ASTATUS

If SPRICESEARCH = null : End : Endif

Gosub AVARIABLES From WMR00SPRICESEARCH
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet SPRICESEARCH.ACTX.AGETAFCRIGHT(SPRICESEARCH,"SYRVENSPSI",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet SPRICESEARCH.ACTX.AGETAFCRIGHTU(SPRICESEARCH,"SYRVENSPSI",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_SPRICESEARCH
Else
 Gosub AUPDATE_SPRICESEARCH
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = SPRICESEARCH.freeSnapshot
SPRICESEARCH.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
SPRICESEARCH.ASTALIN = [V]CST_AUPD
SPRICESEARCH.SPRISEA.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = SPRICESEARCH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet SPRICESEARCH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = SPRICESEARCH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet SPRICESEARCH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet SPRICESEARCH._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet SPRICESEARCH._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet SPRICESEARCH._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If SPRICESEARCH.SPRISEA.ASTALIN = [V]CST_ANEWDEL : Return : Endif
SPRICESEARCH.SPRISEA.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(SPRICESEARCH.SPRISEA.ASTALIN)<>""
 [L]A_ASTALIN = SPRICESEARCH.SPRISEA.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet SPRICESEARCH.SPRISEA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

If [V]CST_ASTALIN(SPRICESEARCH.SPRISEA.ASTALIN)<>""
 [L]A_ASTALIN = SPRICESEARCH.SPRISEA.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet SPRICESEARCH.SPRISEA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_SPRICESEARCH
Gosub AUPDATE_SPRICESEARCH
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(SPRICESEARCH.SPRISEA.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 SPRICESEARCH.SPRISEA.ASTALIN = [V]CST_ALL
Endif
Return

#-----------------------------------------------------#
$AUPDATE_SPRICESEARCH
[L]A_ASTALIN = SPRICESEARCH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet SPRICESEARCH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet SPRICESEARCH.SPRISEA._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = SPRICESEARCH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet SPRICESEARCH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
$_AUPDATE_ABORT
If SPRICESEARCH.snapshotEnabled=1
 [L]A_RETIK = SPRICESEARCH.revertUpdTick
Endif

[L]ASTATUS = max(fmet SPRICESEARCH.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



