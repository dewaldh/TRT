# Supervisor
# Mask management CBN3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 If !(1=2)
  mkstat = 2
 Endif
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]FCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","!",[M]FCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone TWD
mkstat = 0
zoncou = "TWD"
indice = 0
If varmode([M]TWD,indice)=0
 If !(1=2)
  mkstat = 2
 Endif
 If mkstat=1 : Raz [M]TWD : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "TWD" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  Call CONTROLE("TWD","!",[M]TWD,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "TWD" : GZNUM = 1809 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone UOM
mkstat = 0
zoncou = "UOM"
indice = 0
SAVACT = GACTION : GACTION = "STD"
Call AV_UOM([M]UOM) From SUBCBN3
GACTION = SAVACT
If mkstat=1 : Raz [M]UOM : Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"
indice = 0
status = 1
zoncou = "BASPAG"
If varmode([M]BASPAG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BASPAG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]BASPAG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]BASPAG = nolign
   status = 75
   Call SAI_BASPAG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_BASPAG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CBN3"
Return

