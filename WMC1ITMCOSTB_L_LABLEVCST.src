####################################################################
# 1 : LABLEVCST Labor level cost ( Field MD8 0)
####################################################################

$SET_LABLEVCST
If [L]_AOLDVAL<>this.LABLEVCST
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_LABLEVCST
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.LABLEVCST
 Gosub PROPAGATE_LABLEVCST
Endif
Return

$INIT_LABLEVCST
Gosub DECLARE_VAR From WMC0ITMCOSTB_L_LABLEVCST
[L]CURPRO = "LABLEVCST"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_LABLEVCST
Gosub DECLARE_VAR From WMC0ITMCOSTB_L_LABLEVCST
[L]CURPRO = "LABLEVCST"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_LABLEVCST             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_LABLEVCST             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_LABLEVCST
[L]CURPRO = "LABLEVCST"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.LABLEVCST=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_LABLEVCST
[L]CURPRO = "LABLEVCST"
Return

$_AOTHERS_LABLEVCST
[L]CURPRO = "LABLEVCST"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_LABLEVCST
[L]CURPRO = "LABLEVCST"
Return

$PROPAGATE_LABLEVCST
this.isReadonly = 0
[L]CURPRO = "LABLEVCST"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_LABLEVCST
Gosub DECLARE_VAR From WMC0ITMCOSTB_L_LABLEVCST
[L]CURPRO = "LABLEVCST"
$_LNKPRO_LABLEVCST
Return


