$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [03/01/2019]
A_HEURE = "11:15:52"
A_USER  = "DH"
Call ISWINVALID(nomap,GLANGUE,"YELA",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
Local Char NAVMSK(200)
NAVMSK="YELA0;YELA1;YELA2;"
