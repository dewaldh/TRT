#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CONSSTCITG1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:57:32"
A_USER  = "ADMIN"
A_STAMP = "20180827235732"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask CONSSTCITG1 [STG1]
Gosub HORODAT
Local Inpbox "CONSSTCITG1" From GFONCTION At A_STAMP With A_WINDPREV Mask [STG1] As [SIM_]
Grizo [STG1]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [STG1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=112 : Y1(0)=1 : Y2(0)=4
Return

$ON_STCNUM
Return

$ON_STCDESAXX
Onevent INIT_AREA Call D_STCDESAXX(zc) From W1CONSSTCITG1
Return

Subprog D_STCDESAXX(VALEUR)
Variable Char    VALEUR()
If varinit([M]STCDESAXX,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"COSTSTCH","STCDESAXX",STCNUM,"") From ATEXTRA
Endif
Gosub _AFF
End

$ON_TCLCODSTR
Onevent AFTER_CHANGE Gosub AMM_TCLCODSTR From W1CONSSTCITG1
Onevent CONTROL Call C_TCLCODSTR(zc) From W1CONSSTCITG1
Onevent BEFORE_INPUT Call AS_TCLCODSTR(zc) From W1CONSSTCITG1
Return

Subprog AS_TCLCODSTR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3572,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_TCLCODSTR(zc) From W1CONSSTCITG1
Onkey   2 Titled GBOUTS Call S_TCLCODSTR(zc) From W1CONSSTCITG1
Onkey 144 Titled GBOUTP Call P_TCLCODSTR(zc) From W1CONSSTCITG1
Onkey 146 Titled GBOUTA Call SA_TCLCODSTR(zc) From W1CONSSTCITG1
Gosub _AFF
End

Subprog C_TCLCODSTR(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("ITG","!I",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "TCLCODSTR" : GZNUM = 32220 : Endif
Gosub _AFF
End

$AMM_TCLCODSTR
Call AM_TCLCODSTR(zc)
If !mkstat & GOBJET
 zoncou = "TCLCODSTR" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_TCLCODSTR(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_TCLCODSTR(VALEUR) From CNSSTCITGSTD
If mkstat : GERR = 1 : GZONE = "TCLCODSTR" : GZNUM = 32220 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZTCLCODSTR)")>indice
 [M]TCLCODSTR(indice) = VALEUR
 Affzo ZTCLCODSTR(indice)
Endif
Gosub _AFF
End

Subprog S_TCLCODSTR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "ITG"
PARMSK(2) = "!I"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_TCLCODSTR(VALEUR)
Variable Char    VALEUR()
Call OBJET("ITG",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_TCLCODSTR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ITG"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_TCLCODSTR(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ITG",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZTCLCODSTR
Onevent BEFORE_AREA Gosub AV_ZTCLCODSTR From W1CONSSTCITG1
Return

$AV_ZTCLCODSTR
Call LECTEXTRA(zc,"ITMCATEG","TCLAXX",num$([M]TCLCODSTR(indice)),"") From ATEXTRA
Gosub _AFF
Return

$ON_TCLCODEND
Onevent CONTROL Call C_TCLCODEND(zc) From W1CONSSTCITG1
Onevent BEFORE_INPUT Call AS_TCLCODEND(zc) From W1CONSSTCITG1
Onevent AFTER_CHANGE Gosub AMM_TCLCODEND From W1CONSSTCITG1
Return

Subprog AS_TCLCODEND(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3572,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_TCLCODEND(zc) From W1CONSSTCITG1
Onkey   2 Titled GBOUTS Call S_TCLCODEND(zc) From W1CONSSTCITG1
Onkey 144 Titled GBOUTP Call P_TCLCODEND(zc) From W1CONSSTCITG1
Onkey 146 Titled GBOUTA Call SA_TCLCODEND(zc) From W1CONSSTCITG1
Gosub _AFF
End

Subprog C_TCLCODEND(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("ITG","!I",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "TCLCODEND" : GZNUM = 32221 : Endif
If !mkstat
 ## Action STD
 Call C_TCLCODEND(VALEUR) From CNSSTCITGSTD
 If mkstat : GERR = 1 : GZONE = "TCLCODEND" : GZNUM = 32221 : Endif
Endif
Gosub _AFF
End

$AMM_TCLCODEND
Call AM_TCLCODEND(zc)
If !mkstat & GOBJET
 zoncou = "TCLCODEND" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_TCLCODEND(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZTCLCODEND)")>indice
 [M]TCLCODEND(indice) = VALEUR
 Affzo ZTCLCODEND(indice)
Endif
Gosub _AFF
End

Subprog S_TCLCODEND(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "ITG"
PARMSK(2) = "!I"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_TCLCODEND(VALEUR)
Variable Char    VALEUR()
Call OBJET("ITG",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_TCLCODEND(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ITG"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_TCLCODEND(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ITG",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZTCLCODEND
Onevent BEFORE_AREA Gosub AV_ZTCLCODEND From W1CONSSTCITG1
Return

$AV_ZTCLCODEND
Call LECTEXTRA(zc,"ITMCATEG","TCLAXX",num$([M]TCLCODEND(indice)),"") From ATEXTRA
Gosub _AFF
Return

$CHG_STY
Return


