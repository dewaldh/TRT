$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(3)(0..GNBDERNIER)
Local Char     A_BUF2(10)(0..GNBDERNIER)
Local Char     A_BUF3(40)(0..GNBDERNIER)
Local Char     A_BUF4(10)(0..GNBDERNIER)
Local Char     A_BUF5(35)(0..GNBDERNIER)
Local Char     A_BUF6(40)(0..GNBDERNIER)
If clalev([F:POS9])>0 : Close Local File [POS9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(3),
& Char     A_TAB2(10),
& Char     A_TAB3(40),
& Char     A_TAB4(10),
& Char     A_TAB5(35),
& Char     A_TAB6(40)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5,A_BUF6
& As [POS9]
Return

$LEC_DERLU
Return

$CHARGE_DERLU
If clalev([F:POS9])>0
 Read [POS9] = [F:POS9]A_TABI
 [L]CLE = [F:POS9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:POS9])>0
 Filter [POS9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [POS1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:POS9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:POS]POSCOD+"~"
 A_CLEDERLU += [F:POS]CRY+"~"
 A_CLEDERLU += [F:POS]POSCTY+"~"
 A_CLEDERLU += [F:POS]POSCTYCOD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:POS9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:POS9]
 Endif
Endif
Return

