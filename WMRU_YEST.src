# Supervisor
## CRUD-CLASSE ## YEST
# 2019-03-26 00:36:28 270


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YEST,ASTATUS)
Variable Instance YEST Using R_YEST
Variable Integer ASTATUS

If YEST = null : End : Endif

Gosub AVARIABLES From WMR00YEST
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet YEST.ACTX.AGETAFCRIGHT(YEST,"GESYEST",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet YEST.ACTX.AGETAFCRIGHTU(YEST,"GESYEST",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif
If fmet YEST.ACTX.AGETACSRIGHTM(YEST,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AUPDATE_END
  Endif
  
  Gosub AREPCONTROL_AUPDATE
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  
If !adxlog
 Gosub AUPDATE_TR_YEST
Else
 Gosub AUPDATE_YEST
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YEST.freeSnapshot
YEST.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
YEST.ASTALIN = [V]CST_AUPD
YEST.YEST.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = YEST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = YEST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YEST._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YEST._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YEST._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If YEST.YEST.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YEST.YEST.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(YEST.YEST.ASTALIN)<>""
 [L]A_ASTALIN = YEST.YEST.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(YEST.YEST.YRMA) : # YRMA -- 2
 If YEST.YEST.YRMA(A_N2) <> null
& and YEST.YEST.YRMA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YEST.YEST.YRMA(A_N2).ASTALIN<>[V]CST_ANEW & YEST.YEST.ASTALIN=[V]CST_ADEL
   YEST.YEST.YRMA(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YEST.YEST.YRMA(A_N2).ASTALIN=[V]CST_ANEW & find(YEST.YEST.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YEST.YEST.YRMA(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YEST.YEST.YRMA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YEST.YEST.YRMA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST.YRMA(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST.YRMA(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST.YRMA(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEST.YLDP) : # YLNDPUR -- 3
 If YEST.YEST.YLDP(A_N2) <> null
& and YEST.YEST.YLDP(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEST.YSTAGE) : # YSTA -- 4
 If YEST.YEST.YSTAGE(A_N2) <> null
& and YEST.YEST.YSTAGE(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEST.YPRE) : # YPRE -- 5
 If YEST.YEST.YPRE(A_N2) <> null
& and YEST.YEST.YPRE(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(YEST.YEST.YRMA) : # YRMA -- 2
 If YEST.YEST.YRMA(A_N2) <> null
& and YEST.YEST.YRMA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YEST.YEST.YRMA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YEST.YEST.YRMA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST.YRMA(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST.YRMA(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST.YRMA(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEST.YLDP) : # YLNDPUR -- 3
 If YEST.YEST.YLDP(A_N2) <> null
& and YEST.YEST.YLDP(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEST.YSTAGE) : # YSTA -- 4
 If YEST.YEST.YSTAGE(A_N2) <> null
& and YEST.YEST.YSTAGE(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEST.YPRE) : # YPRE -- 5
 If YEST.YEST.YPRE(A_N2) <> null
& and YEST.YEST.YPRE(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(YEST.YEST.ASTALIN)<>""
 [L]A_ASTALIN = YEST.YEST.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_YEST
Trbegin [F:YEST]
 Gosub AUPDATE_YEST
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(YEST.YEST.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YEST.YEST.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEST.YRMA)
 If YEST.YEST.YRMA(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEST.YRMA(A_N2)._AUPDATE_AFTER
  If YEST.YEST.YRMA(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEST.YRMA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEST.YRMA(A_N2)
   YEST.YEST.YRMA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEST.YLDP)
 If YEST.YEST.YLDP(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEST.YLDP(A_N2)._AUPDATE_AFTER
  If YEST.YEST.YLDP(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEST.YLDP(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEST.YLDP(A_N2)
   YEST.YEST.YLDP(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEST.YSTAGE)
 If YEST.YEST.YSTAGE(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEST.YSTAGE(A_N2)._AUPDATE_AFTER
  If YEST.YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEST.YSTAGE(A_N2)
   YEST.YEST.YSTAGE(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEST.YPRE)
 If YEST.YEST.YPRE(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEST.YPRE(A_N2)._AUPDATE_AFTER
  If YEST.YEST.YPRE(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEST.YPRE(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEST.YPRE(A_N2)
   YEST.YEST.YPRE(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_YEST
[L]A_ASTALIN = YEST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet YEST.YEST._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = YEST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If YEST.snapshotEnabled=1
 [L]A_RETIK = YEST.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEST.YRMA)
 If YEST.YEST.YRMA(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEST.YRMA(A_N2)._AUPDATE_ABORT
  If YEST.YEST.YRMA(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEST.YRMA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEST.YRMA(A_N2)
   YEST.YEST.YRMA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEST.YLDP)
 If YEST.YEST.YLDP(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEST.YLDP(A_N2)._AUPDATE_ABORT
  If YEST.YEST.YLDP(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEST.YLDP(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEST.YLDP(A_N2)
   YEST.YEST.YLDP(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEST.YSTAGE)
 If YEST.YEST.YSTAGE(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEST.YSTAGE(A_N2)._AUPDATE_ABORT
  If YEST.YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEST.YSTAGE(A_N2)
   YEST.YEST.YSTAGE(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEST.YPRE)
 If YEST.YEST.YPRE(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEST.YPRE(A_N2)._AUPDATE_ABORT
  If YEST.YEST.YPRE(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEST.YPRE(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEST.YPRE(A_N2)
   YEST.YEST.YPRE(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet YEST.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



