#<AdxTL>@(#)0.0.0.0 $Revision$
# Sage X3 supervisor
# Mask management WH3ALL1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_ITNORI
Onevent INIT_AREA Call D_ITNORI(zc) From W2WH3ALL1
Return

Subprog D_ITNORI(VALEUR)
Variable Integer VALEUR
If varinit([M]ITNORI,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_ITNORIVCR
Onevent CONTROL Call C_ITNORIVCR(zc) From W2WH3ALL1
Onevent BEFORE_INPUT Call AS_ITNORIVCR(zc) From W2WH3ALL1
Return

Subprog IB_ITNORIVCR(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action STD
 Call IB_ITNORIVCR From SUBITN
Endif
Gosub _AFF
End

Subprog AS_ITNORIVCR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(735,GBOUT1) From OBJDIV
Call IB_ITNORIVCR(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_ITNORIVCR(zc) From W2WH3ALL1
Gosub _AFF
End

Subprog C_ITNORIVCR(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CONTVCR
 Call CONTVCR(VALEUR) From TRTDIV
 If mkstat : GERR = 1 : GZONE = "ITNORIVCR" : GZNUM = 15823 : Endif
Endif
Gosub _AFF
End

Subprog B1_ITNORIVCR(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action GOBJETC1
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GOBJETC1"
 Call OBJET_CHAR([M:ITN1]ITNORIVCR,[M:ITN1]ITNORITYP,"") From GOBJET
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_ITNORITYP
Return

$CHG_STY
Return


