$ON_PLNFCY
Onevent CONTROL Call C_PLNFCY(zc) From W1WR1ALL0
Onevent BEFORE_INPUT Call AS_PLNFCY(zc) From W1WR1ALL0
Onevent AFTER_CHANGE Gosub AMM_PLNFCY From W1WR1ALL0
Return

Subprog AS_PLNFCY(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(1427,GBOUT1) From OBJDIV
Call TEXTE(8255,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_PLNFCY(VALEUR) From SUBMFG
If mkstat=2
 [M]PLNFCY(INDIC) = VALEUR
 Affzo PLNFCY(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_PLNFCY(zc) From W1WR1ALL0
Onkey   2 Titled GBOUTS Call S_PLNFCY(zc) From W1WR1ALL0
Onkey 144 Titled GBOUTP Call P_PLNFCY(zc) From W1WR1ALL0
Onkey 146 Titled GBOUTA Call SA_PLNFCY(zc) From W1WR1ALL0
Gosub _AFF
End

Subprog C_PLNFCY(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
If !mkstat
 ## Action ACONTFCY
 Call CONTSITE(VALEUR) From SUBAGF
 If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("FCY","E",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
  If !mkstat
   ## Action CTLFCYAUZ
   Local Char    PARMSK(250)(1..20) , SAVACT(20)
   SAVACT = GACTION : GACTION = "CTLFCYAUZ"
   Call S_AFF_CHAR(VALEUR,PARMSK,"CTLFCYAUZ","CONTX3","") From GACTION
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

$AMM_PLNFCY
Call AM_PLNFCY(zc)
If !mkstat & GOBJET
 zoncou = "PLNFCY" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_PLNFCY(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_PLNFCY(VALEUR) From SUBMFG
If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZPLNFCY)")>indice
 [M]PLNFCY(indice) = VALEUR
 Affzo ZPLNFCY(indice)
Endif
Gosub _AFF
End

Subprog S_PLNFCY(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "FCY"
PARMSK(2) = "E"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_PLNFCY(VALEUR)
Variable Char    VALEUR()
Call OBJET("FCY",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_PLNFCY(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "FCY"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_PLNFCY(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("FCY",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

