############################################################################################
# Program TEST
############################################################################################
$TEST
Local Integer SILENCE
Call OUVRE_TRACE("Test program")From LECFIC

Gosub ATRT_TEST

Call LEC_TRACE From LECFIC
Call SUPP_TRACE From LECFIC


Return

#---------------------------------------------------------------#
$ATRT_TEST
Local Integer ASTATUS
If !SILENCE : Call TRACE("1 --> Open main table" ) From ASYRCOM : Endif
Local File PHYLCT [LCT_]

If !SILENCE : Call TRACE("2 --> Local Instance R_PHYLCT") From ASYRCOM : Endif
Local Instance PHYLCT Using R_PHYLCT

If !SILENCE : Call TRACE("3 --> New Instance R_PHYLCT") From ASYRCOM : Endif
PHYLCT = NewInstance R_PHYLCT AllocGroup null

If !SILENCE : Call TRACE("4 --> Reading main table, Read First") From ASYRCOM : Endif
Read [LCT_]LCT0 First
If !fstat
 If !SILENCE : Call TRACE("Call program read of PHYLCT") From ASYRCOM : Endif
 [L]ASTATUS = max([L]ASTATUS,fmet PHYLCT.AREAD([LCT_]LCTCOD))
Endif

If !SILENCE
 Call TRACE("5 --> Check :"-num$([L]ASTATUS)) From ASYRCOM
 Call TRACE("PHYLCT.LCT.LCTCOD ="-num$(PHYLCT.LCT.LCTCOD)) From ASYRCOM
 Call TRACE("6 --> List of keys :") From ASYRCOM
 For I=1 To maxtab(PHYLCT.LCT.AKEYS)
  Call TRACE("-"-num$(I)-":"-PHYLCT.LCT.AKEYS(I)) From ASYRCOM
 Next
Endif
Close Local File [LCT_]
FreeGroup PHYLCT
Return

############################################################################################
### Funprog _AREADETAG
############################################################################################
Funprog _AREADETAG(PHYLCT,ACLE,ASTATUS,_ETAG)
Variable Instance PHYLCT Using R_PHYLCT
Value Char ACLE
Variable Integer ASTATUS
Variable Char _ETAG
[L]ASTATUS = [V]CST_AOK
Local Char _LCTCOD(30)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_LCTCOD = num$([L]CLE(1)) : Endif
Raz [L]_ETAG
Local File PHYLCT [LCT]
Default File [F:LCT]
Read [F:LCT]LCT0 = [L]_LCTCOD
If fstat
 [L]ASTATUS = [V]CST_AERROR
Endif
If [L]ASTATUS=[V]CST_AOK and dim([F:LCT]UPDTICK)>0
 [L]_ETAG = num$([F:LCT]UPDTICK)
Endif
End [L]ASTATUS

############################################################################################
### Subprog _AREADSUP
############################################################################################
Subprog _AREADSUP(PHYLCT,ACLE,ASTATUS)
Variable Instance PHYLCT Using R_PHYLCT
Value Char ACLE
Variable Integer ASTATUS
Local Char _LCTCOD(30)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_LCTCOD = num$([L]CLE(1)) : Endif
[L]ASTATUS = max([L]ASTATUS,fmet PHYLCT.AREAD([L]_LCTCOD))
End

############################################################################################
### Subprog AREAD
############################################################################################
Subprog AREAD(PHYLCT,_LCTCOD,ASTATUS)
Variable Instance PHYLCT Using R_PHYLCT
Value Char _LCTCOD
Variable Integer ASTATUS

If PHYLCT = null : End : Endif

Gosub AVARIABLES From WMR00PHYLCT
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If PHYLCT.APARENT=null
 [L]A_I = PHYLCT.freeSnapshot
 PHYLCT.snapshotEnabled = 0
 PHYLCT.setAccessorEnabled = 0
Endif

If fmet PHYLCT.ACTX.AGETAFCRIGHT(PHYLCT,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END : Endif
Default File [F:LCT]
Read [F:LCT]LCT0 = [L]_LCTCOD
If fstat
[L]A_ERROR = [L]_LCTCOD
& -":"-mess(23,100,1)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(PHYLCT,"",0,[L]ASTATUS,[V]CST_AHTTP_NOT_FOUND,[L]A_ERROR) : Goto AREAD_END
Endif
[L]A_STRSITE = [F:LCT]FCY
[L]A_STRACC = ""
If fmet PHYLCT.ACTX.AGETAFCRIGHTR(PHYLCT,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END
Endif
If fmet PHYLCT.ACTX.ASTACRITROLE(PHYLCT,[L]_LCTCOD
& ,"PHY","LCT",[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END 
Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYLCT.ACTION("AREAD_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

PHYLCT.LCT.setAccessorEnabled = 0
[L]ASTATUS = max([L]ASTATUS,fmet PHYLCT.LCT.AREAD([L]_LCTCOD))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYLCT.ACTION("AREAD_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If [L]ASTATUS<[V]CST_AERROR & PHYLCT.APARENT=null
 # Load Link
 [L]ASTATUS = max([L]ASTATUS,fmet PHYLCT._ALNKPRO)
 If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
Endif

$AREAD_END
If PHYLCT.APARENT=null
 PHYLCT.snapshotEnabled = 1
 PHYLCT.setAccessorEnabled = 1
Endif
LogicClose File [LCT]
End



