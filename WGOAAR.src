$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "2"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "AAR"
  PARBOUT(2) = [M]CODPAR(nolign-1)
  PARBOUT(3) = ""
  PARBOUT(4) = ""
  PARBOUT(5) = ""
  FLGEXE = 1
  GACTION = "ACOPIE"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ACOPIE","ACOPIE",3,"XYACOPIE") From GTRAITE
Endcase
GACTION = SAVACT
Return

$EXEACT
