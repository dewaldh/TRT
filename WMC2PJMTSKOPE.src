####################################################################
# 31 : STDWST Standard position ( Field WST 8)
####################################################################

$SET_STDWST
If [L]_AOLDVAL<>this.STDWST
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_STDWST
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.STDWST
 Gosub PROPAGATE_STDWST
Endif
Return

$INIT_STDWST
Gosub DECLARE_VAR From WMC0PJMTSKOPE
[L]CURPRO = "STDWST"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_STDWST
Gosub DECLARE_VAR From WMC0PJMTSKOPE
[L]CURPRO = "STDWST"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_STDWST                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_STDWST                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_STDWST
[L]CURPRO = "STDWST"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.STDWST,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_STDWST
[L]CURPRO = "STDWST"
If vireblc(this.STDWST,2)<>vireblc(format$("K:8c",this.STDWST),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_STDWST
[L]CURPRO = "STDWST"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_STDWST
[L]CURPRO = "STDWST"
Return

$PROPAGATE_STDWST
this.isReadonly = 0
[L]CURPRO = "STDWST"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_STDWST
Gosub DECLARE_VAR From WMC0PJMTSKOPE
[L]CURPRO = "STDWST"
$_LNKPRO_STDWST
Return


