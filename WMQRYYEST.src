#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:YEST])=0 : Local File YESTA [YEST] : Endif
If clalev([F:A_])=0 : Local File YESTA [A_] : Endif
If clalev([F:ACC])=0 : Local File ACCES [ACC] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [YEST]
LogicClose File [A_]
LogicClose File [ACC]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
If GACTX.ALLACS=[V]CST_AYES
 Gosub AQUERY_JOIN_ALL 
Else
 Local Char ACRITROLE(250)(1..)
 [L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"YEST","YEST",ACRITROLE,[V]CST_AFALSE)
 Link [YEST] With
& [ACC]CODACC~= GACTX.USER;[F:YEST]YACCESS
& As [LNK_]
& Where ( (1=1)
& and ([F:ACC]CONSUL=2)
& and (evalue([L]ACRITROLE))
&  )
Endif
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN_ALL
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"YEST","YEST",ACRITROLE,[V]CST_AFALSE)
Link [YEST] With
& [A_]YEST0~=[YEST]YESTID
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"YEST","YEST",ACRITROLE,[V]CST_AFALSE)
Link [YEST] With
& [ACC]CODACC~= GACTX.USER;[F:YEST]YACCESS
& As [LNK_]
& Where ( (1=1)
& and ([F:ACC]CONSUL=2)
& and (evalue([L]ACRITROLE))
&  )
Return
 
