#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1SPREASON
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "SPREASON"
REP.CLA = "SPREASON"
REP.ICL = "SPR"
REP.AFCRIGHT = "GESSPR"
REP.NCL = 6985 : # Sales price list reasons
REP.DES = 9027 : # Price reasons
REP.TIT = "DESAXX"
REP.ANX = "SPR0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Reason
     REP.CLE(1).PAR = 'PRIREN' : REP.CLE(1).TYP = 2 : REP.CLE(1).PRO= 'PRIREN' : REP.CLE(1).VPR= 'SPR.PRIREN' : REP.CLE(1).DES = 2675
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'SPREASON_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 1 : # Detail
REP.AENAFAC(2) = 1 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

