#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management MAUTMTO 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone STOFCY
mkstat = 0
zoncou = "STOFCY"
indice = 0
WCHAR = [M]STOFCY
SAVACT = GACTION : GACTION = "STD"
Call AV_STOFCY([M]STOFCY) From FUNMAUTMTO
GACTION = SAVACT
If mkstat=1 : Raz [M]STOFCY : Endif
If !mkstat
 If varmode([M]STOFCY,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "STOFCY" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]STOFCY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTFCY"
   Call CONTSITE([M]STOFCY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("FCY","D",[M]STOFCY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTFCY"
     Call CONTFCY([M]STOFCY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]STOFCY or IMPMOD
       SAVACT = GACTION : GACTION = "STD"
       Call AM_STOFCY([M]STOFCY) From FUNMAUTMTO
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone ITMREFDEB
mkstat = 0
zoncou = "ITMREFDEB"
indice = 0
WCHAR = [M]ITMREFDEB
If varmode([M]ITMREFDEB,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREFDEB" : Gosub ACTION From =IMPTRT
 Endif
 If WCHAR<>[M]ITMREFDEB or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_ITMREFDEB([M]ITMREFDEB) From FUNMAUTMTO
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ITMREFDEB" : GZNUM = 12930 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone ITMREFFIN
mkstat = 0
zoncou = "ITMREFFIN"
indice = 0
If varmode([M]ITMREFFIN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREFFIN" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "CTLALPV"
 Call CTLALPV([M]ITMREFFIN,[M]ITMREFDEB) From CONTDIV
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ITMREFFIN" : GZNUM = 12931 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone FORMULE
mkstat = 0
zoncou = "FORMULE"
indice = 0
If varinit([M]FORMULE,indice)=0
 SAVACT = GACTION : GACTION = "MEMFOR0"
 Call MEMINI([M]FORMULE) From GMEMFOR
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]FORMULE,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "FORMULE" : Gosub ACTION From =IMPTRT
  Endif
  Call CONTROLE("TFO","!",[M]FORMULE,"41",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "FORMULE" : GZNUM = 4574 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone DEMNUM
mkstat = 0
zoncou = "DEMNUM"
indice = 0
If varmode([M]DEMNUM,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_DEMNUM([M]DEMNUM) From FUNMAUTMTO
 GACTION = SAVACT
 If mkstat=1 : Raz [M]DEMNUM : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "DEMNUM" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_DEMNUM([M]DEMNUM) From FUNMAUTMTO
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DEMNUM" : GZNUM = 9806 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone XDEMWIP
mkstat = 0
zoncou = "XDEMWIP"
indice = 0
[M]XDEMWIP = string$([M]DEMNUM<>'',mess([M]DEMWIP,306,1))

## Zone DEMTYP
mkstat = 0
zoncou = "DEMTYP"
indice = 0
IMPENT = [M]DEMTYP
If varinit([M]DEMTYP,indice)=0
 IMPENT = 1
Endif
[M]DEMTYP = IMPENT

## Zone DEMWIP
mkstat = 0
zoncou = "DEMWIP"
indice = 0
IMPENT = [M]DEMWIP
If varinit([M]DEMWIP,indice)=0
 IMPENT = 1
Endif
[M]DEMWIP = IMPENT

## Zone RESSNUM
mkstat = 0
zoncou = "RESSNUM"
indice = 0
If varmode([M]RESSNUM,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_RESSNUM([M]RESSNUM) From FUNMAUTMTO
 GACTION = SAVACT
 If mkstat=1 : Raz [M]RESSNUM : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "RESSNUM" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_RESSNUM([M]RESSNUM) From FUNMAUTMTO
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "RESSNUM" : GZNUM = 14724 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone XRESSWIP
mkstat = 0
zoncou = "XRESSWIP"
indice = 0
[M]XRESSWIP = string$([M]RESSNUM<>'',mess([M]RESSWIP,306,1))

## Zone RESSTYP
mkstat = 0
zoncou = "RESSTYP"
indice = 0
IMPENT = [M]RESSTYP
If varinit([M]RESSTYP,indice)=0
 IMPENT = 1
Endif
[M]RESSTYP = IMPENT

## Zone RESSWIP
mkstat = 0
zoncou = "RESSWIP"
indice = 0
IMPENT = [M]RESSWIP
If varinit([M]RESSWIP,indice)=0
 IMPENT = 1
Endif
[M]RESSWIP = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone PRPMOD
mkstat = 0
zoncou = "PRPMOD"
indice = 0
IMPENT = [M]PRPMOD
If varinit([M]PRPMOD,indice)=0
 IMPENT = 2
Endif
If varmode([M]PRPMOD,indice)=0
 Call INICOMBO(395) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_PRPMOD(IMPENT) From FUNMAUTMTO
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,395,1))))
 Endif
 [M]PRPMOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "PRPMOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]PRPMOD(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "PRPMOD" : GZNUM = 60 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_PRPMOD(IMPENT) From FUNMAUTMTO
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "PRPMOD" : GZNUM = 60 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]PRPMOD = IMPENT

## Zone PRPTYP
mkstat = 0
zoncou = "PRPTYP"
indice = 0
IMPENT = [M]PRPTYP
WNUM = IMPENT
If varinit([M]PRPTYP,indice)=0
 IMPENT = 2
Endif
If varmode([M]PRPTYP,indice)=0
 Call INICOMBO(2382) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_PRPTYP(IMPENT) From FUNMAUTMTO
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2382,1))))
 Endif
 [M]PRPTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "PRPTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]PRPTYP(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "PRPTYP" : GZNUM = 8853 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WNUM<>IMPENT or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_PRPTYP(IMPENT) From FUNMAUTMTO
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "PRPTYP" : GZNUM = 8853 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif
[M]PRPTYP = IMPENT

####################################
## Bloc liste 5
####################################
IMPTBL = 0

## Zone FRCPRO
mkstat = 0
zoncou = "FRCPRO"
indice = 0
IMPENT = [M]FRCPRO
If varinit([M]FRCPRO,indice)=0
 IMPENT = 1
Endif
If varmode([M]FRCPRO,indice)=0
 If !([M]PRPTYP=1)
  mkstat = 2
 Endif
 If mkstat=1 : Raz IMPENT : Endif
 [M]FRCPRO(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FRCPRO" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FRCPRO(indice)
 Endif
Endif
[M]FRCPRO = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "MAUTMTO"
Return

