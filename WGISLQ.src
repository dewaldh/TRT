NOMMSK(0) = "SLQ0"         : ABMSK(0)  = "SLQ0"     : AMA(0) = 1
NOMMSK(1) = "SLQ1"         : ABMSK(1)  = "SLQ1"     : AMA(1) = 1
NOMMSK(2) = "SLQ2"         : ABMSK(2)  = "SLQ2"     : AMA(2) = 1
NOMMSK(3) = "SLQ3"         : ABMSK(3)  = "SLQ3"     : AMA(3) = 1
