$ON_ACCCOD
Onevent CONTROL Call C_ACCCOD(zc) From W1IPM3
Onevent INIT_AREA Call D_ACCCOD(zc) From W1IPM3
Onevent AFTER_CHANGE Gosub AMM_ACCCOD From W1IPM3
Onevent BEFORE_INPUT Call AS_ACCCOD(zc) From W1IPM3
Return

Subprog D_ACCCOD(VALEUR)
Variable Char    VALEUR()
If varinit([M]ACCCOD,indice)=0
 If !GWEBSERV
  ## Action INITACCCOD
  Call INITACCCOD(VALEUR,26) From TRTX3
 Endif
Endif
Gosub _AFF
End

Subprog AS_ACCCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3946,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_ACCCOD(zc) From W1IPM3
Onkey   2 Titled GBOUTS Call S_ACCCOD(zc) From W1IPM3
Onkey 144 Titled GBOUTP Call P_ACCCOD(zc) From W1IPM3
Onkey 146 Titled GBOUTA Call SA_ACCCOD(zc) From W1IPM3
Gosub _AFF
End

Subprog C_ACCCOD(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "ACCCOD" : GZNUM = 1687 : Endif
If !mkstat
 ## Action CONTCAC
 Call CONTCAC(VALEUR,"26") From SUBCAC
 If mkstat : GERR = 1 : GZONE = "ACCCOD" : GZNUM = 1687 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("CAC","",VALEUR,"26",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ACCCOD" : GZNUM = 1687 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_ACCCOD
Call AM_ACCCOD(zc)
If !mkstat & GOBJET
 zoncou = "ACCCOD" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_ACCCOD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action STD
 Call AM_ACCCOD(VALEUR) From SUBIPM
 If mkstat : GERR = 1 : GZONE = "ACCCOD" : GZNUM = 1687 : Endif
Endif
If !mkstat & evalue("dim("+clanam([M])+"ZACCCOD)")>indice
 [M]ACCCOD(indice) = VALEUR
 Affzo ZACCCOD(indice)
Endif
Gosub _AFF
End

Subprog S_ACCCOD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELCAC
 Call SELCAC(VALEUR,"26") From SUBCAC
Endif
Gosub _AFF
End

Subprog B1_ACCCOD(VALEUR)
Variable Char    VALEUR()
Call OBJET("CAC",VALEUR,"26") From GOBJET
Gosub _AFF
End

Subprog SA_ACCCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "CAC"
PARMSK(2) = "26"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_ACCCOD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "26"
Call VISU("CAC",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZACCCOD
Onevent BEFORE_AREA Gosub AV_ZACCCOD From W1IPM3
Return

$AV_ZACCCOD
Call LECTEXTRA(zc,"GACCCODE","DESTRA","26",num$([M]ACCCOD(indice))) From ATEXTRA
Gosub _AFF
Return

