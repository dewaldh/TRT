  Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJN","CONTOBJ","") From GACTION
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CCEDEFL" : GZNUM = 19399 : Endif
 Endif
Endif
Gosub _AFF
End

