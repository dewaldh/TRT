#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:YPRC])=0 : Local File YPRECINT [YPRC] : Endif
If clalev([F:A_])=0 : Local File YPRECINT [A_] : Endif
If clalev([F:ACC])=0 : Local File ACCES [ACC] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [YPRC]
LogicClose File [A_]
LogicClose File [ACC]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
If GACTX.ALLACS=[V]CST_AYES
 Gosub AQUERY_JOIN_ALL 
Else
 Local Char ACRITROLE(250)(1..)
 [L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"YPRE","YPRC",ACRITROLE,[V]CST_AFALSE)
 Link [YPRC] With
& [ACC]CODACC~= GACTX.USER;[F:YPRC]YACCESS
& As [LNK_]
& Where ( (1=1)
& and ([F:ACC]CONSUL=2)
& and (evalue([L]ACRITROLE))
&  )
Endif
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN_ALL
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"YPRE","YPRC",ACRITROLE,[V]CST_AFALSE)
Link [YPRC] With
& [A_]YPRC0~=[YPRC]YPREID
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"YPRE","YPRC",ACRITROLE,[V]CST_AFALSE)
Link [YPRC] With
& [ACC]CODACC~= GACTX.USER;[F:YPRC]YACCESS
& As [LNK_]
& Where ( (1=1)
& and ([F:ACC]CONSUL=2)
& and (evalue([L]ACRITROLE))
&  )
Return
 
#-------------------------------------------------------------------#
# Default filter for the query
#-------------------------------------------------------------------#
$AQUERY_FILTER_DEF
Return
 
#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# YPRC --- YPRECINT(YPRC)
QRY(NBREP) = NewInstance R_YPRE AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:YPRC]AUUID)>0 & [F:YPRC]AUUID=NullUuid
 [F:YPRC]AUUID = getUuid
Endif
SetInstance QRY(NBREP).YPRC With [F:YPRC]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# YPRC --- YPRECINT(YPRC)
QRY = NewInstance R_YPRE AllocGroup PQRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:YPRC]AUUID)>0 & [F:YPRC]AUUID=NullUuid
 [F:YPRC]AUUID = getUuid
Endif
SetInstance QRY.YPRC With [F:YPRC]
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

