############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(YSLT,ASTATUS)
Variable Instance YSLT Using C_YSLT
Variable Integer ASTATUS

If YSLT = null : End : Endif

Gosub _AVARIABLES From WMC00YSLT

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_YSLT

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YSLT,ASTATUS)
Variable Instance YSLT Using C_YSLT
Variable Integer ASTATUS

If YSLT = null : End : Endif

Gosub AVARIABLES From WMC00YSLT
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
YSLT.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_YSLT
Else
 Gosub AUPDATE_YSLT
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YSLT.freeSnapshot
YSLT.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YSLT._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YSLT._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YSLT._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If YSLT.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YSLT.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(YSLT.ASTALIN)<>""
 [L]A_ASTALIN = YSLT.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YSLT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

If [V]CST_ASTALIN(YSLT.ASTALIN)<>""
 [L]A_ASTALIN = YSLT.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YSLT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_YSLT
Trbegin [F:YLOT]
 Gosub AUPDATE_YSLT
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_YSLT
[L]A_ASTALIN = YSLT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YSLT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YSLT._MAJSQL_YLOT(YSLT))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif


[L]A_ASTALIN = YSLT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YSLT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If YSLT.snapshotEnabled=1
 [L]A_RETIK = YSLT.revertUpdTick
Endif

[L]ASTATUS = max(fmet YSLT.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(YSLT.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YSLT.ASTALIN = [V]CST_ALL
Endif
Return



