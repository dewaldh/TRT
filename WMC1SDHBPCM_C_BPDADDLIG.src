####################################################################
# Labels of the main class and methods 
####################################################################

####################################################################
# 1 : BPDADDLIG Delivery address ( Field ADL 50)
####################################################################

$SET_BPDADDLIG
If [L]_AOLDVAL<>this.BPDADDLIG
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_BPDADDLIG
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.BPDADDLIG
 Gosub PROPAGATE_BPDADDLIG
Endif
Return

$INIT_BPDADDLIG
Gosub DECLARE_VAR From WMC0SDHBPCM_C_BPDADDLIG
[L]CURPRO = "BPDADDLIG"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_BPDADDLIG
Gosub DECLARE_VAR From WMC0SDHBPCM_C_BPDADDLIG
[L]CURPRO = "BPDADDLIG"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_BPDADDLIG             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_BPDADDLIG             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_BPDADDLIG
[L]CURPRO = "BPDADDLIG"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.BPDADDLIG,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_BPDADDLIG
[L]CURPRO = "BPDADDLIG"
If vireblc(this.BPDADDLIG,2)<>vireblc(format$("K:50X",this.BPDADDLIG),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_BPDADDLIG
[L]CURPRO = "BPDADDLIG"
[L]ASTATUS =  func ASYRTRTATY.CONTROLADL(this,"BPDADDLIG",this.BPDADDLIG,"")
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_BPDADDLIG
[L]CURPRO = "BPDADDLIG"
Return

$PROPAGATE_BPDADDLIG
this.isReadonly = 0
[L]CURPRO = "BPDADDLIG"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_BPDADDLIG
Gosub DECLARE_VAR From WMC0SDHBPCM_C_BPDADDLIG
[L]CURPRO = "BPDADDLIG"
$_LNKPRO_BPDADDLIG
Local Char AFORMAT(50)
[L]ASTATUS =  func ASYRTRTATY.GETFORMATADL(this,"BPDADDLIG",this.BPDADDLIG,[L]AFORMAT,"")
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]AFORMAT<>''
[L]AFORMAT='"'+[L]AFORMAT+'"'
[L]ASTATUS = fmet this.ASETATTRIBUTE("BPDADDLIG","$x3Format",[L]AFORMAT)
Endif
Return




