####################################################################
# Labels of the main class and methods 
####################################################################

####################################################################
# 1 : DSPPRC Weekly quantity split ( Field C 3)
####################################################################

$SET_DSPPRC
If [L]_AOLDVAL<>this.DSPPRC
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_DSPPRC
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.DSPPRC
 Gosub PROPAGATE_DSPPRC
Endif
Return

$INIT_DSPPRC
Gosub DECLARE_VAR From WMC0SOPENORDERC_C_DSPPRC
[L]CURPRO = "DSPPRC"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_DSPPRC
Gosub DECLARE_VAR From WMC0SOPENORDERC_C_DSPPRC
[L]CURPRO = "DSPPRC"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_DSPPRC                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_DSPPRC                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_DSPPRC
[L]CURPRO = "DSPPRC"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.DSPPRC=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_DSPPRC
[L]CURPRO = "DSPPRC"
Return

$_AOTHERS_DSPPRC
[L]CURPRO = "DSPPRC"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_DSPPRC
[L]CURPRO = "DSPPRC"
Return

$PROPAGATE_DSPPRC
this.isReadonly = 0
[L]CURPRO = "DSPPRC"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_DSPPRC
Gosub DECLARE_VAR From WMC0SOPENORDERC_C_DSPPRC
[L]CURPRO = "DSPPRC"
$_LNKPRO_DSPPRC
Return




