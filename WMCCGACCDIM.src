If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDATTIM From WMC1GACCDIM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDATTIM From WMC1GACCDIM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AUUID From WMC1GACCDIM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AUUID From WMC1GACCDIM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREUSR From WMC1GACCDIM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREUSR From WMC1GACCDIM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDUSR From WMC1GACCDIM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDUSR From WMC1GACCDIM : Gosub TESTALLSTAT :  Endif
[L]ASTATUS = fmet this.ACONTROL
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



