####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0GACCDIM
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_COA From WMC1GACCDIM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_COA From WMC1GACCDIM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LIN From WMC1GACCDIM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LIN From WMC1GACCDIM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_ACCSTR From WMC1GACCDIM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_ACCSTR From WMC1GACCDIM : Gosub TESTALLSTAT :  Endif
