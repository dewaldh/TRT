Subprog AS_PCU(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3612,GBOUT1) From OBJDIV
Call TEXTE(7919,GBOUT2) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_PCU(zc) From W1WK88STDM
Onkey 130 Titled GBOUT2 Call B2_PCU(zc) From W1WK88STDM
Onkey   2 Titled GBOUTS Call S_PCU(zc) From W1WK88STDM
Onkey 144 Titled GBOUTP Call P_PCU(zc) From W1WK88STDM
Onkey 146 Titled GBOUTA Call SA_PCU(zc) From W1WK88STDM
Gosub _AFF
End

Subprog C_PCU(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "PCU" : GZNUM = 2685 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("TUN","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "PCU" : GZNUM = 2685 : Endif
 If !mkstat
  ## Action STD
  Call C_PCU(VALEUR) From TRTSTOENM
  If mkstat : GERR = 1 : GZONE = "PCU" : GZNUM = 2685 : Endif
 Endif
Endif
Gosub _AFF
End

