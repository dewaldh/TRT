$REMP_DERLU
If clalev([F:DMWPAOR9])>0
 Filter [DMWPAOR9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [DMWPAOR] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:DMWPAOR9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = num$([F:DMWPAOR]TYP)+"~"
 A_CLEDERLU += [F:DMWPAOR]COD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:DMWPAOR9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:DMWPAOR9]
 Endif
Endif
Return

