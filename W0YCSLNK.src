# Supervisor
# Mask management YCSLNK 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YOBJTYP
mkstat = 0
zoncou = "YOBJTYP"
indice = 0
If varmode([M]YOBJTYP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YOBJTYP" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YOBJIDENT
mkstat = 0
zoncou = "YOBJIDENT"
indice = 0
If varmode([M]YOBJIDENT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YOBJIDENT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YCSNID
mkstat = 0
zoncou = "YCSNID"
indice = 0
If varmode([M]YCSNID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCSNID" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YCSLNK"
Return

