If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_DEPSHO From WMC1TABDEPAGIO : Gosub TESTALLSTAT :  Endif
For AINDCLA=1 To 12
 If this.C_DEPDAY(AINDCLA)<>null& find(this.C_DEPDAY(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.C_DEPDAY(AINDCLA)._AMANDATORY : Gosub TESTALLSTAT
 Endif
Next AINDCLA
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_ACCCOD From WMC1TABDEPAGIO : Gosub TESTALLSTAT :  Endif
For AINDCLA=1 To 20
 If this.C_DIE(AINDCLA)<>null& find(this.C_DIE(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.C_DIE(AINDCLA)._AMANDATORY : Gosub TESTALLSTAT
