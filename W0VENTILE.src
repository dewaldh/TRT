# Supervisor
# Mask management VENTILE 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone DSP
mkstat = 0
zoncou = "DSP"
indice = 0
If varmode([M]DSP,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_DSP([M]DSP) From SAIVENTILE
 GACTION = SAVACT
 If mkstat=1 : Raz [M]DSP : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "DSP" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  Call CONTROLE("DSP","!",[M]DSP,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 3022 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_DSP([M]DSP) From SAIVENTILE
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 3022 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call AP_DSP([M]DSP) From SAIVENTILE
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 3022 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone ACC
mkstat = 0
zoncou = "ACC"
For indice=0 To 9
 SAVACT = GACTION : GACTION = "INIACCFMT"
 Call INIACCFMT("") From SETGLBCOA
 GACTION = SAVACT
 If mkstat=1 : Raz [M]ACC(indice) : Endif
 mkstat = 0
Next indice
If mkstat : End : Endif

## Zone NBLED
mkstat = 0
zoncou = "NBLED"
indice = 0
If varinit([M]NBLED,indice)=0
 [M]NBLED = 1
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone BAS
mkstat = 0
zoncou = "BAS"
indice = 0
status = 1
zoncou = "BAS"
Gosub SETDEV From TRTDIV
GDEV1(1) = [M]CUR
If varmode([M]BAS,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_BAS From SAIVENTILE
 GACTION = SAVACT
 If mkstat=1 : Raz [M]BAS : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "BAS" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]BAS
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]BAS = nolign
   status = 75
   Call SAI_BAS(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_BAS From SAIVENTILE
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "BAS" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc liste 4
####################################
IMPTBL = 0

####################################
## Bloc liste 5
####################################
IMPTBL = 0

mkstat = 0

End

