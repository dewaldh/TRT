####################################################################
# 1 : NUMTAB Table number ( Field C 3)
####################################################################

$SET_NUMTAB
If [L]_AOLDVAL<>this.NUMTAB
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_NUMTAB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.NUMTAB
 Gosub PROPAGATE_NUMTAB
Endif
Return

$INIT_NUMTAB
Gosub DECLARE_VAR From WMC0AOBJEXTR
[L]CURPRO = "NUMTAB"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_NUMTAB
Gosub DECLARE_VAR From WMC0AOBJEXTR
[L]CURPRO = "NUMTAB"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_NUMTAB                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_NUMTAB                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_NUMTAB
[L]CURPRO = "NUMTAB"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.NUMTAB=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_NUMTAB
[L]CURPRO = "NUMTAB"
Return

$_AOTHERS_NUMTAB
[L]CURPRO = "NUMTAB"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_NUMTAB
[L]CURPRO = "NUMTAB"
Return

$PROPAGATE_NUMTAB
this.isReadonly = 0
[L]CURPRO = "NUMTAB"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_NUMTAB
Gosub DECLARE_VAR From WMC0AOBJEXTR
[L]CURPRO = "NUMTAB"
$_LNKPRO_NUMTAB
Return

$_ALOADATTRIBUTE_NUMTAB
[L]ASTATUS = fmet this.ASETATTRIBUTE("NUMTAB","$isMandatory","true")
Return


