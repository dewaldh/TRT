 Filter [GRY9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [GRY1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:GRY9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:GRY]GRU+"~"
 A_CLEDERLU += [F:GRY]PYM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:GRY9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:GRY9]
 Endif
Endif
Return

