SELFMT(NBSEL) = "K:3X"
SELFOR(NBSEL) = "KTD:3B"
SELMAJ(NBSEL) = 1
NBTRI += 1
TRIZON(NBTRI) = "[F:AYY]LAN"
TRILIB(NBTRI) = SELLIB(NBSEL)
