# Supervisor
# Mask management GRT4 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

indice = 0
status = 1
zoncou = "NBLIG1"
If !mkstat
 NBLIG = [M]NBLIG1
 For nolign=1 To NBLIG
  indice = nolign-1
  [M]NBLIG1 = nolign
  status = 75
  Call SAI_NBLIG1(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG1(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1


## Zone LSTCLODAT
mkstat = 0
zoncou = "LSTCLODAT"
If varmode([M]LSTCLODAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LSTCLODAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NBV
mkstat = 0
zoncou = "NBV"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]NBV(indice) : Endif

## Zone CRBAMT
mkstat = 0
zoncou = "CRBAMT"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]CRBAMT(indice) : Endif

## Zone CRBCUM
mkstat = 0
zoncou = "CRBCUM"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]CRBCUM(indice) : Endif


End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GRT4"
Return

