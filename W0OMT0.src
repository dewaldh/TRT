#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management OMT0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
WCHAR = [M]FCY
SAVACT = GACTION : GACTION = "STD"
Call AV_FCY([M]FCY) From SUBOMT
GACTION = SAVACT
If mkstat=1 : Raz [M]FCY : Endif
If !mkstat
 If varmode([M]FCY,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]FCY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTFCY"
   Call CONTSITE([M]FCY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("FCY","",[M]FCY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTFCY"
     Call CONTFCY([M]FCY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "STD"
      Call C_FCY([M]FCY) From SUBOMT
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       If WCHAR<>[M]FCY or IMPMOD
        SAVACT = GACTION : GACTION = "STD"
        Call AM_FCY([M]FCY) From SUBOMT
        GACTION = SAVACT
        If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
        Gosub AFF From GSAISIE
        If mkstat : End : Endif
       Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone OMTNUM
mkstat = 0
zoncou = "OMTNUM"
indice = 0
If varmode([M]OMTNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "OMTNUM" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "CRMCTLNUM"
 Call CRMCTLNUM([M]OMTNUM,"OMT") From CRMUTIL150
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "OMTNUM" : GZNUM = 1263 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone CUR
mkstat = 0
zoncou = "CUR"
indice = 0
WCHAR = [M]CUR
If varmode([M]CUR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CUR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("TCU","!",[M]CUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CUR" : GZNUM = 323 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WCHAR<>[M]CUR or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_CUR([M]CUR) From SUBOMT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CUR" : GZNUM = 323 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "OMT0"
Return

