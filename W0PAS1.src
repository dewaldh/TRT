#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management PAS1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone TRSTYP
mkstat = 0
zoncou = "TRSTYP"
indice = 0
IMPENT = [M]TRSTYP
If varinit([M]TRSTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]TRSTYP,indice)=0
 Call INICOMBO(704) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_TRSTYP(IMPENT) From SUBPAS
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,704,1))))
 Endif
 [M]TRSTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TRSTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TRSTYP(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "TRSTYP" : GZNUM = 18948 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]TRSTYP = IMPENT

## Zone LEG
mkstat = 0
zoncou = "LEG"
indice = 0
If varinit([M]LEG,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]LEG,"909","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]LEG,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "LEG" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]LEG,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTADI"
   Call CONTADI([M]LEG,"909","") From SUBADI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("ADI","",[M]LEG,"909",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "ACONTLEG"
     Call CONTLEG([M]LEG) From SUBADI
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTCPY"
 Call CONTSOC([M]CPY) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("CPY","!",[M]CPY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTCPYLEG"
   Call CONTCPYLEG([M]CPY,[M]LEG) From TRTX3
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone KEY1
mkstat = 0
zoncou = "KEY1"
indice = 0
If varmode([M]KEY1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "KEY1" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone KEY2
mkstat = 0
zoncou = "KEY2"
indice = 0
If varmode([M]KEY2,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "KEY2" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone KEY3
mkstat = 0
zoncou = "KEY3"
indice = 0
If varmode([M]KEY3,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "KEY3" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone KEY4
mkstat = 0
zoncou = "KEY4"
indice = 0
If varmode([M]KEY4,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "KEY4" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone KEY5
mkstat = 0
zoncou = "KEY5"
indice = 0
If varmode([M]KEY5,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "KEY5" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "PAS1"
Return

