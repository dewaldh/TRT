$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Integer  A_BUF1(0..GNBDERNIER)
Local Char     A_BUF2(12)(0..GNBDERNIER)
Local Char     A_BUF3(80)(0..GNBDERNIER)
Local Shortint A_BUF4(0..GNBDERNIER)
Local Char     A_BUF5(30)(0..GNBDERNIER)
Local Libelle  A_BUF6(0..GNBDERNIER)
Local Shortint A_BUF7(0..GNBDERNIER)
If clalev([F:AI09])>0 : Close Local File [AI09] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Integer  A_TAB1,
& Char     A_TAB2(12),
& Char     A_TAB3(80),
& Shortint A_TAB4,
& Char     A_TAB5(30),
& Libelle  A_TAB6,
& Shortint A_TAB7
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5,A_BUF6,A_BUF7
& As [AI09]
Return

$LEC_DERLU
Read [ATX]NUMERO = GLANGUE ; [F:AI0]INTIT
If fstat : Raz [F:ATX] : Endif
Return

$CHARGE_DERLU
If clalev([F:AI09])>0
 Read [AI09] = [F:AI09]A_TABI
 [L]CLE = [F:AI09]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:AI09])>0
 Filter [AI09]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AM3] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AI09])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AI0]CODE+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AI09]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AI09]
 Endif
Endif
Return

