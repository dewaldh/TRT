#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management YLOT 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YLOTDES
mkstat = 0
zoncou = "YLOTDES"
indice = 0
If varmode([M]YLOTDES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLOTDES" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YLOTDES,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YLOTDES" : GZNUM = 12402 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YLNDTYP
mkstat = 0
zoncou = "YLNDTYP"
indice = 0
IMPENT = [M]YLNDTYP
If varinit([M]YLNDTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]YLNDTYP,indice)=0
 [M]YLNDTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLNDTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YLNDTYP(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YLNDTYP" : GZNUM = 100436 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YLNDTYP = IMPENT

## Zone YINTUSE
mkstat = 0
zoncou = "YINTUSE"
indice = 0
IMPENT = [M]YINTUSE
If varinit([M]YINTUSE,indice)=0
 IMPENT = 1
Endif
If varmode([M]YINTUSE,indice)=0
 [M]YINTUSE(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YINTUSE" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YINTUSE(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YINTUSE" : GZNUM = 100437 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YINTUSE = IMPENT

## Zone YLOTSTA
mkstat = 0
zoncou = "YLOTSTA"
indice = 0
IMPENT = [M]YLOTSTA
If varinit([M]YLOTSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YLOTSTA,indice)=0
 [M]YLOTSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLOTSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YLOTSTA(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YLOTSTA" : GZNUM = 100438 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YLOTSTA = IMPENT

## Zone YJOBACTFLG
mkstat = 0
zoncou = "YJOBACTFLG"
indice = 0
IMPENT = [M]YJOBACTFLG
WNUM = IMPENT
If varinit([M]YJOBACTFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]YJOBACTFLG,indice)=0
 [M]YJOBACTFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YJOBACTFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YJOBACTFLG(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YJOBACTFLG" : GZNUM = 100380 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "SPE"
   Call AM_YJOBACTFLG(IMPENT) From SPEYLOT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "YJOBACTFLG" : GZNUM = 100380 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]YJOBACTFLG = IMPENT

## Zone YJOBCOD
mkstat = 0
zoncou = "YJOBCOD"
indice = 0
SAVACT = GACTION : GACTION = "INICCEFMT"
Call INICCEFMT("DJB") From SETGLBDIE
GACTION = SAVACT
If mkstat=1 : Raz [M]YJOBCOD : Endif
If !mkstat
 If varmode([M]YJOBCOD,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "YJOBCOD" : Gosub ACTION From =IMPTRT
  Endif
  Call CONTROLE("CCE","!",[M]YJOBCOD,"DJB",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YJOBCOD" : GZNUM = 21780 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YLOT"
Return

