# Supervisor
# Mask management FASPARCPY 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_CCE9
Onevent BEFORE_AREA Gosub AV_CCE9 From W2FASPARCPY
Onevent CONTROL Call C_CCE9(zc) From W2FASPARCPY
Onevent BEFORE_INPUT Call AS_CCE9(zc) From W2FASPARCPY
Return

$AV_CCE9
## Action INICCEFMT
Call INICCEFMT("") From SETGLBDIE
If GCCEFMT<>""
 Chgfmt [M]CCE9(indice) With "K:"+GCCEFMT
Else
 Chgfmt [M]CCE9(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog AS_CCE9(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_CCE9(zc) From W2FASPARCPY
Onkey   2 Titled GBOUTS Call S_CCE9(zc) From W2FASPARCPY
Onkey 144 Titled GBOUTP Call P_CCE9(zc) From W2FASPARCPY
Onkey 146 Titled GBOUTA Call SA_CCE9(zc) From W2FASPARCPY
Gosub _AFF
End

Subprog C_CCE9(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CCE9" : GZNUM = 5666 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CCE9" : GZNUM = 5666 : Endif
Endif
Gosub _AFF
End

Subprog S_CCE9(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "CCE"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_CCE9(VALEUR)
Variable Char    VALEUR()
Call OBJET("CCE",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_CCE9(VALEUR)
