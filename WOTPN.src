$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(5)(0..GNBDERNIER)
Local Char     A_BUF2(80)(0..GNBDERNIER)
Local Char     A_BUF3(5)(0..GNBDERNIER)
Local Char     A_BUF4(30)(0..GNBDERNIER)
If clalev([F:TPN9])>0 : Close Local File [TPN9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(5),
& Char     A_TAB2(80),
& Char     A_TAB3(5),
& Char     A_TAB4(30)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4
& As [TPN9]
Return

$LEC_DERLU
Read [AX3]AXX0="TABPNHTYP";"DESAXX";GLANGUE;num$([F:TPN]PNHTYP);num$([F:TPN]LEG)
If fstat : Raz [F:AX3] : Endif
Read [AX4]AXX0="TABPNHTYP";"SHOAXX";GLANGUE;num$([F:TPN]PNHTYP);num$([F:TPN]LEG)
If fstat : Raz [F:AX4] : Endif
Return

$CHARGE_DERLU
If clalev([F:TPN9])>0
 Read [TPN9] = [F:TPN9]A_TABI
 [L]CLE = [F:TPN9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:TPN9])>0
 Filter [TPN9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [TPN1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:TPN9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:TPN]LEG+"~"
 A_CLEDERLU += [F:TPN]PNHTYP+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:TPN9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:TPN9]
 Endif
Endif
Return

