# Supervisor
# Mask management CONSAWW2 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_NUMORG
Onevent CONTROL Call C_NUMORG(zc) From W2CONSAWW2
Return

Subprog C_NUMORG(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CONTVCR
 Call CONTVCR(VALEUR) From TRTDIV
 If mkstat : GERR = 1 : GZONE = "NUMORG" : GZNUM = 27076 : Endif
Endif
Gosub _AFF
End

$ON_USRORG
Onevent CONTROL Call C_USRORG(zc) From W2CONSAWW2
Onevent BEFORE_INPUT Call AS_USRORG(zc) From W2CONSAWW2
Return

Subprog AS_USRORG(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(485,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_USRORG(zc) From W2CONSAWW2
Onkey   2 Titled GBOUTS Call S_USRORG(zc) From W2CONSAWW2
Onkey 144 Titled GBOUTP Call P_USRORG(zc) From W2CONSAWW2
Onkey 146 Titled GBOUTA Call SA_USRORG(zc) From W2CONSAWW2
Gosub _AFF
End

Subprog C_USRORG(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("AUS","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "USRORG" : GZNUM = 27077 : Endif
Gosub _AFF
End

Subprog S_USRORG(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "AUS"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_USRORG(VALEUR)
Variable Char    VALEUR()
Call OBJET("AUS",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_USRORG(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AUS"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_USRORG(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AUS",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

