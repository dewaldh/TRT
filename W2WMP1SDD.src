# Sage X3 supervisor
# Mask management WMP1SDD 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_BID
Onevent BEFORE_AREA Gosub AV_BID From W2WMP1SDD
Onevent CONTROL Call C_BID(zc) From W2WMP1SDD
Onevent INIT_AREA Call D_BID(zc) From W2WMP1SDD
Onevent BEFORE_INPUT Call AS_BID(zc) From W2WMP1SDD
Onevent AFTER_CHANGE Gosub AMM_BID From W2WMP1SDD
Return

$AV_BID
If !GWEBSERV
 ## Action FMTBIDPAY
 Call FMTBIDPAY([M]BIDCRY) From TRTDIV
Endif
If GFMT<>""
 Chgfmt [M]BID(indice) With "K:"+GFMT
Else
 Chgfmt [M]BID(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog D_BID(VALEUR)
Variable Char    VALEUR()
If varinit([M]BID,indice)=0
 ## Action STD
 Call D_BID(VALEUR) From SUBPAY
Endif
Gosub _AFF
End

Subprog AS_BID(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(8255,GBOUTS) From OBJDIV
## Action STD
Call AS_BID(VALEUR) From SUBPAY
If mkstat=2
 [M]BID(INDIC) = VALEUR
 Affzo BID(INDIC) : mkstat = 2
Endif
Onkey   2 Titled GBOUTS Call S_BID(zc) From W2WMP1SDD
Gosub _AFF
End

Subprog C_BID(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "BID" : GZNUM = 0 : Endif
If !mkstat
 ## Action CONTRIB
 Call CONTRIB(VALEUR,[M]BIDCRY) From CONTNUM
 If mkstat : GERR = 1 : GZONE = "BID" : GZNUM = 0 : Endif
Endif
Gosub _AFF
End

$AMM_BID
Call AM_BID(zc)
If !mkstat & GOBJET
 zoncou = "BID" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_BID(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_BID(VALEUR) From SUBPAY
If mkstat : GERR = 1 : GZONE = "BID" : GZNUM = 0 : Endif
Gosub _AFF
End

Subprog S_BID(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action GSELECT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GSELECT"
 PARMSK(1) = "BID"
 PARMSK(2) = "SUBPAY"
 Call S_TAB_CHAR(VALEUR,PARMSK,"GSELECT","ASELGEN","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

$ON_CPY
Return

