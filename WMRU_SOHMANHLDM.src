$AREPCONTROL_AUPDATE
SOHMANHLDM.ASTALIN = [V]CST_AUPD
SOHMANHLDM.SOH.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = SOHMANHLDM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet SOHMANHLDM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = SOHMANHLDM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet SOHMANHLDM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


