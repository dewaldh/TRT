$ON_RATCURCNX
Onevent BEFORE_INPUT Call AS_RATCURCNX(zc) From W2FASISSP
Onevent AFTER_CHANGE Gosub AMM_RATCURCNX From W2FASISSP
Onevent CONTROL Call C_RATCURCNX(zc) From W2FASISSP
Return

Subprog AS_RATCURCNX(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(694,GBOUT2) From OBJDIV
If !GWEBSERV
 ## Action ARCUINI
 Call RCUINI(VALEUR,[M]CURCNX(0),[M]CURCNX(nolign-1)) From AINICOURS
 If mkstat=2
  [M]RATCURCNX(INDIC) = VALEUR
  Affzo RATCURCNX(INDIC) : mkstat = 2
 Endif
Endif
Onkey 130 Titled GBOUT2 Call B2_RATCURCNX(zc) From W2FASISSP
Gosub _AFF
End

Subprog C_RATCURCNX(VALEUR)
Variable Decimal VALEUR
Gosub _AFF
End

$AMM_RATCURCNX
Call AM_RATCURCNX(zc)
If !mkstat & GOBJET
 zoncou = "RATCURCNX" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_RATCURCNX(VALEUR)
Variable Decimal VALEUR
## Action STD
Call AM_RATCURCNX(VALEUR) From TRTFASISS
If mkstat : GERR = 1 : GZONE = "RATCURCNX" : GZNUM = 2793 : Endif
Gosub _AFF
End

Subprog B2_RATCURCNX(VALEUR)
Variable Decimal VALEUR
If !GWEBSERV
 ## Action AINICOURS
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "AINICOURS"
 PARMSK(1) = [M]CURCNX(0)
 PARMSK(2) = [M]CURCNX(nolign-1)
 PARMSK(3) = [M]ISSCPY
 PARMSK(4) = num$([M]CURTYPCNX(nolign-1))
 PARMSK(5) = [M]ISSDAT
 PARMSK(6) = ""
 PARMSK(7) = ""
 Call S_AFF_NUM(VALEUR,PARMSK,"AINICOURS","AINICOURS","") From GACTION
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

