# Supervisor
## CRUD-CLASSE ## PHYELTM
# 2018-08-27 13:22:30 844


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PHYELTM,ASTATUS)
Variable Instance PHYELTM Using R_PHYELTM
Variable Integer ASTATUS

If PHYELTM = null : End : Endif

Gosub AVARIABLES From WMR00PHYELTM
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PHYELTM.ACTX.AGETAFCRIGHT(PHYELTM,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PHYELTM.ACTX.AGETAFCRIGHTU(PHYELTM,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PHYELTM
Else
 Gosub AUPDATE_PHYELTM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PHYELTM.freeSnapshot
PHYELTM.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PHYELTM.ASTALIN = [V]CST_AUPD
PHYELTM.PHY.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PHYELTM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PHYELTM.PHY.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PHYELTM.PHY.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PHYELTM.PHY.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTM.PHY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PHYELTM.PHY.PHYMVT) : # PHYMVT -- 2
 If PHYELTM.PHY.PHYMVT(A_N2) <> null
& and PHYELTM.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PHYELTM.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEW & PHYELTM.PHY.ASTALIN=[V]CST_ADEL
   PHYELTM.PHY.PHYMVT(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PHYELTM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEW & find(PHYELTM.PHY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PHYELTM.PHY.PHYMVT(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PHYELTM.PHY.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTM.PHY.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY.PHYMVT(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY.PHYMVT(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY.PHYMVT(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PHYELTM.PHY.PHYMVT) : # PHYMVT -- 2
 If PHYELTM.PHY.PHYMVT(A_N2) <> null
& and PHYELTM.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PHYELTM.PHY.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTM.PHY.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY.PHYMVT(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY.PHYMVT(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY.PHYMVT(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PHYELTM.PHY.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTM.PHY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PHYELTM
Trbegin [F:PHY]
 Gosub AUPDATE_PHYELTM
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PHYELTM.PHY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PHYELTM.PHY.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTM.PHY.PHYMVT)
 If PHYELTM.PHY.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTM.PHY.PHYMVT(A_N2)._AUPDATE_AFTER
  If PHYELTM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTM.PHY.PHYMVT(A_N2)
   PHYELTM.PHY.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_PHYELTM
[L]A_ASTALIN = PHYELTM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.PHY._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PHYELTM.snapshotEnabled=1
 [L]A_RETIK = PHYELTM.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTM.PHY.PHYMVT)
 If PHYELTM.PHY.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTM.PHY.PHYMVT(A_N2)._AUPDATE_ABORT
  If PHYELTM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTM.PHY.PHYMVT(A_N2)
   PHYELTM.PHY.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PHYELTM.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



