# Supervisor
# Mask management YPLA 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YPLLID
mkstat = 0
zoncou = "YPLLID"
indice = 0
If varmode([M]YPLLID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YPLLID" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YPLLID,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YPLLID" : GZNUM = 100868 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YNUM
mkstat = 0
zoncou = "YNUM"
indice = 0
If varmode([M]YNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YNUM" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YNUM=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YNUM" : GZNUM = 100606 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YSTA
mkstat = 0
zoncou = "YSTA"
indice = 0
IMPENT = [M]YSTA
If varinit([M]YSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YSTA,indice)=0
 [M]YSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YSTA(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YSTA" : GZNUM = 3860 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YSTA = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YPLA"
Return

