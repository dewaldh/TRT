Onevent CONTROL Call C_STALOT(zc) From W1CPTMURANO
Return

Subprog D_STALOT(VALEUR)
Variable Integer VALEUR
If varinit([M]STALOT,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_STALOT(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(617) From COMBOS
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,617,1))))
Endif
If !mkstat
 Discombo 0 From STALOT(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From STALOT(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_STALOT(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
Gosub _AFF
End

