#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:APF])=0 : Local File APROFIL [APF] : Endif
If clalev([F:A_])=0 : Local File APROFIL [A_] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [APF]
LogicClose File [A_]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
Link [APF] With
& [A_]CODPRF~=[APF]MODULE;[APF]CODPRF
& As [LNK_]
& Where ( (1=1)
&  )
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Link [APF] With
& [A_]CODPRF~=[APF]MODULE;[APF]CODPRF
& As [LNK_]
& Where ( (1=1)
&  )
Return
 
#-------------------------------------------------------------------#
# Default filter for the query
#-------------------------------------------------------------------#
$AQUERY_FILTER_DEF
Return
 
#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# APF --- APROFIL(APF)
QRY(NBREP) = NewInstance R_ALISTMENUS AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:APF]AUUID)>0 & [F:APF]AUUID=NullUuid
 [F:APF]AUUID = getUuid
Endif
SetInstance QRY(NBREP).APF With [F:APF]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# APF --- APROFIL(APF)
QRY = NewInstance R_ALISTMENUS AllocGroup PQRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:APF]AUUID)>0 & [F:APF]AUUID=NullUuid
 [F:APF]AUUID = getUuid
Endif
SetInstance QRY.APF With [F:APF]
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

#-------------------------------------------------------------------#
# Additionnal criteria of query
#-------------------------------------------------------------------#
$AQUERY_CRITERIA
Gosub DECLARE_VAR From WMC0ALISTMENUS
CURQRY = "R_ALISTMENUS"
ACTION = "AQUERY_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# LNKPRO of query 
#-------------------------------------------------------------------#
$AQUERY_LNKPRO
Callmet QRY(NBREP).APF.CODPRF.LNKPRO
Return

#-------------------------------------------------------------------#
# LNKPRO of query INSTANCE
#-------------------------------------------------------------------#
$AQUERY_LNKPRO_INSTANCE
Callmet QRY.APF.CODPRF.LNKPRO
Return

#-------------------------------------------------------------------#
# LOAD_PROPERTIES of query
#-------------------------------------------------------------------#
$AQUERY_DECODE_CRITERIA
NBPRO = 0
NBPRO += 1 : TBPRO(NBPRO) = "CODPRF" : TBFIL(NBPRO) = "[F:APF]CODPRF"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "APF.CODPRF"
NBPRO += 1 : TBPRO(NBPRO) = "TYPPRF" : TBFIL(NBPRO) = "[F:APF]TYPPRF"
           : TBTYP(NBPRO) = 1 : TBPTH(NBPRO) = "APF.TYPPRF"
NBPRO += 1 : TBPRO(NBPRO) = "MODULE" : TBFIL(NBPRO) = "[F:APF]MODULE"
           : TBTYP(NBPRO) = 1 : TBPTH(NBPRO) = "APF.MODULE"
ACTION = "AQUERY_DECODE_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# Standard key of query
#-------------------------------------------------------------------#
$AQUERY_PRIMARYKEYS
Local Char ASTDKEY(250)
ASTDKEY = ""
ASTDKEY += "[F:APF]MODULE"
ASTDKEY += ";"+"[F:APF]CODPRF"
ACTION = "AQUERY_PRIMARYKEYS_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# ACTIONS
#-------------------------------------------------------------------#
$AQUERY_ACTION
Local Char CURPRO(250),CURPTH(250),AMETHOD(50),AEVENT(50)
[L]AEVENT = [L]ACTION
Return
 


