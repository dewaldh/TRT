#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management WH5STD2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone HDKITNTYP
mkstat = 0
zoncou = "HDKITNTYP"
indice = 0
If varinit([M]HDKITNTYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]HDKITNTYP,"407","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]HDKITNTYP,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "HDKITNTYP" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]HDKITNTYP,"407","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "HDKITNTYP" : GZNUM = 707 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!",[M]HDKITNTYP,"407",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "HDKITNTYP" : GZNUM = 707 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone ITINERAIRE
mkstat = 0
zoncou = "ITINERAIRE"
indice = 0
[M]ITINERAIRE = "226"
If varmode([M]ITINERAIRE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITINERAIRE" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone ITNSCO
mkstat = 0
zoncou = "ITNSCO"
indice = 0
IMPENT = [M]ITNSCO
If varinit([M]ITNSCO,indice)=0
 IMPENT = 1
Endif
If varmode([M]ITNSCO,indice)=0
 [M]ITNSCO(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITNSCO" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ITNSCO(indice)
 Endif
Endif
[M]ITNSCO = IMPENT

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBLIGITN
mkstat = 0
zoncou = "NBLIGITN"
indice = 0
status = 1
zoncou = "NBLIGITN"
If varmode([M]NBLIGITN,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBLIGITN From SUBHDKAGD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBLIGITN : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIGITN" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIGITN
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIGITN = nolign
   status = 75
   Call SAI_NBLIGITN(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBLIGITN(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIGITN
mkstat = 0
zoncou = "NBLIGITN"

## Zone TYPACT
mkstat = 0
zoncou = "TYPACT"
IMPENT = [M]TYPACT(indice)
If varinit([M]TYPACT,indice)=0
 IMPENT = 1
Endif
[M]TYPACT(indice) = IMPENT

## Zone EXE
mkstat = 0
zoncou = "EXE"
IMPENT = [M]EXE(indice)
If varinit([M]EXE,indice)=0
 IMPENT = 1
Endif
[M]EXE(indice) = IMPENT

## Zone TYP
mkstat = 0
zoncou = "TYP"
If varinit([M]TYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TYP(indice),"407","") From SUBADI
 GACTION = SAVACT
Endif

## Zone SCO
mkstat = 0
zoncou = "SCO"
IMPENT = [M]SCO(indice)
If varinit([M]SCO,indice)=0
 IMPENT = 1
Endif
[M]SCO(indice) = IMPENT

## Zone CTY
mkstat = 0
zoncou = "CTY"
WCHAR = [M]CTY(indice)
SAVACT = GACTION : GACTION = "FMTCTYPAY"
Call FMTCTYPAY("") From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]CTY(indice) : Endif

## Zone ADD0
mkstat = 0
zoncou = "ADD0"
SAVACT = GACTION : GACTION = "FMTADRPAY"
Call FMTADRPAY("") From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]ADD0(indice) : Endif

## Zone ZIP
mkstat = 0
zoncou = "ZIP"
WCHAR = [M]ZIP(indice)
SAVACT = GACTION : GACTION = "FMTPOSPAY"
Call FMTPOSPAY("") From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]ZIP(indice) : Endif

## Zone CRY
mkstat = 0
zoncou = "CRY"
If varinit([M]CRY,indice)=0
 [M]CRY(indice) = GPAYS
Endif

## Zone NBLIGITN
mkstat = 0
zoncou = "NBLIGITN"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "WH5STD2"
Return

