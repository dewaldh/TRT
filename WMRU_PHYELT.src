# Supervisor
## CRUD-CLASSE ## PHYELT
# 2018-08-27 13:21:45 074


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PHYELT,ASTATUS)
Variable Instance PHYELT Using R_PHYELT
Variable Integer ASTATUS

If PHYELT = null : End : Endif

Gosub AVARIABLES From WMR00PHYELT
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PHYELT.ACTX.AGETAFCRIGHT(PHYELT,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PHYELT.ACTX.AGETAFCRIGHTU(PHYELT,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PHYELT
Else
 Gosub AUPDATE_PHYELT
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PHYELT.freeSnapshot
PHYELT.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PHYELT.ASTALIN = [V]CST_AUPD
PHYELT.PHY.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PHYELT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELT._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELT._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELT._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PHYELT.PHY.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PHYELT.PHY.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PHYELT.PHY.ASTALIN)<>""
 [L]A_ASTALIN = PHYELT.PHY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PHYELT.PHY.PHYMVT) : # PHYMVT -- 2
 If PHYELT.PHY.PHYMVT(A_N2) <> null
& and PHYELT.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PHYELT.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEW & PHYELT.PHY.ASTALIN=[V]CST_ADEL
   PHYELT.PHY.PHYMVT(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PHYELT.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEW & find(PHYELT.PHY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PHYELT.PHY.PHYMVT(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PHYELT.PHY.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELT.PHY.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY.PHYMVT(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY.PHYMVT(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY.PHYMVT(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PHYELT.PHY.PHYMVT) : # PHYMVT -- 2
 If PHYELT.PHY.PHYMVT(A_N2) <> null
& and PHYELT.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PHYELT.PHY.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELT.PHY.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY.PHYMVT(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY.PHYMVT(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY.PHYMVT(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PHYELT.PHY.ASTALIN)<>""
 [L]A_ASTALIN = PHYELT.PHY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PHYELT
Trbegin [F:PHY]
 Gosub AUPDATE_PHYELT
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PHYELT.PHY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PHYELT.PHY.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELT.PHY.PHYMVT)
 If PHYELT.PHY.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELT.PHY.PHYMVT(A_N2)._AUPDATE_AFTER
  If PHYELT.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELT.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELT.PHY.PHYMVT(A_N2)
   PHYELT.PHY.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_PHYELT
[L]A_ASTALIN = PHYELT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELT.PHY._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PHYELT.snapshotEnabled=1
 [L]A_RETIK = PHYELT.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELT.PHY.PHYMVT)
 If PHYELT.PHY.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELT.PHY.PHYMVT(A_N2)._AUPDATE_ABORT
  If PHYELT.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELT.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELT.PHY.PHYMVT(A_N2)
   PHYELT.PHY.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PHYELT.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



