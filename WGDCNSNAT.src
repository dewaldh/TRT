$BOUTON
STDBOUT = "O"
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "P" : LBOUT(NBTEX) = 1101 : Call TEXTE(5042,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "Q" : LBOUT(NBTEX) = 1102 : Call TEXTE(641,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "O" : LBOUT(NBTEX) = 1103 : Call TEXTE(549,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "M" : LBOUT(NBTEX) = 1104 : Call TEXTE(845,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "R" : LBOUT(NBTEX) = 1805 : Call TEXTE(12424,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "E" : LBOUT(NBTEX) = 1806 : Call TEXTE(13717,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "P"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "HAE"
  PARBOUT(2) = "HAE"
  PARBOUT(3) = [M:CNAT]FILTECT(0)
  FLGEXE = 1
  GACTION = "FILTSAI"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"DFILTSAI","FILTSAI","XWDFILTSAI") From GSAISIE
  [M:CNAT]FILTECT(0) = PARBOUT(3)
 When "Q"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "DAE"
  PARBOUT(2) = "DAE"
  PARBOUT(3) = [M:CNAT]FILTECT(1)
  FLGEXE = 1
  GACTION = "FILTSAI"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"DFILTSAI","FILTSAI","XWDFILTSAI") From GSAISIE
  [M:CNAT]FILTECT(1) = PARBOUT(3)
 When "O"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "DAA"
  PARBOUT(2) = "DAA"
  PARBOUT(3) = [M:CNAT]FILTECT(2)
  FLGEXE = 1
  GACTION = "FILTSAI"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"DFILTSAI","FILTSAI","XWDFILTSAI") From GSAISIE
  [M:CNAT]FILTECT(2) = PARBOUT(3)
 When "M"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "[M:CNAT]"
  PARBOUT(2) = "nat"
  PARBOUT(3) = "1"
  FLGEXE = 1
  GACTION = "MEMO"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"GMEMO","GMEMO","XWGMEMO") From GSAISIE
 When "R"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "[M:CNAT]"
  PARBOUT(2) = "nat"
  PARBOUT(3) = "2"
  FLGEXE = 1
  GACTION = "MEMO"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"GMEMO","GMEMO","XWGMEMO") From GSAISIE
 When "E"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "[M:CNAT]"
  PARBOUT(2) = "nat"
  PARBOUT(3) = "3"
  FLGEXE = 1
  GACTION = "MEMO"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"GMEMO","GMEMO","XWGMEMO") From GSAISIE
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
