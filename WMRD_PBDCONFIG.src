############################################################################################
### Subprog ADELETE
############################################################################################
Subprog ADELETE(PBDCONFIG,ASTATUS)
Variable Instance PBDCONFIG Using R_PBDCONFIG
Variable Integer ASTATUS

If PBDCONFIG = null : End : Endif

Gosub AVARIABLES From WMR00PBDCONFIG
If [L]ASTATUS>=[V]CST_AERROR : Goto ADELETE_END : Endif

Local Instance ALNKDELTAB Using C_ALNKDELTAB

If fmet PBDCONFIG.ACTX.AGETAFCRIGHT(PBDCONFIG,"GESPBDECO",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto ADELETE_END : Endif
If fmet PBDCONFIG.ACTX.AGETAFCRIGHTD(PBDCONFIG,"GESPBDECO",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto ADELETE_END
Endif

Gosub AREPCONTROL_ADELETE
If [L]ASTATUS>=[V]CST_AERROR : Goto ADELETE_END : Endif

If !adxlog
 Gosub ADELETE_TR_PBDCONFIG
Else
 Gosub ADELETE_PBDCONFIG
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub ADELETE_END : Return : Endif

$ADELETE_END
End

#-----------------------------------------------------#
$AREPCONTROL_ADELETE
PBDCONFIG.ASTALIN = [V]CST_ADEL
PBDCONFIG.PBDCNF.ASTALIN = [V]CST_ADEL
[L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.ACTION("ADELETE_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub ADELETE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.ACTION("ADELETE_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$ADELETE_CONTROL
Gosub _ADELETE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Verification delete
[L]ASTATUS = max([L]ASTATUS,func ASYRSUB.VERIFY_INSTANCE(PBDCONFIG.PBDCNF,ALNKDELTAB))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _ADELETE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_ADELETE_CONTROL_BEFORE
If PBDCONFIG.PBDCNF.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PBDCONFIG.PBDCNF.ASTALIN = [V]CST_ADEL
[L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.PBDCNF.ACTION("ADELETE_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(PBDCONFIG.PBDCNF.LINESEXCL) : # PBDCONFIGD -- 2
 If PBDCONFIG.PBDCNF.LINESEXCL(A_N2) <> null
& and PBDCONFIG.PBDCNF.LINESEXCL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PBDCONFIG.PBDCNF.LINESEXCL(A_N2).ASTALIN<>[V]CST_ANEW
   PBDCONFIG.PBDCNF.LINESEXCL(A_N2).ASTALIN = [V]CST_ADEL
   [L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.PBDCNF.LINESEXCL(A_N2)._ADELETE_CONTROL_BEFORE)
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(PBDCONFIG.PBDCNF.LINESINCL) : # PBDCONFIGD -- 3
 If PBDCONFIG.PBDCNF.LINESINCL(A_N2) <> null
& and PBDCONFIG.PBDCNF.LINESINCL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PBDCONFIG.PBDCNF.LINESINCL(A_N2).ASTALIN<>[V]CST_ANEW
   PBDCONFIG.PBDCNF.LINESINCL(A_N2).ASTALIN = [V]CST_ADEL
   [L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.PBDCNF.LINESINCL(A_N2)._ADELETE_CONTROL_BEFORE)
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_ADELETE_CONTROL_AFTER
For A_N2=1 To maxtab(PBDCONFIG.PBDCNF.LINESEXCL) : # PBDCONFIGD -- 2
 If PBDCONFIG.PBDCNF.LINESEXCL(A_N2) <> null
& and PBDCONFIG.PBDCNF.LINESEXCL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  [L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.PBDCNF.LINESEXCL(A_N2)._ADELETE_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(PBDCONFIG.PBDCNF.LINESINCL) : # PBDCONFIGD -- 3
 If PBDCONFIG.PBDCNF.LINESINCL(A_N2) <> null
& and PBDCONFIG.PBDCNF.LINESINCL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  [L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.PBDCNF.LINESINCL(A_N2)._ADELETE_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.PBDCNF.ACTION("ADELETE_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$ADELETE_TR_PBDCONFIG
Trbegin [F:PBDCNF]
 Gosub ADELETE_PBDCONFIG
 If [L]ASTATUS>=[V]CST_AERROR : Gosub ADELETE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_ADELETE_AFTER
Return

#-----------------------------------------------------#
$ADELETE_PBDCONFIG
[L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.ACTION("ADELETE_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.PBDCNF._ADELETE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PBDCONFIG.ACTION("ADELETE_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$ADELETE_ABORT
Rollback
$_ADELETE_ABORT
If PBDCONFIG.snapshotEnabled=1
 [L]A_RETIK = PBDCONFIG.revertUpdTick
Endif
[L]ASTATUS = max(fmet PBDCONFIG.ACTION("ADELETE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



