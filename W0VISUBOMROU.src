Endif
[M]STOMGTCOD = IMPENT

## Zone XREOCOD
mkstat = 0
zoncou = "XREOCOD"
indice = 0
IMPENT = [M]XREOCOD
If varinit([M]XREOCOD,indice)=0
 IMPENT = 1
Endif
[M]XREOCOD = IMPENT

## Zone XCPNTYP
mkstat = 0
zoncou = "XCPNTYP"
indice = 0
IMPENT = [M]XCPNTYP
If varinit([M]XCPNTYP,indice)=0
 IMPENT = 1
Endif
[M]XCPNTYP = IMPENT

####################################
## Bloc liste 6
####################################
IMPTBL = 0

## Zone XROOTIMCOD
mkstat = 0
zoncou = "XROOTIMCOD"
indice = 0
IMPENT = [M]XROOTIMCOD
If varinit([M]XROOTIMCOD,indice)=0
 IMPENT = 1
Endif
[M]XROOTIMCOD = IMPENT

####################################
## Bloc liste 7
####################################
IMPTBL = 0

####################################
## Bloc liste 8
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone ALTTYPR
mkstat = 0
zoncou = "ALTTYPR"
IMPENT = [M]ALTTYPR(indice)
If varinit([M]ALTTYPR,indice)=0
 IMPENT = [M]BOMALTTYP
Endif
[M]ALTTYPR(indice) = IMPENT

## Zone ITMDES1
mkstat = 0
zoncou = "ITMDES1"
If varinit([M]ITMDES1,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]ITMDES1(indice),"ITMMASTER","DES1AXX",[M:VBOR]GRPITMREF(nolign-1),"") From ATEXTRA
 GACTION = SAVACT
Endif

## Zone REOCOD
mkstat = 0
zoncou = "REOCOD"
IMPENT = [M]REOCOD(indice)
If varinit([M]REOCOD,indice)=0
 IMPENT = 1
Endif
[M]REOCOD(indice) = IMPENT

## Zone CPNTYP
mkstat = 0
zoncou = "CPNTYP"
IMPENT = [M]CPNTYP(indice)
If varinit([M]CPNTYP,indice)=0
 IMPENT = 1
Endif
[M]CPNTYP(indice) = IMPENT

## Zone ICO
mkstat = 0
zoncou = "ICO"
If varinit([M]ICO,indice)=0
 [M]ICO(indice) = "Folder.gif"
Endif

## Zone EXPFLG
mkstat = 0
zoncou = "EXPFLG"
If varinit([M]EXPFLG,indice)=0
 [M]EXPFLG(indice) = 2
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
