# Supervisor
# Mask management VISUBOMROU 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone BOMALTTYP
mkstat = 0
zoncou = "BOMALTTYP"
indice = 0
IMPENT = [M]BOMALTTYP
SAVACT = GACTION : GACTION = "STD"
Call AV_BOMALTTYP(IMPENT) From SUBBOMROU
GACTION = SAVACT
If mkstat=1 : Raz IMPENT : Endif
If !mkstat
 If varinit([M]BOMALTTYP,indice)=0
  IMPENT = 2
 Endif
Endif
[M]BOMALTTYP = IMPENT

## Zone STOFCY
mkstat = 0
zoncou = "STOFCY"
indice = 0
WCHAR = [M]STOFCY
If varmode([M]STOFCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "STOFCY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]STOFCY,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1720 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]STOFCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1720 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","",[M]STOFCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1720 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WCHAR<>[M]STOFCY or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_STOFCY([M]STOFCY) From SUBBOMROU
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1720 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone PARITMREF
mkstat = 0
zoncou = "PARITMREF"
indice = 0
WCHAR = [M]PARITMREF
If varmode([M]PARITMREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PARITMREF" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]PARITMREF,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "PARITMREF" : GZNUM = 7352 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "VXADCUPC"
  Call ADCCTRUPC([M]PARITMREF) From VXADCUPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "PARITMREF" : GZNUM = 7352 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ITM","",[M]PARITMREF,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "PARITMREF" : GZNUM = 7352 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_PARITMREF([M]PARITMREF) From SUBBOMROU
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "PARITMREF" : GZNUM = 7352 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     If WCHAR<>[M]PARITMREF or IMPMOD
      SAVACT = GACTION : GACTION = "STD"
      Call AM_PARITMREF([M]PARITMREF) From SUBBOMROU
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "PARITMREF" : GZNUM = 7352 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone BOMALT
mkstat = 0
zoncou = "BOMALT"
indice = 0
If varmode([M]BOMALT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BOMALT" : Gosub ACTION From =IMPTRT
 Endif
 If [M]BOMALT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 9555 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If [M]BOMALT<0
   mkstat = 1 : GMESSAGE = mess(180,141,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 9555 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTNUM("TBO","",[M]BOMALT,num$([M]BOMALTTYP),mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 9555 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone ROUALT
mkstat = 0
zoncou = "ROUALT"
indice = 0
If varmode([M]ROUALT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ROUALT" : Gosub ACTION From =IMPTRT
 Endif
 If [M]ROUALT<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ROUALT" : GZNUM = 2269 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone DATREF
mkstat = 0
zoncou = "DATREF"
indice = 0
If varmode([M]DATREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATREF" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_DATREF([M]DATREF) From SUBBOMROU
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DATREF" : GZNUM = 6424 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone QTYCOD
mkstat = 0
zoncou = "QTYCOD"
indice = 0
IMPENT = [M]QTYCOD
If varinit([M]QTYCOD,indice)=0
 IMPENT = 1
Endif
[M]QTYCOD = IMPENT

## Zone USESTA
mkstat = 0
zoncou = "USESTA"
indice = 0
IMPENT = [M]USESTA
If varinit([M]USESTA,indice)=0
 IMPENT = 1
Endif
[M]USESTA = IMPENT

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBLIG From SUBBOMROU
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBLIG : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 4
####################################
IMPTBL = 0

####################################
## Bloc liste 5
####################################
IMPTBL = 0

## Zone XLIKQTYCOD
mkstat = 0
zoncou = "XLIKQTYCOD"
indice = 0
IMPENT = [M]XLIKQTYCOD
If varinit([M]XLIKQTYCOD,indice)=0
 IMPENT = 1
Endif
[M]XLIKQTYCOD = IMPENT

## Zone XQTYRND
mkstat = 0
zoncou = "XQTYRND"
indice = 0
IMPENT = [M]XQTYRND
If varinit([M]XQTYRND,indice)=0
 IMPENT = 1
Endif
[M]XQTYRND = IMPENT

## Zone STOMGTCOD
mkstat = 0
zoncou = "STOMGTCOD"
indice = 0
IMPENT = [M]STOMGTCOD
If varinit([M]STOMGTCOD,indice)=0
