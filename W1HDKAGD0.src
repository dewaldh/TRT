#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management HDKAGD0 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:02:57"
A_USER  = "ADMIN"
A_STAMP = "20180828000257"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask HDKAGD0 [HDK0]
Gosub HORODAT
Local Inpbox "HDKAGD0" From GFONCTION At A_STAMP With A_WINDPREV Mask [HDK0] As [SIM_]
Grizo [HDK0]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [HDK0]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=112 : Y1(0)=1 : Y2(0)=2
Return

$ON_HDKCOL
Onevent BEFORE_INPUT Call AS_HDKCOL(zc) From W1HDKAGD0
Onevent CONTROL Call C_HDKCOL(zc) From W1HDKAGD0
Onevent AFTER_CHANGE Gosub AMM_HDKCOL From W1HDKAGD0
Return

Subprog AS_HDKCOL(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(485,GBOUT1) From OBJDIV
Call TEXTE(23626,GBOUT4) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_HDKCOL(zc) From W1HDKAGD0
Onkey 132 Titled GBOUT4 Call B4_HDKCOL(zc) From W1HDKAGD0
Onkey   2 Titled GBOUTS Call S_HDKCOL(zc) From W1HDKAGD0
Onkey 144 Titled GBOUTP Call P_HDKCOL(zc) From W1HDKAGD0
Onkey 146 Titled GBOUTA Call SA_HDKCOL(zc) From W1HDKAGD0
Gosub _AFF
End

Subprog C_HDKCOL(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("AUS","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "HDKCOL" : GZNUM = 16058 : Endif
Gosub _AFF
End

$AMM_HDKCOL
Call AM_HDKCOL(zc)
If !mkstat & GOBJET
 zoncou = "HDKCOL" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_HDKCOL(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZHDKCOL)")>indice
 [M]HDKCOL(indice) = VALEUR
 Affzo ZHDKCOL(indice)
Endif
Gosub _AFF
End

Subprog S_HDKCOL(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELITN1REP
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELITN1REP"
 Call S_LIS_CHAR(VALEUR,PARMSK,"SELITN1REP","TRTCRMSEL","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_HDKCOL(VALEUR)
Variable Char    VALEUR()
Call OBJET("AUS",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog B4_HDKCOL(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SAIHDK6
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SAIHDK6"
 PARMSK(1) = "HDKAGD0"
 PARMSK(2) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"FHDK6","TRTHDK6","XWFHDK6") From GSAISIE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog SA_HDKCOL(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AUS"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_HDKCOL(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AUS",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZHDKCOL
Onevent BEFORE_AREA Gosub AV_ZHDKCOL From W1HDKAGD0
Return

$AV_ZHDKCOL
Call LECTEXTRA(zc,"AUTILIS","INTUSR",num$([M]HDKCOL(indice)),"") From ATEXTRA
Gosub _AFF
Return

$ON_DATMIN
Onevent BEFORE_AREA Gosub AV_DATMIN From W1HDKAGD0
Return

$AV_DATMIN
Chgfmt [M]DATMIN(indice) With "DZ:"+GFMDAT
Gosub _AFF
Return

$ON_DATMAX
Onevent BEFORE_AREA Gosub AV_DATMAX From W1HDKAGD0
Return

$AV_DATMAX
Chgfmt [M]DATMAX(indice) With "DzZ:"+GFMDAT
Gosub _AFF
Return

$ON_HDKSRE
Return

$ON_HDKSALFCY
Return

$ON_HDKCCN
Return

$ON_HDKHOUDEB
Onevent AFTER_AREA Call AP_HDKHOUDEB(zc) From W1HDKAGD0
Return

