#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management ATU1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CRY
mkstat = 0
zoncou = "CRY"
indice = 0
If varinit([M]CRY,indice)=0
 [M]CRY = GPAYS
Endif
If varmode([M]CRY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CRY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CRY,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CRY" : GZNUM = 263 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("TCY","",[M]CRY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CRY" : GZNUM = 263 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_CRY([M]CRY) From SUBATU
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CRY" : GZNUM = 263 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone TYP
mkstat = 0
zoncou = "TYP"
indice = 0
IMPENT = [M]TYP
If varinit([M]TYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]TYP,indice)=0
 Call INICOMBO(7831) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_TYP(IMPENT) From SUBATU
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,7831,1))))
 Endif
 [M]TYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TYP(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "TYP" : GZNUM = 31744 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_TYP(IMPENT) From SUBATU
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "TYP" : GZNUM = 31744 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]TYP = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  SAVACT = GACTION : GACTION = "STD"
  Call C_NBLIG From SUBATU
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
If !mkstat
 SAVACT = GACTION : GACTION = "STD"
 Call C_NBLIG From SUBATU
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone COD
mkstat = 0
zoncou = "COD"
If varmode([M]COD,indice)=0
 SAVACT = GACTION : GACTION = "SAISAT"
 Call SAISAT([M]CRY,"1~" + num$([M]TYP)) From TRTDIV
 GACTION = SAVACT
 If mkstat=1 : Raz [M]COD(indice) : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "COD" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]COD(indice),2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CTLSAT"
   PARAM(1) = [M]CRY
   PARAM(2) = "1~" + num$([M]TYP)
   Local Char    VALEUR(250)
   VALEUR = [M]COD(indice)
   Call S_AFF_CHAR(VALEUR,PARAM,"CTLSAT","TRTDIV","") From GACTION
   [M]COD(indice) = VALEUR
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "CONTUNI"
    Call UNICITE2([M]COD(indice),[M]NBLIG,[M]COD) From OBJDIV
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 0 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone INTIT
mkstat = 0
zoncou = "INTIT"
If varinit([M]INTIT,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]INTIT(indice),"TABSUBDIV","INTIT",[M]CRYTYP,[M]COD(nolign-1)) From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]INTIT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "INTIT" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ATU1"
Return

