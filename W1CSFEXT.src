Onevent BEFORE_INPUT Call AS_CPY(zc) From W1CSFEXT
Onevent AFTER_CHANGE Gosub AMM_CPY From W1CSFEXT
Return

Subprog AS_CPY(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action INICPY
Call INICPY(VALEUR) From EXEFNC
If mkstat=2
 [M]CPY(INDIC) = VALEUR
 Affzo CPY(INDIC) : mkstat = 2
Endif
If !mkstat
 ## Action STD
 Call AS_CPY(VALEUR) From CSFEXT
 If mkstat=2
  [M]CPY(INDIC) = VALEUR
  Affzo CPY(INDIC) : mkstat = 2
 Endif
 Onkey   2 Titled GBOUTS Call S_CPY(zc) From W1CSFEXT
 Onkey 144 Titled GBOUTP Call P_CPY(zc) From W1CSFEXT
 Onkey 146 Titled GBOUTA Call SA_CPY(zc) From W1CSFEXT
Endif
Gosub _AFF
End

Subprog C_CPY(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
If !mkstat
 ## Action ACONTCPY
 Call CONTSOC(VALEUR) From SUBAGF
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("CPY","",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  If !mkstat
   ## Action CONTCPY
   Call CONTCPY(VALEUR) From EXEFNC
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   If !mkstat
    ## Action STD
    Call C_CPY(VALEUR) From CSFEXT
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Endif
  Endif
 Endif
Endif
Gosub _AFF
End

