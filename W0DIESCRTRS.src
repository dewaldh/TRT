# Supervisor
# Mask management DIESCRTRS 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone HEACCECOD
mkstat = 0
zoncou = "HEACCECOD"
indice = 0
IMPENT = [M]HEACCECOD
If varinit([M]HEACCECOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]HEACCECOD,indice)=0
 Call INICOMBO(35) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_HEACCECOD(IMPENT) From SUBDST
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,35,1))))
 Endif
 [M]HEACCECOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "HEACCECOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]HEACCECOD(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "HEACCECOD" : GZNUM = 9430 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]HEACCECOD = IMPENT

## Zone DSPCOD
mkstat = 0
zoncou = "DSPCOD"
indice = 0
IMPENT = [M]DSPCOD
WNUM = IMPENT
If varinit([M]DSPCOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]DSPCOD,indice)=0
 Call INICOMBO(35) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_DSPCOD(IMPENT) From SUBDST
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,35,1))))
 Endif
 [M]DSPCOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DSPCOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DSPCOD(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "DSPCOD" : GZNUM = 29989 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WNUM<>IMPENT or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_DSPCOD(IMPENT) From SUBDST
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "DSPCOD" : GZNUM = 29989 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif
[M]DSPCOD = IMPENT

## Zone DSPSCR
mkstat = 0
zoncou = "DSPSCR"
indice = 0
IMPENT = [M]DSPSCR
If varinit([M]DSPSCR,indice)=0
 IMPENT = 1
Endif
If varmode([M]DSPSCR,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_DSPSCR(IMPENT) From SUBDST
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]DSPSCR(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DSPSCR" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DSPSCR(indice)
 Endif
Endif
[M]DSPSCR = IMPENT

## Zone STOCCECOD
mkstat = 0
zoncou = "STOCCECOD"
indice = 0
IMPENT = [M]STOCCECOD
If varinit([M]STOCCECOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]STOCCECOD,indice)=0
 [M]STOCCECOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "STOCCECOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]STOCCECOD(indice)
 Endif
Endif
[M]STOCCECOD = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone CCESCR
mkstat = 0
zoncou = "CCESCR"
indice = 0
IMPENT = [M]CCESCR
If varinit([M]CCESCR,indice)=0
 IMPENT = 1
Endif
If varmode([M]CCESCR,indice)=0
 [M]CCESCR(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "CCESCR" : Gosub ACTION From =IMPTRT
  IMPENT = [M]CCESCR(indice)
 Endif
Endif
[M]CCESCR = IMPENT

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBDIE
mkstat = 0
zoncou = "NBDIE"
indice = 0
status = 1
zoncou = "NBDIE"
If varmode([M]NBDIE,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBDIE From SUBDST
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBDIE : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBDIE" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBDIE
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBDIE = nolign
   status = 75
   Call SAI_NBDIE(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone CCESCR2
mkstat = 0
zoncou = "CCESCR2"
indice = 0
IMPENT = [M]CCESCR2
If varinit([M]CCESCR2,indice)=0
 IMPENT = 1
Endif
[M]CCESCR2 = IMPENT

## Zone CCECOD2
mkstat = 0
zoncou = "CCECOD2"
For indice=0 To 99
 IMPENT = [M]CCECOD2(indice)
 If varinit([M]CCECOD2,indice)=0
  IMPENT = 1
 Endif
 [M]CCECOD2(indice) = IMPENT
 mkstat = 0
Next indice
If mkstat : End : Endif

## Zone CCESCR3
mkstat = 0
zoncou = "CCESCR3"
indice = 0
IMPENT = [M]CCESCR3
If varinit([M]CCESCR3,indice)=0
 IMPENT = 1
Endif
[M]CCESCR3 = IMPENT

## Zone CCECOD3
mkstat = 0
zoncou = "CCECOD3"
For indice=0 To 99
 IMPENT = [M]CCECOD3(indice)
 If varinit([M]CCECOD3,indice)=0
  IMPENT = 1
 Endif
 [M]CCECOD3(indice) = IMPENT
 mkstat = 0
Next indice
If mkstat : End : Endif

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBDIE(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBDIE
mkstat = 0
zoncou = "NBDIE"

## Zone DIE
mkstat = 0
zoncou = "DIE"
If varmode([M]DIE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIE" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]DIE(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DIE" : GZNUM = 3984 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("DIE","",[M]DIE(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "DIE" : GZNUM = 3984 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_DIE([M]DIE(indice)) From SUBDST
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DIE" : GZNUM = 3984 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone CCECOD
mkstat = 0
zoncou = "CCECOD"
IMPENT = [M]CCECOD(indice)
WNUM = IMPENT
If varinit([M]CCECOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]CCECOD,indice)=0
 Call INICOMBO(35) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CCECOD(IMPENT) From SUBDST
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,35,1))))
 Endif
 [M]CCECOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "CCECOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]CCECOD(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "CCECOD" : GZNUM = 1220 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
    mkstat = 1 : GMESSAGE = mess(99,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "CCECOD" : GZNUM = 1220 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WNUM<>IMPENT or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_CCECOD(IMPENT) From SUBDST
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CCECOD" : GZNUM = 1220 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif
[M]CCECOD(indice) = IMPENT

## Zone NBDIE
mkstat = 0
zoncou = "NBDIE"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "DIESCRTRS"
Return

