####################################################################

$SET_SIVTYP
If [L]_AOLDVAL<>this.SIVTYP
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_SIVTYP
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.SIVTYP
 Gosub PROPAGATE_SIVTYP
Endif
Return

$INIT_SIVTYP
Gosub DECLARE_VAR From WMC0TABSIVTYP
[L]CURPRO = "SIVTYP"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_SIVTYP
Gosub DECLARE_VAR From WMC0TABSIVTYP
[L]CURPRO = "SIVTYP"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_SIVTYP                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_SIVTYP                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_SIVTYP
[L]CURPRO = "SIVTYP"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.SIVTYP,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_SIVTYP
[L]CURPRO = "SIVTYP"
If vireblc(this.SIVTYP,2)<>vireblc(format$("K:5c",this.SIVTYP),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_SIVTYP
[L]CURPRO = "SIVTYP"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_SIVTYP
[L]CURPRO = "SIVTYP"
Return

$PROPAGATE_SIVTYP
this.isReadonly = 0
[L]CURPRO = "SIVTYP"
Gosub _LNKPRO_SIVTYP
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_SIVTYP
Gosub DECLARE_VAR From WMC0TABSIVTYP
[L]CURPRO = "SIVTYP"
$_LNKPRO_SIVTYP
If vireblc(this.SIVTYP,2)<>""
 # Link TABSIVTYP
 Local File TABSIVTYP [WTSV]
 Read [F:WTSV]TSV0 = this.SIVTYP;this.LEG
 If !fstat
  Local File ATEXTRA [WAXX]
  Local Char ILAN(GLONLAN)
  For ILAN=this.ACTX.LAN, this.ACTX.AFOLD.ALANGDEF
   For [F:WAXX] Where CODFIC="TABSIVTYP" & ZONE="SHOAXX" & IDENT1=[F:WTSV]sivtyp & IDENT2=[F:WTSV]LEG & LANGUE = ILAN
    [L]ASTATUS = fmet this.ASETATTRIBUTE("SIVTYP","$title",[WAXX]TEXTE)
    Break 2
   Next WAXX
  Next ILAN
  LogicClose File [WAXX]
  Local File ATEXTRA [WAXX]
  Local Char ILAN(GLONLAN)
  For ILAN=this.ACTX.LAN, this.ACTX.AFOLD.ALANGDEF
   For [F:WAXX] Where CODFIC="TABSIVTYP" & ZONE="DESAXX" & IDENT1=[F:WTSV]sivtyp & IDENT2=[F:WTSV]LEG & LANGUE = ILAN
    [L]ASTATUS = fmet this.ASETATTRIBUTE("SIVTYP","$description",[WAXX]TEXTE)
    Break 2
   Next WAXX
  Next ILAN
  LogicClose File [WAXX]
 Endif
 LogicClose File [WTSV]
Endif
Return


