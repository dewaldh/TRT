Subprog GETACT(NOL,TITBOUT,VALBOUT)
Value Integer NOL
Variable Char TITBOUT()(1..) , VALBOUT()(1..)
Local Integer ONOL , NBBOUT
ONOL   = nolign
nolign = NOL
indice = max(0,nolign-1)
Call AS_NBLIG From ="W1CONSBAN2"
NBBOUT = 0
If GBOUT2<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT2
 VALBOUT(NBBOUT) = "BAN"
 VALBOUT(NBBOUT) += "~B2"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans([M]NUMPAY(nolign-1),"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("PAY","~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("","~","¨")
 Onerrgo
Endif
If GBOUT3<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT3
 VALBOUT(NBBOUT) = "BAN"
 VALBOUT(NBBOUT) += "~B3"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans([M]NUM(nolign-1),"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("GAS","~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans([M]TYP(nolign-1),"~","¨")
 Onerrgo
Endif
nolign = ONOL
indice = max(0,nolign-1)
End

Subprog ACTRUN(INFO)
Value Char INFO
Local Char    BOUT(3)
Local Integer ONOL
ONOL    = nolign
BOUT    = func AFNC.PART(INFO,1)
nolign  = val(func AFNC.PART(INFO,2))
indice  = max(0,nolign-1)
Gosub NOMFIC
For MA=0 to dim(NOMMSK)-1
 Gosub OUVRE_MSK
Next MA
For MA=0 to dim(NOMMSK)-1
 Gosub SETMASK
Next MA
Case BOUT
 When "B2"
  Onerrgo RIEN
  Assign "[M]NUMPAY(nolign-1)" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Onerrgo
  Call B2_NBLIG From ="W1CONSBAN2"
 When "B3"
  Onerrgo RIEN
  Assign "[M]NUM(nolign-1)" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Assign "[M]TYP(nolign-1)" With ctrans(func AFNC.PART(INFO,5),"¨","~")
  Onerrgo
  Call B3_NBLIG From ="W1CONSBAN2"
Endcase
nolign = ONOL
indice = max(0,nolign-1)
End

$RIEN
Resume


