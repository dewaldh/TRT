Append WCLOB,',"$properties":{'
Append WCLOB,'"$resources":{'
Append WCLOB,'"$type":"application/x-array"'
Append WCLOB,',"$item":{'
Append WCLOB,'"$title":"'+func GET_RES_STRING(ACTX.LAN,7710)+' {$key}"'
Append WCLOB,',"$key":"{LCTCOD}"'
Append WCLOB,',"$url":"{$baseUrl}/PHYLCT('+"'"+'{$key}'+"'"+')'+PSYR.URLREP+'PHYLCT.searchItem"'
Append WCLOB,',"$properties":{'
Append WCLOB,'"LCTCOD":{'
Append WCLOB,'"$title":"'+func GET_RES_STRING(ACTX.LAN,7710)+'"'
Append WCLOB,',"$capabilities":"sort,filter"'
Append WCLOB,',"$type":"application/x-string"'
Append WCLOB,',"$maxLength":30'
