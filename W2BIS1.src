# Supervisor
# Mask management BIS1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_FRM1099
Onevent AFTER_CHANGE Gosub AMM_FRM1099 From W2BIS1
Onevent INIT_AREA Call D_FRM1099(zc) From W2BIS1
Onevent BEFORE_INPUT Call AS_FRM1099(zc) From W2BIS1
Return

Subprog D_FRM1099(VALEUR)
Variable Integer VALEUR
If varinit([M]FRM1099,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_FRM1099(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$AMM_FRM1099
Call AM_FRM1099(zc)
If !mkstat & GOBJET
 zoncou = "FRM1099" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_FRM1099(VALEUR)
Variable Integer VALEUR
## Action STD
Call AM_FRM1099(VALEUR) From SUBBIS2
If mkstat : GERR = 1 : GZONE = "FRM1099" : GZNUM = 44093 : Endif
Gosub _AFF
End

$ON_BOX1099
Onevent CONTROL Call C_BOX1099(zc) From W2BIS1
Onevent BEFORE_INPUT Call AS_BOX1099(zc) From W2BIS1
Return

Subprog AS_BOX1099(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_BOX1099(zc) From W2BIS1
Gosub _AFF
End

Subprog C_BOX1099(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CNTBOX1099
 Call CNTBOX1099(VALEUR,[M]FRM1099) From TEN99LIB
 If mkstat : GERR = 1 : GZONE = "BOX1099" : GZNUM = 44094 : Endif
Endif
Gosub _AFF
End

Subprog S_BOX1099(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELBX9
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELBX9"
 PARMSK(1) = num$([M]FRM1099)
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELBX9","TEN99LIB","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

$ON_AMTSUBJ1099
Onevent BEFORE_AREA Gosub AV_AMTSUBJ1099 From W2BIS1
Return

$AV_AMTSUBJ1099
## Action INIDEV
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR,GDEVISE(1..GNBDEV)),1))
## Action STD
Call AV_AMTSUBJ1099(zc) From SUBBIS2
If GFMT<>""
 Chgfmt [M]AMTSUBJ1099(indice) With GFMT
Else
 Chgfmt [M]AMTSUBJ1099(indice) With "-K:1X"
Endif
Gosub _AFF
Return

