# Sage X3 supervisor
# Mask management WR1STD0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone PLNFCY
mkstat = 0
zoncou = "PLNFCY"
indice = 0
WCHAR = [M]PLNFCY
If varmode([M]PLNFCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_PLNFCY([M]PLNFCY) From SUBMFG
 GACTION = SAVACT
 If mkstat=1 : Raz [M]PLNFCY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "PLNFCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]PLNFCY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTFCY"
   Call CONTSITE([M]PLNFCY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("FCY","E",[M]PLNFCY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CTLFCYAUZ"
     Local Char    VALEUR(250)
     VALEUR = [M]PLNFCY
     Call S_AFF_CHAR(VALEUR,PARAM,"CTLFCYAUZ","CONTX3","") From GACTION
     [M]PLNFCY = VALEUR
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]PLNFCY or IMPMOD
       SAVACT = GACTION : GACTION = "STD"
       Call AM_PLNFCY([M]PLNFCY) From SUBMFG
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "PLNFCY" : GZNUM = 10067 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone MFGFCY
mkstat = 0
zoncou = "MFGFCY"
indice = 0
WCHAR = [M]MFGFCY
If varinit([M]MFGFCY,indice)=0
 [M]MFGFCY = GFCYDEF(8)
Endif
If varmode([M]MFGFCY,indice)=0
 If !([M]ALLSTA < 2 & [M]SCDFLG <> 2)
  mkstat = 2
 Endif
 If mkstat=1 : Raz [M]MFGFCY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "MFGFCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]MFGFCY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTFCY"
   Call CONTSITE([M]MFGFCY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("FCY","E",[M]MFGFCY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call C_MFGFCY([M]MFGFCY) From SUBMFG
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "CTLFCYAUZ"
      Local Char    VALEUR(250)
      VALEUR = [M]MFGFCY
      Call S_AFF_CHAR(VALEUR,PARAM,"CTLFCYAUZ","CONTX3","") From GACTION
      [M]MFGFCY = VALEUR
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       If WCHAR<>[M]MFGFCY or IMPMOD
        SAVACT = GACTION : GACTION = "STD"
        Call AM_MFGFCY([M]MFGFCY) From SUBMFG
        GACTION = SAVACT
        If mkstat : GERR = 1 : GZONE = "MFGFCY" : GZNUM = 9144 : Endif
        Gosub AFF From GSAISIE
        If mkstat : End : Endif
       Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone MFGNUM
mkstat = 0
zoncou = "MFGNUM"
indice = 0
If varmode([M]MFGNUM,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_MFGNUM([M]MFGNUM) From SUBMFG
 GACTION = SAVACT
 If mkstat=1 : Raz [M]MFGNUM : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "MFGNUM" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_MFGNUM([M]MFGNUM) From SUBMFG
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "MFGNUM" : GZNUM = 9595 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone MFGSTA
mkstat = 0
zoncou = "MFGSTA"
indice = 0
IMPENT = [M]MFGSTA
WNUM = IMPENT
If varinit([M]MFGSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]MFGSTA,indice)=0
 Call INICOMBO(317) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_MFGSTA(IMPENT) From SUBMFG
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,317,1))))
 Endif
 [M]MFGSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "MFGSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]MFGSTA(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "MFGSTA" : GZNUM = 8254 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_MFGSTA(IMPENT) From SUBMFG
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "MFGSTA" : GZNUM = 8254 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WNUM<>IMPENT or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_MFGSTA(IMPENT) From SUBMFG
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "MFGSTA" : GZNUM = 8254 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif
[M]MFGSTA = IMPENT

## Zone MFGMOD
mkstat = 0
zoncou = "MFGMOD"
indice = 0
IMPENT = [M]MFGMOD
WNUM = IMPENT
If varinit([M]MFGMOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]MFGMOD,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_MFGMOD(IMPENT) From SUBMFG
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]MFGMOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "MFGMOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]MFGMOD(indice)
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_MFGMOD(IMPENT) From SUBMFG
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "MFGMOD" : GZNUM = 9146 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WNUM<>IMPENT or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_MFGMOD(IMPENT) From SUBMFG
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "MFGMOD" : GZNUM = 9146 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif
[M]MFGMOD = IMPENT

## Zone FLGMOD
mkstat = 0
zoncou = "FLGMOD"
indice = 0
IMPENT = [M]FLGMOD
If varinit([M]FLGMOD,indice)=0
 IMPENT = 1
Endif
[M]FLGMOD = IMPENT

## Zone XFLGMOD
mkstat = 0
zoncou = "XFLGMOD"
indice = 0
SAVACT = GACTION : GACTION = "STD"
Call AV_XFLGMOD([M]XFLGMOD) From SUBMFG
GACTION = SAVACT
If mkstat=1 : Raz [M]XFLGMOD : Endif

## Zone SCDFLG
mkstat = 0
zoncou = "SCDFLG"
indice = 0
IMPENT = [M]SCDFLG
If varinit([M]SCDFLG,indice)=0
 IMPENT = 1
Endif
[M]SCDFLG = IMPENT

## Zone CFMFLG
mkstat = 0
zoncou = "CFMFLG"
indice = 0
IMPENT = [M]CFMFLG
If varinit([M]CFMFLG,indice)=0
 IMPENT = 1
Endif
[M]CFMFLG = IMPENT

## Zone XMFGMOD
mkstat = 0
zoncou = "XMFGMOD"
indice = 0
IMPENT = [M]XMFGMOD
If varinit([M]XMFGMOD,indice)=0
 IMPENT = 1
Endif
[M]XMFGMOD = IMPENT

## Zone ALLSTA
mkstat = 0
zoncou = "ALLSTA"
indice = 0
IMPENT = [M]ALLSTA
If varinit([M]ALLSTA,indice)=0
 IMPENT = 1
Endif
[M]ALLSTA = IMPENT

## Zone SUSFLG
mkstat = 0
zoncou = "SUSFLG"
indice = 0
IMPENT = [M]SUSFLG
If varinit([M]SUSFLG,indice)=0
 IMPENT = 1
Endif
[M]SUSFLG = IMPENT

## Zone CMPUSBFLG
mkstat = 0
zoncou = "CMPUSBFLG"
indice = 0
IMPENT = [M]CMPUSBFLG
If varinit([M]CMPUSBFLG,indice)=0
 IMPENT = 1
Endif
[M]CMPUSBFLG = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
