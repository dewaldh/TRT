A_HEURE = "00:18:59"
A_USER  = "ADMIN"
A_STAMP = "20180828001859"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBAXE"
  If [M]NBAXE=0
  Endif
Endcase
