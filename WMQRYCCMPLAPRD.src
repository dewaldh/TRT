#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:CCMIPRD])=0 : Local File CCMIMPPRD [CCMIPRD] : Endif
If clalev([F:A_])=0 : Local File CCMIMPPRD [A_] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [CCMIPRD]
LogicClose File [A_]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"CCM","CCMIPRD",ACRITROLE,[V]CST_AFALSE)
Link [CCMIPRD] With
& [A_]CCMPRD0~=[CCMIPRD]CRID;[CCMIPRD]LINENO
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"CCM","CCMIPRD",ACRITROLE,[V]CST_AFALSE)
Link [CCMIPRD] With
& [A_]CCMPRD0~=[CCMIPRD]CRID;[CCMIPRD]LINENO
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
Return
 
#-------------------------------------------------------------------#
# Default filter for the query
#-------------------------------------------------------------------#
$AQUERY_FILTER_DEF
Return
 
#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# CCMIPRD --- CCMIMPPRD(CCMIPRD)
QRY(NBREP) = NewInstance R_CCMPLAPRD AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:CCMIPRD]AUUID)>0 & [F:CCMIPRD]AUUID=NullUuid
 [F:CCMIPRD]AUUID = getUuid
Endif
SetInstance QRY(NBREP).CCMIPRD With [F:CCMIPRD]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# CCMIPRD --- CCMIMPPRD(CCMIPRD)
QRY = NewInstance R_CCMPLAPRD AllocGroup PQRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:CCMIPRD]AUUID)>0 & [F:CCMIPRD]AUUID=NullUuid
 [F:CCMIPRD]AUUID = getUuid
Endif
SetInstance QRY.CCMIPRD With [F:CCMIPRD]
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

#-------------------------------------------------------------------#
# Additionnal criteria of query
#-------------------------------------------------------------------#
$AQUERY_CRITERIA
Gosub DECLARE_VAR From WMC0CCMIMPPRD
CURQRY = "R_CCMPLAPRD"
ACTION = "AQUERY_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# LNKPRO of query 
#-------------------------------------------------------------------#
$AQUERY_LNKPRO
Callmet QRY(NBREP).CCMIPRD.CRID.LNKPRO
Callmet QRY(NBREP).CCMIPRD.PSHFCY.LNKPRO
Callmet QRY(NBREP).CCMIPRD.ACTIONER.LNKPRO
Return

#-------------------------------------------------------------------#
# LNKPRO of query INSTANCE
#-------------------------------------------------------------------#
$AQUERY_LNKPRO_INSTANCE
Callmet QRY.CCMIPRD.CRID.LNKPRO
Callmet QRY.CCMIPRD.PSHFCY.LNKPRO
Callmet QRY.CCMIPRD.ACTIONER.LNKPRO
Return

#-------------------------------------------------------------------#
# LOAD_PROPERTIES of query
#-------------------------------------------------------------------#
$AQUERY_DECODE_CRITERIA
NBPRO = 0
NBPRO += 1 : TBPRO(NBPRO) = "CRID" : TBFIL(NBPRO) = "[F:CCMIPRD]CRID"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "CCMIPRD.CRID"
NBPRO += 1 : TBPRO(NBPRO) = "LINENO" : TBFIL(NBPRO) = "[F:CCMIPRD]LINENO"
           : TBTYP(NBPRO) = 2 : TBPTH(NBPRO) = "CCMIPRD.LINENO"
NBPRO += 1 : TBPRO(NBPRO) = "PSHNUM" : TBFIL(NBPRO) = "[F:CCMIPRD]PSHNUM"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "CCMIPRD.PSHNUM"
NBPRO += 1 : TBPRO(NBPRO) = "PSHFCY" : TBFIL(NBPRO) = "[F:CCMIPRD]PSHFCY"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "CCMIPRD.PSHFCY"
NBPRO += 1 : TBPRO(NBPRO) = "ACTIONID" : TBFIL(NBPRO) = "[F:CCMIPRD]ACTIONID"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "CCMIPRD.ACTIONID"
NBPRO += 1 : TBPRO(NBPRO) = "ACTIONDESC" : TBFIL(NBPRO) = "[F:CCMIPRD]ACTIONDESC"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "CCMIPRD.ACTIONDESC"
NBPRO += 1 : TBPRO(NBPRO) = "COMPLETEDATE" : TBFIL(NBPRO) = "[F:CCMIPRD]COMPLETEDATE"
           : TBTYP(NBPRO) = 8 : TBPTH(NBPRO) = "CCMIPRD.COMPLETEDATE"
NBPRO += 1 : TBPRO(NBPRO) = "ACTIONER" : TBFIL(NBPRO) = "[F:CCMIPRD]ACTIONER"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "CCMIPRD.ACTIONER"
NBPRO += 1 : TBPRO(NBPRO) = "LINESTATUS" : TBFIL(NBPRO) = "[F:CCMIPRD]LINESTATUS"
           : TBTYP(NBPRO) = 1 : TBPTH(NBPRO) = "CCMIPRD.LINESTATUS"
NBPRO += 1 : TBPRO(NBPRO) = "COMMENT" : TBFIL(NBPRO) = "[F:CCMIPRD]COMMENT"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "CCMIPRD.COMMENT"
NBPRO += 1 : TBPRO(NBPRO) = "STARTDATE" : TBFIL(NBPRO) = "[F:CCMIPRD]STARTDATE"
           : TBTYP(NBPRO) = 8 : TBPTH(NBPRO) = "CCMIPRD.STARTDATE"
ACTION = "AQUERY_DECODE_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# Standard key of query
#-------------------------------------------------------------------#
$AQUERY_PRIMARYKEYS
Local Char ASTDKEY(250)
ASTDKEY = ""
ASTDKEY += "[F:CCMIPRD]CRID"
ASTDKEY += ";"+"[F:CCMIPRD]LINENO"
ACTION = "AQUERY_PRIMARYKEYS_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# ACTIONS
#-------------------------------------------------------------------#
$AQUERY_ACTION
Local Char CURPRO(250),CURPTH(250),AMETHOD(50),AEVENT(50)
[L]AEVENT = [L]ACTION
Gosub METHODS From CCMPLAPRD_RSTD
Gosub EVENTS From CCMPLAPRD_RSTD
Return
 


