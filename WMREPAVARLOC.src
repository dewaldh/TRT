$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "AVARLOC"
REP.CLA = "AVARLOC"
REP.ICL = "AVR"
REP.AFCRIGHT = ""
REP.NCL = 28079 : # Formula wizard setup
REP.DES = 28079 : # Formula wizard setup
REP.TIT = ""
REP.ANX = ""
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Type
     REP.CLE(1).PAR = 'TYP' : REP.CLE(1).TYP = 10 : REP.CLE(1).PRO= 'TYP' : REP.CLE(1).VPR= 'AVR.TYP' : REP.CLE(1).DES = 16
REP.CLE(2) = NewInstance C_AREPPAR AllocGroup REP : # Function
