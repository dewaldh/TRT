#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CCC3 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:54:00"
A_USER  = "ADMIN"
A_STAMP = "20180827235400"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBITM"
  If [M]NBITM=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask CCC3 [CCC3]
Gosub HORODAT
Local Inpbox "CCC3" From GFONCTION At A_STAMP With A_WINDPREV Mask [CCC3] As [SIM_]
Grizo [CCC3]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [CCC3]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=25
Return

$ON_ITMNUM
Onevent CONTROL Call C_ITMNUM(zc) From W1CCC3
Onevent AFTER_CHANGE Gosub AMM_ITMNUM From W1CCC3
Onevent BEFORE_INPUT Call AS_ITMNUM(zc) From W1CCC3
Return

Subprog IB_ITMNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action IBPRDINF
 Call IBPRDINF(VALEUR) From PRDINFLIB
Endif
Gosub _AFF
End

Subprog AS_ITMNUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(2061,GBOUT1) From OBJDIV
Call TEXTE(12837,GBOUT13) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_ITMNUM(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_ITMNUM(zc) From W1CCC3
Onkey 170 Titled GBOUT13 Call B13_ITMNUM(zc) From W1CCC3
Onkey   2 Titled GBOUTS Call S_ITMNUM(zc) From W1CCC3
Onkey 144 Titled GBOUTP Call P_ITMNUM(zc) From W1CCC3
Onkey 146 Titled GBOUTA Call SA_ITMNUM(zc) From W1CCC3
Gosub _AFF
End

Subprog C_ITMNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ITMCHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ITMCHOICE"
 Call S_TAB_CHAR(VALEUR,PARMSK,"ITMCHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ITMNUM" : GZNUM = 14912 : Endif
Endif
If !mkstat
 ## Action VXADCUPC
 Call ADCCTRUPC(VALEUR) From VXADCUPC
 If mkstat : GERR = 1 : GZONE = "ITMNUM" : GZNUM = 14912 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ITM","!",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ITMNUM" : GZNUM = 14912 : Endif
  If !mkstat
   ## Action STD
   Call C_ITMNUM(VALEUR) From SUBCCC
   If mkstat : GERR = 1 : GZONE = "ITMNUM" : GZNUM = 14912 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

$AMM_ITMNUM
Call AM_ITMNUM(zc)
If !mkstat & GOBJET
 zoncou = "ITMNUM" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_ITMNUM(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_ITMNUM(VALEUR) From SUBCCC
If mkstat : GERR = 1 : GZONE = "ITMNUM" : GZNUM = 14912 : Endif
Gosub _AFF
End

Subprog S_ITMNUM(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "ITM"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_ITMNUM(VALEUR)
Variable Char    VALEUR()
Call OBJET("ITM",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog B13_ITMNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action PRDINFA
 Call PRDINF(VALEUR) From PRDINFLIB
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog SA_ITMNUM(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ITM"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_ITMNUM(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ITM",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$CHG_STY
Return


