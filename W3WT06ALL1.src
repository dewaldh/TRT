# Sage X3 supervisor
# Mask management WT06ALL1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_ZERSTOFLG
Onevent BEFORE_AREA Gosub AV_ZERSTOFLG From W3WT06ALL1
Onevent BEFORE_INPUT Call AS_ZERSTOFLG(zc) From W3WT06ALL1
Onevent CONTROL Call C_ZERSTOFLG(zc) From W3WT06ALL1
Onevent AFTER_CHANGE Gosub AMM_ZERSTOFLG From W3WT06ALL1
Onevent INIT_AREA Call D_ZERSTOFLG(zc) From W3WT06ALL1
Return

$AV_ZERSTOFLG
## Action STD
Call AV_ZERSTOFLG(zc) From SUBSNL
Gosub _AFF
Return

Subprog D_ZERSTOFLG(VALEUR)
Variable Integer VALEUR
If varinit([M]ZERSTOFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_ZERSTOFLG(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action PASSAISNL0
Call PASSAISNL0("ZERSTOFLG",nolign-1) From STKINV
If mkstat=2
 [M]ZERSTOFLG(INDIC) = VALEUR
 Affzo ZERSTOFLG(INDIC) : mkstat = 2
Endif
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog C_ZERSTOFLG(VALEUR)
Variable Integer VALEUR
## Action STD
Call C_ZERSTOFLG(VALEUR) From SUBSNL
If mkstat : GERR = 1 : GZONE = "ZERSTOFLG" : GZNUM = 9708 : Endif
Gosub _AFF
End

$AMM_ZERSTOFLG
Call AM_ZERSTOFLG(zc)
If !mkstat & GOBJET
 zoncou = "ZERSTOFLG" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_ZERSTOFLG(VALEUR)
Variable Integer VALEUR
## Action STD
Call AM_ZERSTOFLG(VALEUR) From SUBSNL
If mkstat : GERR = 1 : GZONE = "ZERSTOFLG" : GZNUM = 9708 : Endif
Gosub _AFF
End

