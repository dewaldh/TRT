# Supervisor
# Mask management CCN4 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CCNLNA
mkstat = 0
zoncou = "CCNLNA"
indice = 0
If varmode([M]CCNLNA,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CCNLNA" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone CCNFNA
mkstat = 0
zoncou = "CCNFNA"
indice = 0
If varmode([M]CCNFNA,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CCNFNA" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBREC
mkstat = 0
zoncou = "NBREC"
indice = 0
status = 1
zoncou = "NBREC"
If varmode([M]NBREC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBREC" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBREC
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBREC = nolign
   status = 75
   Call SAI_NBREC(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBREC(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBREC
mkstat = 0
zoncou = "NBREC"

## Zone ZIP
mkstat = 0
zoncou = "ZIP"
WCHAR = [M]ZIP(indice)
SAVACT = GACTION : GACTION = "FMTPOSPAY"
Call FMTPOSPAY([M:CCN4]CRY(nolign - 1)) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]ZIP(indice) : Endif

## Zone CTY
mkstat = 0
zoncou = "CTY"
WCHAR = [M]CTY(indice)
SAVACT = GACTION : GACTION = "FMTCTYPAY"
Call FMTCTYPAY([M:CCN4]CRY(nolign - 1)) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]CTY(indice) : Endif

## Zone TEL
mkstat = 0
zoncou = "TEL"
WCHAR = [M]TEL(indice)
SAVACT = GACTION : GACTION = "FMTTELPAY"
Call FMTTELPAY([M:CCN4]CRY(nolign - 1)) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]TEL(indice) : Endif

## Zone CRY
mkstat = 0
zoncou = "CRY"
If varinit([M]CRY,indice)=0
 [M]CRY(indice) = GPAYS
Endif

## Zone NBREC
mkstat = 0
zoncou = "NBREC"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CCN4"
Return

