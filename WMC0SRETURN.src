####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "C_ANA"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_ANA",this)
 When "C_BPCNAM"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_BPCNAM",this)
 When "C_BPDADDLIG"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_BPDADDLIG",this)
 When "SRD"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"SRD",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# ADDLINE
####################################################################
$ADDLINE
Case _ACOLLECTION
 When "C_ANA"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_ANA","C_SRETURN_C_ANA",this)
 When "C_BPCNAM"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_BPCNAM","C_SRETURN_C_BPCNAM",this)
 When "C_BPDADDLIG"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_BPDADDLIG","C_SRETURN_C_BPDADDLIG",this)
 When "SRD"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"SRD","C_SRETURND",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

