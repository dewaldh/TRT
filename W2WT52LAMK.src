$ON_LIKQTY1
Onevent BEFORE_AREA Gosub AV_LIKQTY1 From W2WT52LAMK
Onevent BEFORE_INPUT Call AS_LIKQTY1(zc) From W2WT52LAMK
Onevent CONTROL Call C_LIKQTY1(zc) From W2WT52LAMK
Onevent AFTER_CHANGE Gosub AMM_LIKQTY1 From W2WT52LAMK
Return

$AV_LIKQTY1
If !GWEBSERV
 ## Action FMTQTY
 GUNTFMT="15F" : GUNTFMT = GUNTFMT1(max(find([M]STU,GUNITE(1..GNBUNT)),1))
Endif
Chgfmt [M]LIKQTY1(indice) With "N+z:"+GUNTFMT
Gosub _AFF
Return

Subprog AS_LIKQTY1(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action STD
Call AS_LIKQTY1(VALEUR) From FUNMLINK
If mkstat=2
 [M]LIKQTY1(INDIC) = VALEUR
 Affzo LIKQTY1(INDIC) : mkstat = 2
Endif
Gosub _AFF
End

Subprog C_LIKQTY1(VALEUR)
Variable Decimal VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "LIKQTY1" : GZNUM = 27287 : Endif
If !mkstat
 ## Action STD
 Call C_LIKQTY1(VALEUR) From FUNMLINK
 If mkstat : GERR = 1 : GZONE = "LIKQTY1" : GZNUM = 27287 : Endif
Endif
Gosub _AFF
End

$AMM_LIKQTY1
Call AM_LIKQTY1(zc)
If !mkstat & GOBJET
 zoncou = "LIKQTY1" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_LIKQTY1(VALEUR)
Variable Decimal VALEUR
## Action STD
Call AM_LIKQTY1(VALEUR) From FUNMLINK
If mkstat : GERR = 1 : GZONE = "LIKQTY1" : GZNUM = 27287 : Endif
Gosub _AFF
End

