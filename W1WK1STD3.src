$ON_SFISSTCOD
Onevent INIT_AREA Call D_SFISSTCOD(zc) From W1WK1STD3
Onevent CONTROL Call C_SFISSTCOD(zc) From W1WK1STD3
Onevent BEFORE_INPUT Call AS_SFISSTCOD(zc) From W1WK1STD3
Return

Subprog D_SFISSTCOD(VALEUR)
Variable Char    VALEUR()
If varinit([M]SFISSTCOD,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"203","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_SFISSTCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"203") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_SFISSTCOD(VALEUR) From SUBSQH
If mkstat=2
 [M]SFISSTCOD(INDIC) = VALEUR
 Affzo SFISSTCOD(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_SFISSTCOD(zc) From W1WK1STD3
Onkey   2 Titled GBOUTS Call S_SFISSTCOD(zc) From W1WK1STD3
Onkey 144 Titled GBOUTP Call P_SFISSTCOD(zc) From W1WK1STD3
Onkey 146 Titled GBOUTA Call SA_SFISSTCOD(zc) From W1WK1STD3
Gosub _AFF
End

Subprog C_SFISSTCOD(VALEUR)
Variable Char    VALEUR()
## Action CONTADI
Call CONTADI(VALEUR,"203","") From SUBADI
If mkstat : GERR = 1 : GZONE = "SFISSTCOD" : GZNUM = 48666 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("ADI","!",VALEUR,"203",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "SFISSTCOD" : GZNUM = 48666 : Endif
Endif
Gosub _AFF
End

Subprog S_SFISSTCOD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = "203"
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_SFISSTCOD(VALEUR)
Variable Char    VALEUR()
Call OBJET("ADI",VALEUR,"203") From GOBJET
Gosub _AFF
End

Subprog SA_SFISSTCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(2) = "203"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_SFISSTCOD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "203"
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

