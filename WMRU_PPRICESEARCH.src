############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PPRICESEARCH,ASTATUS)
Variable Instance PPRICESEARCH Using R_PPRICESEARCH
Variable Integer ASTATUS

If PPRICESEARCH = null : End : Endif

Gosub AVARIABLES From WMR00PPRICESEARCH
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PPRICESEARCH.ACTX.AGETAFCRIGHT(PPRICESEARCH,"SYRACHPPSI",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PPRICESEARCH.ACTX.AGETAFCRIGHTU(PPRICESEARCH,"SYRACHPPSI",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PPRICESEARCH
Else
 Gosub AUPDATE_PPRICESEARCH
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PPRICESEARCH.freeSnapshot
PPRICESEARCH.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PPRICESEARCH.ASTALIN = [V]CST_AUPD
PPRICESEARCH.PPRISEA.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PPRICESEARCH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PPRICESEARCH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PPRICESEARCH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PPRICESEARCH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PPRICESEARCH._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PPRICESEARCH._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PPRICESEARCH._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PPRICESEARCH.PPRISEA.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PPRICESEARCH.PPRISEA.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PPRICESEARCH.PPRISEA.ASTALIN)<>""
 [L]A_ASTALIN = PPRICESEARCH.PPRISEA.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PPRICESEARCH.PPRISEA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

If [V]CST_ASTALIN(PPRICESEARCH.PPRISEA.ASTALIN)<>""
 [L]A_ASTALIN = PPRICESEARCH.PPRISEA.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PPRICESEARCH.PPRISEA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PPRICESEARCH
Gosub AUPDATE_PPRICESEARCH
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PPRICESEARCH.PPRISEA.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PPRICESEARCH.PPRISEA.ASTALIN = [V]CST_ALL
Endif
Return

#-----------------------------------------------------#
$AUPDATE_PPRICESEARCH
[L]A_ASTALIN = PPRICESEARCH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PPRICESEARCH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PPRICESEARCH.PPRISEA._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PPRICESEARCH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PPRICESEARCH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
$_AUPDATE_ABORT
If PPRICESEARCH.snapshotEnabled=1
 [L]A_RETIK = PPRICESEARCH.revertUpdTick
Endif

[L]ASTATUS = max(fmet PPRICESEARCH.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



