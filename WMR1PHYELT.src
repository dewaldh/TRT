####################################################################
# Labels of the main class and methods 
####################################################################
 
####################################################################
# 1 : MAP Map ( Field EHTML 1)
####################################################################
 
$GET_MAP
Gosub DECLARE_VAR From WMR0PHYELT
[L]CURPRO = "MAP"
Local Char _CNT(100)
Local Tinyint _EXI : [L]_EXI=1
[L]ASTATUS = fmet this.AREAD_CLB("MAP",
& "GOOGLE MAPS"
& ,[L]_CNT,this.MAP)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isLoaded")<>"true"
 [L]ASTATUS = fmet this.ASETATTRIBUTE([L]CURPRO,"$isLoaded","true")
Endif
[L]ARULE = "GET" : [L]ACTION = "GET" : Gosub ACTION From ASYRSUB
Return

$SET_MAP
If fmet this.AGETATTRIBUTE("MAP","$isLoaded")<>"true" or [L]_AOLDVAL<>this.MAP
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_MAP
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If fmet this.AGETATTRIBUTE("MAP","$isLoaded")<>"true" or [L]_AOLDVAL<>this.MAP
 Gosub PROPAGATE_MAP
Endif
Return

$INIT_MAP
Gosub DECLARE_VAR From WMR0PHYELT
[L]CURPRO = "MAP"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_MAP
Gosub DECLARE_VAR From WMR0PHYELT
[L]CURPRO = "MAP"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_MAP                   : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_MAP                   : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_MAP
[L]CURPRO = "MAP"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & len(this.MAP)=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_MAP
[L]CURPRO = "MAP"
Return

$_AOTHERS_MAP
[L]CURPRO = "MAP"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_MAP
[L]CURPRO = "MAP"
Return

$PROPAGATE_MAP
this.isReadonly = 0
[L]CURPRO = "MAP"
[L]ASTATUS = fmet this.ASETATTRIBUTE([L]CURPRO,"$isLoaded","true")
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_MAP
Gosub DECLARE_VAR From WMR0PHYELT
[L]CURPRO = "MAP"
$_LNKPRO_MAP
Local Tinyint _EXI : [L]_EXI=1
[L]ASTATUS = func EHTML.GET_CLOBEXIST(this,"MAP",[L]_EXI,"GOOGLE MAPS",this.PHY.GEOLOC)
[L]ASTATUS = fmet this.ASETATTRIBUTE("MAP","$isLobFound",func ASYRSUB.BOOLEAN_TO_SYR([L]_EXI+1))
Return




