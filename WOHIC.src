$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(20)(0..GNBDERNIER)
Local Char     A_BUF2(5)(0..GNBDERNIER)
Local Libelle  A_BUF3(0..GNBDERNIER)
Local Integer  A_BUF4(0..GNBDERNIER)
If clalev([F:ITC9])>0 : Close Local File [ITC9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(20),
& Char     A_TAB2(5),
& Libelle  A_TAB3,
& Integer  A_TAB4
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4
& As [ITC9]
Return

$LEC_DERLU
Read [AFF]AFF0 = [F:ITC]STOFCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Return

$CHARGE_DERLU
If clalev([F:ITC9])>0
 Read [ITC9] = [F:ITC9]A_TABI
 [L]CLE = [F:ITC9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:ITC9])>0
 Filter [ITC9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [HIC1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:ITC9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:ITC]ITMREF+"~"
 A_CLEDERLU += [F:ITC]STOFCY+"~"
 A_CLEDERLU += num$([F:ITC]CSTTYP)+"~"
 A_CLEDERLU += num$([F:ITC]ITCSEQ)+"~"
 A_CLEDERLU += num$([F:ITC]UID)+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:ITC9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:ITC9]
 Endif
Endif
Return

