# Supervisor
# Mask management STOCKPUL 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone PURLOTDAT
mkstat = 0
zoncou = "PURLOTDAT"
indice = 0
If varmode([M]PURLOTDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PURLOTDAT" : Gosub ACTION From =IMPTRT
 Endif
 If [M]PURLOTDAT=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "PURLOTDAT" : GZNUM = 54774 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_PURLOTDAT([M]PURLOTDAT) From FUNSTPL
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "PURLOTDAT" : GZNUM = 54774 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone SIM
mkstat = 0
zoncou = "SIM"
indice = 0
IMPENT = [M]SIM
If varinit([M]SIM,indice)=0
 IMPENT = 1
Endif
If varmode([M]SIM,indice)=0
 [M]SIM(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "SIM" : Gosub ACTION From =IMPTRT
  IMPENT = [M]SIM(indice)
 Endif
Endif
[M]SIM = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "STOCKPUL"
Return

