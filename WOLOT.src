$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(20)(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
Local Libelle  A_BUF3(0..GNBDERNIER)
If clalev([F:LOT9])>0 : Close Local File [LOT9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(20),
& Char     A_TAB2(30),
& Libelle  A_TAB3
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3
& As [LOT9]
Return

$LEC_DERLU
Read [AFF]AFF0 = [F:LOT]FCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Return

$CHARGE_DERLU
If clalev([F:LOT9])>0
 Read [LOT9] = [F:LOT9]A_TABI
 [L]CLE = [F:LOT9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:LOT9])>0
 Filter [LOT9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [HAE1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:LOT9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:LOT]LOT+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:LOT9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:LOT9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 0
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:LOT]LOT"
Call TEXTE(53559,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:20X"
SELFOR(NBSEL) = "K:20X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:LOT]LOT"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:LOT]DESLOT"
Call TEXTE(1470,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = "K:30X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:LOT]DESLOT"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:LOT]UPDFLG"
Call TEXTE(399,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 1
SELFMT(NBSEL) = "LA00001:4X"
SELFOR(NBSEL) = ""
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:LOT]UPDFLG"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:LOT_])=0
 Gosub OUVFIC_LISTE
Endif
Return

