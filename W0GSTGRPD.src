# Supervisor
# Mask management GSTGRPD 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone CPY
mkstat = 0
zoncou = "CPY"
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CPY(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 57079 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTCPY"
  Call CONTSOC([M]CPY(indice)) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 57079 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("CPY","A",[M]CPY(indice),"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 57079 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call AP_CPY([M]CPY(indice)) From SUBGSTGRP
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 57079 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone GSTHEA
mkstat = 0
zoncou = "GSTHEA"
IMPENT = [M]GSTHEA(indice)
If varinit([M]GSTHEA,indice)=0
 IMPENT = 1
Endif
If varmode([M]GSTHEA,indice)=0
 [M]GSTHEA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "GSTHEA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]GSTHEA(indice)
 Endif
Endif
[M]GSTHEA(indice) = IMPENT

## Zone DATFOR
mkstat = 0
zoncou = "DATFOR"
If varmode([M]DATFOR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATFOR" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone DATCES
mkstat = 0
zoncou = "DATCES"
If varmode([M]DATCES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATCES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone UPDFLG
mkstat = 0
zoncou = "UPDFLG"
IMPENT = [M]UPDFLG(indice)
If varinit([M]UPDFLG,indice)=0
 IMPENT = 1
Endif
[M]UPDFLG(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
status = 71
SAVACT = GACTION : GACTION = "DIVLINNUM"
Call LINNUM("LIN") From TABLEAUX
GACTION = SAVACT

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GSTGRPD"
Return

