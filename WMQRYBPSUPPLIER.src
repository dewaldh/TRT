#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:BPS])=0 : Local File BPSUPPLIER [BPS] : Endif
If clalev([F:A_])=0 : Local File BPSUPPLIER [A_] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [BPS]
LogicClose File [A_]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"BPS","BPS",ACRITROLE,[V]CST_AFALSE)
Link [BPS] With
& [A_]BPS0~=[BPS]BPSNUM
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"BPS","BPS",ACRITROLE,[V]CST_AFALSE)
Link [BPS] With
& [A_]BPS0~=[BPS]BPSNUM
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
Return
 
#-------------------------------------------------------------------#
# Default filter for the query
#-------------------------------------------------------------------#
$AQUERY_FILTER_DEF
Return
 
#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# BPS --- BPSUPPLIER(BPS)
QRY(NBREP) = NewInstance R_BPSUPPLIER AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:BPS]AUUID)>0 & [F:BPS]AUUID=NullUuid
 [F:BPS]AUUID = getUuid
Endif
SetInstance QRY(NBREP).BPS With [F:BPS]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# BPS --- BPSUPPLIER(BPS)
QRY = NewInstance R_BPSUPPLIER AllocGroup PQRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:BPS]AUUID)>0 & [F:BPS]AUUID=NullUuid
 [F:BPS]AUUID = getUuid
Endif
SetInstance QRY.BPS With [F:BPS]
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

#-------------------------------------------------------------------#
# Additionnal criteria of query
#-------------------------------------------------------------------#
$AQUERY_CRITERIA
Gosub DECLARE_VAR From WMC0BPSUPPLIER
CURQRY = "R_BPSUPPLIER"
ACTION = "AQUERY_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# LNKPRO of query 
#-------------------------------------------------------------------#
$AQUERY_LNKPRO
Callmet QRY(NBREP).BPS.BSGCOD.LNKPRO
Callmet QRY(NBREP).BPS.BPSNUM.LNKPRO
Return

#-------------------------------------------------------------------#
# LNKPRO of query INSTANCE
#-------------------------------------------------------------------#
$AQUERY_LNKPRO_INSTANCE
Callmet QRY.BPS.BSGCOD.LNKPRO
Callmet QRY.BPS.BPSNUM.LNKPRO
Return

#-------------------------------------------------------------------#
# LOAD_PROPERTIES of query
#-------------------------------------------------------------------#
$AQUERY_DECODE_CRITERIA
NBPRO = 0
NBPRO += 1 : TBPRO(NBPRO) = "BSGCOD" : TBFIL(NBPRO) = "[F:BPS]BSGCOD"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "BPS.BSGCOD"
NBPRO += 1 : TBPRO(NBPRO) = "BPSNUM" : TBFIL(NBPRO) = "[F:BPS]BPSNUM"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "BPS.BPSNUM"
NBPRO += 1 : TBPRO(NBPRO) = "BPSNAM" : TBFIL(NBPRO) = "[F:BPS]BPSNAM"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "BPS.BPSNAM"
ACTION = "AQUERY_DECODE_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# Standard key of query
#-------------------------------------------------------------------#
$AQUERY_PRIMARYKEYS
Local Char ASTDKEY(250)
ASTDKEY = ""
ASTDKEY += "[F:BPS]BPSNUM"
ACTION = "AQUERY_PRIMARYKEYS_AFTER" : Gosub AQUERY_ACTION
Return

#-------------------------------------------------------------------#
# ACTIONS
#-------------------------------------------------------------------#
$AQUERY_ACTION
Local Char CURPRO(250),CURPTH(250),AMETHOD(50),AEVENT(50)
[L]AEVENT = [L]ACTION
Gosub METHODS From BPSUPPLIER_RSTD
Gosub EVENTS From BPSUPPLIER_RSTD
Return
 


