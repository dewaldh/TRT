# Supervisor
# Mask management WT53APP1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_PLANNER2
Return

$ON_BUY
Return

$ON_NETPRI
Onevent BEFORE_AREA Gosub AV_NETPRI From W3WT53APP1
Return

$AV_NETPRI
## Action PARDEV
Gosub _AFF
Return

$ON_CUR
Return

$ON_REQDAT
Onevent CONTROL Call C_REQDAT(zc) From W3WT53APP1
Onevent AFTER_CHANGE Gosub AMM_REQDAT From W3WT53APP1
Onevent INIT_AREA Call D_REQDAT(zc) From W3WT53APP1
Onevent BEFORE_AREA Gosub AV_REQDAT From W3WT53APP1
Return

$AV_REQDAT
Chgfmt [M]REQDAT(indice) With "D:"+GFMDAT
Gosub _AFF
Return

Subprog D_REQDAT(VALEUR)
Variable Date    VALEUR
If varinit([M]REQDAT,indice)=0
 VALEUR = date$
Endif
Gosub _AFF
End

Subprog C_REQDAT(VALEUR)
Variable Date    VALEUR
## Action %OBLIG
If VALEUR=[0/0/0]
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "REQDAT" : GZNUM = 2240 : Endif
If !mkstat
 ## Action CTLJOU
 Call CTL_JOU(VALEUR,[F:FCY]UVYDAY,[F:FCY]UVYCOD,1) From CONTX3
 If mkstat : GERR = 1 : GZONE = "REQDAT" : GZNUM = 2240 : Endif
 If !mkstat
  ## Action STD
  Call C_REQDAT(VALEUR) From FUNMPICK
  If mkstat : GERR = 1 : GZONE = "REQDAT" : GZNUM = 2240 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_REQDAT
Call AM_REQDAT(zc)
If !mkstat & GOBJET
 zoncou = "REQDAT" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_REQDAT(VALEUR)
Variable Date    VALEUR
## Action STD
Call AM_REQDAT(VALEUR) From FUNMPICK
If mkstat : GERR = 1 : GZONE = "REQDAT" : GZNUM = 2240 : Endif
Gosub _AFF
End

$ON_PUU
Return

$ON_PUUPECQTY
Onevent BEFORE_AREA Gosub AV_PUUPECQTY From W3WT53APP1
Onevent AFTER_CHANGE Gosub AMM_PUUPECQTY From W3WT53APP1
Onevent BEFORE_INPUT Call AS_PUUPECQTY(zc) From W3WT53APP1
Return

$AV_PUUPECQTY
If !GWEBSERV
 ## Action FMTQTY
 GUNTFMT="15F" : GUNTFMT = GUNTFMT1(max(find([M:MGP]PUU(nolign-1),GUNITE(1..GNBUNT)),1))
Endif
## Action STD
Call AV_PUUPECQTY(zc) From FUNMPICK
Chgfmt [M]PUUPECQTY(indice) With "Nzc:"+GUNTFMT
Gosub _AFF
Return

Subprog AS_PUUPECQTY(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(12817,GBOUT1) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_PUUPECQTY(zc) From W3WT53APP1
Gosub _AFF
End

$AMM_PUUPECQTY
Call AM_PUUPECQTY(zc)
If !mkstat & GOBJET
 zoncou = "PUUPECQTY" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_PUUPECQTY(VALEUR)
Variable Decimal VALEUR
## Action STD
Call AM_PUUPECQTY(VALEUR) From FUNMPICK
If mkstat : GERR = 1 : GZONE = "PUUPECQTY" : GZNUM = 12857 : Endif
Gosub _AFF
End

Subprog B1_PUUPECQTY(VALEUR)
Variable Decimal VALEUR
If !GWEBSERV
 ## Action CONSSFC
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONSSFC"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_PUUPECQTY" : PROGSUB = "FUNMPICK" : PROGSPE = "SPEMPICK" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "1"
 PARMSK(2) = [M]STOFCY(nolign-1)
 PARMSK(3) = [M]ITMREF(nolign-1)
 PARMSK(4) = ""
 Call CONSULT_NUM(VALEUR,"SFC",PARMSK) From GCONSULT
 ACTION = "BB1_PUUPECQTY" : PROGSUB = "FUNMPICK" : PROGSPE = "SPEMPICK" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_ORI
Onevent INIT_AREA Call D_ORI(zc) From W3WT53APP1
Return

Subprog D_ORI(VALEUR)
Variable Integer VALEUR
If varinit([M]ORI,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_UOM
Return

$CHG_STY
Return


