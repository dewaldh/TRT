# Supervisor
# Mask management YBUSBAT3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone YLIG
mkstat = 0
zoncou = "YLIG"
indice = 0
status = 1
zoncou = "YLIG"
If varmode([M]YLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]YLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]YLIG = nolign
   status = 75
   Call SAI_YLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_YLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone YLIG
mkstat = 0
zoncou = "YLIG"

## Zone YBATNBR
mkstat = 0
zoncou = "YBATNBR"
If varmode([M]YBATNBR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBATNBR" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YSRC
mkstat = 0
zoncou = "YSRC"
If varmode([M]YSRC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSRC" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YDETLIG
mkstat = 0
zoncou = "YDETLIG"
If varmode([M]YDETLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YDETLIG" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YFCY
mkstat = 0
zoncou = "YFCY"
If varmode([M]YFCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YFCY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTFCY"
 Call CONTSITE([M]YFCY(indice)) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "YFCY" : GZNUM = 1344 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("FCY","!",[M]YFCY(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YFCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone YENTTYP
mkstat = 0
zoncou = "YENTTYP"
If varmode([M]YENTTYP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YENTTYP" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "CONTGTE"
 Call CONTGTE([M]YENTTYP(indice),"","") From SUBGTE
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "YENTTYP" : GZNUM = 4969 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJN"
  Local Char    VALEUR(250)
  VALEUR = [M]YENTTYP(indice)
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJN","CONTOBJ","") From GACTION
  [M]YENTTYP(indice) = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "YENTTYP" : GZNUM = 4969 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone YJOU
mkstat = 0
zoncou = "YJOU"
If varmode([M]YJOU,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YJOU" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "CONTJOU"
 Call CONTJOU([M]YJOU(indice),"","") From SUBJOU
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "YJOU" : GZNUM = 4314 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJN"
  Local Char    VALEUR(250)
  VALEUR = [M]YJOU(indice)
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJN","CONTOBJ","") From GACTION
  [M]YJOU(indice) = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "YJOU" : GZNUM = 4314 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone YDETDES
mkstat = 0
zoncou = "YDETDES"
If varmode([M]YDETDES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YDETDES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YACC
mkstat = 0
zoncou = "YACC"
SAVACT = GACTION : GACTION = "INIACCFMT"
Call INIACCFMT("") From SETGLBCOA
GACTION = SAVACT
If mkstat=1 : Raz [M]YACC(indice) : Endif
If !mkstat
 If varmode([M]YACC,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "YACC" : Gosub ACTION From =IMPTRT
  Endif
  Call CONTROLE("GAC","!",[M]YACC(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YACC" : GZNUM = 3956 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone YLIGDES
mkstat = 0
zoncou = "YLIGDES"
If varmode([M]YLIGDES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLIGDES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YMESS
mkstat = 0
zoncou = "YMESS"
If varmode([M]YMESS,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YMESS" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YLIG
mkstat = 0
zoncou = "YLIG"

End

