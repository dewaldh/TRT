# Supervisor
# Mask management YBUSBAT3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone YLIG
mkstat = 0
zoncou = "YLIG"
indice = 0
status = 1
zoncou = "YLIG"
If varmode([M]YLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]YLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]YLIG = nolign
   status = 75
   Call SAI_YLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_YLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone YLIG
mkstat = 0
zoncou = "YLIG"

## Zone YTRANSNBR
mkstat = 0
zoncou = "YTRANSNBR"
If varmode([M]YTRANSNBR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YTRANSNBR" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YACC
mkstat = 0
zoncou = "YACC"
SAVACT = GACTION : GACTION = "INIACCFMT"
Call INIACCFMT("") From SETGLBCOA
GACTION = SAVACT
If mkstat=1 : Raz [M]YACC(indice) : Endif

## Zone YLIG
mkstat = 0
zoncou = "YLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YBUSBAT3"
Return

