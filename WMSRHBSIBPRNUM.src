Funprog IS_IMPLEMENTED()
End 1
#---------------------------------------------------------------------------------------------------#
# List of tables to open
#---------------------------------------------------------------------------------------------------#
$ASEARCH_OPEN
If filinfo(filpath("FIL","BSIBPRNUM","fde",-1),1) <0  : ERROR=[V]CST_AERROR : Return : Endif 
Local File BSIBPRNUM [BSIBPN]
If filinfo(filpath("FIL","BSIBPRNUMD","fde",-1),1) <0  : ERROR=[V]CST_AERROR : Return : Endif 
Local File BSIBPRNUMD [BSIBPND]
Local File BSIBPRNUM [BSIBPN_]
Local Char KEY_BSIBPN(120)(1..3)
Local Char KEY_BSIBPND(120)(1..3)
#Empty keys :
KEY_BSIBPN(3) = "++"
KEY_BSIBPND(3) = "+++0"
ACTION = "ASEARCH_OPEN_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# List of tables to close
#---------------------------------------------------------------------------------------------------#
$ASEARCH_CLOSE
LogicClose File [BSIBPN]
LogicClose File [BSIBPND]
LogicClose File [BSIBPN_]
ACTION = "ASEARCH_CLOSE_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# List of tables to join
#---------------------------------------------------------------------------------------------------#
$ASEARCH_JOIN
Link [BSIBPN] With
& [BSIBPND]BSIBPN0=[F:BSIBPN]CPY;[F:BSIBPN]FCY;[F:BSIBPN]FILFMT
& As [LNK_]
& Where (1=1
& )
ACTION = "ASEARCH_JOIN_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# List of tables to join
#---------------------------------------------------------------------------------------------------#
$ASEARCH_JOIN_INC
Link [BSIBPN] With
& [BSIBPND]BSIBPN0=[F:BSIBPN]CPY;[F:BSIBPN]FCY;[F:BSIBPN]FILFMT
& As [LNK_]
& Where (UPDDATTIM > gdatetime$(QUPDDATTIM)
& )
ACTION = "ASEARCH_JOIN_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# Create an instance of
#---------------------------------------------------------------------------------------------------#
$ASEARCH_CREATEINSTANCE
Local Integer ASTATUS
Local Integer FINS
Return

#---------------------------------------------------------------------------------------------------#
# Set an instance
#---------------------------------------------------------------------------------------------------#
$ASEARCH_SETINSTANCE
KEY_BSIBPN(2) = [F:BSIBPN]CPY+"+"+[F:BSIBPN]FCY+"+"+[F:BSIBPN]FILFMT
KEY_BSIBPND(2) = [F:BSIBPND]CPY+"+"+[F:BSIBPND]FCY+"+"+[F:BSIBPND]FILFMT+"+"+num$([F:BSIBPND]LIN)

If (KEY_BSIBPN(2) <> KEY_BSIBPN(3)) & (KEY_BSIBPN(1) <> KEY_BSIBPN(2))
 If INS <> null
  [L]ASTATUS = fmet INS.ACTION("ASEARCH_TRANS_AFTER")
  #serialize INS
  Gosub ASEARCH_TO_JSON
 Endif
#Create a new instance
INS = NewInstance C_BSIBPRNUM(GACTX,UIDINS,NUMLIN,null,"") AllocGroup null
INS.setaccessorEnabled = 0
SetInstance INS With [F:BSIBPN]
FINS = 1
 KEY_BSIBPN(1) = KEY_BSIBPN(2)
Endif : # KEY_BSIBPN(1) <> KEY_BSIBPN(2)

Return

#---------------------------------------------------------------------------------------------------#
# Serialize the entity in a JSON format
#---------------------------------------------------------------------------------------------------#
$ASEARCH_TO_JSON
If FINS=0 : Return : Endif
QCOUNT += 1
Onerrgo ERROR_VALUE
Append ASND.DATA ,SEPINS1
SEPINS1 = ','+chr$(10)
Append ASND.DATA ,'{'
Append ASND.DATA ,'"CPY":'             +'"'+escJson(INS.CPY)+'"'
Append ASND.DATA ,',"FCY":'            +'"'+escJson(INS.FCY)+'"'
Append ASND.DATA ,',"FILFMT":'         +'"'+escJson(INS.FILFMT)+'"'
Append ASND.DATA ,',"CREUSR":'         +'"'+escJson(INS.CREUSR)+'"'
Append ASND.DATA ,',"UPDUSR":'         +'"'+escJson(INS.UPDUSR)+'"'
Append ASND.DATA ,'}'
Onerrgo
Freegroup INS
INS = null
Raz KEY_BSIBPN(1),KEY_BSIBPND(1)
FINS = 0
Return

#---------------------------------------------------------------------------------------------------#
# Errors
#---------------------------------------------------------------------------------------------------#
$ERROR_VALUE
SVALUE ='"<error'-errmes$(errn)+'>"'
Resume
#---------------------------------------------------------------------------------------------------#
# Link standard key
#---------------------------------------------------------------------------------------------------#
$AQUERY_PRIMARYKEYS
Local Char ASTDKEY(250)
ASTDKEY = ""
ASTDKEY += "[F:BSIBPN]CPY"
ASTDKEY += ";[F:BSIBPN]FCY"
ASTDKEY += ";[F:BSIBPN]FILFMT"
ACTION = "ASEARCH_PRIMARYKEYS_AFTER" : Gosub ASEARCH_ACTION
Return

#---------------------------------------------------------------------------------------------------#
# Actions
#---------------------------------------------------------------------------------------------------#
$ASEARCH_ACTION
Local Char CURPRO(250),CURPTH(250),AMETHOD(50),AEVENT(50)
[L]AEVENT = [L]ACTION
Gosub METHODS From BSIBPRNUM_CSTD
Return

