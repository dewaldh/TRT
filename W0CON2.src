# Supervisor
# Mask management CON2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone NEXINVAMT
mkstat = 0
zoncou = "NEXINVAMT"
indice = 0
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CONCUR4,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]NEXINVAMT : Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBINV
mkstat = 0
zoncou = "NBINV"
indice = 0
status = 1
zoncou = "NBINV"
If varmode([M]NBINV,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBINV From SUBCON
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBINV : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBINV" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBINV
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBINV = nolign
   status = 75
   Call SAI_NBINV(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_NBINV From SUBCON
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NBINV" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBDUD
mkstat = 0
zoncou = "NBDUD"
indice = 0
status = 1
zoncou = "NBDUD"
If varmode([M]NBDUD,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBDUD From SUBCON
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBDUD : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBDUD" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBDUD
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBDUD = nolign
   status = 75
   Call SAI_NBDUD(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_NBDUD From SUBCON
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NBDUD" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

## Zone NBPAY
mkstat = 0
zoncou = "NBPAY"
indice = 0
status = 1
zoncou = "NBPAY"
If varmode([M]NBPAY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBPAY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBPAY
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBPAY = nolign
   status = 75
   Call SAI_NBPAY(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBINV(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBINV
mkstat = 0
zoncou = "NBINV"
SAVACT = GACTION : GACTION = "STD"
Call C_NBINV From SUBCON
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "NBINV" : GZNUM = 0 : Endif
Gosub AFF From GSAISIE
If mkstat : End : Endif

## Zone INVAMT
mkstat = 0
zoncou = "INVAMT"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CONCUR4,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]INVAMT(indice) : Endif

## Zone NBINV
mkstat = 0
zoncou = "NBINV"

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBDUD(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBDUD
mkstat = 0
zoncou = "NBDUD"
SAVACT = GACTION : GACTION = "STD"
Call C_NBDUD From SUBCON
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "NBDUD" : GZNUM = 0 : Endif
Gosub AFF From GSAISIE
If mkstat : End : Endif

## Zone DUDAMT
mkstat = 0
zoncou = "DUDAMT"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CONCUR4,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]DUDAMT(indice) : Endif

## Zone DUDDPTCOD
mkstat = 0
zoncou = "DUDDPTCOD"
If varinit([M]DUDDPTCOD,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]DUDDPTCOD(indice),"315","") From SUBADI
 GACTION = SAVACT
Endif

## Zone NBDUD
mkstat = 0
zoncou = "NBDUD"

End

