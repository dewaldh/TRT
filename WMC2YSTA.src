####################################################################
# 31 : YLOTACT Lots-Actual ( Field DCB 9)
####################################################################

$SET_YLOTACT
If [L]_AOLDVAL<>this.YLOTACT
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_YLOTACT
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.YLOTACT
 Gosub PROPAGATE_YLOTACT
Endif
Return

$INIT_YLOTACT
Gosub DECLARE_VAR From WMC0YSTA
[L]CURPRO = "YLOTACT"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_YLOTACT
Gosub DECLARE_VAR From WMC0YSTA
[L]CURPRO = "YLOTACT"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_YLOTACT               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_YLOTACT               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_YLOTACT
[L]CURPRO = "YLOTACT"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.YLOTACT=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_YLOTACT
[L]CURPRO = "YLOTACT"
Return

$_AOTHERS_YLOTACT
[L]CURPRO = "YLOTACT"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_YLOTACT
[L]CURPRO = "YLOTACT"
Return

$PROPAGATE_YLOTACT
this.isReadonly = 0
[L]CURPRO = "YLOTACT"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_YLOTACT
Gosub DECLARE_VAR From WMC0YSTA
[L]CURPRO = "YLOTACT"
$_LNKPRO_YLOTACT
Return


