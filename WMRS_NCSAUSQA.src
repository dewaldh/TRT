# Supervisor
## ACLA-CLASSE ## NCSAUSQA
# 2018-08-27 13:20:19 397

#-------------------------------------------------------#
$METHODS
$EVENTS
Case [L]AEVENT
 When "AREAD_AFTER"
  Case [L]CURPTH
   When "AUS" : Gosub ADDACS1
   When "AUS.FNCCOL" : Gosub ADDACS2
   When "AUS.BPA" : Gosub ADDACS3
   When "AUS.BPA.COLTEL" : Gosub ADDACS4
   When "AUS.BPA.COLWEB" : Gosub ADDACS5
   When "AUS.BID" : Gosub ADDACS6
   When "AUS.ADP" : Gosub ADDACS7
   When "AUS.ADP.COLVAL" : Gosub ADDACS8
   When "BIDCRY" : Gosub ADDACS9
   When "" : Gosub ADDACSREP
  Endcase
 When "AINIT"
  Case [L]CURPTH
   When "AUS" : Gosub ADDACS1
   When "AUS.FNCCOL" : Gosub ADDACS2
   When "AUS.BPA" : Gosub ADDACS3
   When "AUS.BPA.COLTEL" : Gosub ADDACS4
   When "AUS.BPA.COLWEB" : Gosub ADDACS5
   When "AUS.BID" : Gosub ADDACS6
   When "AUS.ADP" : Gosub ADDACS7
   When "AUS.ADP.COLVAL" : Gosub ADDACS8
   When "BIDCRY" : Gosub ADDACS9
   When "" : Gosub ADDACSREP
  Endcase
 When Default
Endcase
Return

#-------------------------------------------------------#
$PROPERTIES
Case [L]ARULE
 When "CONTROL"
  Case [L]CURPRO
   When Default
  Endcase
 When "PROPAGATE"
  Case [L]CURPRO
   When "AUS.ENAFLG"
    If [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTN(this,"GESAUS","GESAUS",[V]CST_AFALSE)>=[V]CST_AERROR
     this.ENAFLG = [L]_AOLDVAL
    Endif
   When Default
  Endcase
 Endcase
 Return
 
 #-------------------------------------------------------#
 # AUS
 $ADDACS1
 If fmet this.ACTX.AGETAFCRIGHTN(this,"GESAUS","",[V]CST_AFALSE)>=[V]CST_AERROR
  [L]ASTATUS = fmet this.ASETATTRIBUTE("ENAFLG","$isReadOnly","true")
 Endif
 Return
 
 # AUS.FNCCOL
 $ADDACS2
 Return
 
 # AUS.BPA
 $ADDACS3
 Return
 
 # AUS.BPA.COLTEL
 $ADDACS4
 Return
 
 # AUS.BPA.COLWEB
 $ADDACS5
 Return
 
 # AUS.BID
 $ADDACS6
 Return
 
 # AUS.ADP
 $ADDACS7
 Return
 
 # AUS.ADP.COLVAL
 $ADDACS8
 Return
 
 # BIDCRY
 $ADDACS9
 Return
 
 $ADDACSREP
 Return
 


