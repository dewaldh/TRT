If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YVALTO From WMC1YRMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YVALTO From WMC1YRMA : Gosub TESTALLSTAT :  Endif
