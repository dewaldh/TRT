SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:PIH]BPRVCR"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:PIH]BPRDAT"
