Append WCLOB,',"$properties":{'
Append WCLOB,'"$resources":{'
Append WCLOB,'"$type":"application/x-array"'
Append WCLOB,',"$item":{'
Append WCLOB,'"$title":"'+func GET_RES_STRING(ACTX.LAN,3531)+' {$key}"'
Append WCLOB,',"$key":"{DIE}~{CCE}"'
Append WCLOB,',"$url":"{$baseUrl}/CACCE('+"'"+'{$key}'+"'"+')'+PSYR.URLREP+'CACCE.searchItem"'
Append WCLOB,',"$properties":{'
Append WCLOB,'"CCE":{'
Append WCLOB,'"$title":"'+func GET_RES_STRING(ACTX.LAN,3997)+'"'
Append WCLOB,',"$capabilities":"sort,filter"'
