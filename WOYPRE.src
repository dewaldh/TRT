$LEC_DERLU
Read [ACC]CODACC = GUSER ; [F:YPRC]YACCESS
If fstat : Raz [F:ACC] : Endif
Return

$CHARGE_DERLU
If clalev([F:YPRC9])>0
 Read [YPRC9] = [F:YPRC9]A_TABI
 [L]CLE = [F:YPRC9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:YPRC9])>0
 Filter [YPRC9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [YPRE1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:YPRC9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:YPRC]YPREID+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:YPRC9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:YPRC9]
 Endif
Endif
Return

