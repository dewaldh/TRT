# Supervisor
# Window management OYSLC

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [02/10/2018]
A_HEURE = "07:13:22"
A_USER  = "MB"
Call ISWINVALID(nomap,GLANGUE,"OYSLC",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
Local Char NAVMSK(200)
NAVMSK="YSLC;YSLC1;YSLC2;"
Return

$OUVRE_BOITE
Gosub INIT_BOITE
Gosub DEF_BOITE
Gosub AFF_BOITE
Return

$INIT_BOITE
Local Char    OBJLIS(GLONAOB)(0..10), DESLIS(80)(0..10), BOX1(10)
Local Integer CHGLIS(0..10), BROLIS(0..10), TRELIS(0..10)
Local Char    SAIRAP(50)(1..16)
Local Integer DERLU , AFOLDEF
Local Char    ABRENA(GLONABR)
DERLU = 1
AFOLDEF = 1
ABRENA = ""
BOX1 = "GAU_CHE"
OBJLIS(0) = "YSLC"
CHGLIS(0) = 2
BROLIS(0) = 2
TRELIS(0) = 1
DESLIS(0) = "YSLCID"
If clalev([M:YSLC])=0 : Local Mask YSLC [YSLC] : Endif
If clalev([M:YSLC1])=0 : Local Mask YSLC1 [YSLC1] : Endif
If clalev([M:YSLC2])=0 : Local Mask YSLC2 [YSLC2] : Endif
Gosub TIT
nolign = 1
Return

$DEF_BOITE
Gosub HORODAT From WGOYSLC
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "OYSLC" From GFONCTION At A_STAMP With A_WINDPREV Mask [YSLC]
&   Folder Mask [YSLC1]
& , Folder Mask [YSLC2]
&  Listbox [YSLC] GAU_CHE
&   [L]SAIRAP(1) = [F:YSLC]YSLCID Using "K:7X" Titled TITSEL(1)
&  Listbox [YSLC9] GAU_CHE9
&   [L]SAIRAP(1) = [F:YSLC9]A_TAB1 Using "K:7X" Titled TITSEL9(1) 
& As [YSLC1]
Titled [YSLC1] TIT
Return

$VISU_BOITE
Gosub INIT_BOITE
Gosub HORODAT From WGOYSLC
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "OYSLC" From GFONCTION At A_STAMP With A_WINDPREV Mask [YSLC]
&   Folder Mask [YSLC1]
& , Folder Mask [YSLC2]
& As [YSLC1]
Titled [YSLC1] TIT
Gosub AFF_BOITE
Return

$AFF_BOITE
Boxact [YSLC1] Folder AFOLDEF
Return

$SAISIE_BOITE
Boxinp [YSLC1] Using REPONSE
Return

$FERME_BOITE
Onerrgo RIEN
Close Local Inpbox [YSLC1]
Onerrgo
Return

$RIEN
Resume

$TIT
Local Char TITLIS(30)
Call TEXTE(100909,TITLIS) From OBJDIV
Local Char TITLIS9(30)
TITLIS9 = mess(196,123,1)
Local Char TITSEL(30)(1..1)
Call TEXTE(100473,TITSEL(1)) From OBJDIV
Local Char TITSEL9(30)(1..1)
Call TEXTE(100473,TITSEL9(1)) From OBJDIV
Local Char TIT(60) , TITTRN(60)
Call TEXTE(100909,TIT) From OBJDIV
If GPROGRAMME<>""
 If dim([L]ACTION)<0 : Local Char ACTION(20) : Endif
 [L]ACTION = "TITRE" : Gosub ACTION From =GPROGRAMME
Endif
Return

$REMPLIT_
Gosub SETCLE_
If dim(FILTREG)<=0 or FILTREG
 If CRITERE=""  : CRITERE  = "1=1" : Endif
 If FILTSUP=""  : FILTSUP  = "1=1" : Endif
 If FILROLE=""  : FILROLE  = "1=1" : Endif
 If FILSUITE="" : FILSUITE = "1=1" : Endif
 Filter [YSLC] Where evalue(CRITERE) & evalue(FILTSUP) & evalue(FILROLE) & evalue(FILSUITE)
&       Order With CLELISTE
Endif
GPOINT = "AV_FILLBOX" : Gosub ENTREE From EXEFNC
ACTION = "HINT" : Gosub ACTION From =GPROGRAMME
If HINTLIS(0)<>'' or NOHLIS(0)<>0
 Fillbox [YSLC1] GAU_CHE Hint Key =HINTLIS(0)
Else
 Gosub CALNOHOBJ From GOBJET
 If A_NOH
  Fillbox [YSLC1] GAU_CHE
 Else
  Fillbox [YSLC1] GAU_CHE Hint Key =""
 Endif
Endif
GPOINT = "AP_FILLBOX" : Gosub ENTREE From EXEFNC
Filter [YSLC]
Return

$SETCLE_
Local Char    CLELISTE(250)
Local Integer INDLIS , SNSLIS , LENLIS, GAUTRELIS
INDLIS = 1
SNSLIS = 1
LENLIS = 1
GAUTRELIS = 1
CLELISTE = "YSLCID"
ACTION = "CLE_GAUCHE" : Gosub ACTION From =GPROGRAMME
Return

$DEFAUT_BRO
Case ILIS
 When 0 : Default file [YSLC]
Endcase
Return

$SETBOX
Case currbox
 When "GAU_CHE"
  Gosub SETCLE_
  Filter [YSLC] Order With CLELISTE
  Setlbox [YSLC1] GAU_CHE
  Filter [YSLC]
 When "GAU_CHE9"
  Gosub CLE_DERLU From =PROGOBJ
  Setlbox [YSLC1] GAU_CHE9
Endcase
Return

$SETORDER
Case currbox
 When "GAU_CHE"
  Gosub SETCLE_
  Filter [YSLC] Order With CLELISTE
Endcase
Return

$ENDORDER
Case currbox
 When "GAU_CHE"
  Gosub SETCLE_
  Filter [YSLC]
Endcase
Return

$PICKING
Return

$DEPICK
Return

$REPICK
Return

$SETCUR
Return

$DEFVAR
Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "YSLC"         : ABMSK(0)  = "YSLC"     : AMA(0) = 1
NOMMSK(1) = "YSLC1"        : ABMSK(1)  = "YSLC1"    : AMA(1) = 1
NOMMSK(2) = "YSLC2"        : ABMSK(2)  = "YSLC2"    : AMA(2) = 1
NBMASK = 2
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 0 : If clalev([M:YSLC])>0 : Default Mask [YSLC] : Endif
 When 1 : If clalev([M:YSLC1])>0 : Default Mask [YSLC1] : Endif
 When 2 : If clalev([M:YSLC2])>0 : Default Mask [YSLC2] : Endif
Endcase
Return

$TITMASK
Case MA
 When 0 : If clalev([M:YSLC])>0 : Gosub TITRES From W1YSLC : Endif
 When 1 : If clalev([M:YSLC1])>0 : Gosub TITRES From W1YSLC1 : Endif
 When 2 : If clalev([M:YSLC2])>0 : Gosub TITRES From W1YSLC2 : Endif
Endcase
Return

$FERME_MASK
If clalev([M:YSLC])>0 : Close Local Mask [YSLC] : Endif
If clalev([M:YSLC1])>0 : Close Local Mask [YSLC1] : Endif
If clalev([M:YSLC2])>0 : Close Local Mask [YSLC2] : Endif
Return

$RAZMASK
Case MA
 When 0 : If clalev([M:YSLC])>0 : Raz [M:YSLC] : Endif
 When 1 : If clalev([M:YSLC1])>0 : Raz [M:YSLC1] : Endif
 When 2 : If clalev([M:YSLC2])>0 : Raz [M:YSLC2] : Endif
Endcase
Return

$AFFMASK
Case MA
 When 0 : If clalev([M:YSLC])>0 : Affzo [M:YSLC] : Endif
 When 1 : If clalev([M:YSLC1])>0 : Affzo [M:YSLC1] : Endif
 When 2 : If clalev([M:YSLC2])>0 : Affzo [M:YSLC2] : Endif
Endcase
Return

$OUVMSKCMP
Return

$FERMSKCMP
Return

$RAZMSKCMP
Return

$AFF_ENAFLG
Return

$CHARGEFIC
Case IMA
 When 0 : If clalev([M:YSLC])>0 : [F:YSLC] = [M:YSLC] : Endif
 When 1 : If clalev([M:YSLC1])>0 : [F:YSLC] = [M:YSLC1] : Endif
 When 2 : If clalev([M:YSLC2])>0 : [F:YSLC] = [M:YSLC2] : Endif
Endcase
Return

$CHARGEMSK
Case IMA
 When 0 : If clalev([M:YSLC])>0 : [M:YSLC] = [F:YSLC] : Endif
 When 1 : If clalev([M:YSLC1])>0 : [M:YSLC1] = [F:YSLC] : Endif
 When 2 : If clalev([M:YSLC2])>0 : [M:YSLC2] = [F:YSLC] : Endif
Endcase
Return

$GRISE_CLE
If AMA(0)
 Grizo [YSLC]YSLCID
Else
 Grizo [YSLC1]YSLCID
Endif
Return

$DEGRISE_CLE
If AMA(0)
 Actzo [YSLC]YSLCID
Else
 Actzo [YSLC1]YSLCID
Endif
Return

$BOUTON
STDBOUT = "ECSAN09<>RHILJMP"
NBTEX  = 0
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
Return

