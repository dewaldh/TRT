 When 0 : If clalev([M:YSLC])>0 : [F:YSLC] = [M:YSLC] : Endif
 When 1 : If clalev([M:YSLC1])>0 : [F:YSLC] = [M:YSLC1] : Endif
 When 2 : If clalev([M:YSLC2])>0 : [F:YSLC] = [M:YSLC2] : Endif
Endcase
