####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "YGSC"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"YGSC",this)
 When "YREL"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"YREL",this)
 When "YSTA"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"YSTA",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

