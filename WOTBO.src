$LEC_DERLU
Read [ACC]CODACC = GUSER ; [F:TBO]ACSCOD
If fstat : Raz [F:ACC] : Endif
Read [AXX]AXX0="TABBOMALT";"TBODESAXX";GLANGUE;num$([F:TBO]BOMALTTYP);num$([F:TBO]BOMALT)
If fstat : Raz [F:AXX] : Endif
Return

$CHARGE_DERLU
If clalev([F:TBO9])>0
 Read [TBO9] = [F:TBO9]A_TABI
 [L]CLE = [F:TBO9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:TBO9])>0
 Filter [TBO9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [TBO1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:TBO9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = num$([F:TBO]BOMALT)+"~"
 A_CLEDERLU += num$([F:TBO]BOMALTTYP)+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:TBO9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:TBO9]
 Endif
Endif
Return

