#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management DCLVATITA 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ALLCPY
mkstat = 0
zoncou = "ALLCPY"
indice = 0
IMPENT = [M]ALLCPY
WNUM = IMPENT
If varinit([M]ALLCPY,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLCPY,indice)=0
 [M]ALLCPY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLCPY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLCPY(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_ALLCPY(IMPENT) From DCLVATIT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ALLCPY" : GZNUM = 6052 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]ALLCPY = IMPENT

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 SAVACT = GACTION : GACTION = "INICPY"
 Call INICPY([M]CPY) From EXEFNC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CPY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]CPY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTCPY"
   Call CONTSOC([M]CPY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("CPY","A",[M]CPY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call C_CPY([M]CPY) From DCLVATIT
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone ALLFCY
mkstat = 0
zoncou = "ALLFCY"
indice = 0
IMPENT = [M]ALLFCY
WNUM = IMPENT
If varinit([M]ALLFCY,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLFCY,indice)=0
 [M]ALLFCY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLFCY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLFCY(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_ALLFCY(IMPENT) From DCLVATIT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ALLFCY" : GZNUM = 6053 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]ALLFCY = IMPENT

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 SAVACT = GACTION : GACTION = "INIFCY"
 Call INIFCY([M]FCY,[M]CPY) From EXEFNC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]FCY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTFCY"
   Call CONTSITE([M]FCY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("FCY","",[M]FCY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call C_FCY([M]FCY) From DCLVATIT
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "CONTFCYCPY"
      Call CONTFCYCPY([M]FCY,[M]CPY) From EXEFNC
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone STRDAT
mkstat = 0
zoncou = "STRDAT"
indice = 0
If varinit([M]STRDAT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_STRDAT([M]STRDAT) From DCLVATIT
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]STRDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "STRDAT" : Gosub ACTION From =IMPTRT
  Endif
  If [M]STRDAT=[0/0/0]
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 9938 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone ENDDAT
mkstat = 0
zoncou = "ENDDAT"
indice = 0
If varinit([M]ENDDAT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_ENDDAT([M]ENDDAT) From DCLVATIT
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]ENDDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "ENDDAT" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "DATDEBFIN"
  Call DATDEBFIN([M]ENDDAT,[M]STRDAT) From EXEFNC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ENDDAT" : GZNUM = 9942 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone SIM
mkstat = 0
zoncou = "SIM"
indice = 0
IMPENT = [M]SIM
If varinit([M]SIM,indice)=0
 IMPENT = 2
Endif
If varmode([M]SIM,indice)=0
 [M]SIM(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "SIM" : Gosub ACTION From =IMPTRT
  IMPENT = [M]SIM(indice)
 Endif
Endif
[M]SIM = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone TRC
mkstat = 0
zoncou = "TRC"
indice = 0
IMPENT = [M]TRC
If varinit([M]TRC,indice)=0
 IMPENT = 2
Endif
If varmode([M]TRC,indice)=0
 [M]TRC(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TRC" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TRC(indice)
 Endif
Endif
[M]TRC = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "DCLVATITA"
Return

