############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(OPPORPJM,ASTATUS)
Variable Instance OPPORPJM Using C_OPPORPJM
Variable Integer ASTATUS

If OPPORPJM = null : End : Endif

Gosub _AVARIABLES From WMC00OPPORPJM

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_OPPORPJM

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(OPPORPJM,ASTATUS)
Variable Instance OPPORPJM Using C_OPPORPJM
Variable Integer ASTATUS

If OPPORPJM = null : End : Endif

Gosub AVARIABLES From WMC00OPPORPJM
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
OPPORPJM.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_OPPORPJM
Else
 Gosub AUPDATE_OPPORPJM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = OPPORPJM.freeSnapshot
OPPORPJM.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet OPPORPJM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet OPPORPJM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet OPPORPJM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If OPPORPJM.ASTALIN = [V]CST_ANEWDEL : Return : Endif
OPPORPJM.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(OPPORPJM.ASTALIN)<>""
 [L]A_ASTALIN = OPPORPJM.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(OPPORPJM.CDIE)
 If OPPORPJM.CDIE([L]A_I)<>null
  [L]A_ASTALIN = OPPORPJM.CDIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.CDIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(OPPORPJM.BUD) : # PJMBUD -- 2
 If OPPORPJM.BUD(A_N2) <> null
& and OPPORPJM.BUD(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If OPPORPJM.BUD(A_N2).ASTALIN<>[V]CST_ANEW & OPPORPJM.ASTALIN=[V]CST_ADEL
   OPPORPJM.BUD(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If OPPORPJM.BUD(A_N2).ASTALIN=[V]CST_ANEW & find(OPPORPJM.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   OPPORPJM.BUD(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(OPPORPJM.BUD(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPORPJM.BUD(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.BUD(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.BUD(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.BUD(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(OPPORPJM.TAS) : # PJMTSK -- 3
 If OPPORPJM.TAS(A_N2) <> null
& and OPPORPJM.TAS(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If OPPORPJM.TAS(A_N2).ASTALIN<>[V]CST_ANEW & OPPORPJM.ASTALIN=[V]CST_ADEL
   OPPORPJM.TAS(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If OPPORPJM.TAS(A_N2).ASTALIN=[V]CST_ANEW & find(OPPORPJM.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   OPPORPJM.TAS(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(OPPORPJM.TAS(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPORPJM.TAS(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.TAS(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.TAS(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.TAS(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(OPPORPJM.PAF) : # PJMAFF -- 4
 If OPPORPJM.PAF(A_N2) <> null
& and OPPORPJM.PAF(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If OPPORPJM.PAF(A_N2).ASTALIN<>[V]CST_ANEW & OPPORPJM.ASTALIN=[V]CST_ADEL
