  Endif
  If OPPORPJM.PAF(A_N2).ASTALIN=[V]CST_ANEW & find(OPPORPJM.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   OPPORPJM.PAF(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(OPPORPJM.PAF(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPORPJM.PAF(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.PAF(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.PAF(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.PAF(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(OPPORPJM.SPLD) : # PJMSALITMD -- 5
 If OPPORPJM.SPLD(A_N2) <> null
& and OPPORPJM.SPLD(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If OPPORPJM.SPLD(A_N2).ASTALIN<>[V]CST_ANEW & OPPORPJM.ASTALIN=[V]CST_ADEL
   OPPORPJM.SPLD(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If OPPORPJM.SPLD(A_N2).ASTALIN=[V]CST_ANEW & find(OPPORPJM.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   OPPORPJM.SPLD(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(OPPORPJM.SPLD(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(OPPORPJM.SPLD(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.SPLD(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.SPLD(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.SPLD(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

