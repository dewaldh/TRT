 If OPPORPJM.CDIE([L]A_I)<>null
  [L]A_ASTALIN = OPPORPJM.CDIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet OPPORPJM.CDIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
