####################################################################
# 31 : CAD Rate ( Field DCB 6)
####################################################################

$SET_CAD
If [L]_AOLDVAL<>this.CAD
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_CAD
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.CAD
 Gosub PROPAGATE_CAD
Endif
Return

$INIT_CAD
Gosub DECLARE_VAR From WMC0ROUOPESTD
[L]CURPRO = "CAD"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_CAD
Gosub DECLARE_VAR From WMC0ROUOPESTD
[L]CURPRO = "CAD"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_CAD                   : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_CAD                   : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_CAD
[L]CURPRO = "CAD"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.CAD=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_CAD
[L]CURPRO = "CAD"
Return

$_AOTHERS_CAD
[L]CURPRO = "CAD"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_CAD
[L]CURPRO = "CAD"
Return

$PROPAGATE_CAD
this.isReadonly = 0
[L]CURPRO = "CAD"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_CAD
Gosub DECLARE_VAR From WMC0ROUOPESTD
[L]CURPRO = "CAD"
$_LNKPRO_CAD
Return


