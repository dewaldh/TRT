$BOUTON
STDBOUT = "ECSAN09<>RHILJMP"
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "1" : LBOUT(NBTEX) = 1101 : Call TEXTE(1049,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "2" : LBOUT(NBTEX) = 1102 : Call TEXTE(1401,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "3" : LBOUT(NBTEX) = 1803 : Call TEXTE(15372,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
NBMEN += 1
NMEN(NBMEN) = 10 : Call TEXTE(12708,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "4" : LMEN(NBMEN) = 2001 : Call TEXTE(4120,TMEN(NBMEN)) From OBJDIV
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "1"
  FLGEXE = 1
  GACTION = "STD"
 When "2"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "GTC"
  PARBOUT(2) = [M]CNSCOD
  PARBOUT(3) = [M]COD
  PARBOUT(4) = ""
  PARBOUT(5) = ""
  FLGEXE = 1
  GACTION = "ACOPIE"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ACOPIE","ACOPIE",3,"XYACOPIE") From GTRAITE
 When "3"
  FLGEXE = 1
  GACTION = "STD"
 When "4"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:GTC1]CNSCOD
  PARBOUT(2) = [M:GTC1]COD
  FLGEXE = 1
  GACTION = "SAIGRASOL"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"FGRASOL","SAIGRASOL","XWFGRASOL") From GSAISIE
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


