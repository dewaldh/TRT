Endif
Return

$REMP_DERLU
If clalev([F:CAC9])>0
 Filter [CAC9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [CAC1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:CAC9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:CAC]ACCCOD+"~"
 A_CLEDERLU += num$([F:CAC]TYP)+"~"
 A_CLEDERLU += [F:CAC]COA+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:CAC9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:CAC9]
 Endif
Endif
Return

