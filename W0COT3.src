# Supervisor
# Mask management COT3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone INVFRY
mkstat = 0
zoncou = "INVFRY"
indice = 0
WNUM = [M]INVFRY
If varmode([M]INVFRY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "INVFRY" : Gosub ACTION From =IMPTRT
 Endif
 If [M]INVFRY<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "INVFRY" : GZNUM = 19019 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>[M]INVFRY or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_INVFRY([M]INVFRY) From SUBCOT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "INVFRY" : GZNUM = 19019 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone INVFRYBAS
mkstat = 0
zoncou = "INVFRYBAS"
indice = 0
IMPENT = [M]INVFRYBAS
If varinit([M]INVFRYBAS,indice)=0
 IMPENT = 1
Endif
If varmode([M]INVFRYBAS,indice)=0
 Call INICOMBO(976) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_INVFRYBAS(IMPENT) From SUBCOT
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,976,1))))
 Endif
 [M]INVFRYBAS(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INVFRYBAS" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INVFRYBAS(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "INVFRYBAS" : GZNUM = 2685 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]INVFRYBAS = IMPENT

## Zone INVMET
mkstat = 0
zoncou = "INVMET"
indice = 0
IMPENT = [M]INVMET
If varinit([M]INVMET,indice)=0
 IMPENT = 1
Endif
If varmode([M]INVMET,indice)=0
 [M]INVMET(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INVMET" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INVMET(indice)
 Endif
Endif
[M]INVMET = IMPENT

## Zone INVFRYCOE
mkstat = 0
zoncou = "INVFRYCOE"
indice = 0
If varmode([M]INVFRYCOE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "INVFRYCOE" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone INVLTI
mkstat = 0
zoncou = "INVLTI"
indice = 0
WNUM = [M]INVLTI
If varmode([M]INVLTI,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "INVLTI" : Gosub ACTION From =IMPTRT
 Endif
 If [M]INVLTI<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "INVLTI" : GZNUM = 45954 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>[M]INVLTI or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_INVLTI([M]INVLTI) From SUBCOT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "INVLTI" : GZNUM = 45954 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone INVLTIBAS
mkstat = 0
zoncou = "INVLTIBAS"
indice = 0
IMPENT = [M]INVLTIBAS
If varinit([M]INVLTIBAS,indice)=0
 IMPENT = 1
Endif
If varmode([M]INVLTIBAS,indice)=0
 Call INICOMBO(976) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_INVLTIBAS(IMPENT) From SUBCOT
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,976,1))))
 Endif
 [M]INVLTIBAS(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INVLTIBAS" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INVLTIBAS(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "INVLTIBAS" : GZNUM = 2685 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]INVLTIBAS = IMPENT

## Zone PTE
mkstat = 0
zoncou = "PTE"
indice = 0
If varmode([M]PTE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PTE" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "CONTTPT"
 Call CONTTPT([M]PTE,"","","","","","") From SUBTPT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "PTE" : GZNUM = 4114 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJN"
  Local Char    VALEUR(250)
  VALEUR = [M]PTE
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJN","CONTOBJ","") From GACTION
  [M]PTE = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "PTE" : GZNUM = 4114 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBDPT
mkstat = 0
zoncou = "NBDPT"
indice = 0
status = 1
zoncou = "NBDPT"
If varmode([M]NBDPT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBDPT" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBDPT
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBDPT = nolign
   status = 75
   Call SAI_NBDPT(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBDPT(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBDPT
mkstat = 0
zoncou = "NBDPT"

## Zone ACPDPTCOD
mkstat = 0
zoncou = "ACPDPTCOD"
If varinit([M]ACPDPTCOD,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]ACPDPTCOD(indice),"315","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]ACPDPTCOD,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "ACPDPTCOD" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]ACPDPTCOD(indice),"315","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ACPDPTCOD" : GZNUM = 53812 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!",[M]ACPDPTCOD(indice),"315",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "ACPDPTCOD" : GZNUM = 53812 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_ACPDPTCOD([M]ACPDPTCOD(indice)) From SUBCOT
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "ACPDPTCOD" : GZNUM = 53812 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone NBDPT
mkstat = 0
zoncou = "NBDPT"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "COT3"
Return

