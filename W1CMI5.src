# Supervisor
# Mask management CMI5 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:54:49"
A_USER  = "ADMIN"
A_STAMP = "20180827235449"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIG"
  If [M]NBLIG=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask CMI5 [CMI5]
Gosub HORODAT
Local Inpbox "CMI5" From GFONCTION At A_STAMP With A_WINDPREV Mask [CMI5] As [SIM_]
Grizo [CMI5]5
Grizo [CMI5]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [CMI5]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=22
X1(1)=1 : X2(1)=74 : Y1(1)=23 : Y2(1)=24
Return

$ON_OBJLIS
Onevent CONTROL Call C_OBJLIS(zc) From W1CMI5
Onevent AFTER_CHANGE Gosub AMM_OBJLIS From W1CMI5
Onevent BEFORE_INPUT Call AS_OBJLIS(zc) From W1CMI5
Return

Subprog AS_OBJLIS(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(1158,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_OBJLIS(zc) From W1CMI5
Onkey   2 Titled GBOUTS Call S_OBJLIS(zc) From W1CMI5
Onkey 144 Titled GBOUTP Call P_OBJLIS(zc) From W1CMI5
Onkey 146 Titled GBOUTA Call SA_OBJLIS(zc) From W1CMI5
Gosub _AFF
End

Subprog C_OBJLIS(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("AOB","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "OBJLIS" : GZNUM = 1158 : Endif
If !mkstat
 ## Action STD
 Call C_OBJLIS(VALEUR) From SUBCMI
 If mkstat : GERR = 1 : GZONE = "OBJLIS" : GZNUM = 1158 : Endif
Endif
Gosub _AFF
End

$AMM_OBJLIS
Call AM_OBJLIS(zc)
If !mkstat & GOBJET
 zoncou = "OBJLIS" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_OBJLIS(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_OBJLIS(VALEUR) From SUBCMI
If mkstat : GERR = 1 : GZONE = "OBJLIS" : GZNUM = 1158 : Endif
Gosub _AFF
End

Subprog S_OBJLIS(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELCMI5AOB
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELCMI5AOB"
 Call S_LIS_CHAR(VALEUR,PARMSK,"SELCMI5AOB","CRMSEL140","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_OBJLIS(VALEUR)
Variable Char    VALEUR()
Call OBJET("AOB",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_OBJLIS(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AOB"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_OBJLIS(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AOB",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

