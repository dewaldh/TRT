#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management MFC0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
indice = 0
If varmode([M]ITMREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREF" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "VXADCUPC"
 Call ADCCTRUPC([M]ITMREF) From VXADCUPC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("ITM","!",[M]ITMREF,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone STOFCY
mkstat = 0
zoncou = "STOFCY"
indice = 0
If varmode([M]STOFCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "STOFCY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTFCY"
 Call CONTSITE([M]STOFCY) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("FCY","!",[M]STOFCY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_STOFCY([M]STOFCY) From SUBMFC
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "CONTFCY"
    Call CONTFCY([M]STOFCY) From EXEFNC
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1347 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone CUR
mkstat = 0
zoncou = "CUR"
indice = 0
SAVACT = GACTION : GACTION = "STD"
Call AV_CUR([M]CUR) From SUBMFC
GACTION = SAVACT
If mkstat=1 : Raz [M]CUR : Endif

## Zone MFCTYP
mkstat = 0
zoncou = "MFCTYP"
indice = 0
IMPENT = [M]MFCTYP
If varinit([M]MFCTYP,indice)=0
 IMPENT = 1
Endif
[M]MFCTYP = IMPENT

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
indice = 0
IMPENT = [M]VCRTYP
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP = IMPENT

## Zone VCRNUM
mkstat = 0
zoncou = "VCRNUM"
indice = 0
If varmode([M]VCRNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "VCRNUM" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_VCRNUM([M]VCRNUM) From SUBMFC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone UID
mkstat = 0
zoncou = "UID"
indice = 0
If varinit([M]UID,indice)=0
 [M]UID = 0
Endif

## Zone MFGSTA
mkstat = 0
zoncou = "MFGSTA"
indice = 0
IMPENT = [M]MFGSTA
If varinit([M]MFGSTA,indice)=0
 IMPENT = 1
Endif
[M]MFGSTA = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone ITMSTA
mkstat = 0
zoncou = "ITMSTA"
indice = 0
IMPENT = [M]ITMSTA
If varinit([M]ITMSTA,indice)=0
 IMPENT = 1
Endif
[M]ITMSTA = IMPENT

## Zone BRDCOD
mkstat = 0
zoncou = "BRDCOD"
indice = 0
IMPENT = [M]BRDCOD
If varinit([M]BRDCOD,indice)=0
 IMPENT = 1
Endif
[M]BRDCOD = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc liste 4
####################################
IMPTBL = 0

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "MFC0"
Return

