#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:INO])=0 : Local File ITMCUSNOM [INO] : Endif
If clalev([F:WAXX1])=0 : Local File ATEXTRA [WAXX1] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [INO]
LogicClose File [WAXX1]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"INO","INO",ACRITROLE,[V]CST_AFALSE)
Link [INO] With
& [F:WAXX1]AXX0 = "ITMCUSNOM";"DESAXX";GACTX.LAN;[F:INO]ITMNOM;""
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Local Char ACRITROLE(250)(1..)
[L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"INO","INO",ACRITROLE,[V]CST_AFALSE)
Link [INO] With
& [F:WAXX1]AXX0 = "ITMCUSNOM";"DESAXX";GACTX.LAN;[F:INO]ITMNOM;""
& As [LNK_]
& Where ( (1=1)
& and (evalue([L]ACRITROLE))
&  )
Return
 
#-------------------------------------------------------------------#
# Default filter for the query
#-------------------------------------------------------------------#
$AQUERY_FILTER_DEF
Return
 
#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# INO --- ITMCUSNOM(INO)
QRY(NBREP) = NewInstance R_ITMCUSNOM AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:INO]AUUID)>0 & [F:INO]AUUID=NullUuid
 [F:INO]AUUID = getUuid
Endif
SetInstance QRY(NBREP).INO With [F:INO]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
Call ASETTEXTRA(QRY(NBREP).INO.AXX,"QRY","DESAXX",[F:WAXX1]TEXTE,[F:WAXX1]LANGUE) From ASYRSUB
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# INO --- ITMCUSNOM(INO)
QRY = NewInstance R_ITMCUSNOM AllocGroup PQRY
QRY.INO.AXX(1) = NewInstance C_ATXTTRA AllocGroup QRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:INO]AUUID)>0 & [F:INO]AUUID=NullUuid
 [F:INO]AUUID = getUuid
Endif
SetInstance QRY.INO With [F:INO]
QRY.INO.AXX(1).PRO = "DESAXX"
QRY.INO.AXX(1).LAN = [F:WAXX1]LANGUE
QRY.INO.AXX(1).TEX = [F:WAXX1]TEXTE
QRY.INO.AXX(1).FLG = 1
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

