#-------------------------------------------------------------------#
# LOAD_PROPERTIES of query
#-------------------------------------------------------------------#
$AQUERY_DECODE_CRITERIA
NBPRO = 0
NBPRO += 1 : TBPRO(NBPRO) = "ITMNOM" : TBFIL(NBPRO) = "[F:INO]ITMNOM"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "INO.ITMNOM"
NBPRO += 1 : TBPRO(NBPRO) = "DESAXX" : TBFIL(NBPRO) = "[F:WAXX1]TEXTE"
           : TBTYP(NBPRO) = 26 : TBPTH(NBPRO) = "INO.DESAXX"
NBPRO += 1 : TBPRO(NBPRO) = "ITMCUSUOM" : TBFIL(NBPRO) = "[F:INO]ITMCUSUOM"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "INO.ITMCUSUOM"
ACTION = "AQUERY_DECODE_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

