#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CRISTA 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CPY([M]CPY) From SAICRISTA
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CPY : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "INICPY"
  Call INICPY([M]CPY) From EXEFNC
  GACTION = SAVACT
  If mkstat=1 : Raz [M]CPY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTCPY"
   Call CONTSOC([M]CPY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("CPY","!",[M]CPY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTCPY"
     Call CONTCPY([M]CPY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "STD"
      Call C_CPY([M]CPY) From SAICRISTA
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FCY([M]FCY) From SAICRISTA
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]FCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","!",[M]FCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_FCY([M]FCY) From SAICRISTA
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTFCY"
     Call CONTFCY([M]FCY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "CONTFCYCPY"
      Call CONTFCYCPY([M]FCY,[M]CPY) From EXEFNC
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone DATDEB
mkstat = 0
zoncou = "DATDEB"
indice = 0
WDATE = [M]DATDEB
SAVACT = GACTION : GACTION = "ADATRELI"
Call INIDAT([M]DATDEB,[M:DIA]DATFRMD) From ADATREL
GACTION = SAVACT
If mkstat=1 : Raz [M]DATDEB : Endif
If !mkstat
 If varinit([M]DATDEB,indice)=0
  [M]DATDEB = gdat$(1,1,year(datesyst))
 Endif
Endif
If varmode([M]DATDEB,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATDEB" : Gosub ACTION From =IMPTRT
 Endif
 If WDATE<>[M]DATDEB or IMPMOD
  SAVACT = GACTION : GACTION = "ADATRELM"
  Call MODIFDAT([M]DATDEB,[M:DIA]DATFRMD) From ADATREL
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DATFIN
mkstat = 0
zoncou = "DATFIN"
indice = 0
WDATE = [M]DATFIN
SAVACT = GACTION : GACTION = "ADATRELI"
Call INIDAT([M]DATFIN,[M:DIA]DATFRMF) From ADATREL
GACTION = SAVACT
If mkstat=1 : Raz [M]DATFIN : Endif
If !mkstat
 If varinit([M]DATFIN,indice)=0
  [M]DATFIN = gdat$(31,12,year(datesyst))
 Endif
Endif
If varmode([M]DATFIN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATFIN" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "DATDEBFIN"
 Call DATDEBFIN([M]DATFIN,[M]DATDEB) From EXEFNC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_DATFIN([M]DATFIN) From SAICRISTA
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WDATE<>[M]DATFIN or IMPMOD
    SAVACT = GACTION : GACTION = "ADATRELM"
    Call MODIFDAT([M]DATFIN,[M:DIA]DATFRMF) From ADATREL
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CRISTA"
Return

