 Endif
Endif
Return

$ON_VCRNUM
Onevent CONTROL Call C_VCRNUM(zc) From W2WMP2SCT
Onevent BEFORE_INPUT Call AS_VCRNUM(zc) From W2WMP2SCT
Onevent AFTER_AREA Call AP_VCRNUM(zc) From W2WMP2SCT
Onevent AFTER_CHANGE Gosub AMM_VCRNUM From W2WMP2SCT
Return

Subprog AS_VCRNUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15493,GBOUT3) From OBJDIV
Call TEXTE(8255,GBOUTS) From OBJDIV
## Action STD
Call AS_VCRNUM(VALEUR) From SUBPAY
If mkstat=2
 [M]VCRNUM(INDIC) = VALEUR
 Affzo VCRNUM(INDIC) : mkstat = 2
Endif
Onkey 131 Titled GBOUT3 Call B3_VCRNUM(zc) From W2WMP2SCT
Onkey   2 Titled GBOUTS Call S_VCRNUM(zc) From W2WMP2SCT
Gosub _AFF
End

Subprog C_VCRNUM(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 3170 : Endif
If !GWEBSERV
 If !mkstat
  ## Action CONTVCR
  Call CONTVCR(VALEUR) From TRTDIV
  If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 3170 : Endif
 Endif
Endif
If !mkstat
 ## Action STD
 Call C_VCRNUM(VALEUR) From SUBPAY
 If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 3170 : Endif
Endif
Gosub _AFF
End

Subprog AP_VCRNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELECH
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELECH"
 PARMSK(1) = num$([M]DUDNUM(nolign-1))
 PARMSK(2) = num$([M]DUDLIG(nolign-1))
 PARMSK(3) = num$(GSTATUS)
 PARMSK(4) = "SUBPAY"
 Call SAISIE_CHAR(VALEUR,PARMSK,"DREGECH","SELECH","XWDREGECH") From GSAISIE
 [M]DUDNUM(nolign-1) = val(PARMSK(1))
 [M]DUDLIG(nolign-1) = val(PARMSK(2))
 GSTATUS = val(PARMSK(3))
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 3170 : Endif
Endif
If !mkstat
 ## Action STD
 Call AP_VCRNUM(VALEUR) From SUBPAY
 If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 3170 : Endif
Endif
Gosub _AFF
End

$AMM_VCRNUM
