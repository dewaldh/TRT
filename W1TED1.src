# Supervisor
# Mask management TED1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:16:12"
A_USER  = "ADMIN"
A_STAMP = "20180828001612"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask TED1 [TED1]
Gosub HORODAT
Local Inpbox "TED1" From GFONCTION At A_STAMP With A_WINDPREV Mask [TED1] As [SIM_]
Grizo [TED1]5
Grizo [TED1]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [TED1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=86 : Y1(0)=1 : Y2(0)=3
X1(1)=1 : X2(1)=86 : Y1(1)=4 : Y2(1)=28
Return

$ON_CODEDT
Onevent CONTROL Call C_CODEDT(zc) From W1TED1
Onevent BEFORE_INPUT Call AS_CODEDT(zc) From W1TED1
Return

Subprog IB_CODEDT(VALEUR)
Variable Char    VALEUR()
## Action STD
Call IB_CODEDT From SUBTED
Gosub _AFF
End

Subprog AS_CODEDT(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(10591,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_CODEDT(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_CODEDT(zc) From W1TED1
Onkey   2 Titled GBOUTS Call S_CODEDT(zc) From W1TED1
Onkey 144 Titled GBOUTP Call P_CODEDT(zc) From W1TED1
Onkey 146 Titled GBOUTA Call SA_CODEDT(zc) From W1TED1
Gosub _AFF
End

Subprog C_CODEDT(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "CODEDT" : GZNUM = 10591 : Endif
If !mkstat
 ## Action CONTTED
 Call CONTTED(VALEUR,"") From SUBTED
 If mkstat : GERR = 1 : GZONE = "CODEDT" : GZNUM = 10591 : Endif
 If !mkstat
  ## Action CONTOBJN
  Local Char    PARMSK(250)(1..20) , SAVACT(20)
  SAVACT = GACTION : GACTION = "CONTOBJN"
  Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJN","CONTOBJ","") From GACTION
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CODEDT" : GZNUM = 10591 : Endif
  If !mkstat
   ## Action CONTOBJW
   Local Char    PARMSK(250)(1..20) , SAVACT(20)
   SAVACT = GACTION : GACTION = "CONTOBJW"
   PARMSK(1) = "TED"
   Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJW","CONTOBJ","") From GACTION
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CODEDT" : GZNUM = 10591 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_CODEDT(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELOBJOPTA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELOBJOPTA"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AS_CODEDT" : PROGSUB = "SUBTED" : PROGSPE = "SPETED" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "TED"
 PARMSK(2) = "CODEDT"
 PARMSK(3) = "|"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJET","SELOBJ","") From GACTION
 ACTION = "BS_CODEDT" : PROGSUB = "SUBTED" : PROGSPE = "SPETED" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_CODEDT(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action TUNTED
 Call TUNTED(VALEUR,"") From SUBTED
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog SA_CODEDT(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TED"
PARMSK(3) = "$"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CODEDT(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TED",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

