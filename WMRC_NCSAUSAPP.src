# Supervisor
## CRUD-CLASSE ## NCSAUSAPP
# 2018-08-27 13:20:06 499


############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(NCSAUSAPP,ASTATUS)
Variable Instance NCSAUSAPP Using R_NCSAUSAPP
Variable Integer ASTATUS

If NCSAUSAPP = null : End : Endif

Gosub AVARIABLES From WMR00NCSAUSAPP
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If fmet NCSAUSAPP.ACTX.AGETAFCRIGHT(NCSAUSAPP,"GESAUS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END : Endif
If fmet NCSAUSAPP.ACTX.AGETAFCRIGHTC(NCSAUSAPP,"GESAUS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END
Endif
If fmet NCSAUSAPP.ACTX.AGETACSRIGHTM(NCSAUSAPP,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AINSERT_END
  Endif
  
  Gosub AREPCONTROL_AINSERT
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
  
If !adxlog
 Gosub AINSERT_TR_NCSAUSAPP
Else
 Gosub AINSERT_NCSAUSAPP
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_END : Return : Endif

Gosub _AINSERT_AFTER
$AINSERT_END
[L]A_I = NCSAUSAPP.freeSnapshot
NCSAUSAPP.snapshotEnabled = 1
End

#-----------------------------------------------------#
$AREPCONTROL_AINSERT
NCSAUSAPP.ASTALIN = [V]CST_ANEW
NCSAUSAPP.AUS.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet NCSAUSAPP._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet NCSAUSAPP._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet NCSAUSAPP._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If NCSAUSAPP.AUS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
NCSAUSAPP.AUS.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.AUS.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(NCSAUSAPP.AUS.FNCCOL)
 If NCSAUSAPP.AUS.FNCCOL([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.AUS.FNCCOL([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If NCSAUSAPP.AUS.BPA(A_N2) <> null
& and !find(NCSAUSAPP.AUS.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(NCSAUSAPP.AUS.BID) : # BID -- 3
 If NCSAUSAPP.AUS.BID(A_N2) <> null
& and !find(NCSAUSAPP.AUS.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
A_N2=1
If NCSAUSAPP.AUS.BPA(A_N2) <> null
& and !find(NCSAUSAPP.AUS.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(NCSAUSAPP.AUS.BID) : # BID -- 3
 If NCSAUSAPP.AUS.BID(A_N2) <> null
& and !find(NCSAUSAPP.AUS.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(NCSAUSAPP.AUS.FNCCOL)
 If NCSAUSAPP.AUS.FNCCOL([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.AUS.FNCCOL([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.AUS.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_NCSAUSAPP
Trbegin [F:AUS]
 Gosub AINSERT_NCSAUSAPP
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AINSERT_AFTER
If find(NCSAUSAPP.AUS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 NCSAUSAPP.AUS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(NCSAUSAPP.AUS.FNCCOL)
 If NCSAUSAPP.AUS.FNCCOL([L]A_I)<>null
  If NCSAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ADEL or NCSAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance NCSAUSAPP.AUS.FNCCOL([L]A_I)
   NCSAUSAPP.AUS.FNCCOL([L]A_I) = null
  Elsif NCSAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN <> [V]CST_ALL
   NCSAUSAPP.AUS.FNCCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
Local Integer ASTATUS2
A_N2=1
If NCSAUSAPP.AUS.BPA(A_N2) <> null
 If NCSAUSAPP.AUS.BPA(A_N2).ASTALIN=[V]CST_ADEL or NCSAUSAPP.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance NCSAUSAPP.AUS.BPA(A_N2)
  NCSAUSAPP.AUS.BPA(A_N2) = null
 Elsif NCSAUSAPP.AUS.BPA(A_N2).ASTALIN <> [V]CST_ALL
  NCSAUSAPP.AUS.BPA(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(NCSAUSAPP.AUS.BPA(A_N2).COLTEL)
   If NCSAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If NCSAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or NCSAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance NCSAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I)
     NCSAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I) = null
    Elsif NCSAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
     NCSAUSAPP.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
  For [L]A_I=1 To maxtab(NCSAUSAPP.AUS.BPA(A_N2).COLWEB)
   If NCSAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If NCSAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or NCSAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance NCSAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I)
     NCSAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I) = null
    Elsif NCSAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
     NCSAUSAPP.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(NCSAUSAPP.AUS.BID)
 If NCSAUSAPP.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet NCSAUSAPP.AUS.BID(A_N2)._AINSERT_AFTER
  If NCSAUSAPP.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or NCSAUSAPP.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance NCSAUSAPP.AUS.BID(A_N2)
   NCSAUSAPP.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AINSERT_NCSAUSAPP
[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.AUS._AINSERT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSAPP.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If NCSAUSAPP.snapshotEnabled=1
 [L]A_RETIK = NCSAUSAPP.revertUpdTick
Endif
[L]ASTATUS = max(fmet NCSAUSAPP.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



