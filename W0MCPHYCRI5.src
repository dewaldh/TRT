# Supervisor
# Mask management MCPHYCRI5 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone FLGSIM
mkstat = 0
zoncou = "FLGSIM"
indice = 0
IMPENT = [M]FLGSIM
If varinit([M]FLGSIM,indice)=0
 IMPENT = 2
Endif
If varmode([M]FLGSIM,indice)=0
 [M]FLGSIM(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLGSIM" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLGSIM(indice)
 Endif
Endif
[M]FLGSIM = IMPENT

## Zone FLGREP
mkstat = 0
zoncou = "FLGREP"
indice = 0
IMPENT = [M]FLGREP
WNUM = IMPENT
If varinit([M]FLGREP,indice)=0
 IMPENT = 2
 SAVACT = GACTION : GACTION = "STD"
 Call D_FLGREP(IMPENT) From TRMPROACT
 GACTION = SAVACT
Endif
[M]FLGREP = IMPENT

## Zone FLGTRC
mkstat = 0
zoncou = "FLGTRC"
indice = 0
IMPENT = [M]FLGTRC
If varinit([M]FLGTRC,indice)=0
 IMPENT = 2
Endif
[M]FLGTRC = IMPENT

## Zone FLGETA
mkstat = 0
zoncou = "FLGETA"
indice = 0
IMPENT = [M]FLGETA
If varinit([M]FLGETA,indice)=0
 IMPENT = 1
Endif
If varmode([M]FLGETA,indice)=0
 [M]FLGETA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLGETA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLGETA(indice)
 Endif
Endif
[M]FLGETA = IMPENT

## Zone FLGDET
mkstat = 0
zoncou = "FLGDET"
indice = 0
IMPENT = [M]FLGDET
If varinit([M]FLGDET,indice)=0
 IMPENT = 2
Endif
If varmode([M]FLGDET,indice)=0
 [M]FLGDET(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLGDET" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLGDET(indice)
 Endif
Endif
[M]FLGDET = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBCPY
mkstat = 0
zoncou = "NBCPY"
indice = 0
status = 1
zoncou = "NBCPY"
If varmode([M]NBCPY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBCPY From TRMPROACT
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBCPY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBCPY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBCPY
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBCPY = nolign
   status = 75
   Call SAI_NBCPY(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call AP_NBCPY From TRMPROACT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NBCPY" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBFCY
mkstat = 0
zoncou = "NBFCY"
indice = 0
status = 1
zoncou = "NBFCY"
SAVACT = GACTION : GACTION = "STD"
Call AV_NBFCY From TRMPROACT
GACTION = SAVACT
If mkstat=1 : Raz [M]NBFCY : Endif
If !mkstat
 If varmode([M]NBFCY,indice)=0
  SAVACT = GACTION : GACTION = "STD"
  Call AS_NBFCY From TRMPROACT
  GACTION = SAVACT
  If mkstat=1 : Raz [M]NBFCY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "NBFCY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   NBLIG = [M]NBFCY
   For nolign=1 To NBLIG
    indice = nolign-1
    [M]NBFCY = nolign
    status = 75
    Call SAI_NBFCY(IMPFIC,ACTION,IMPTRT)
    If mkstat : Break : Endif
   Next nolign
   If mkstat : End : Endif
   indice = 0
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call AP_NBFCY From TRMPROACT
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "NBFCY" : GZNUM = 0 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBCPY(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBCPY
mkstat = 0
zoncou = "NBCPY"

## Zone FLGCPY
mkstat = 0
zoncou = "FLGCPY"
IMPENT = [M]FLGCPY(indice)
WNUM = IMPENT
If varinit([M]FLGCPY,indice)=0
 IMPENT = 1
Endif
If varmode([M]FLGCPY,indice)=0
 [M]FLGCPY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLGCPY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLGCPY(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_FLGCPY(IMPENT) From TRMPROACT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FLGCPY" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CHGACTCRIT"
   Call ACTCHGCRIT From TRCPROCOM
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FLGCPY" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]FLGCPY(indice) = IMPENT

## Zone NBCPY
mkstat = 0
zoncou = "NBCPY"

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBFCY(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBFCY
mkstat = 0
zoncou = "NBFCY"

## Zone FLGFCY
mkstat = 0
zoncou = "FLGFCY"
IMPENT = [M]FLGFCY(indice)
WNUM = IMPENT
If varinit([M]FLGFCY,indice)=0
 IMPENT = 1
Endif
If varmode([M]FLGFCY,indice)=0
 [M]FLGFCY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLGFCY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLGFCY(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_FLGFCY(IMPENT) From TRMPROACT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FLGFCY" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CHGACTCRIT"
   Call ACTCHGCRIT From TRCPROCOM
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FLGFCY" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]FLGFCY(indice) = IMPENT

## Zone NBFCY
mkstat = 0
zoncou = "NBFCY"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "MCPHYCRI5"
Return

