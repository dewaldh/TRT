Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(1) = "WMBUDSTDMD"   : ABMSK(1)  = "BUD"      : AMA(1) = 1
NBMASK = 1
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 1 : If clalev([M:BUD])>0 : Default Mask [BUD] : Endif
Endcase
Return

$TITMASK
Case MA
 When 1 : If clalev([M:BUD])>0 : Gosub TITRES From W1WMBUDSTDMD : Endif
Endcase
Return

$FERME_MASK
