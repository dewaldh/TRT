Endif
Return

$REMP_DERLU
If clalev([F:CON9])>0
 Filter [CON9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [CON1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:CON9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:CON]CONNUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:CON9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:CON9]
 Endif
Endif
Return

