# Supervisor
# Mask management GAF0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone COD
mkstat = 0
zoncou = "COD"
indice = 0
If varmode([M]COD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]COD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 41525 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "GAF"
  Local Char    VALEUR(250)
  VALEUR = [M]COD
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]COD = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 41525 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DESTRA
mkstat = 0
zoncou = "DESTRA"
indice = 0
If varinit([M]DESTRA,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DESTRA,"GACCFIX","DESTRA",[M:GAF0]COD,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DESTRA,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DESTRA" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
If varinit([M]ENAFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]ENAFLG,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_ENAFLG(IMPENT) From SUBGAF
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
Endif
[M]ENAFLG = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GAF0"
Return

