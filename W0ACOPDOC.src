#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management ACOPDOC 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ALLLAN
mkstat = 0
zoncou = "ALLLAN"
indice = 0
IMPENT = [M]ALLLAN
If varinit([M]ALLLAN,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLLAN,indice)=0
 [M]ALLLAN(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLLAN" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLLAN(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_ALLLAN(IMPENT) From ACOPDOC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ALLLAN" : GZNUM = 12108 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]ALLLAN = IMPENT

## Zone LAN
mkstat = 0
zoncou = "LAN"
indice = 0
If varmode([M]LAN,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_LAN([M]LAN) From ACOPDOC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]LAN : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "LAN" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  Call CONTROLE("TLA","!",[M]LAN,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "LAN" : GZNUM = 1505 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone ALLTYP
mkstat = 0
zoncou = "ALLTYP"
indice = 0
IMPENT = [M]ALLTYP
If varinit([M]ALLTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLTYP,indice)=0
 [M]ALLTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLTYP(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_ALLTYP(IMPENT) From ACOPDOC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ALLTYP" : GZNUM = 10412 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]ALLTYP = IMPENT

## Zone TYP
mkstat = 0
zoncou = "TYP"
indice = 0
If varinit([M]TYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TYP,"910","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]TYP,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "TYP" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]TYP,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "TYP" : GZNUM = 680 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTADI"
   Call CONTADI([M]TYP,"910","") From SUBADI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "TYP" : GZNUM = 680 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("ADI","",[M]TYP,"910",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "TYP" : GZNUM = 680 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone ALLCOD
mkstat = 0
zoncou = "ALLCOD"
indice = 0
IMPENT = [M]ALLCOD
If varinit([M]ALLCOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLCOD,indice)=0
 [M]ALLCOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLCOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLCOD(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_ALLCOD(IMPENT) From ACOPDOC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ALLCOD" : GZNUM = 27400 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]ALLCOD = IMPENT

## Zone COD
mkstat = 0
zoncou = "COD"
indice = 0
If varmode([M]COD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COD" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone DOSORG
mkstat = 0
zoncou = "DOSORG"
indice = 0
WCHAR = [M]DOSORG
SAVACT = GACTION : GACTION = "STD"
Call AV_DOSORG([M]DOSORG) From ACOPDOC
GACTION = SAVACT
If mkstat=1 : Raz [M]DOSORG : Endif
If !mkstat
 If varmode([M]DOSORG,indice)=0
  SAVACT = GACTION : GACTION = "STD"
  Call AS_DOSORG([M]DOSORG) From ACOPDOC
  GACTION = SAVACT
  If mkstat=1 : Raz [M]DOSORG : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "DOSORG" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTDOS"
   Call C_DOSSIER([M]DOSORG) From DOSCONT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1090 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "ACCESDOS"
    Call CTL_DROIT_DOSSIER([M]DOSORG,"L") From DOSCONT
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1090 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     If WCHAR<>[M]DOSORG or IMPMOD
      SAVACT = GACTION : GACTION = "STD"
      Call AM_DOSORG([M]DOSORG) From ACOPDOC
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1090 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone DOSDES
mkstat = 0
zoncou = "DOSDES"
indice = 0
If varmode([M]DOSDES,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_DOSDES([M]DOSDES) From ACOPDOC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]DOSDES : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "DOSDES" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTDOS"
  Call C_DOSSIER([M]DOSDES) From DOSCONT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DOSDES" : GZNUM = 1091 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACCESDOS"
   Call CTL_DROIT_DOSSIER([M]DOSDES,"E") From DOSCONT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DOSDES" : GZNUM = 1091 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_DOSDES([M]DOSDES) From ACOPDOC
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "DOSDES" : GZNUM = 1091 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone TOUDOS
mkstat = 0
zoncou = "TOUDOS"
indice = 0
IMPENT = [M]TOUDOS
If varinit([M]TOUDOS,indice)=0
 IMPENT = 1
Endif
[M]TOUDOS = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ACOPDOC"
Return

