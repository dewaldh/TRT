Subprog AS_FMI(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(725) From COMBOS
## Action STD
Call AS_FMI(VALEUR) From CNSCSPSTD
If mkstat=2
 [M]FMI(INDIC) = VALEUR
 Affzo FMI(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,725,1))))
Endif
If !mkstat
 Discombo 0 From FMI(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From FMI(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_FMI(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
Gosub _AFF
End

