# Supervisor
## CRUD-CLASSE ## NCSAUSQA
# 2018-08-27 13:20:20 082


############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(NCSAUSQA,ASTATUS)
Variable Instance NCSAUSQA Using R_NCSAUSQA
Variable Integer ASTATUS

If NCSAUSQA = null : End : Endif

Gosub AVARIABLES From WMR00NCSAUSQA
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If fmet NCSAUSQA.ACTX.AGETAFCRIGHT(NCSAUSQA,"GESAUS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END : Endif
If fmet NCSAUSQA.ACTX.AGETAFCRIGHTC(NCSAUSQA,"GESAUS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END
Endif
If fmet NCSAUSQA.ACTX.AGETACSRIGHTM(NCSAUSQA,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AINSERT_END
  Endif
  
  Gosub AREPCONTROL_AINSERT
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
  
If !adxlog
 Gosub AINSERT_TR_NCSAUSQA
Else
 Gosub AINSERT_NCSAUSQA
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_END : Return : Endif

Gosub _AINSERT_AFTER
$AINSERT_END
[L]A_I = NCSAUSQA.freeSnapshot
NCSAUSQA.snapshotEnabled = 1
End

#-----------------------------------------------------#
$AREPCONTROL_AINSERT
NCSAUSQA.ASTALIN = [V]CST_ANEW
NCSAUSQA.AUS.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet NCSAUSQA._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet NCSAUSQA._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet NCSAUSQA._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If NCSAUSQA.AUS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
NCSAUSQA.AUS.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.AUS.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(NCSAUSQA.AUS.FNCCOL)
 If NCSAUSQA.AUS.FNCCOL([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.AUS.FNCCOL([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If NCSAUSQA.AUS.BPA(A_N2) <> null
& and !find(NCSAUSQA.AUS.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(NCSAUSQA.AUS.BID) : # BID -- 3
 If NCSAUSQA.AUS.BID(A_N2) <> null
& and !find(NCSAUSQA.AUS.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
A_N2=1
If NCSAUSQA.AUS.BPA(A_N2) <> null
& and !find(NCSAUSQA.AUS.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(NCSAUSQA.AUS.BID) : # BID -- 3
 If NCSAUSQA.AUS.BID(A_N2) <> null
& and !find(NCSAUSQA.AUS.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(NCSAUSQA.AUS.FNCCOL)
 If NCSAUSQA.AUS.FNCCOL([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.AUS.FNCCOL([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.AUS.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_NCSAUSQA
Trbegin [F:AUS]
 Gosub AINSERT_NCSAUSQA
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AINSERT_AFTER
If find(NCSAUSQA.AUS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 NCSAUSQA.AUS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(NCSAUSQA.AUS.FNCCOL)
 If NCSAUSQA.AUS.FNCCOL([L]A_I)<>null
  If NCSAUSQA.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ADEL or NCSAUSQA.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance NCSAUSQA.AUS.FNCCOL([L]A_I)
   NCSAUSQA.AUS.FNCCOL([L]A_I) = null
  Elsif NCSAUSQA.AUS.FNCCOL([L]A_I).ASTALIN <> [V]CST_ALL
   NCSAUSQA.AUS.FNCCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
Local Integer ASTATUS2
A_N2=1
If NCSAUSQA.AUS.BPA(A_N2) <> null
 If NCSAUSQA.AUS.BPA(A_N2).ASTALIN=[V]CST_ADEL or NCSAUSQA.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance NCSAUSQA.AUS.BPA(A_N2)
  NCSAUSQA.AUS.BPA(A_N2) = null
 Elsif NCSAUSQA.AUS.BPA(A_N2).ASTALIN <> [V]CST_ALL
  NCSAUSQA.AUS.BPA(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(NCSAUSQA.AUS.BPA(A_N2).COLTEL)
   If NCSAUSQA.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If NCSAUSQA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or NCSAUSQA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance NCSAUSQA.AUS.BPA(A_N2).COLTEL([L]A_I)
     NCSAUSQA.AUS.BPA(A_N2).COLTEL([L]A_I) = null
    Elsif NCSAUSQA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
     NCSAUSQA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
  For [L]A_I=1 To maxtab(NCSAUSQA.AUS.BPA(A_N2).COLWEB)
   If NCSAUSQA.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If NCSAUSQA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or NCSAUSQA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance NCSAUSQA.AUS.BPA(A_N2).COLWEB([L]A_I)
     NCSAUSQA.AUS.BPA(A_N2).COLWEB([L]A_I) = null
    Elsif NCSAUSQA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
     NCSAUSQA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(NCSAUSQA.AUS.BID)
 If NCSAUSQA.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet NCSAUSQA.AUS.BID(A_N2)._AINSERT_AFTER
  If NCSAUSQA.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or NCSAUSQA.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance NCSAUSQA.AUS.BID(A_N2)
   NCSAUSQA.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AINSERT_NCSAUSQA
[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.AUS._AINSERT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet NCSAUSQA.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If NCSAUSQA.snapshotEnabled=1
 [L]A_RETIK = NCSAUSQA.revertUpdTick
Endif
[L]ASTATUS = max(fmet NCSAUSQA.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



