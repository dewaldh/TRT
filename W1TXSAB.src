Subprog IB_FRM0(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action BOUTTEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "BOUTTEXTRA"
 Call S_AFF_CHAR(VALEUR,PARMSK,"","ATEXTRA","") From GACTION
 GACTION = SAVACT
Endif
If !GWEBSERV
 ## Action STD
 Call IB_FRM0 From SUBTXSA
Endif
Gosub _AFF
End

Subprog D_FRM0(VALEUR)
Variable Char    VALEUR()
If varinit([M]FRM0,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"TXSD","DES",[M:TXA0]TXSNAM,num$([M:TXA2]LIG(nolign-1))) From ATEXTRA
Endif
Gosub _AFF
End

Subprog AS_FRM0(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(9637,GBOUT1) From OBJDIV
Call TEXTE(10087,GBOUT2) From OBJDIV
Call IB_FRM0(VALEUR)
## Action STD
Call AS_FRM0(VALEUR) From SUBTXSA
If mkstat=2
 [M]FRM0(INDIC) = VALEUR
 Affzo FRM0(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_FRM0(zc) From W1TXSAB
Onkey 130 Titled GBOUT2 Call B2_FRM0(zc) From W1TXSAB
Gosub _AFF
End

