   FreeInstance ITMMASTERM.ITM.CDIE([L]A_I)
   ITMMASTERM.ITM.CDIE([L]A_I) = null
  Elsif ITMMASTERM.ITM.CDIE([L]A_I).ASTALIN <> [V]CST_ALL
   ITMMASTERM.ITM.CDIE([L]A_I).ASTALIN = [V]CST_ALL
