# Supervisor
# Mask management ALT2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone NBRLIG
mkstat = 0
zoncou = "NBRLIG"
indice = 0
If varmode([M]NBRLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRLIG" : Gosub ACTION From =IMPTRT
 Endif
 If [M]NBRLIG<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "NBRLIG" : GZNUM = 769 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone MAXLIG
mkstat = 0
zoncou = "MAXLIG"
indice = 0
If varmode([M]MAXLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "MAXLIG" : Gosub ACTION From =IMPTRT
 Endif
 If [M]MAXLIG<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "MAXLIG" : GZNUM = 13794 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone NBRCOL
mkstat = 0
zoncou = "NBRCOL"
indice = 0
If varmode([M]NBRCOL,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRCOL" : Gosub ACTION From =IMPTRT
 Endif
 If [M]NBRCOL<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "NBRCOL" : GZNUM = 11966 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone MAXTIM
mkstat = 0
zoncou = "MAXTIM"
indice = 0
If varmode([M]MAXTIM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "MAXTIM" : Gosub ACTION From =IMPTRT
 Endif
 If [M]MAXTIM<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "MAXTIM" : GZNUM = 13843 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone RPT
mkstat = 0
zoncou = "RPT"
indice = 0
If varmode([M]RPT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "RPT" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ARP","!",[M]RPT,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "RPT" : GZNUM = 13564 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone SEL
mkstat = 0
zoncou = "SEL"
For indice=0 To 4
 If varmode([M]SEL,indice)=0
  SAVACT = GACTION : GACTION = "STD"
  Call AS_SEL([M]SEL(indice)) From SUBALT
  GACTION = SAVACT
  If mkstat=1 : Raz [M]SEL(indice) : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "SEL" : Gosub ACTION From =IMPTRT
  Endif
 Endif
 mkstat = 0
Next indice
If mkstat : End : Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone LNK
mkstat = 0
zoncou = "LNK"
For indice=0 To 4
 If varmode([M]LNK,indice)=0
  SAVACT = GACTION : GACTION = "STD"
  Call AS_LNK([M]LNK(indice)) From SUBALT
  GACTION = SAVACT
  If mkstat=1 : Raz [M]LNK(indice) : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "LNK" : Gosub ACTION From =IMPTRT
  Endif
 Endif
 mkstat = 0
Next indice
If mkstat : End : Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ALT2"
Return

