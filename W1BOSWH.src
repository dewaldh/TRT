#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management BOSWH 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:52:41"
A_USER  = "ADMIN"
A_STAMP = "20180827235241"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask BOSWH [BOWH]
Gosub HORODAT
Local Inpbox "BOSWH" From GFONCTION At A_STAMP With A_WINDPREV Mask [BOWH] As [SIM_]
Grizo [BOWH]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [BOWH]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=112 : Y1(0)=1 : Y2(0)=2
Return

$ON_CPNAFF
Onevent CONTROL Call C_CPNAFF(zc) From W1BOSWH
Onevent BEFORE_INPUT Call AS_CPNAFF(zc) From W1BOSWH
Onevent AFTER_CHANGE Gosub AMM_CPNAFF From W1BOSWH
Return

Subprog IB_CPNAFF(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action IBPRDINF
 Call IBPRDINF(VALEUR) From PRDINFLIB
Endif
Gosub _AFF
End

Subprog AS_CPNAFF(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(2061,GBOUT1) From OBJDIV
Call TEXTE(12837,GBOUT13) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_CPNAFF(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_CPNAFF(zc) From W1BOSWH
Onkey 170 Titled GBOUT13 Call B13_CPNAFF(zc) From W1BOSWH
Onkey   2 Titled GBOUTS Call S_CPNAFF(zc) From W1BOSWH
Onkey 144 Titled GBOUTP Call P_CPNAFF(zc) From W1BOSWH
Onkey 146 Titled GBOUTA Call SA_CPNAFF(zc) From W1BOSWH
Gosub _AFF
End

Subprog C_CPNAFF(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ITMCHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ITMCHOICE"
 Call S_TAB_CHAR(VALEUR,PARMSK,"ITMCHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CPNAFF" : GZNUM = 13255 : Endif
Endif
If !mkstat
 ## Action VXADCUPC
 Call ADCCTRUPC(VALEUR) From VXADCUPC
 If mkstat : GERR = 1 : GZONE = "CPNAFF" : GZNUM = 13255 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ITM","!E",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CPNAFF" : GZNUM = 13255 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_CPNAFF
Call AM_CPNAFF(zc)
If !mkstat & GOBJET
 zoncou = "CPNAFF" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_CPNAFF(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZCPNAFF)")>indice
 [M]CPNAFF(indice) = VALEUR
 Affzo ZCPNAFF(indice)
Endif
Gosub _AFF
End

Subprog S_CPNAFF(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "ITM"
PARMSK(2) = "!E"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_CPNAFF(VALEUR)
Variable Char    VALEUR()
Call OBJET("ITM",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog B13_CPNAFF(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action PRDINFA
 Call PRDINF(VALEUR) From PRDINFLIB
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog SA_CPNAFF(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ITM"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CPNAFF(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ITM",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZCPNAFF
Onevent BEFORE_AREA Gosub AV_ZCPNAFF From W1BOSWH
Return

$AV_ZCPNAFF
Call LECTEXTRA(zc,"ITMMASTER","DES1AXX",num$([M]CPNAFF(indice)),"") From ATEXTRA
Gosub _AFF
Return

$CHG_STY
Return


