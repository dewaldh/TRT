####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMR0CCMPLASOH
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_PRODUCT From WMR1CCMPLASOH : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_PRODUCT From WMR1CCMPLASOH : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_ECCVALMAJ From WMR1CCMPLASOH : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_ECCVALMAJ From WMR1CCMPLASOH : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_ECCVALMIN From WMR1CCMPLASOH : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_ECCVALMIN From WMR1CCMPLASOH : Gosub TESTALLSTAT :  Endif
