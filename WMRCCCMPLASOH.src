If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_DEFACTIONID From WMR1CCMPLASOH : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_DEFACTIONID From WMR1CCMPLASOH : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_DEFACTIONER From WMR1CCMPLASOH : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_DEFACTIONER From WMR1CCMPLASOH : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_DEFCOMPDATE From WMR1CCMPLASOH : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_DEFCOMPDATE From WMR1CCMPLASOH : Gosub TESTALLSTAT :  Endif
