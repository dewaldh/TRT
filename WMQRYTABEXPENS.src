#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# TES --- TABEXPENS(TES)
QRY(NBREP) = NewInstance R_TABEXPENS AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:TES]AUUID)>0 & [F:TES]AUUID=NullUuid
 [F:TES]AUUID = getUuid
Endif
SetInstance QRY(NBREP).TES With [F:TES]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
Call ASETTEXTRA(QRY(NBREP).TES.AXX,"QRY","DESTRA",[F:WAXX1]TEXTE,[F:WAXX1]LANGUE) From ASYRSUB
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# TES --- TABEXPENS(TES)
QRY = NewInstance R_TABEXPENS AllocGroup PQRY
QRY.TES.AXX(1) = NewInstance C_ATXTTRA AllocGroup QRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:TES]AUUID)>0 & [F:TES]AUUID=NullUuid
 [F:TES]AUUID = getUuid
Endif
SetInstance QRY.TES With [F:TES]
QRY.TES.AXX(1).PRO = "DESTRA"
QRY.TES.AXX(1).LAN = [F:WAXX1]LANGUE
QRY.TES.AXX(1).TEX = [F:WAXX1]TEXTE
QRY.TES.AXX(1).FLG = 1
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

