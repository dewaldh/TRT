$AQUERY_DECODE_CRITERIA
NBPRO = 0
NBPRO += 1 : TBPRO(NBPRO) = "RPTCUR1" : TBFIL(NBPRO) = "[F:TES]"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "RPTCUR1"
NBPRO += 1 : TBPRO(NBPRO) = "RPTCUR2" : TBFIL(NBPRO) = "[F:TES]"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "RPTCUR2"
NBPRO += 1 : TBPRO(NBPRO) = "CODEXP" : TBFIL(NBPRO) = "[F:TES]CODEXP"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "TES.CODEXP"
NBPRO += 1 : TBPRO(NBPRO) = "DESTRA" : TBFIL(NBPRO) = "[F:WAXX1]TEXTE"
           : TBTYP(NBPRO) = 26 : TBPTH(NBPRO) = "TES.DESTRA"
ACTION = "AQUERY_DECODE_CRITERIA_AFTER" : Gosub AQUERY_ACTION
Return

