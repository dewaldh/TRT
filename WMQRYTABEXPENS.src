LogicClose File [WAXX1]
LogicClose File [ACC]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
If GACTX.ALLACS=[V]CST_AYES
 Gosub AQUERY_JOIN_ALL 
Else
 Local Char ACRITROLE(250)(1..)
 [L]ASTATUS = fmet GACTX.AGETCRITROLE(GACTX,"TES","TES",ACRITROLE,[V]CST_AFALSE)
 Link [TES] With
& [ACC]CODACC~= GACTX.USER;[F:TES]ACS,
& [F:WAXX1]AXX0 = "TABEXPENS";"DESTRA";GACTX.LAN;[F:TES]CODEXP;""
& As [LNK_]
& Where ( (1=1)
& and ([F:ACC]CONSUL=2)
& and (evalue([L]ACRITROLE))
&  )
Endif
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
