$LEC_DERLU
Read [ATY]CODTYP=[F:AGS]CODATY
If fstat : Raz [F:ATY] : Endif
Read [ATX]DERNUM=[F:ATY]INTITTYP;GLANGUE
If fstat : Raz [F:ATX] : Endif
Return

$CHARGE_DERLU
If clalev([F:AGS9])>0
 Read [AGS9] = [F:AGS9]A_TABI
 [L]CLE = [F:AGS9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:AGS9])>0
 Filter [AGS9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AGS1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AGS9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AGS]CODE+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AGS9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AGS9]
 Endif
Endif
Return

