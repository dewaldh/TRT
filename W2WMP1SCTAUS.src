GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR,GDEVISE(1..GNBDEV)),1))
Chgfmt [M]AMTCUR(indice) With "N+z3:"+GDEVFMT
Gosub _AFF
Return

Subprog C_AMTCUR(VALEUR)
Variable Decimal VALEUR
Gosub _AFF
End

$AMM_AMTCUR
