# Supervisor
# Mask management BOS2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
SAVACT = GACTION : GACTION = "STD"
Call AVANT_NBLIG From SUBBODB
GACTION = SAVACT

## Zone BOMSEQ
mkstat = 0
zoncou = "BOMSEQ"
If varinit([M]BOMSEQ,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_BOMSEQ([M]BOMSEQ(indice)) From SUBBODB
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]BOMSEQ,indice)=0
  SAVACT = GACTION : GACTION = "STD"
  Call AS_BOMSEQ([M]BOMSEQ(indice)) From SUBBODB
  GACTION = SAVACT
  If mkstat=1 : Raz [M]BOMSEQ(indice) : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "BOMSEQ" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   If [M]BOMSEQ(indice)=0
    mkstat = 1 : GMESSAGE = mess(10,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "BOMSEQ" : GZNUM = 1547 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If [M]BOMSEQ(indice)<0
     mkstat = 1 : GMESSAGE = mess(180,141,1)
    Endif
    If mkstat : GERR = 1 : GZONE = "BOMSEQ" : GZNUM = 1547 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone CPNITMREF
mkstat = 0
zoncou = "CPNITMREF"
WCHAR = [M]CPNITMREF(indice)
If varmode([M]CPNITMREF,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CPNITMREF([M]CPNITMREF(indice)) From SUBBODB
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CPNITMREF(indice) : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPNITMREF" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]CPNITMREF(indice),2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "CPNITMREF" : GZNUM = 38667 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "VXADCUPC"
   Call ADCCTRUPC([M]CPNITMREF(indice)) From VXADCUPC
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPNITMREF" : GZNUM = 38667 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("ITM","",[M]CPNITMREF(indice),"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "CPNITMREF" : GZNUM = 38667 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call C_CPNITMREF([M]CPNITMREF(indice)) From SUBBODB
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CPNITMREF" : GZNUM = 38667 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]CPNITMREF(indice) or IMPMOD
       SAVACT = GACTION : GACTION = "STD"
       Call AM_CPNITMREF([M]CPNITMREF(indice)) From SUBBODB
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "CPNITMREF" : GZNUM = 38667 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone BOMSTRDAT
mkstat = 0
zoncou = "BOMSTRDAT"
If varinit([M]BOMSTRDAT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_BOMSTRDAT([M]BOMSTRDAT(indice)) From SUBBODB
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]BOMSTRDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "BOMSTRDAT" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "STD"
  Call C_BOMSTRDAT([M]BOMSTRDAT(indice)) From SUBBODB
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "BOMSTRDAT" : GZNUM = 1536 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone BOMENDDAT
mkstat = 0
zoncou = "BOMENDDAT"
If varinit([M]BOMENDDAT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_BOMENDDAT([M]BOMENDDAT(indice)) From SUBBODB
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]BOMENDDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "BOMENDDAT" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CTLDATV"
  Call CTLDATV([M]BOMENDDAT(indice),[M:BOS2]BOMSTRDAT(nolign-1)) From CONTDIV
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "BOMENDDAT" : GZNUM = 5463 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_BOMENDDAT([M]BOMENDDAT(indice)) From SUBBODB
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "BOMENDDAT" : GZNUM = 5463 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone BOMOFS
mkstat = 0
zoncou = "BOMOFS"
If varmode([M]BOMOFS,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BOMOFS" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone LIKQTYCOD
mkstat = 0
zoncou = "LIKQTYCOD"
IMPENT = [M]LIKQTYCOD(indice)
If varinit([M]LIKQTYCOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]LIKQTYCOD,indice)=0
 [M]LIKQTYCOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LIKQTYCOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LIKQTYCOD(indice)
 Endif
Endif
[M]LIKQTYCOD(indice) = IMPENT

## Zone WBOMSTUCOE
mkstat = 0
zoncou = "WBOMSTUCOE"
If varinit([M]WBOMSTUCOE,indice)=0
 [M]WBOMSTUCOE(indice) = 1
Endif

## Zone LIKQTY
mkstat = 0
zoncou = "LIKQTY"
If varinit([M]LIKQTY,indice)=0
 [M]LIKQTY(indice) = 1
Endif
If varmode([M]LIKQTY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LIKQTY" : Gosub ACTION From =IMPTRT
 Endif
 If [M]LIKQTY(indice)<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "LIKQTY" : GZNUM = 37190 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone QTYRND
mkstat = 0
zoncou = "QTYRND"
IMPENT = [M]QTYRND(indice)
If varinit([M]QTYRND,indice)=0
 IMPENT = 1
Endif
If varmode([M]QTYRND,indice)=0
 [M]QTYRND(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "QTYRND" : Gosub ACTION From =IMPTRT
  IMPENT = [M]QTYRND(indice)
 Endif
Endif
[M]QTYRND(indice) = IMPENT

## Zone BOMSHO
mkstat = 0
zoncou = "BOMSHO"
If varmode([M]BOMSHO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BOMSHO" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone CPNTYP
mkstat = 0
zoncou = "CPNTYP"
IMPENT = [M]CPNTYP(indice)
If varinit([M]CPNTYP,indice)=0
 IMPENT = 7
Endif
[M]CPNTYP(indice) = IMPENT

## Zone CSTFLG
mkstat = 0
zoncou = "CSTFLG"
IMPENT = [M]CSTFLG(indice)
If varinit([M]CSTFLG,indice)=0
 IMPENT = 2
Endif
[M]CSTFLG(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
status = 71
SAVACT = GACTION : GACTION = "STD"
Call APRES_NBLIG From SUBBODB
GACTION = SAVACT

End

