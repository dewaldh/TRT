#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management WL1STD8 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIGCMG
mkstat = 0
zoncou = "NBLIGCMG"
indice = 0
status = 1
zoncou = "NBLIGCMG"
If varmode([M]NBLIGCMG,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBLIGCMG From SUBCRMAGD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBLIGCMG : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIGCMG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIGCMG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIGCMG = nolign
   status = 75
   Call SAI_NBLIGCMG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone DELAY
mkstat = 0
zoncou = "DELAY"
indice = 0
IMPENT = [M]DELAY
If varinit([M]DELAY,indice)=0
 IMPENT = 1
Endif
If varmode([M]DELAY,indice)=0
 [M]DELAY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DELAY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DELAY(indice)
 Endif
Endif
[M]DELAY = IMPENT

## Zone EVRCLL
mkstat = 0
zoncou = "EVRCLL"
indice = 0
IMPENT = [M]EVRCLL
WNUM = IMPENT
If varinit([M]EVRCLL,indice)=0
 IMPENT = 1
Endif
If varmode([M]EVRCLL,indice)=0
 [M]EVRCLL(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "EVRCLL" : Gosub ACTION From =IMPTRT
  IMPENT = [M]EVRCLL(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_EVRCLL(IMPENT) From SUBCRMAGD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "EVRCLL" : GZNUM = 37363 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]EVRCLL = IMPENT

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBLIGCLLX
mkstat = 0
zoncou = "NBLIGCLLX"
indice = 0
status = 1
zoncou = "NBLIGCLLX"
If varmode([M]NBLIGCLLX,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIGCLLX" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIGCLLX
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIGCLLX = nolign
   status = 75
   Call SAI_NBLIGCLLX(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIGCMG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIGCMG
mkstat = 0
zoncou = "NBLIGCMG"

## Zone NBLIGCMG
mkstat = 0
zoncou = "NBLIGCMG"

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBLIGCLLX(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIGCLLX
mkstat = 0
zoncou = "NBLIGCLLX"

## Zone ETS
mkstat = 0
zoncou = "ETS"
WCHAR = [M]ETS(indice)
SAVACT = GACTION : GACTION = "FMTTELPAY"
Call FMTTELPAY([M:CRM8]CRYX(nolign - 1)) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]ETS(indice) : Endif

## Zone MOB
mkstat = 0
zoncou = "MOB"
WCHAR = [M]MOB(indice)
SAVACT = GACTION : GACTION = "FMTTELPAY"
Call FMTTELPAY([M:CRM8]CRYX(nolign - 1)) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]MOB(indice) : Endif

## Zone CRYX
mkstat = 0
zoncou = "CRYX"
If varinit([M]CRYX,indice)=0
 [M]CRYX(indice) = GPAYS
Endif

## Zone NBLIGCLLX
mkstat = 0
zoncou = "NBLIGCLLX"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "WL1STD8"
Return

