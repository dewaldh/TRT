####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0YLDM
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YLNDPURID From WMC1YLDM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YLNDPURID From WMC1YLDM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YNBR From WMC1YLDM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YNBR From WMC1YLDM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YALLOCL From WMC1YLDM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YALLOCL From WMC1YLDM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YVALFRML From WMC1YLDM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YVALFRML From WMC1YLDM : Gosub TESTALLSTAT :  Endif
