# Supervisor
# Mask management AUSP 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone CHAPITRE
mkstat = 0
zoncou = "CHAPITRE"
indice = 0
If varinit([M]CHAPITRE,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]CHAPITRE,"901","") From SUBADI
 GACTION = SAVACT
Endif

## Zone GRPPAR
mkstat = 0
zoncou = "GRPPAR"
indice = 0
If varinit([M]GRPPAR,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]GRPPAR,"903","") From SUBADI
 GACTION = SAVACT
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBVAR
mkstat = 0
zoncou = "NBVAR"
indice = 0
status = 1
zoncou = "NBVAR"
If varmode([M]NBVAR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBVAR" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBVAR
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBVAR = nolign
   status = 75
   Call SAI_NBVAR(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBVAR(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBVAR
mkstat = 0
zoncou = "NBVAR"

## Zone VALEUR
mkstat = 0
zoncou = "VALEUR"
WCHAR = [M]VALEUR(indice)
If varmode([M]VALEUR,indice)=0
 SAVACT = GACTION : GACTION = "AFMTSAI"
 Call ASAISIE([M]VALEUR(indice),[M]TYP(nolign-1)) From FMTVAR
 GACTION = SAVACT
 If mkstat=1 : Raz [M]VALEUR(indice) : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call AS_VALEUR([M]VALEUR(indice)) From ASAIPARAUS
  GACTION = SAVACT
  If mkstat=1 : Raz [M]VALEUR(indice) : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "VALEUR" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "AFMTCONT"
   Call CONTROLE([M]VALEUR(indice),[M]TYP(nolign-1),[M]NOLIB(nolign-1),[M]LONG(nolign-1),[M]VALINT(nolign-1)) From FMTVAR
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "VALEUR" : GZNUM = 122 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "ACONTACL"
    Call CONTACL([M]VALINT(nolign-1),[M]TYP(nolign-1),"","") From SUBACL
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "VALEUR" : GZNUM = 122 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call C_VALEUR([M]VALEUR(indice)) From ASAIPARAUS
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "VALEUR" : GZNUM = 122 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]VALEUR(indice) or IMPMOD
       SAVACT = GACTION : GACTION = "STD"
       Call AM_VALEUR([M]VALEUR(indice)) From ASAIPARAUS
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "VALEUR" : GZNUM = 122 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone GRPDEF
mkstat = 0
zoncou = "GRPDEF"
If varinit([M]GRPDEF,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]GRPDEF(indice),"912","") From SUBADI
 GACTION = SAVACT
Endif

## Zone NIVEAU
mkstat = 0
zoncou = "NIVEAU"
IMPENT = [M]NIVEAU(indice)
If varinit([M]NIVEAU,indice)=0
 IMPENT = 1
Endif
[M]NIVEAU(indice) = IMPENT

## Zone NIVDEF
mkstat = 0
zoncou = "NIVDEF"
IMPENT = [M]NIVDEF(indice)
If varinit([M]NIVDEF,indice)=0
 IMPENT = 1
Endif
[M]NIVDEF(indice) = IMPENT

## Zone LEG
mkstat = 0
zoncou = "LEG"
If varinit([M]LEG,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]LEG(indice),"909","") From SUBADI
 GACTION = SAVACT
Endif

## Zone VALDEF
mkstat = 0
zoncou = "VALDEF"
IMPENT = [M]VALDEF(indice)
If varinit([M]VALDEF,indice)=0
 IMPENT = 1
Endif
[M]VALDEF(indice) = IMPENT

## Zone AUSMODIF
mkstat = 0
zoncou = "AUSMODIF"
IMPENT = [M]AUSMODIF(indice)
If varinit([M]AUSMODIF,indice)=0
 IMPENT = 1
Endif
[M]AUSMODIF(indice) = IMPENT

## Zone AUTMOD
mkstat = 0
zoncou = "AUTMOD"
IMPENT = [M]AUTMOD(indice)
If varinit([M]AUTMOD,indice)=0
 IMPENT = 1
Endif
[M]AUTMOD(indice) = IMPENT

## Zone NBVAR
mkstat = 0
zoncou = "NBVAR"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "AUSP"
Return

