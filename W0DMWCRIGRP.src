# Supervisor
# Mask management DMWCRIGRP 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone SCMCOD
mkstat = 0
zoncou = "SCMCOD"
indice = 0
WCHAR = [M]SCMCOD
If varmode([M]SCMCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SCMCOD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]SCMCOD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "SCMCOD" : GZNUM = 56828 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("DMWSC","",[M]SCMCOD,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "SCMCOD" : GZNUM = 56828 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WCHAR<>[M]SCMCOD or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_SCMCOD([M]SCMCOD) From CNSDMWGSTD
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "SCMCOD" : GZNUM = 56828 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone STRDAT
mkstat = 0
zoncou = "STRDAT"
indice = 0
If varinit([M]STRDAT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_STRDAT([M]STRDAT) From CNSDMWGSTD
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]STRDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "STRDAT" : Gosub ACTION From =IMPTRT
  Endif
  If [M]STRDAT=[0/0/0]
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 5956 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone ENDDAT
mkstat = 0
zoncou = "ENDDAT"
indice = 0
If varinit([M]ENDDAT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_ENDDAT([M]ENDDAT) From CNSDMWGSTD
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]ENDDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "ENDDAT" : Gosub ACTION From =IMPTRT
  Endif
  If [M]ENDDAT=[0/0/0]
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "ENDDAT" : GZNUM = 978 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "DATDEBFIN"
   Call DATDEBFIN([M]ENDDAT,[M]STRDAT) From EXEFNC
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "ENDDAT" : GZNUM = 978 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone BPR
mkstat = 0
zoncou = "BPR"
indice = 0
If varmode([M]BPR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BPR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("BPR","!",[M]BPR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "BPR" : GZNUM = 3662 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
indice = 0
If varmode([M]ITMREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREF" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "VXADCUPC"
 Call ADCCTRUPC([M]ITMREF) From VXADCUPC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("ITM","!",[M]ITMREF,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone MEMO
mkstat = 0
zoncou = "MEMO"
indice = 0
If varmode([M]MEMO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "MEMO" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "CTLSLASH"
 Call CTLSLASH([M]MEMO) From CONTOBJ
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "MEMO" : GZNUM = 6123 : Endif
 Gosub AFF From GSAISIE
