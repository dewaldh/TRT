# Supervisor
# Mask management AFT1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ALLFCT
mkstat = 0
zoncou = "ALLFCT"
indice = 0
IMPENT = [M]ALLFCT
If varinit([M]ALLFCT,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLFCT,indice)=0
 [M]ALLFCT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLFCT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLFCT(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_ALLFCT(IMPENT) From SUBAFT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ALLFCT" : GZNUM = 13996 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]ALLFCT = IMPENT

## Zone FLGPOR
mkstat = 0
zoncou = "FLGPOR"
indice = 0
IMPENT = [M]FLGPOR
If varinit([M]FLGPOR,indice)=0
 IMPENT = 1
Endif
If varmode([M]FLGPOR,indice)=0
 [M]FLGPOR(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLGPOR" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLGPOR(indice)
 Endif
Endif
[M]FLGPOR = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone ALLACS
mkstat = 0
zoncou = "ALLACS"
indice = 0
IMPENT = [M]ALLACS
If varinit([M]ALLACS,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLACS,indice)=0
 [M]ALLACS(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLACS" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLACS(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_ALLACS(IMPENT) From SUBAFT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ALLACS" : GZNUM = 14179 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]ALLACS = IMPENT

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBACC
mkstat = 0
zoncou = "NBACC"
indice = 0
status = 1
zoncou = "NBACC"
If varmode([M]NBACC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBACC" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBACC
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBACC = nolign
   status = 75
   Call SAI_NBACC(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBACC(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBACC
mkstat = 0
zoncou = "NBACC"

## Zone CODACC
mkstat = 0
zoncou = "CODACC"
WCHAR = [M]CODACC(indice)
If varmode([M]CODACC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CODACC" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CODACC(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CODACC" : GZNUM = 71 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("ACS","",[M]CODACC(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CODACC" : GZNUM = 71 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_CODACC([M]CODACC(indice)) From SUBAFT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CODACC" : GZNUM = 71 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WCHAR<>[M]CODACC(indice) or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_CODACC([M]CODACC(indice)) From SUBAFT
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CODACC" : GZNUM = 71 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone CONSUL
mkstat = 0
zoncou = "CONSUL"
IMPENT = [M]CONSUL(indice)
WNUM = IMPENT
If varinit([M]CONSUL,indice)=0
 IMPENT = 1
Endif
If varmode([M]CONSUL,indice)=0
 [M]CONSUL(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "CONSUL" : Gosub ACTION From =IMPTRT
  IMPENT = [M]CONSUL(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_CONSUL(IMPENT) From SUBAFT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CONSUL" : GZNUM = 31737 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]CONSUL(indice) = IMPENT

## Zone MODIF
mkstat = 0
zoncou = "MODIF"
IMPENT = [M]MODIF(indice)
If varinit([M]MODIF,indice)=0
 IMPENT = [M]CONSUL(nolign-1)=2
Endif
If varmode([M]MODIF,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_MODIF(IMPENT) From SUBAFT
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]MODIF(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "MODIF" : Gosub ACTION From =IMPTRT
  IMPENT = [M]MODIF(indice)
 Endif
Endif
[M]MODIF(indice) = IMPENT

## Zone EXEC
mkstat = 0
zoncou = "EXEC"
IMPENT = [M]EXEC(indice)
If varinit([M]EXEC,indice)=0
 IMPENT = 1
Endif
If varmode([M]EXEC,indice)=0
 [M]EXEC(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "EXEC" : Gosub ACTION From =IMPTRT
  IMPENT = [M]EXEC(indice)
 Endif
Endif
[M]EXEC(indice) = IMPENT

## Zone NBACC
mkstat = 0
zoncou = "NBACC"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "AFT1"
Return

