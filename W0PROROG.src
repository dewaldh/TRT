# Supervisor
# Mask management PROROG 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ACCDAT1
mkstat = 0
zoncou = "ACCDAT1"
indice = 0
If varinit([M]ACCDAT1,indice)=0
 [M]ACCDAT1 = date$
Endif
If varmode([M]ACCDAT1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ACCDAT1" : Gosub ACTION From =IMPTRT
 Endif
 If [M]ACCDAT1=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ACCDAT1" : GZNUM = 2512 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone NUM
mkstat = 0
zoncou = "NUM"
If varmode([M]NUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NUM" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]NUM(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "NUM" : GZNUM = 9379 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_NUM([M]NUM(indice)) From PROROG
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NUM" : GZNUM = 9379 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone AMTCUR
mkstat = 0
zoncou = "AMTCUR"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]AMTCUR(indice) : Endif

## Zone DUDDAT1
mkstat = 0
zoncou = "DUDDAT1"
If varmode([M]DUDDAT1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DUDDAT1" : Gosub ACTION From =IMPTRT
 Endif
 If [M]DUDDAT1(indice)=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DUDDAT1" : GZNUM = 2736 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone DES
mkstat = 0
zoncou = "DES"
If varmode([M]DES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "PROROG"
Return

