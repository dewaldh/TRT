# Supervisor
# Mask management VWHTA0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone SCPY
mkstat = 0
zoncou = "SCPY"
indice = 0
If varmode([M]SCPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SCPY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTCPY"
 Call CONTSOC([M]SCPY) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "SCPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("CPY","!",[M]SCPY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "SCPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone SRPTNUM
mkstat = 0
zoncou = "SRPTNUM"
indice = 0
If varmode([M]SRPTNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SRPTNUM" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone SFCY
mkstat = 0
zoncou = "SFCY"
indice = 0
If varmode([M]SFCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SFCY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTFCY"
 Call CONTSITE([M]SFCY) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "SFCY" : GZNUM = 1344 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("FCY","!",[M]SFCY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "SFCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DAT
mkstat = 0
zoncou = "DAT"
indice = 0
If varmode([M]DAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone SELECT
mkstat = 0
zoncou = "SELECT"
IMPENT = [M]SELECT(indice)
WNUM = IMPENT
If varinit([M]SELECT,indice)=0
 IMPENT = 1
Endif
If varmode([M]SELECT,indice)=0
 [M]SELECT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "SELECT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]SELECT(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_SELECT(IMPENT) From SUBVWHTA
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "SELECT" : GZNUM = 42317 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]SELECT(indice) = IMPENT

## Zone ALLCPY
mkstat = 0
zoncou = "ALLCPY"
IMPENT = [M]ALLCPY(indice)
If varinit([M]ALLCPY,indice)=0
 IMPENT = 1
Endif
[M]ALLCPY(indice) = IMPENT

## Zone ALLFCY
mkstat = 0
zoncou = "ALLFCY"
IMPENT = [M]ALLFCY(indice)
If varinit([M]ALLFCY,indice)=0
 IMPENT = 1
Endif
[M]ALLFCY(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "VWHTA0"
Return

