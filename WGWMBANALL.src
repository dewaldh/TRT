# Supervisor
# Window management WMBANALL

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [28/08/2018]
A_HEURE = "00:46:25"
A_USER  = "ADMIN"
Call ISWINVALID(nomap,GLANGUE,"WMBANALL",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
Local Char NAVMSK(200)
NAVMSK="CBA1;CBA2;"
Return

$OUVRE_BOITE
Gosub INIT_BOITE
Gosub DEF_BOITE
Gosub AFF_BOITE
Return

$INIT_BOITE
Local Char    OBJLIS(GLONAOB)(0..10), DESLIS(80)(0..10), BOX1(10)
Local Integer CHGLIS(0..10), BROLIS(0..10), TRELIS(0..10)
Local Char    SAIRAP(50)(1..16)
Local Integer DERLU , AFOLDEF
Local Char    ABRENA(GLONABR)
DERLU = 0
AFOLDEF = 1
ABRENA = ""
BOX1 = "GAU_CHE"
If clalev([M:CBA1])=0 : Local Mask CONSBAN1 [CBA1] : Endif
If clalev([M:CBA2])=0 : Local Mask WMBANALL [CBA2] : Endif
Gosub TIT
nolign = 1
Return

$DEF_BOITE
Gosub HORODAT From WGWMBANALL
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
If GFUSION=1
  A_WINDPREV=";;;FCNSBAN"
Endif
Local Inpbox "WMBANALL" From GFONCTION At A_STAMP With A_WINDPREV Mask [CBA1]
&   Folder Mask [CBA2]
& As [CBA2]
Titled [CBA2] TIT
Return

$VISU_BOITE
Gosub INIT_BOITE
Gosub HORODAT From WGWMBANALL
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "WMBANALL" From GFONCTION At A_STAMP With A_WINDPREV Mask [CBA1]
&   Folder Mask [CBA2]
& As [CBA2]
Titled [CBA2] TIT
Gosub AFF_BOITE
Return

$AFF_BOITE
Boxact [CBA2] Folder AFOLDEF
Return

$SAISIE_BOITE
Boxinp [CBA2] Using REPONSE
Return

$FERME_BOITE
Onerrgo RIEN
Close Local Inpbox [CBA2]
Onerrgo
Return

$RIEN
Resume

$TIT
Local Char TIT(60) , TITTRN(60)
Call TEXTE(22865,TIT) From OBJDIV
If GPROGRAMME<>""
 If dim([L]ACTION)<0 : Local Char ACTION(20) : Endif
 [L]ACTION = "TITRE" : Gosub ACTION From =GPROGRAMME
Endif
Return

$REMPLIT_
Return

$DEFAUT_BRO
Return

$SETBOX
Return

$SETORDER
Return

$ENDORDER
Return

$PICKING
Return

$DEPICK
Return

$REPICK
Return

$SETCUR
Return

$DEFVAR
Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "CONSBAN1"     : ABMSK(0)  = "CBA1"     : AMA(0) = 1
NOMMSK(1) = "WMBANALL"     : ABMSK(1)  = "CBA2"     : AMA(1) = 1
NBMASK = 1
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 0 : If clalev([M:CBA1])>0 : Default Mask [CBA1] : Endif
 When 1 : If clalev([M:CBA2])>0 : Default Mask [CBA2] : Endif
Endcase
Return

$TITMASK
Case MA
 When 0 : If clalev([M:CBA1])>0 : Gosub TITRES From W1CONSBAN1 : Endif
 When 1 : If clalev([M:CBA2])>0 : Gosub TITRES From W1WMBANALL : Endif
Endcase
Return

$FERME_MASK
If clalev([M:CBA1])>0 : Close Local Mask [CBA1] : Endif
If clalev([M:CBA2])>0 : Close Local Mask [CBA2] : Endif
Return

$RAZMASK
Case MA
 When 0 : If clalev([M:CBA1])>0 : Raz [M:CBA1] : Endif
 When 1 : If clalev([M:CBA2])>0 : Raz [M:CBA2] : Endif
Endcase
Return

$AFFMASK
Case MA
 When 0 : If clalev([M:CBA1])>0 : Affzo [M:CBA1] : Endif
 When 1 : If clalev([M:CBA2])>0 : Affzo [M:CBA2] : Endif
Endcase
Return

$OUVMSKCMP
Return

$FERMSKCMP
Return

$RAZMSKCMP
Return

