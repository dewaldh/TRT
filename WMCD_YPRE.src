############################################################################################
### Subprog _ADELETE
############################################################################################
Subprog _ADELETE(YPRE,ASTATUS)
Variable Instance YPRE Using C_YPRE
Variable Integer ASTATUS

If YPRE = null : End : Endif

Gosub _AVARIABLES From WMC00YPRE

[L]A_STRSITE = ""
[L]A_STRACC = ""
Local Instance ALNKDELTAB Using C_ALNKDELTAB
Gosub ADELETE_YPRE

End

############################################################################################
### Subprog ADELETE
############################################################################################
Subprog ADELETE(YPRE,ASTATUS)
Variable Instance YPRE Using C_YPRE
Variable Integer ASTATUS

If YPRE = null : End : Endif

Gosub AVARIABLES From WMC00YPRE
If [L]ASTATUS>=[V]CST_AERROR : Goto ADELETE_END : Endif

Local Instance ALNKDELTAB Using C_ALNKDELTAB

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub ADELETE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto ADELETE_END : Endif

If !adxlog
 Gosub ADELETE_TR_YPRE
Else
 Gosub ADELETE_YPRE
Endif

$ADELETE_END
End


$ADELETE_CONTROL
Gosub _ADELETE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Verification delete
[L]ASTATUS = max([L]ASTATUS,func ASYRSUB.VERIFY_INSTANCE(YPRE,ALNKDELTAB))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _ADELETE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_ADELETE_CONTROL_BEFORE
If YPRE.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YPRE.ASTALIN = [V]CST_ADEL
[L]ASTATUS = max([L]ASTATUS,fmet YPRE.ACTION("ADELETE_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YPRE.YGSC) : # YGSC -- 2
 If YPRE.YGSC(A_N2) <> null
& and YPRE.YGSC(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YPRE.YREL) : # YREL -- 3
 If YPRE.YREL(A_N2) <> null
& and YPRE.YREL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YPRE.YREL(A_N2).ASTALIN<>[V]CST_ANEW
   YPRE.YREL(A_N2).ASTALIN = [V]CST_ADEL
   [L]ASTATUS = max([L]ASTATUS,fmet YPRE.YREL(A_N2)._ADELETE_CONTROL_BEFORE)
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YPRE.YSTA) : # YSTA -- 4
 If YPRE.YSTA(A_N2) <> null
& and YPRE.YSTA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YPRE.YSTA(A_N2).ASTALIN<>[V]CST_ANEW
   YPRE.YSTA(A_N2).ASTALIN = [V]CST_ADEL
   [L]ASTATUS = max([L]ASTATUS,fmet YPRE.YSTA(A_N2)._ADELETE_CONTROL_BEFORE)
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_ADELETE_CONTROL_AFTER
For A_N2=1 To maxtab(YPRE.YGSC) : # YGSC -- 2
 If YPRE.YGSC(A_N2) <> null
& and YPRE.YGSC(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YPRE.YREL) : # YREL -- 3
 If YPRE.YREL(A_N2) <> null
& and YPRE.YREL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  [L]ASTATUS = max([L]ASTATUS,fmet YPRE.YREL(A_N2)._ADELETE_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YPRE.YSTA) : # YSTA -- 4
 If YPRE.YSTA(A_N2) <> null
& and YPRE.YSTA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  [L]ASTATUS = max([L]ASTATUS,fmet YPRE.YSTA(A_N2)._ADELETE_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet YPRE.ACTION("ADELETE_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$ADELETE_TR_YPRE
Trbegin [F:YPRC]
 Gosub ADELETE_YPRE
 If [L]ASTATUS>=[V]CST_AERROR : Gosub ADELETE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$ADELETE_YPRE
[L]ASTATUS = max([L]ASTATUS,fmet YPRE.ACTION("ADELETE_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

#-----------------------------------------------------#
For A_N2=1 To maxtab(YPRE.YGSC)
 If YPRE.YGSC(A_N2) <> null
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

#-----------------------------------------------------#
For A_N2=1 To maxtab(YPRE.YREL)
 If YPRE.YREL(A_N2) <> null
  [L]ASTATUS = max([L]ASTATUS,fmet YPRE.YREL(A_N2)._ADELETE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

#-----------------------------------------------------#
For A_N2=1 To maxtab(YPRE.YSTA)
 If YPRE.YSTA(A_N2) <> null
  [L]ASTATUS = max([L]ASTATUS,fmet YPRE.YSTA(A_N2)._ADELETE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

[L]ASTATUS = func ASYRSUB.DELETE_LINKS(YPRE,ALNKDELTAB)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YPRE._MAJSQL_YPRECINT(YPRE))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet YPRE.ACTION("ADELETE_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$ADELETE_ABORT
Rollback
$_ADELETE_ABORT
If YPRE.snapshotEnabled=1
 [L]A_RETIK = YPRE.revertUpdTick
Endif
[L]ASTATUS = max(fmet YPRE.ACTION("ADELETE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#

#-----------------------------------------------------#
$_ADELETE_AFTER
Return



