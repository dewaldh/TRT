$ON_ORICRY
Onevent BEFORE_INPUT Call AS_ORICRY(zc) From W4WE3ALL9
Onevent CONTROL Call C_ORICRY(zc) From W4WE3ALL9
Onevent INIT_AREA Call D_ORICRY(zc) From W4WE3ALL9
Return

Subprog D_ORICRY(VALEUR)
Variable Char    VALEUR()
If varinit([M]ORICRY,indice)=0
 VALEUR = GPAYS
Endif
Gosub _AFF
End

Subprog AS_ORICRY(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(53316,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action PASLIN
If GMODIF<>2 mkstat=2 : Endif
If mkstat=2
 [M]ORICRY(INDIC) = VALEUR
 Affzo ORICRY(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_ORICRY(zc) From W4WE3ALL9
Onkey   2 Titled GBOUTS Call S_ORICRY(zc) From W4WE3ALL9
Onkey 144 Titled GBOUTP Call P_ORICRY(zc) From W4WE3ALL9
Onkey 146 Titled GBOUTA Call SA_ORICRY(zc) From W4WE3ALL9
Gosub _AFF
End

Subprog C_ORICRY(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("TCY","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "ORICRY" : GZNUM = 263 : Endif
Gosub _AFF
End

Subprog S_ORICRY(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "TCY"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_ORICRY(VALEUR)
Variable Char    VALEUR()
Call OBJET("TCY",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_ORICRY(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TCY"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_ORICRY(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TCY",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

