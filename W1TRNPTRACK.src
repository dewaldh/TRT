Subprog D_TRKCOD(VALEUR)
Variable Char    VALEUR()
If varinit([M]TRKCOD,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"108","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_TRKCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"108") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_TRKCOD(zc) From W1TRNPTRACK
Onkey   2 Titled GBOUTS Call S_TRKCOD(zc) From W1TRNPTRACK
Onkey 144 Titled GBOUTP Call P_TRKCOD(zc) From W1TRNPTRACK
Onkey 146 Titled GBOUTA Call SA_TRKCOD(zc) From W1TRNPTRACK
If VALEUR="" : Call INIADI(VALEUR,"108") From TRTDIV : Endif
Gosub _AFF
End

Subprog C_TRKCOD(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "TRKCOD" : GZNUM = 12056 : Endif
If !mkstat
 ## Action CONTADI
 Call CONTADI(VALEUR,"108","") From SUBADI
 If mkstat : GERR = 1 : GZONE = "TRKCOD" : GZNUM = 12056 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ADI","",VALEUR,"108",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "TRKCOD" : GZNUM = 12056 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_TRKCOD
