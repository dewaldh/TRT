Subprog AM_TRKCOD(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZTRKCOD)")>indice
 [M]TRKCOD(indice) = VALEUR
 Affzo ZTRKCOD(indice)
Endif
Gosub _AFF
End

