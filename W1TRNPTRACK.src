Subprog AS_TRKNUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_TRKNUM(zc) From W1TRNPTRACK
Onkey 144 Titled GBOUTP Call P_TRKNUM(zc) From W1TRNPTRACK
Onkey 146 Titled GBOUTA Call SA_TRKNUM(zc) From W1TRNPTRACK
Gosub _AFF
End

Subprog C_TRKNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CONTVCR
 Call CONTVCR(VALEUR) From TRTDIV
 If mkstat : GERR = 1 : GZONE = "TRKNUM" : GZNUM = 55826 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("TRACKT","!B",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "TRKNUM" : GZNUM = 55826 : Endif
Endif
Gosub _AFF
End

$AMM_TRKNUM
Call AM_TRKNUM(zc)
If !mkstat & GOBJET
 zoncou = "TRKNUM" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_TRKNUM(VALEUR)
Variable Char    VALEUR()
## Action TRACKTPL
Local Char    PARMSK(250)(1..20) , SAVACT(20)
SAVACT = GACTION : GACTION = "TRACKTPL"
PARMSK(1) = "TRNPT"
Call TRAITE_CHAR(VALEUR,PARMSK,"","SUBTRACKTPL","",1,"") From GTRAITE
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "TRKNUM" : GZNUM = 55826 : Endif
Gosub _AFF
End

Subprog S_TRKNUM(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "TRACKT"
PARMSK(2) = "!B"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_TRKNUM(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TRACKT"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_TRKNUM(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TRACKT",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

