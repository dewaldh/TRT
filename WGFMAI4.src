NMEN(NBMEN) = 10 : Call TEXTE(19357,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "H" : LMEN(NBMEN) = 2001 : Call TEXTE(16732,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "I" : LMEN(NBMEN) = 2002 : Call TEXTE(16731,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "J" : LMEN(NBMEN) = 2003 : Call TEXTE(19358,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "K" : LMEN(NBMEN) = 2004 : Call TEXTE(19359,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "L" : LMEN(NBMEN) = 2005 : Call TEXTE(18297,TMEN(NBMEN)) From OBJDIV
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "A"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "SAIMAX3"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"FMAX3","TRTMAXY","",1,"XYTRTMAXY") From GTRAITE
 When "C"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "SAIMAX3V"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"FMAX3V","TRTMAXYV","",1,"XYTRTMAXYV") From GTRAITE
 When "E"
  FLGEXE = 1
  GACTION = "STD"
 When "H"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "UNION"
  FLGEXE = 1
  GACTION = "SAIMAX4"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"FMAX4","TRTMAX4","XWFMAX4") From GSAISIE
 When "I"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "INTER"
  FLGEXE = 1
  GACTION = "SAIMAX4"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"FMAX4","TRTMAX4","XWFMAX4") From GSAISIE
 When "J"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "UNIONX"
  FLGEXE = 1
  GACTION = "SAIMAX4"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"FMAX4","TRTMAX4","XWFMAX4") From GSAISIE
 When "K"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "INTERX"
  FLGEXE = 1
  GACTION = "SAIMAX4"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"FMAX4","TRTMAX4","XWFMAX4") From GSAISIE
 When "L"
  FLGEXE = 1
  GACTION = "STD"
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "S"
  FLGEXE = 1
  GACTION = "STD"
Endcase
