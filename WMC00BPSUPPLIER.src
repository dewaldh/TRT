Gosub TEST From WMCR_BPSUPPLIER : End

############################################################################################
## R : Subprog AREAD(INSTANCE,BPSNUM)
############################################################################################
# BPSUPPLIER : BPSUPPLIER
# BPARTNER : BPSUPPLIER BPARTNER
############################################################################################

############################################################################################
### AVARIABLES
############################################################################################
$AVARIABLES
[L]ASTATUS =max([L]ASTATUS,fmet BPSUPPLIER._ADELERRORS([V]CST_ALLERRORS))
$_AVARIABLES
Local    Integer     A_LEVEL,A_I,A_NEWCOL,A_N1,A_N2,A_S2
Local    Integer    ASTATUS,A_RETIK,A_TYPMAJ,A_ASTALIN
Local    Char        A_STRSITE(GLONFCY),A_STRACC(GLONACS),CURPRO(50),A_PARENT(50),A_PATH(200),A_ABR(GLONABR),A_ERROR(250)
Local    Uuident    A_UUID

[L]A_LEVEL    = BPSUPPLIER.ACTX.LEVWML
[L]ASTATUS  = 0

Onerrgo ERR_OPENTAB
If find(BPSUPPLIER.ACTX.AFOLDER,nomap,"")>0
 Local File BPSUPPLIER [BPS]
 Local File BPARTNER [BPR]
Else
 Local File =BPSUPPLIER.ACTX.AFOLDER+".BPSUPPLIER" [BPS]
 Local File =BPSUPPLIER.ACTX.AFOLDER+".BPARTNER" [BPR]
Endif
Onerrgo
Return

$ERR_OPENTAB
[L]ASTATUS = max([L]ASTATUS,fmet BPSUPPLIER.ASETERROR("",errp+"("+num$(errl)+")"-mess(6,123,1)-num$(errn)-errmes$(errn)-errm,[V]CST_AERROR))
Resume

############################################################################################
### Link
############################################################################################
$AOPEN_LINK
Gosub AVARIABLES
Link [BPS] With
& [BPR]BPR0 ~= [F:BPS]BPSNUM
& As [LNK_]
Return



