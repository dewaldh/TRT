############################################################################################
# Program TEST
############################################################################################
$TEST
Local Integer SILENCE
Call OUVRE_TRACE("Test program")From LECFIC

Gosub ATRT_TEST

Call LEC_TRACE From LECFIC
Call SUPP_TRACE From LECFIC


Return

#---------------------------------------------------------------#
$ATRT_TEST
Local Integer ASTATUS
If !SILENCE : Call TRACE("1 --> Open main table" ) From ASYRCOM : Endif
Local File TABEECNAT [TEC_]

If !SILENCE : Call TRACE("2 --> Local Instance R_TABEECNAT") From ASYRCOM : Endif
Local Instance TABEECNAT Using R_TABEECNAT

If !SILENCE : Call TRACE("3 --> New Instance R_TABEECNAT") From ASYRCOM : Endif
TABEECNAT = NewInstance R_TABEECNAT AllocGroup null

If !SILENCE : Call TRACE("4 --> Reading main table, Read First") From ASYRCOM : Endif
Read [TEC_]TEC0 First
If !fstat
 If !SILENCE : Call TRACE("Call program read of TABEECNAT") From ASYRCOM : Endif
 [L]ASTATUS = max([L]ASTATUS,fmet TABEECNAT.AREAD([TEC_]EECNAT,[TEC_]LEG))
Endif

If !SILENCE
 Call TRACE("5 --> Check :"-num$([L]ASTATUS)) From ASYRCOM
 Call TRACE("TABEECNAT.TEC.EECNAT ="-num$(TABEECNAT.TEC.EECNAT)) From ASYRCOM
 Call TRACE("TABEECNAT.TEC.LEG ="-num$(TABEECNAT.TEC.LEG)) From ASYRCOM
 Call TRACE("6 --> List of keys :") From ASYRCOM
 For I=1 To maxtab(TABEECNAT.TEC.AKEYS)
  Call TRACE("-"-num$(I)-":"-TABEECNAT.TEC.AKEYS(I)) From ASYRCOM
 Next
Endif
Close Local File [TEC_]
FreeGroup TABEECNAT
Return

############################################################################################
### Funprog _AREADETAG
############################################################################################
Funprog _AREADETAG(TABEECNAT,ACLE,ASTATUS,_ETAG)
Variable Instance TABEECNAT Using R_TABEECNAT
Value Char ACLE
Variable Integer ASTATUS
Variable Char _ETAG
[L]ASTATUS = [V]CST_AOK
Local Char _EECNAT(5)
Local Char _LEG(20)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_EECNAT = num$([L]CLE(1)) : Endif
If [L]CLE(2)<>"" : [L]_LEG = num$([L]CLE(2)) : Endif
Raz [L]_ETAG
Local File TABEECNAT [TEC]
Default File [F:TEC]
Read [F:TEC]TEC0 = [L]_EECNAT;[L]_LEG
If fstat
 [L]ASTATUS = [V]CST_AERROR
Endif
If [L]ASTATUS=[V]CST_AOK and dim([F:TEC]UPDTICK)>0
 [L]_ETAG = num$([F:TEC]UPDTICK)
Endif
End [L]ASTATUS

############################################################################################
### Subprog _AREADSUP
############################################################################################
Subprog _AREADSUP(TABEECNAT,ACLE,ASTATUS)
Variable Instance TABEECNAT Using R_TABEECNAT
Value Char ACLE
Variable Integer ASTATUS
Local Char _EECNAT(5)
Local Char _LEG(20)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_EECNAT = num$([L]CLE(1)) : Endif
If [L]CLE(2)<>"" : [L]_LEG = num$([L]CLE(2)) : Endif
[L]ASTATUS = max([L]ASTATUS,fmet TABEECNAT.AREAD([L]_EECNAT,[L]_LEG))
End

############################################################################################
### Subprog AREAD
############################################################################################
Subprog AREAD(TABEECNAT,_EECNAT,_LEG,ASTATUS)
Variable Instance TABEECNAT Using R_TABEECNAT
Value Char _EECNAT
Value Char _LEG
Variable Integer ASTATUS

If TABEECNAT = null : End : Endif

Gosub AVARIABLES From WMR00TABEECNAT
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If TABEECNAT.APARENT=null
 [L]A_I = TABEECNAT.freeSnapshot
 TABEECNAT.snapshotEnabled = 0
 TABEECNAT.setAccessorEnabled = 0
Endif

If fmet TABEECNAT.ACTX.AGETAFCRIGHT(TABEECNAT,"GESTEC",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END : Endif
Default File [F:TEC]
Read [F:TEC]TEC0 = [L]_EECNAT;[L]_LEG
If fstat
[L]A_ERROR = [L]_EECNAT+"~"+[L]_LEG
& -":"-mess(23,100,1)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(TABEECNAT,"",0,[L]ASTATUS,[V]CST_AHTTP_NOT_FOUND,[L]A_ERROR) : Goto AREAD_END
Endif
[L]A_STRSITE = ""
[L]A_STRACC = ""
If fmet TABEECNAT.ACTX.AGETAFCRIGHTR(TABEECNAT,"GESTEC",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END
Endif
If fmet TABEECNAT.ACTX.ASTACRITROLE(TABEECNAT,[L]_EECNAT+"~"+[L]_LEG
& ,"TEC","TEC",[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END 
Endif

[L]ASTATUS = max([L]ASTATUS,fmet TABEECNAT.ACTION("AREAD_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

TABEECNAT.TEC.setAccessorEnabled = 0
[L]ASTATUS = max([L]ASTATUS,fmet TABEECNAT.TEC.AREAD([L]_EECNAT,[L]_LEG))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

[L]ASTATUS = max([L]ASTATUS,fmet TABEECNAT.ACTION("AREAD_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If [L]ASTATUS<[V]CST_AERROR & TABEECNAT.APARENT=null
 # Load Link
 [L]ASTATUS = max([L]ASTATUS,fmet TABEECNAT._ALNKPRO)
 If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
Endif

$AREAD_END
If TABEECNAT.APARENT=null
 TABEECNAT.snapshotEnabled = 1
 TABEECNAT.setAccessorEnabled = 1
Endif
LogicClose File [TEC]
End



