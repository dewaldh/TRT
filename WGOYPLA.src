Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "YPLA"         : ABMSK(0)  = "YPLA"     : AMA(0) = 1
NOMMSK(1) = "YPLA1"        : ABMSK(1)  = "YPLA1"    : AMA(1) = 1
NOMMSK(2) = "YPLA2"        : ABMSK(2)  = "YPLA2"    : AMA(2) = 1
NOMMSK(3) = "YCSWEB"       : ABMSK(3)  = "YCS0"     : AMA(3) = 1
