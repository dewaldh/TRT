#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management TXSM1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:17:18"
A_USER  = "ADMIN"
A_STAMP = "20180828001718"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIG"
  If [M]NBLIG=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask TXSM1 [TXM1]
Gosub HORODAT
Local Inpbox "TXSM1" From GFONCTION At A_STAMP With A_WINDPREV Mask [TXM1] As [SIM_]
Grizo [TXM1]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [TXM1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(1)=1 : X2(1)=66 : Y1(1)=1 : Y2(1)=24
Return

$ON_SAICOL
Onevent INIT_AREA Call D_SAICOL(zc) From W1TXSM1
Return

Subprog D_SAICOL(VALEUR)
Variable Integer VALEUR
If varinit([M]SAICOL,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_NAMCOL
Onevent INIT_AREA Call D_NAMCOL(zc) From W1TXSM1
Return

Subprog D_NAMCOL(VALEUR)
Variable Char    VALEUR()
If varinit([M]NAMCOL,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"TXSA","NAMCOL",TXSNAM,num$(nolign)) From ATEXTRA
Endif
Gosub _AFF
End

$ON_NBLIG
Onevent BEFORE_INPUT Call AS_NBLIG From W1TXSM1
Onevent BEFORE_AREA Call AV_NBLIG From W1TXSM1
Return

Subprog AV_NBLIG
Local Decimal BASPAG
If adxgtb=1 & !GORITITCOL & nolign=1
 Local Char BASPAG(10)
 BASPAG = "NBLIG" : Gosub TIT_COL From W1TXSM1
Endif
Gosub _AFF
End

Subprog AS_NBLIG
Local Decimal BASPAG
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(24415,GBOUT1) From OBJDIV
## Action STD
Call AS_NBLIG From SUBTXWM
Onkey 129 Titled GBOUT1 Call B1_NBLIG From W1TXSM1
Gosub _AFF
End

Subprog B1_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action TXSFMTC
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "TXSFMTC"
 Call SAISIE_NUM(BASPAG,PARMSK,"DTXSFMTC","TXSFMTC","XWDTXSFMTC") From GSAISIE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

