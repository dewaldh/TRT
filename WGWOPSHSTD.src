$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "2"
  FLGEXE = 1
  GACTION = "TRTPSHGEN"
  Call TRTPSHGEN From TRTACHPSH1
 When "3"
  FLGEXE = 1
  GACTION = "TRTCLEPSH"
  Call TRTCLEPSH From TRTACHPSH1
 When "F"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:PSH1]TEX1
  PARBOUT(2) = "PSH"
  PARBOUT(3) = "TRTX3TEX"
  PARBOUT(4) = ""
  PARBOUT(5) = num$(0)
  PARBOUT(6) = num$(1)
  FLGEXE = 1
  GACTION = "ACLOB"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ACLOB","ACLOB",3,"XYACLOB") From GTRAITE
  [M:PSH1]TEX1 = PARBOUT(1)
 When "G"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:PSH1]TEX2
  PARBOUT(2) = "PSH"
  PARBOUT(3) = "TRTX3TEX"
  PARBOUT(4) = ""
  PARBOUT(5) = num$(0)
  PARBOUT(6) = num$(1)
  FLGEXE = 1
  GACTION = "ACLOB"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ACLOB","ACLOB",3,"XYACLOB") From GTRAITE
  [M:PSH1]TEX2 = PARBOUT(1)
 When "H"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:PSH0]PSHNUM
  FLGEXE = 1
  GACTION = "VISUPOHPSH"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"DPSOW","TRTACHFEN1","XWDPSOW") From GSAISIE
 When "I"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GPTRNUM
  FLGEXE = 1
  GACTION = "VISUPTD"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IPTD","SELTRS","XWIPTD") From GSAISIE
 When "N"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "PSH"
  PARBOUT(2) = [M:PSH0]PSHNUM
  PARBOUT(3) = num$("")
  FLGEXE = 1
  GACTION = "CONSPIA"
  Call CONSULT_CHAR(VALBOUT,"PIA",PARBOUT) From GCONSULT
Endcase
GACTION = SAVACT
Return

$EXEACT
