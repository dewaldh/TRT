$ON_YENTTYP
Onevent CONTROL Call C_YENTTYP(zc) From W1YSRT
Onevent BEFORE_INPUT Call AS_YENTTYP(zc) From W1YSRT
Return

Subprog AS_YENTTYP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(5152,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YENTTYP(zc) From W1YSRT
Onkey   2 Titled GBOUTS Call S_YENTTYP(zc) From W1YSRT
Onkey 144 Titled GBOUTP Call P_YENTTYP(zc) From W1YSRT
Onkey 146 Titled GBOUTA Call SA_YENTTYP(zc) From W1YSRT
Gosub _AFF
End

Subprog C_YENTTYP(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YENTTYP" : GZNUM = 101127 : Endif
If !mkstat
 ## Action CONTGTE
 Call CONTGTE(VALEUR,"","") From SUBGTE
 If mkstat : GERR = 1 : GZONE = "YENTTYP" : GZNUM = 101127 : Endif
 If !mkstat
  ## Action CONTOBJN
  Local Char    PARMSK(250)(1..20) , SAVACT(20)
  SAVACT = GACTION : GACTION = "CONTOBJN"
  Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJN","CONTOBJ","") From GACTION
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "YENTTYP" : GZNUM = 101127 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_YENTTYP(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELGTE
 Call SELGTE(VALEUR,"","","") From SUBGTE
Endif
Gosub _AFF
End

Subprog B1_YENTTYP(VALEUR)
Variable Char    VALEUR()
Call OBJET("GTE",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_YENTTYP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "GTE"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YENTTYP(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("GTE",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

