############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(YLNDPUR,ASTATUS)
Variable Instance YLNDPUR Using C_YLNDPUR
Variable Integer ASTATUS

If YLNDPUR = null : End : Endif

Gosub _AVARIABLES From WMC00YLNDPUR

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_YLNDPUR

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YLNDPUR,ASTATUS)
Variable Instance YLNDPUR Using C_YLNDPUR
Variable Integer ASTATUS

If YLNDPUR = null : End : Endif

Gosub AVARIABLES From WMC00YLNDPUR
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
YLNDPUR.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_YLNDPUR
Else
 Gosub AUPDATE_YLNDPUR
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YLNDPUR.freeSnapshot
YLNDPUR.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YLNDPUR._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YLNDPUR._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YLNDPUR._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If YLNDPUR.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YLNDPUR.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(YLNDPUR.ASTALIN)<>""
 [L]A_ASTALIN = YLNDPUR.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(YLNDPUR.YLDS) : # YLDS -- 2
 If YLNDPUR.YLDS(A_N2) <> null
& and YLNDPUR.YLDS(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YLNDPUR.YLDS(A_N2).ASTALIN<>[V]CST_ANEW & YLNDPUR.ASTALIN=[V]CST_ADEL
   YLNDPUR.YLDS(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YLNDPUR.YLDS(A_N2).ASTALIN=[V]CST_ANEW & find(YLNDPUR.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YLNDPUR.YLDS(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YLNDPUR.YLDS(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDS(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLNDPUR.YLDM) : # YLDM -- 3
 If YLNDPUR.YLDM(A_N2) <> null
& and YLNDPUR.YLDM(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YLNDPUR.YLDM(A_N2).ASTALIN<>[V]CST_ANEW & YLNDPUR.ASTALIN=[V]CST_ADEL
   YLNDPUR.YLDM(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YLNDPUR.YLDM(A_N2).ASTALIN=[V]CST_ANEW & find(YLNDPUR.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YLNDPUR.YLDM(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YLNDPUR.YLDM(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDM(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLNDPUR.YLPR) : # YLPR -- 4
 If YLNDPUR.YLPR(A_N2) <> null
& and YLNDPUR.YLPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YLNDPUR.YLPR(A_N2).ASTALIN<>[V]CST_ANEW & YLNDPUR.ASTALIN=[V]CST_ADEL
   YLNDPUR.YLPR(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YLNDPUR.YLPR(A_N2).ASTALIN=[V]CST_ANEW & find(YLNDPUR.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YLNDPUR.YLPR(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YLNDPUR.YLPR(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLPR(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(YLNDPUR.YLDS) : # YLDS -- 2
 If YLNDPUR.YLDS(A_N2) <> null
& and YLNDPUR.YLDS(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YLNDPUR.YLDS(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDS(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLNDPUR.YLDM) : # YLDM -- 3
 If YLNDPUR.YLDM(A_N2) <> null
& and YLNDPUR.YLDM(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YLNDPUR.YLDM(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLDM(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLNDPUR.YLPR) : # YLPR -- 4
 If YLNDPUR.YLPR(A_N2) <> null
& and YLNDPUR.YLPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YLNDPUR.YLPR(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YLNDPUR.YLPR(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(YLNDPUR.ASTALIN)<>""
 [L]A_ASTALIN = YLNDPUR.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_YLNDPUR
Trbegin [F:YLDP]
 Gosub AUPDATE_YLNDPUR
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_YLNDPUR
[L]A_ASTALIN = YLNDPUR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YLNDPUR._MAJSQL_YLNDPUR(YLNDPUR))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 For A_N2=1 To maxtab(YLNDPUR.YLDS)
  If YLNDPUR.YLDS(A_N2) <> null and
& (YLNDPUR.YLDS(A_N2).ASTALIN=[L]A_TYPMAJ or
& YLNDPUR.YLDS(A_N2).ASTALIN=[V]CST_ALL)
   If YLNDPUR.YLDS(A_N2).ASTALIN=[L]A_TYPMAJ
    Case YLNDPUR.YLDS(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDS(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(YLNDPUR.YLDM)
  If YLNDPUR.YLDM(A_N2) <> null and
& (YLNDPUR.YLDM(A_N2).ASTALIN=[L]A_TYPMAJ or
& YLNDPUR.YLDM(A_N2).ASTALIN=[V]CST_ALL)
   If YLNDPUR.YLDM(A_N2).ASTALIN=[L]A_TYPMAJ
    Case YLNDPUR.YLDM(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLDM(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(YLNDPUR.YLPR)
  If YLNDPUR.YLPR(A_N2) <> null and
& (YLNDPUR.YLPR(A_N2).ASTALIN=[L]A_TYPMAJ or
& YLNDPUR.YLPR(A_N2).ASTALIN=[V]CST_ALL)
   If YLNDPUR.YLPR(A_N2).ASTALIN=[L]A_TYPMAJ
    Case YLNDPUR.YLPR(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.YLPR(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = YLNDPUR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YLNDPUR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If YLNDPUR.snapshotEnabled=1
 [L]A_RETIK = YLNDPUR.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDS)
 If YLNDPUR.YLDS(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDS(A_N2)._AUPDATE_ABORT
  If YLNDPUR.YLDS(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDS(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDS(A_N2)
   YLNDPUR.YLDS(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDM)
 If YLNDPUR.YLDM(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDM(A_N2)._AUPDATE_ABORT
  If YLNDPUR.YLDM(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDM(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDM(A_N2)
   YLNDPUR.YLDM(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLPR)
 If YLNDPUR.YLPR(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLPR(A_N2)._AUPDATE_ABORT
  If YLNDPUR.YLPR(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLPR(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLPR(A_N2)
   YLNDPUR.YLPR(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet YLNDPUR.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(YLNDPUR.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YLNDPUR.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDS)
 If YLNDPUR.YLDS(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDS(A_N2)._AUPDATE_AFTER
  If YLNDPUR.YLDS(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDS(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDS(A_N2)
   YLNDPUR.YLDS(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLDM)
 If YLNDPUR.YLDM(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLDM(A_N2)._AUPDATE_AFTER
  If YLNDPUR.YLDM(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLDM(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLDM(A_N2)
   YLNDPUR.YLDM(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLNDPUR.YLPR)
 If YLNDPUR.YLPR(A_N2) <> null
  [L]ASTATUS2 = fmet YLNDPUR.YLPR(A_N2)._AUPDATE_AFTER
  If YLNDPUR.YLPR(A_N2).ASTALIN=[V]CST_ADEL or YLNDPUR.YLPR(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLNDPUR.YLPR(A_N2)
   YLNDPUR.YLPR(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



