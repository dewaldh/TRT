############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(SORDER,ASTATUS)
Variable Instance SORDER Using R_SORDER
Variable Integer ASTATUS

If SORDER = null : End : Endif

Gosub AVARIABLES From WMR00SORDER
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet SORDER.ACTX.AGETAFCRIGHT(SORDER,"GESSOH",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet SORDER.ACTX.AGETAFCRIGHTU(SORDER,"GESSOH",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_SORDER
Else
 Gosub AUPDATE_SORDER
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = SORDER.freeSnapshot
SORDER.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
SORDER.ASTALIN = [V]CST_AUPD
SORDER.SOH.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = SORDER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet SORDER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = SORDER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet SORDER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet SORDER._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet SORDER._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet SORDER._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

