# Supervisor
# Mask management ABC1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone COD
mkstat = 0
zoncou = "COD"
indice = 0
If varmode([M]COD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]COD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 1802 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "ABC"
  Local Char    VALEUR(250)
  VALEUR = [M]COD
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]COD = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 1802 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DES
mkstat = 0
zoncou = "DES"
indice = 0
If varmode([M]DES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DES" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBRDAT
mkstat = 0
zoncou = "NBRDAT"
indice = 0
status = 1
zoncou = "NBRDAT"
If varmode([M]NBRDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRDAT" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRDAT
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRDAT = nolign
   status = 75
   Call SAI_NBRDAT(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBRDAT(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBRDAT
mkstat = 0
zoncou = "NBRDAT"

## Zone STRDAT
mkstat = 0
zoncou = "STRDAT"
If varmode([M]STRDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "STRDAT" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_STRDAT([M]STRDAT(indice)) From SUBABC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 1488 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone ENDDAT
mkstat = 0
zoncou = "ENDDAT"
If varmode([M]ENDDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENDDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NBRDAT
mkstat = 0
zoncou = "NBRDAT"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ABC1"
Return

