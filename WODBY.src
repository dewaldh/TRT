$REMP_DERLU
If clalev([F:SMH9])>0
 Filter [SMH9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [DBY1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:SMH9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:SMH]VCRNUM+"~"
 A_CLEDERLU += num$([F:SMH]VCRTYP)+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:SMH9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:SMH9]
 Endif
Endif
Return

