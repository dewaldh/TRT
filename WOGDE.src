$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(5)(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
Local Char     A_BUF3(30)(0..GNBDERNIER)
Local Char     A_BUF4(20)(0..GNBDERNIER)
If clalev([F:GDE9])>0 : Close Local File [GDE9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(5),
& Char     A_TAB2(30),
& Char     A_TAB3(30),
& Char     A_TAB4(20)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4
& As [GDE9]
Return

$LEC_DERLU
Read [ACC]CODACC = GUSER ; [F:GDE]ACS
If fstat : Raz [F:ACC] : Endif
Read [AXX1]AXX0="GDIAENTRY";"DESTRA";GLANGUE;[F:GDE]DIA;""
If fstat : Raz [F:AXX1] : Endif
Read [AXX2]AXX0="GDIAENTRY";"SHOTRA";GLANGUE;[F:GDE]DIA;""
If fstat : Raz [F:AXX2] : Endif
Return

$CHARGE_DERLU
If clalev([F:GDE9])>0
 Read [GDE9] = [F:GDE9]A_TABI
 [L]CLE = [F:GDE9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:GDE9])>0
 Filter [GDE9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [GDE1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:GDE9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:GDE]DIA+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:GDE9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:GDE9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 0
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:GDE]DIA"
Call TEXTE(3211,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:5X"
SELFOR(NBSEL) = "K:5B"
SELMAJ(NBSEL) = 1
NBTRI += 1
TRIZON(NBTRI) = "[F:GDE]DIA"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(2)"
SELEXP(NBSEL) = "[F:AXX1]TEXTE"
Call TEXTE(8,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = "K:30X"
SELMAJ(NBSEL) = 0
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(3)"
SELEXP(NBSEL) = "[F:AXX2]TEXTE"
Call TEXTE(4010,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = "K:30X"
SELMAJ(NBSEL) = 0
NBSEL += 1
SELZON(NBSEL) = "[F:GDE]LEG"
Call TEXTE(26213,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:20X"
SELFOR(NBSEL) = "K:20X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:GDE]LEG"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:GDE_])=0
 Gosub OUVFIC_LISTE
Endif
Return

