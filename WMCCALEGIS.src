####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0ALEGIS
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CODE From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CODE From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LNGDES From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LNGDES From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_SHODES From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_SHODES From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_DEFVAL From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_DEFVAL From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_ENAFLG From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_ENAFLG From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDAT From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDAT From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREDAT From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREDAT From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDUSR From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDUSR From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREUSR From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREUSR From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREDATTIM From WMC1ALEGIS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREDATTIM From WMC1ALEGIS : Gosub TESTALLSTAT :  Endif
