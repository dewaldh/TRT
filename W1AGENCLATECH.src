#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management AGENCLATECH 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:48:22"
A_USER  = "ADMIN"
A_STAMP = "20180827234822"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask AGENCLATECH [AGEN]
Gosub HORODAT
Local Inpbox "AGENCLATECH" From GFONCTION At A_STAMP With A_WINDPREV Mask [AGEN] As [SIM_]
Grizo [AGEN]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [AGEN]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=20 : X2(0)=93 : Y1(0)=14 : Y2(0)=15
Return

$ON_AFOLDER
Onevent AFTER_CHANGE Gosub AMM_AFOLDER From W1AGENCLATECH
Onevent CONTROL Call C_AFOLDER(zc) From W1AGENCLATECH
Onevent BEFORE_INPUT Call AS_AFOLDER(zc) From W1AGENCLATECH
Onevent INIT_AREA Call D_AFOLDER(zc) From W1AGENCLATECH
Return

Subprog D_AFOLDER(VALEUR)
Variable Char    VALEUR()
If varinit([M]AFOLDER,indice)=0
 VALEUR = nomap
Endif
Gosub _AFF
End

Subprog AS_AFOLDER(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_AFOLDER(zc) From W1AGENCLATECH
Onkey 144 Titled GBOUTP Call P_AFOLDER(zc) From W1AGENCLATECH
Onkey 146 Titled GBOUTA Call SA_AFOLDER(zc) From W1AGENCLATECH
Gosub _AFF
End

Subprog C_AFOLDER(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "AFOLDER" : GZNUM = 10035 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("ADS","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "AFOLDER" : GZNUM = 10035 : Endif
Endif
Gosub _AFF
End

$AMM_AFOLDER
Call AM_AFOLDER(zc)
If !mkstat & GOBJET
 zoncou = "AFOLDER" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_AFOLDER(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action APASSWD
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "APASSWD"
 PARMSK(1) = ""
 PARMSK(2) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"APASSWD","APASSWD","SPEPASSWD") From GSAISIE
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "AFOLDER" : GZNUM = 10035 : Endif
Endif
If !mkstat & evalue("dim("+clanam([M])+"ZAFOLDER)")>indice
 [M]AFOLDER(indice) = VALEUR
 Affzo ZAFOLDER(indice)
Endif
Gosub _AFF
End

Subprog S_AFOLDER(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "ADS"
PARMSK(2) = ""
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_AFOLDER(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADS"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_AFOLDER(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ADS",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZAFOLDER
Onevent BEFORE_AREA Gosub AV_ZAFOLDER From W1AGENCLATECH
Return

$AV_ZAFOLDER
If clalev([F:ADS])=0 : Local File ADOSSIER [ADS] : Endif
Call LECTURE("ADS",num$([M]AFOLDER(indice)),"") From CONTOBJ
zc = [F:ADS]NOMDOS
Gosub _AFF
Return

$CHG_STY
Return


