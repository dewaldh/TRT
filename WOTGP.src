SELFMT(NBSEL) = "K:20X"
SELFOR(NBSEL) = "K:20X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:TGP]TGRSSPNUM"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
