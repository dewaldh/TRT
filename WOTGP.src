Return

$CHARGE_DERLU
If clalev([F:TGP9])>0
 Read [TGP9] = [F:TGP9]A_TABI
 [L]CLE = [F:TGP9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:TGP9])>0
 Filter [TGP9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [TGP1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:TGP9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:TGP]TGRSSPNUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:TGP9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:TGP9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
