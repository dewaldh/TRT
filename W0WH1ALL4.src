# Supervisor
# Mask management WH1ALL4 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone ITNTYP
mkstat = 0
zoncou = "ITNTYP"
IMPENT = [M]ITNTYP(indice)
If varinit([M]ITNTYP,indice)=0
 IMPENT = 1
Endif
[M]ITNTYP(indice) = IMPENT

## Zone LND
mkstat = 0
zoncou = "LND"
IMPENT = [M]LND(indice)
If varinit([M]LND,indice)=0
 IMPENT = 1
Endif
[M]LND(indice) = IMPENT

## Zone ENDITN
mkstat = 0
zoncou = "ENDITN"
If varmode([M]ENDITN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENDITN" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone SECPRI
mkstat = 0
zoncou = "SECPRI"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:MAC4]SECCUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]SECPRI(indice) : Endif
If !mkstat
 If varmode([M]SECPRI,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "SECPRI" : Gosub ACTION From =IMPTRT
  Endif
  If [M]SECPRI(indice)<0
   mkstat = 1 : GMESSAGE = mess(180,141,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "SECPRI" : GZNUM = 14561 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone SECCUR
mkstat = 0
zoncou = "SECCUR"
If varmode([M]SECCUR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SECCUR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("TCU","!",[M]SECCUR(indice),"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "SECCUR" : GZNUM = 20650 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone ORI
mkstat = 0
zoncou = "ORI"
IMPENT = [M]ORI(indice)
If varinit([M]ORI,indice)=0
 IMPENT = 1
Endif
[M]ORI(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "WH1ALL4"
Return

