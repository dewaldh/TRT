Subprog GETACT(NOL,TITBOUT,VALBOUT)
Value Integer NOL
Variable Char TITBOUT()(1..) , VALBOUT()(1..)
Local Integer ONOL , NBBOUT
ONOL   = nolign
nolign = NOL
indice = max(0,nolign-1)
Call AS_NBLIG From ="W1CONSCPT2"
NBBOUT = 0
If GBOUT1<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT1
 VALBOUT(NBBOUT) = "CPT"
 VALBOUT(NBBOUT) += "~B1"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans(GBIDC2,"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans(GBIDC1,"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans(GBIDC3,"~","¨")
 Onerrgo
Endif
If GBOUT2<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT2
 VALBOUT(NBBOUT) = "CPT"
 VALBOUT(NBBOUT) += "~B2"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans(GBIDC2,"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans(GBIDC1,"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans(GBIDC3,"~","¨")
 Onerrgo
Endif
If GBOUT3<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT3
 VALBOUT(NBBOUT) = "CPT"
 VALBOUT(NBBOUT) += "~B3"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 Onerrgo
Endif
If GBOUT4<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT4
 VALBOUT(NBBOUT) = "CPT"
 VALBOUT(NBBOUT) += "~B4"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 Onerrgo
Endif
If GBOUT5<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT5
 VALBOUT(NBBOUT) = "CPT"
 VALBOUT(NBBOUT) += "~B5"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans("1","~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("","~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("","~","¨")
 Onerrgo
Endif
If GBOUT6<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT6
 VALBOUT(NBBOUT) = "CPT"
 VALBOUT(NBBOUT) += "~B6"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans("","~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("MWI","~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("[M:CPT2]FCYLIN(nolign-1)","~","¨")
 Onerrgo
Endif
nolign = ONOL
indice = max(0,nolign-1)
End

Subprog ACTRUN(INFO)
Value Char INFO
Local Char    BOUT(3)
Local Integer ONOL
ONOL    = nolign
BOUT    = func AFNC.PART(INFO,1)
nolign  = val(func AFNC.PART(INFO,2))
indice  = max(0,nolign-1)
Gosub NOMFIC
For MA=0 to dim(NOMMSK)-1
 Gosub OUVRE_MSK
Next MA
For MA=0 to dim(NOMMSK)-1
 Gosub SETMASK
Next MA
Case BOUT
 When "B1"
  Onerrgo RIEN
  Assign "GBIDC2" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Assign "GBIDC1" With ctrans(func AFNC.PART(INFO,4),"¨","~")
  Assign "GBIDC3" With ctrans(func AFNC.PART(INFO,5),"¨","~")
  Onerrgo
  Call B1_NBLIG From ="W1CONSCPT2"
 When "B2"
  Onerrgo RIEN
  Assign "GBIDC2" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Assign "GBIDC1" With ctrans(func AFNC.PART(INFO,4),"¨","~")
  Assign "GBIDC3" With ctrans(func AFNC.PART(INFO,5),"¨","~")
  Onerrgo
  Call B2_NBLIG From ="W1CONSCPT2"
 When "B3"
  Onerrgo RIEN
  Onerrgo
  Call B3_NBLIG From ="W1CONSCPT2"
 When "B4"
  Onerrgo RIEN
  Onerrgo
  Call B4_NBLIG From ="W1CONSCPT2"
 When "B5"
  Onerrgo RIEN
  Assign "1" With val(ctrans(func AFNC.PART(INFO,3),"¨","~"))
  Onerrgo
  Call B5_NBLIG From ="W1CONSCPT2"
 When "B6"
  Onerrgo RIEN
  Onerrgo
  Call B6_NBLIG From ="W1CONSCPT2"
Endcase
nolign = ONOL
indice = max(0,nolign-1)
End

$RIEN
Resume


