Subprog D_HDKSAT(VALEUR)
Variable Char    VALEUR()
If varinit([M]HDKSAT,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"422","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_HDKSAT(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"422") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_HDKSAT(zc) From W1WH5STD1
Onkey   2 Titled GBOUTS Call S_HDKSAT(zc) From W1WH5STD1
Onkey 144 Titled GBOUTP Call P_HDKSAT(zc) From W1WH5STD1
Onkey 146 Titled GBOUTA Call SA_HDKSAT(zc) From W1WH5STD1
Gosub _AFF
End

Subprog C_HDKSAT(VALEUR)
Variable Char    VALEUR()
## Action CONTADI
Call CONTADI(VALEUR,"422","") From SUBADI
If mkstat : GERR = 1 : GZONE = "HDKSAT" : GZNUM = 1179 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("ADI","!",VALEUR,"422",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "HDKSAT" : GZNUM = 1179 : Endif
Endif
Gosub _AFF
End

$AMM_HDKSAT
Call AM_HDKSAT(zc)
If !mkstat & GOBJET
 zoncou = "HDKSAT" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_HDKSAT(VALEUR)
