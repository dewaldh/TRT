If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_LEG From WMC1GACM : Gosub TESTALLSTAT :  Endif
For AINDCLA=1 To 10
 If this.LISLED(AINDCLA)<>null& find(this.LISLED(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.LISLED(AINDCLA)._AMANDATORY : Gosub TESTALLSTAT
