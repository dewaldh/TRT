####################################################################

$_AMANDATORY
Gosub DECLARE_VAR From WMC0GACM
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_GCM From WMC1GACM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_DES From WMC1GACM : Gosub TESTALLSTAT :  Endif
