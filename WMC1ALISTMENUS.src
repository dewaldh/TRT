####################################################################
# 1 : CODPRF Profile code ( Field APM 5)
####################################################################

$SET_CODPRF
If [L]_AOLDVAL<>this.CODPRF
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_CODPRF
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.CODPRF
 Gosub PROPAGATE_CODPRF
Endif
Return

$INIT_CODPRF
Gosub DECLARE_VAR From WMC0ALISTMENUS
[L]CURPRO = "CODPRF"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_CODPRF
Gosub DECLARE_VAR From WMC0ALISTMENUS
[L]CURPRO = "CODPRF"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_CODPRF                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_CODPRF                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_CODPRF
[L]CURPRO = "CODPRF"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.CODPRF,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_CODPRF
[L]CURPRO = "CODPRF"
If vireblc(this.CODPRF,2)<>vireblc(format$("K:5c",this.CODPRF),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_CODPRF
[L]CURPRO = "CODPRF"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_CODPRF
[L]CURPRO = "CODPRF"
If vireblc(this.CODPRF,2)<>""
 If fmet this.AGETATTRIBUTE([L]CURPRO,"$namProParent")=""
  [L]ASTATUS =  func WMTYPAPM.ACTRLREF(this,"CODPRF",this.CODPRF,this.CODPRF,this.MODULE,"")
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Else
  If this.CODPRF<>evalue("this.APARENT."+fmet this.AGETATTRIBUTE([L]CURPRO,"$namProParent"))
   [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(83,123,1)) :  Return : # Invalid value
  Endif
 Endif
Endif
Return

$PROPAGATE_CODPRF
this.isReadonly = 0
[L]CURPRO = "CODPRF"
Gosub _LNKPRO_CODPRF
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_CODPRF
Gosub DECLARE_VAR From WMC0ALISTMENUS
[L]CURPRO = "CODPRF"
$_LNKPRO_CODPRF
If vireblc(this.CODPRF,2)<>""
 # Link APROFIL
 Local File APROFIL [WAPF]
 Read [F:WAPF]APF1 = this.CODPRF;this.MODULE
 If !fstat
  Local File ATEXTRA [WAXX]
  Local Char ILAN(GLONLAN)
  For ILAN=this.ACTX.LAN, this.ACTX.AFOLD.ALANGDEF
   For [F:WAXX] Where CODFIC="APROFIL" & ZONE="INTPRF" & IDENT1="0" & IDENT2=[F:WAPF]CODPRF & LANGUE = ILAN
    [L]ASTATUS = fmet this.ASETATTRIBUTE("CODPRF","$description",[WAXX]TEXTE)
    Break 2
   Next WAXX
  Next ILAN
  LogicClose File [WAXX]
 Endif
 LogicClose File [WAPF]
Endif
Return


