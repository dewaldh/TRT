$ASEARCH_TO_JSON
If FINS=0 : Return : Endif
QCOUNT += 1
Onerrgo ERROR_VALUE
Append ASND.DATA ,SEPINS1
SEPINS1 = ','+chr$(10)
Append ASND.DATA ,'{'
Append ASND.DATA ,'"TYP":'             +'"'+escJson(INS.TYP)+'"'
Append ASND.DATA ,',"$access":'        +'"'+escJson(INS.ACS)+'"'
Append ASND.DATA ,',"LEG":'            +'"'+escJson(INS.LEG)+'"'
Append ASND.DATA ,'}'
Onerrgo
FINS = 0
Return

