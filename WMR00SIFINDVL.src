$AVARIABLES
[L]ASTATUS =max([L]ASTATUS,fmet SIFINDVL._ADELERRORS([V]CST_ALLERRORS))
$_AVARIABLES
Local    Integer     A_LEVEL,A_I,A_NEWCOL,A_N1
