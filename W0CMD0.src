#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CMD0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone HTRTYP
mkstat = 0
zoncou = "HTRTYP"
indice = 0
IMPENT = [M]HTRTYP
If varinit([M]HTRTYP,indice)=0
 IMPENT = 1
Endif
[M]HTRTYP = IMPENT

## Zone HTRNUM
mkstat = 0
zoncou = "HTRNUM"
indice = 0
If varmode([M]HTRNUM,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_HTRNUM([M]HTRNUM) From SUBCMD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]HTRNUM : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "HTRNUM" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone DESAXX
mkstat = 0
zoncou = "DESAXX"
indice = 0
If varinit([M]DESAXX,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DESAXX,"HDKTRS","DESAXX",num$(HTRTYP),HTRNUM) From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DESAXX,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DESAXX" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]DESAXX,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "DESAXX" : GZNUM = 6468 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
If varinit([M]ENAFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]ENAFLG,indice)=0
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
Endif
[M]ENAFLG = IMPENT

## Zone ACSCOD
mkstat = 0
zoncou = "ACSCOD"
indice = 0
If varmode([M]ACSCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ACSCOD" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ACS","!",[M]ACSCOD,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "ACSCOD" : GZNUM = 729 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CTLACS"
  Call CTLACS([M]ACSCOD,"E") From CONTX3
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ACSCOD" : GZNUM = 729 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone GFY
mkstat = 0
zoncou = "GFY"
indice = 0
If varmode([M]GFY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "GFY" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("AGF","!",[M]GFY,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "GFY" : GZNUM = 616 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CMD0"
Return

