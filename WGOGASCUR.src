$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [18/01/2019]
A_HEURE = "01:19:12"
A_USER  = "MB"
Call ISWINVALID(nomap,GLANGUE,"OGASCUR",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
