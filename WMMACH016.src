# Sage X3 supervisor
## LISTE ## ACH016 Contracts / Product

$PARAM
Local Integer PARTYP(1..99)
PARTYP(1) = 7
PARTYP(2) = 7
PARTYP(3) = 7
PARTYP(4) = 7
PARTYP(5) = 7
PARTYP(6) = 7
PARTYP(7) = 8
Return

$OUVRE
If clalev([F:POQ])=0 : Local File PORDERQ [POQ] : Endif
If clalev([F:POP])=0 : Local File PORDERP [POP] : Endif
If clalev([F:BPS])=0 : Local File BPSUPPLIER [BPS] : Endif

Local Char AFILOPE(250)
AFILOPE = ""
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[POQ]"
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[POP]"
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[BPS]"

CRITSUP(1) = "([F:POQ]POHTYP=2 & [F:POQ]LINCLEFLG<>2)"
Gosub REMPLACE_SUP From SUBALH9

Link [POQ] With
&   [POP]POP0 ~= [POQ]POHNUM;[POQ]POPLIN;[POQ]POQSEQ
& , [BPS]BPS0 = [POQ]BPSINV
& As [LNK]
& Where evalue(CRITERE) & evalue(CRITSUP)
& Order by Key CLE = [F:POP]ITMREF;[F:POQ]POHFCY;[F:POQ]BPSNUM;[F:POQ]POHNUM

Columns [LNK] ([F:POP]ITMREF,[F:POP]ITMDES1,[F:POQ]QTYPUU,[F:POQ]PUU,
& [F:POQ]POHFCY,[F:POQ]BPSNUM,[F:BPS]BPSNAM,[F:POQ]POHNUM,[F:POQ]DEMENDDAT,
& [F:POQ]POPLIN,[F:POQ]POQSEQ)
Return

$LIENS
If dim(AFILOPE)>0 & AFILOPE<>"" : Default File AFILOPE : Endif
Default [F],[L],[S],[V],[M]
Default [L],[S],[V],[M],[F]
Return

$CONT
OKCONT = 1
Return

$DEFVAR
[L]NBGRP = 3
[L]NBCOL = 10
[L]NBLIG = 100
[L]GRP(1) = 1
[L]COLGRP = 1
[L]SNSGRP = 2
[L]GRP(5) = 2
[L]GRP(6) = 3
[L]CUM(10) = 1
[L]TRI(1) = 3
[L]TRI(2) = 10
[L]TRI(3) = 10
[L]TRI(4) = 10
[L]AOBSIT = "PRHFCY"
[L]AOBACC = ""
[L]AOBFIC = "POP"
Return

$TUNLIEN
Local Char    TLIEN(30)(1..16) , TTYP(GLONATY)(1..16)
Local Decimal TLONG(1..16)
Local Integer TMEN(1..16)
Case NOBOUT
 When 1
  TTYP(1)  = "ITM"      : TLONG(1)  = 20    : TMEN(1)  = 0    : TLIEN(1)  = "01"
 When 2
  TTYP(1)  = "UOM"      : TLONG(1)  = 3     : TMEN(1)  = 0    : TLIEN(1)  = "04"
 When 3
  TTYP(1)  = "FCY"      : TLONG(1)  = 5     : TMEN(1)  = 0    : TLIEN(1)  = "05"
 When 4
  TTYP(1)  = "BPS"      : TLONG(1)  = 15    : TMEN(1)  = 0    : TLIEN(1)  = "06"
 When 5
  TTYP(1)  = "VCR"      : TLONG(1)  = 20    : TMEN(1)  = 0    : TLIEN(1)  = "08"
 When Default
Endcase
Return

$BOUTONS
Raz GBOUT1, GBOUT2, GBOUT3, GBOUT4, GBOUT5, GBOUT6, GBOUT7, GBOUT8
Raz GBOUT9, GBOUT10, GBOUT11, GBOUT12, GBOUT13, GBOUT14, GBOUT15, GBOUT16
If [M:LST]C01(nolign-1) <> "" 
 If [M:LST]NIV >= 1
  Call TEXTE(2061,GBOUT1) From OBJDIV
  [M:LST]TUNNEL(1) = "ITM"
 Endif
Endif
If [M:LST]C04(nolign-1) <> "" 
 Call TEXTE(3612,GBOUT2) From OBJDIV
 [M:LST]TUNNEL(2) = "TUN"
Endif
If [M:LST]C05(nolign-1) <> "" 
 If [M:LST]NIV >= 2
  Call TEXTE(1427,GBOUT3) From OBJDIV
  [M:LST]TUNNEL(3) = "FCY"
 Endif
Endif
If [M:LST]C06(nolign-1) <> "" 
 If [M:LST]NIV >= 3
  Call TEXTE(1515,GBOUT4) From OBJDIV
  [M:LST]TUNNEL(4) = "BPS"
 Endif
Endif
If [M:LST]C08(nolign-1) <> "" 
 Call TEXTE(33920,GBOUT5) From OBJDIV
 [M:LST]TUNNEL(5) = "POH"
Endif
Return

$LECTURE
If I=1
 Default File "[POQ] , [POP] , [BPS]"
Endif
Case I
 When 1: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:POP]ITMREF
 When 2: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:POP]ITMDES1
 When 3: [F:ALR]TYP = 4 : [F:ALR]NUM = [F:POQ]QTYPUU
 When 4: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:POQ]PUU
 When 5: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:POQ]POHFCY
 When 6: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:POQ]BPSNUM
 When 7: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:BPS]BPSNAM
 When 8: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:POQ]POHNUM
 When 9: [F:ALR]TYP = 8 : [F:ALR]DAT = [F:POQ]DEMENDDAT
 When 10: [F:ALR]TYP = 2 : [F:ALR]NUM = +1
Endcase
Return


