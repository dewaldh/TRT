# Supervisor
# Mask management WMSATSTD 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone REGFLG
mkstat = 0
zoncou = "REGFLG"
IMPENT = [M]REGFLG(indice)
If varinit([M]REGFLG,indice)=0
 IMPENT = 1
Endif
[M]REGFLG(indice) = IMPENT

## Zone STOFCY
mkstat = 0
zoncou = "STOFCY"
SAVACT = GACTION : GACTION = "STD"
Call AV_STOFCY([M]STOFCY(indice)) From CNSSATSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]STOFCY(indice) : Endif

## Zone ISSMGTCOD
mkstat = 0
zoncou = "ISSMGTCOD"
IMPENT = [M]ISSMGTCOD(indice)
