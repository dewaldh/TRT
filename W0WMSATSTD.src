 IMPENT = 1
Endif
[M]ISSMGTCOD(indice) = IMPENT

## Zone AVAFLG
mkstat = 0
zoncou = "AVAFLG"
IMPENT = [M]AVAFLG(indice)
SAVACT = GACTION : GACTION = "STD"
Call AV_AVAFLG(IMPENT) From CNSSATSTD
GACTION = SAVACT
If mkstat=1 : Raz IMPENT : Endif
If !mkstat
 If varinit([M]AVAFLG,indice)=0
  IMPENT = 1
 Endif
Endif
[M]AVAFLG(indice) = IMPENT

## Zone TRSTYP
mkstat = 0
zoncou = "TRSTYP"
