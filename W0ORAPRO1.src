# Supervisor
# Mask management ORAPRO1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBPRO
mkstat = 0
zoncou = "NBPRO"
indice = 0
status = 1
zoncou = "NBPRO"
If varmode([M]NBPRO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBPRO" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBPRO
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBPRO = nolign
   status = 75
   Call SAI_NBPRO(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone FLT
mkstat = 0
zoncou = "FLT"
indice = 0
IMPENT = [M]FLT
If varinit([M]FLT,indice)=0
 IMPENT = 1
Endif
If varmode([M]FLT,indice)=0
 [M]FLT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLT(indice)
 Endif
Endif
[M]FLT = IMPENT

## Zone TRI
mkstat = 0
zoncou = "TRI"
indice = 0
IMPENT = [M]TRI
If varinit([M]TRI,indice)=0
 IMPENT = 1
Endif
If varmode([M]TRI,indice)=0
 [M]TRI(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TRI" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TRI(indice)
 Endif
Endif
[M]TRI = IMPENT

## Zone TYPTRI
mkstat = 0
zoncou = "TYPTRI"
indice = 0
IMPENT = [M]TYPTRI
If varinit([M]TYPTRI,indice)=0
 IMPENT = 1
Endif
If varmode([M]TYPTRI,indice)=0
 [M]TYPTRI(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYPTRI" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TYPTRI(indice)
 Endif
Endif
[M]TYPTRI = IMPENT

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBPRO(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBPRO
mkstat = 0
zoncou = "NBPRO"

## Zone BCKGRD
mkstat = 0
zoncou = "BCKGRD"
IMPENT = [M]BCKGRD(indice)
If varinit([M]BCKGRD,indice)=0
 IMPENT = 1
Endif
[M]BCKGRD(indice) = IMPENT

## Zone NBPRO
mkstat = 0
zoncou = "NBPRO"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ORAPRO1"
Return

