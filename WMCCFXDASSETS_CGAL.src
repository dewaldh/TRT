####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0FXDASSETS_CGAL
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_ISSAMT From WMC1FXDASSETS_CGAL : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_ISSAMT From WMC1FXDASSETS_CGAL : Gosub TESTALLSTAT :  Endif
