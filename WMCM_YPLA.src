#-------------------------------------------------------#
$METHODS
$EVENTS
Case [L]AEVENT
 When "ADDLINE_AFTER"
  Case [L]CURPTH
   When "YPLL"
   When "YRMA"
    [L]ASTATUS = fmet this.ASETATTRIBUTE("YGUID","$namProParent","YGUID")
    this.YGUID = this.APARENT.YGUID
   When Default
  Endcase
 When Default
Endcase
Return

#-------------------------------------------------------#
$PROPERTIES
Local Integer ALINE
Case [L]CURPRO
 When "YGUID" : Gosub PRO_YGUID
Endcase
Return

#-------------------------------------------------------#
$PRO_YGUID
Case [L]ARULE
 When "PROPAGATE"
  For ALINE=1 to maxtab(this.YRMA)
   If this.YRMA([L]ALINE)<>null
    this.YRMA([L]ALINE).YGUID = this.YGUID
   Endif
  Next
Endcase
Return



