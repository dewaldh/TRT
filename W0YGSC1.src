# Supervisor
# Mask management YGSC1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YESTID
mkstat = 0
zoncou = "YESTID"
indice = 0
If varmode([M]YESTID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YESTID" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YESTID=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YESTID" : GZNUM = 100372 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTNUM("YEST","",[M]YESTID,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YESTID" : GZNUM = 100372 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone YPREID
mkstat = 0
zoncou = "YPREID"
indice = 0
If varmode([M]YPREID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YPREID" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YPREID,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YPREID" : GZNUM = 100373 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("YPRE","",[M]YPREID,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YPREID" : GZNUM = 100373 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone YDEVPJMNBR
mkstat = 0
zoncou = "YDEVPJMNBR"
indice = 0
If varmode([M]YDEVPJMNBR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YDEVPJMNBR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("PIM","!A",[M]YDEVPJMNBR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YDEVPJMNBR" : GZNUM = 100596 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YPRJPJMNBR
mkstat = 0
zoncou = "YPRJPJMNBR"
indice = 0
If varmode([M]YPRJPJMNBR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YPRJPJMNBR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("PIM","!A",[M]YPRJPJMNBR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YPRJPJMNBR" : GZNUM = 100770 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YDEVFLG
mkstat = 0
zoncou = "YDEVFLG"
indice = 0
IMPENT = [M]YDEVFLG
If varinit([M]YDEVFLG,indice)=0
 IMPENT = 1
Endif
[M]YDEVFLG = IMPENT

## Zone YPRJFLG
mkstat = 0
zoncou = "YPRJFLG"
indice = 0
IMPENT = [M]YPRJFLG
If varinit([M]YPRJFLG,indice)=0
 IMPENT = 1
Endif
[M]YPRJFLG = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YGSC1"
Return

