# Mask management XVERPAT2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBOBJ
mkstat = 0
zoncou = "NBOBJ"
indice = 0
status = 1
zoncou = "NBOBJ"
If varmode([M]NBOBJ,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBOBJ" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBOBJ
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBOBJ = nolign
   status = 75
   Call SAI_NBOBJ(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

indice = 0
status = 1
zoncou = "NBLAN"
If !mkstat
 NBLIG = [M]NBLAN
 For nolign=1 To NBLIG
  indice = nolign-1
  [M]NBLAN = nolign
  status = 75
  Call SAI_NBLAN(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBOBJ(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBOBJ
mkstat = 0
zoncou = "NBOBJ"

## Zone FLGGEN
mkstat = 0
zoncou = "FLGGEN"
IMPENT = [M]FLGGEN(indice)
If varinit([M]FLGGEN,indice)=0
 IMPENT = 2
Endif
If varmode([M]FLGGEN,indice)=0
 [M]FLGGEN(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLGGEN" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLGGEN(indice)
 Endif
Endif
[M]FLGGEN(indice) = IMPENT

## Zone CREUPDDAT
mkstat = 0
zoncou = "CREUPDDAT"
If varinit([M]CREUPDDAT,indice)=0
 [M]CREUPDDAT(indice) = date$
Endif

## Zone TYPOBJ
mkstat = 0
zoncou = "TYPOBJ"
If varmode([M]TYPOBJ,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYPOBJ" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACTLTYPATC"
 PARAM(1) = nomap(0)
 Local Char    VALEUR(250)
 VALEUR = [M]TYPOBJ(indice)
 Call S_AFF_CHAR(VALEUR,PARAM,"ACTLTYPATC","APATCH","") From GACTION
 [M]TYPOBJ(indice) = VALEUR
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "TYPOBJ" : GZNUM = 12788 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "SPV"
  Call AP_TYPOBJ([M]TYPOBJ(indice)) From X08VERPAT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "TYPOBJ" : GZNUM = 12788 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone NOMOBJ
mkstat = 0
zoncou = "NOMOBJ"
If varmode([M]NOMOBJ,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NOMOBJ" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "SPV"
 Call C_NOMOBJ([M]NOMOBJ(indice)) From X08VERPAT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "NOMOBJ" : GZNUM = 725 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACTLOBJPAT"
  PARAM(1) = [M]DOSSIER
  PARAM(2) = [M]TYPOBJ(nolign-1)
  PARAM(3) = "[M]LANGUE"
  PARAM(4) = "[M]INTITOBJ(nolign-1)"
  Local Char    VALEUR(250)
  VALEUR = [M]NOMOBJ(indice)
  Call S_AFF_CHAR(VALEUR,PARAM,"ACTLOBJPAT","APATCH","") From GACTION
  [M]NOMOBJ(indice) = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NOMOBJ" : GZNUM = 725 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DES
mkstat = 0
zoncou = "DES"
If varmode([M]DES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone CODACT
mkstat = 0
zoncou = "CODACT"
If varmode([M]CODACT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CODACT" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ACV","!",[M]CODACT(indice),"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CODACT" : GZNUM = 4 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone TYPMODIF
mkstat = 0
zoncou = "TYPMODIF"
If varmode([M]TYPMODIF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYPMODIF" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "SPV"
 Call C_TYPMODIF([M]TYPMODIF(indice)) From X08VERPAT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "TYPMODIF" : GZNUM = 24027 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone LIVDAT
mkstat = 0
zoncou = "LIVDAT"
If varmode([M]LIVDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LIVDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone MODIF
mkstat = 0
zoncou = "MODIF"
If varmode([M]MODIF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "MODIF" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NBOBJ
mkstat = 0
zoncou = "NBOBJ"

End

