  If YSTA.YSRG(A_N2).ASTALIN=[V]CST_ADEL or YSTA.YSRG(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YSTA.YSRG(A_N2)
   YSTA.YSRG(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
