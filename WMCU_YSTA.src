 If YSTA.YSLL(A_N2) <> null
  [L]ASTATUS2 = fmet YSTA.YSLL(A_N2)._AUPDATE_ABORT
  If YSTA.YSLL(A_N2).ASTALIN=[V]CST_ADEL or YSTA.YSLL(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YSTA.YSLL(A_N2)
   YSTA.YSLL(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
