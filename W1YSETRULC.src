$ON_YLOTT
Onevent CONTROL Call C_YLOTT(zc) From W1YSETRULC
Onevent BEFORE_INPUT Call AS_YLOTT(zc) From W1YSETRULC
Return

Subprog AS_YLOTT(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(2721,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YLOTT(zc) From W1YSETRULC
Onkey   2 Titled GBOUTS Call S_YLOTT(zc) From W1YSETRULC
Onkey 144 Titled GBOUTP Call P_YLOTT(zc) From W1YSETRULC
Onkey 146 Titled GBOUTA Call SA_YLOTT(zc) From W1YSETRULC
Gosub _AFF
End

Subprog C_YLOTT(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("YLOT","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "YLOTT" : GZNUM = 2721 : Endif
If !mkstat
 ## Action SPE
 Call C_YLOTT(VALEUR) From SPEYSETRULC
 If mkstat : GERR = 1 : GZONE = "YLOTT" : GZNUM = 2721 : Endif
Endif
Gosub _AFF
End

Subprog S_YLOTT(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action YSELLOT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "YSELLOT"
 PARMSK(1) = num$([M:YSTRC]YEST)
 PARMSK(2) = [M:YSTRC]YSTAG
 PARMSK(3) = [M:YSTRC]YLANDPUR
 Call S_LIS_CHAR(VALEUR,PARMSK,"","","YSELLOT") From GACTION
 [M:YSTRC]YSTAG = PARMSK(2)
 [M:YSTRC]YLANDPUR = PARMSK(3)
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_YLOTT(VALEUR)
Variable Char    VALEUR()
Call OBJET("YLOT",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_YLOTT(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "YLOT"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YLOTT(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("YLOT",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

