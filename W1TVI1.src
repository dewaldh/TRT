Subprog C_SOC(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("AGF","!C",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "SOC" : GZNUM = 8294 : Endif
If !mkstat
 ## Action CONTGRPLEG
 Call CTLGFYLEG(VALEUR,string$(GLEGACT=1,GDOSLEG)+string$(GLEGACT=2,[M:TVI1]LEG),"TVI1","SOC") From TRTVENCTL
 If mkstat : GERR = 1 : GZONE = "SOC" : GZNUM = 8294 : Endif
 If !mkstat
  ## Action STD
  Call C_SOC(VALEUR) From SUBTVI
  If mkstat : GERR = 1 : GZONE = "SOC" : GZNUM = 8294 : Endif
 Endif
Endif
Gosub _AFF
End

