############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(TABPLACE,ASTATUS)
Variable Instance TABPLACE Using C_TABPLACE
Variable Integer ASTATUS

If TABPLACE = null : End : Endif

Gosub _AVARIABLES From WMC00TABPLACE

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_TABPLACE

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(TABPLACE,ASTATUS)
Variable Instance TABPLACE Using C_TABPLACE
Variable Integer ASTATUS

If TABPLACE = null : End : Endif

Gosub AVARIABLES From WMC00TABPLACE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
TABPLACE.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_TABPLACE
Else
 Gosub AUPDATE_TABPLACE
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = TABPLACE.freeSnapshot
TABPLACE.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet TABPLACE._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet TABPLACE._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet TABPLACE._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If TABPLACE.ASTALIN = [V]CST_ANEWDEL : Return : Endif
TABPLACE.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(TABPLACE.ASTALIN)<>""
 [L]A_ASTALIN = TABPLACE.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(TABPLACE.BPA) : # BPADDRESS -- 2
 If TABPLACE.BPA(A_N2) <> null
& and TABPLACE.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If TABPLACE.BPA(A_N2).ASTALIN<>[V]CST_ANEW & TABPLACE.ASTALIN=[V]CST_ADEL
   TABPLACE.BPA(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If TABPLACE.BPA(A_N2).ASTALIN=[V]CST_ANEW & find(TABPLACE.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   TABPLACE.BPA(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(TABPLACE.BPA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(TABPLACE.BPA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
  For [L]A_I=1 To maxtab(TABPLACE.BPA(A_N2).COLTEL)
   If TABPLACE.BPA(A_N2).COLTEL([L]A_I)<>null
    [L]A_ASTALIN = TABPLACE.BPA(A_N2).COLTEL([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2).COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
  For [L]A_I=1 To maxtab(TABPLACE.BPA(A_N2).COLWEB)
   If TABPLACE.BPA(A_N2).COLWEB([L]A_I)<>null
    [L]A_ASTALIN = TABPLACE.BPA(A_N2).COLWEB([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2).COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
For A_N2=1 to maxtab(TABPLACE.BPA)
 If TABPLACE.BPA(A_N2)<>null
  If TABPLACE.BPA(A_N2).ASTALIN<>[V]CST_ADEL & TABPLACE.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
   [L]A_OK1 = 1 : Break
  Endif
 Endif
Next
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(TABPLACE,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPADDRESS:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(TABPLACE.BPA) : # BPADDRESS -- 2
 If TABPLACE.BPA(A_N2) <> null
& and TABPLACE.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(TABPLACE.BPA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(TABPLACE.BPA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
  For [L]A_I=1 To maxtab(TABPLACE.BPA(A_N2).COLTEL)
   If TABPLACE.BPA(A_N2).COLTEL([L]A_I)<>null
    [L]A_ASTALIN = TABPLACE.BPA(A_N2).COLTEL([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2).COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
  For [L]A_I=1 To maxtab(TABPLACE.BPA(A_N2).COLWEB)
   If TABPLACE.BPA(A_N2).COLWEB([L]A_I)<>null
    [L]A_ASTALIN = TABPLACE.BPA(A_N2).COLWEB([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2).COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
For A_N2=1 to maxtab(TABPLACE.BPA)
 If TABPLACE.BPA(A_N2)<>null
  If TABPLACE.BPA(A_N2).ASTALIN<>[V]CST_ADEL & TABPLACE.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
   [L]A_OK1 = 1 : Break
  Endif
 Endif
Next
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(TABPLACE,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPADDRESS:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

If [V]CST_ASTALIN(TABPLACE.ASTALIN)<>""
 [L]A_ASTALIN = TABPLACE.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_TABPLACE
Trbegin [F:TPC]
 Gosub AUPDATE_TABPLACE
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_TABPLACE
[L]A_ASTALIN = TABPLACE.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00TABPLACE._MAJSQL_TABPLACE(TABPLACE))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 For A_N2=1 To maxtab(TABPLACE.BPA)
  If TABPLACE.BPA(A_N2) <> null and
& (TABPLACE.BPA(A_N2).ASTALIN=[L]A_TYPMAJ or
& TABPLACE.BPA(A_N2).ASTALIN=[V]CST_ALL)
   If TABPLACE.BPA(A_N2).ASTALIN=[L]A_TYPMAJ
    Case TABPLACE.BPA(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.BPA(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = TABPLACE.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet TABPLACE.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If TABPLACE.snapshotEnabled=1
 [L]A_RETIK = TABPLACE.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(TABPLACE.BPA)
 If TABPLACE.BPA(A_N2) <> null
  [L]ASTATUS2 = fmet TABPLACE.BPA(A_N2)._AUPDATE_ABORT
  If TABPLACE.BPA(A_N2).ASTALIN=[V]CST_ADEL or TABPLACE.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance TABPLACE.BPA(A_N2)
   TABPLACE.BPA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet TABPLACE.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(TABPLACE.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 TABPLACE.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(TABPLACE.BPA)
 If TABPLACE.BPA(A_N2) <> null
  [L]ASTATUS2 = fmet TABPLACE.BPA(A_N2)._AUPDATE_AFTER
  If TABPLACE.BPA(A_N2).ASTALIN=[V]CST_ADEL or TABPLACE.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance TABPLACE.BPA(A_N2)
   TABPLACE.BPA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



