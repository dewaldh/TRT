# Supervisor
## CRUD-CLASSE ## PHYELTTRF
# 2018-08-27 13:22:36 198


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PHYELTTRF,ASTATUS)
Variable Instance PHYELTTRF Using R_PHYELTTRF
Variable Integer ASTATUS

If PHYELTTRF = null : End : Endif

Gosub AVARIABLES From WMR00PHYELTTRF
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PHYELTTRF.ACTX.AGETAFCRIGHT(PHYELTTRF,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PHYELTTRF.ACTX.AGETAFCRIGHTU(PHYELTTRF,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PHYELTTRF
Else
 Gosub AUPDATE_PHYELTTRF
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PHYELTTRF.freeSnapshot
PHYELTTRF.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PHYELTTRF.ASTALIN = [V]CST_AUPD
PHYELTTRF.PHYTRF.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PHYELTTRF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTTRF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTTRF._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTTRF._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTTRF._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PHYELTTRF.PHYTRF.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PHYELTTRF.PHYTRF.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PHYELTTRF.PHYTRF.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTTRF.PHYTRF.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PHYELTTRF.PHYTRF.PHYMVT) : # PHYMVT -- 2
 If PHYELTTRF.PHYTRF.PHYMVT(A_N2) <> null
& and PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEW & PHYELTTRF.PHYTRF.ASTALIN=[V]CST_ADEL
   PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ANEW & find(PHYELTTRF.PHYTRF.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF.PHYMVT(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF.PHYMVT(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF.PHYMVT(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PHYELTTRF.PHYTRF.PHYMVT) : # PHYMVT -- 2
 If PHYELTTRF.PHYTRF.PHYMVT(A_N2) <> null
& and PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF.PHYMVT(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF.PHYMVT(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF.PHYMVT(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PHYELTTRF.PHYTRF.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTTRF.PHYTRF.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PHYELTTRF
Trbegin [F:PHY]
 Gosub AUPDATE_PHYELTTRF
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PHYELTTRF.PHYTRF.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PHYELTTRF.PHYTRF.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTTRF.PHYTRF.PHYMVT)
 If PHYELTTRF.PHYTRF.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTTRF.PHYTRF.PHYMVT(A_N2)._AUPDATE_AFTER
  If PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTTRF.PHYTRF.PHYMVT(A_N2)
   PHYELTTRF.PHYTRF.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_PHYELTTRF
[L]A_ASTALIN = PHYELTTRF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.PHYTRF._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTTRF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTTRF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PHYELTTRF.snapshotEnabled=1
 [L]A_RETIK = PHYELTTRF.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTTRF.PHYTRF.PHYMVT)
 If PHYELTTRF.PHYTRF.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTTRF.PHYTRF.PHYMVT(A_N2)._AUPDATE_ABORT
  If PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTTRF.PHYTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTTRF.PHYTRF.PHYMVT(A_N2)
   PHYELTTRF.PHYTRF.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PHYELTTRF.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



