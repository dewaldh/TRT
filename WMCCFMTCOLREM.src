####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0FMTCOLREM
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_BPCBPS From WMC1FMTCOLREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_BPCBPS From WMC1FMTCOLREM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_PLISTC From WMC1FMTCOLREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_PLISTC From WMC1FMTCOLREM : Gosub TESTALLSTAT :  Endif
For AINDCLA=1 To 9
 If this.C_FMTCOL(AINDCLA)<>null& find(this.C_FMTCOL(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.C_FMTCOL(AINDCLA)._AOTHERS : Gosub TESTALLSTAT
 Endif
Next AINDCLA
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREDATTIM From WMC1FMTCOLREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREDATTIM From WMC1FMTCOLREM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDATTIM From WMC1FMTCOLREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDATTIM From WMC1FMTCOLREM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AUUID From WMC1FMTCOLREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AUUID From WMC1FMTCOLREM : Gosub TESTALLSTAT :  Endif
