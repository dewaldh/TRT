Subprog C_CCE6(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$([M]DIE(5))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CCE6" : GZNUM = 0 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$([M]DIE(5)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CCE6" : GZNUM = 0 : Endif
 If !mkstat
  ## Action FLDCTLCAR
  Call FLDCTLCAR(VALEUR,"CCE6."+[M:HAEF]DIE(5)) From TRTGAS
  If mkstat : GERR = 1 : GZONE = "CCE6" : GZNUM = 0 : Endif
  If !mkstat
   ## Action STD
   Call C_CCE6(VALEUR) From SUBGA0
   If mkstat : GERR = 1 : GZONE = "CCE6" : GZNUM = 0 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

