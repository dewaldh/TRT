Subprog GETACT(NOL,TITBOUT,VALBOUT)
Value Integer NOL
Variable Char TITBOUT()(1..) , VALBOUT()(1..)
Local Integer ONOL , NBBOUT
ONOL   = nolign
nolign = NOL
indice = max(0,nolign-1)
Call AS_NBLIG From ="W1CONSSED2"
NBBOUT = 0
If GBOUT1<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT1
 VALBOUT(NBBOUT) = "SED"
 VALBOUT(NBBOUT) += "~B1"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans([M:SED1]FCY,"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans([M]ITMREF(nolign-1),"~","¨")
 Onerrgo
Endif
If GBOUT2<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT2
 VALBOUT(NBBOUT) = "SED"
 VALBOUT(NBBOUT) += "~B2"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans([M:SED1]FCY,"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans([M]ITMREF(nolign-1),"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("","~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("","~","¨")
 Onerrgo
Endif
If GBOUT3<>""
 NBBOUT += 1
 TITBOUT(NBBOUT) = GBOUT3
 VALBOUT(NBBOUT) = "SED"
 VALBOUT(NBBOUT) += "~B3"
 VALBOUT(NBBOUT) += "~"+num$(nolign)
 Onerrgo RIEN
 VALBOUT(NBBOUT) += "~"+ctrans([M:SED1]FCY,"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans([M]ITMREF(nolign-1),"~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("","~","¨")
 VALBOUT(NBBOUT) += "~"+ctrans("","~","¨")
 Onerrgo
Endif
nolign = ONOL
indice = max(0,nolign-1)
End

Subprog ACTRUN(INFO)
Value Char INFO
Local Char    BOUT(3)
Local Integer ONOL
ONOL    = nolign
BOUT    = func AFNC.PART(INFO,1)
nolign  = val(func AFNC.PART(INFO,2))
indice  = max(0,nolign-1)
Gosub NOMFIC
For MA=0 to dim(NOMMSK)-1
 Gosub OUVRE_MSK
Next MA
For MA=0 to dim(NOMMSK)-1
 Gosub SETMASK
Next MA
Case BOUT
 When "B1"
  Onerrgo RIEN
  Assign "[M:SED1]FCY" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Assign "[M]ITMREF(nolign-1)" With ctrans(func AFNC.PART(INFO,4),"¨","~")
  Onerrgo
  Call B1_NBLIG From ="W1CONSSED2"
 When "B2"
  Onerrgo RIEN
  Assign "[M:SED1]FCY" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Assign "[M]ITMREF(nolign-1)" With ctrans(func AFNC.PART(INFO,4),"¨","~")
  Onerrgo
  Call B2_NBLIG From ="W1CONSSED2"
 When "B3"
  Onerrgo RIEN
  Assign "[M:SED1]FCY" With ctrans(func AFNC.PART(INFO,3),"¨","~")
  Assign "[M]ITMREF(nolign-1)" With ctrans(func AFNC.PART(INFO,4),"¨","~")
  Onerrgo
  Call B3_NBLIG From ="W1CONSSED2"
Endcase
nolign = ONOL
indice = max(0,nolign-1)
End

$RIEN
Resume


