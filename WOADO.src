$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(3)(0..GNBDERNIER)
Local Char     A_BUF2(20)(0..GNBDERNIER)
Local Char     A_BUF3(30)(0..GNBDERNIER)
Local Char     A_BUF4(30)(0..GNBDERNIER)
Local Char     A_BUF5(20)(0..GNBDERNIER)
Local Char     A_BUF6(80)(0..GNBDERNIER)
Local Shortint A_BUF7(0..GNBDERNIER)
Local Integer  A_BUF8(0..GNBDERNIER)
If clalev([F:ADO9])>0 : Close Local File [ADO9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(3),
& Char     A_TAB2(20),
& Char     A_TAB3(30),
& Char     A_TAB4(30),
& Char     A_TAB5(20),
& Char     A_TAB6(80),
& Shortint A_TAB7,
& Integer  A_TAB8
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5,A_BUF6,A_BUF7,A_BUF8
& As [ADO9]
Return

$LEC_DERLU
Return

$CHARGE_DERLU
If clalev([F:ADO9])>0
 Read [ADO9] = [F:ADO9]A_TABI
 [L]CLE = [F:ADO9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:ADO9])>0
 Filter [ADO9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [ADO1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:ADO9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:ADO]TYP+"~"
 A_CLEDERLU += [F:ADO]LAN+"~"
 A_CLEDERLU += [F:ADO]COD+"~"
 A_CLEDERLU += num$([F:ADO]LEV)+"~"
 A_CLEDERLU += num$([F:ADO]SUBLEV)+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:ADO9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:ADO9]
 Endif
Endif
Return

