Onevent CONTROL Call C_ORI(zc) From W1VXBST
Onevent AFTER_CHANGE Gosub AMM_ORI From W1VXBST
Onevent INIT_AREA Call D_ORI(zc) From W1VXBST
Return

Subprog D_ORI(VALEUR)
Variable Integer VALEUR
If varinit([M]ORI,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_ORI(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(2769) From COMBOS
## Action STD
Call AS_ORI(VALEUR) From VXBST
If mkstat=2
 [M]ORI(INDIC) = VALEUR
 Affzo ORI(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,2769,1))))
Endif
If !mkstat
 Discombo 0 From ORI(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From ORI(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_ORI(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
## Action STD
Call C_ORI(VALEUR) From VXBST
If mkstat : GERR = 1 : GZONE = "ORI" : GZNUM = 0 : Endif
Gosub _AFF
End

