$ON_YESTCPYGRP
Onevent CONTROL Call C_YESTCPYGRP(zc) From W1YEST
Onevent BEFORE_INPUT Call AS_YESTCPYGRP(zc) From W1YEST
Return

Subprog AS_YESTCPYGRP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(8582,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YESTCPYGRP(zc) From W1YEST
Onkey   2 Titled GBOUTS Call S_YESTCPYGRP(zc) From W1YEST
Onkey 144 Titled GBOUTP Call P_YESTCPYGRP(zc) From W1YEST
Onkey 146 Titled GBOUTA Call SA_YESTCPYGRP(zc) From W1YEST
Gosub _AFF
End

Subprog C_YESTCPYGRP(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YESTCPYGRP" : GZNUM = 100382 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("AGF","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YESTCPYGRP" : GZNUM = 100382 : Endif
Endif
Gosub _AFF
End

Subprog S_YESTCPYGRP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "AGF"
PARMSK(2) = ""
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_YESTCPYGRP(VALEUR)
Variable Char    VALEUR()
Call OBJET("AGF",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_YESTCPYGRP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AGF"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YESTCPYGRP(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AGF",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

