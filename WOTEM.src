$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Shortint A_BUF1(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
If clalev([F:SFTTM9])>0 : Close Local File [SFTTM9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Shortint A_TAB1,
& Char     A_TAB2(30)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2
& As [SFTTM9]
Return

$LEC_DERLU
Read [TEM]TMA0=[F:SFTTM]TEAMNUM
If fstat : Raz [F:TEM] : Endif
Return

$CHARGE_DERLU
If clalev([F:SFTTM9])>0
 Read [SFTTM9] = [F:SFTTM9]A_TABI
 [L]CLE = [F:SFTTM9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:SFTTM9])>0
 Filter [SFTTM9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [SFTT] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:SFTTM9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = num$([F:SFTTM]TEAMNUM)+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:SFTTM9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:SFTTM9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 1
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:SFTTM]TEAMNUM"
Call TEXTE(54339,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 2
SELFMT(NBSEL) = "N:4F"
SELFOR(NBSEL) = "Nz:4#"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:SFTTM]TEAMNUM"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:TEM]EMPDES"
Call TEXTE(8,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = "K:30X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:TEM]EMPDES"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:SFTTM_])=0
 Gosub OUVFIC_LISTE
Endif
Return

