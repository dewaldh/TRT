$ON_DEFFOR
Onevent CONTROL Call C_DEFFOR(zc) From W1MDL1
Onevent BEFORE_INPUT Call AS_DEFFOR(zc) From W1MDL1
Onevent AFTER_CHANGE Gosub AMM_DEFFOR From W1MDL1
Return

Subprog AS_DEFFOR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(4574,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_DEFFOR(zc) From W1MDL1
Onkey   2 Titled GBOUTS Call S_DEFFOR(zc) From W1MDL1
Onkey 144 Titled GBOUTP Call P_DEFFOR(zc) From W1MDL1
Onkey 146 Titled GBOUTA Call SA_DEFFOR(zc) From W1MDL1
Gosub _AFF
End

Subprog C_DEFFOR(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("TFO","!",VALEUR,"10",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "DEFFOR" : GZNUM = 12877 : Endif
Gosub _AFF
End

$AMM_DEFFOR
Call AM_DEFFOR(zc)
If !mkstat & GOBJET
 zoncou = "DEFFOR" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_DEFFOR(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZDEFFOR)")>indice
 [M]DEFFOR(indice) = VALEUR
 Affzo ZDEFFOR(indice)
Endif
Gosub _AFF
End

Subprog S_DEFFOR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "TFO"
PARMSK(2) = "!"
PARMSK(3) = "10"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_DEFFOR(VALEUR)
Variable Char    VALEUR()
Call OBJET("TFO",VALEUR,"10") From GOBJET
Gosub _AFF
End

Subprog SA_DEFFOR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TFO"
PARMSK(2) = "10"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_DEFFOR(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "10"
Call VISU("TFO",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZDEFFOR
Onevent BEFORE_AREA Gosub AV_ZDEFFOR From W1MDL1
Return

$AV_ZDEFFOR
Call LECTEXTRA(zc,"TABFOR","DES","10",num$([M]DEFFOR(indice))) From ATEXTRA
Gosub _AFF
Return

