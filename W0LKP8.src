# Supervisor
# Mask management LKP8 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBSOL
mkstat = 0
zoncou = "NBSOL"
indice = 0
status = 1
zoncou = "NBSOL"
If varmode([M]NBSOL,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBSOL From SUBLKP8
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBSOL : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBSOL" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBSOL
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBSOL = nolign
   status = 75
   Call SAI_NBSOL(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_NBSOL From SUBLKP8
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NBSOL" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone DESICO
mkstat = 0
zoncou = "DESICO"
indice = 0
If varmode([M]DESICO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DESICO" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBSOL(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBSOL
mkstat = 0
zoncou = "NBSOL"
SAVACT = GACTION : GACTION = "STD"
Call C_NBSOL From SUBLKP8
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "NBSOL" : GZNUM = 0 : Endif
Gosub AFF From GSAISIE
If mkstat : End : Endif

## Zone NBSOL
mkstat = 0
zoncou = "NBSOL"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "LKP8"
Return

