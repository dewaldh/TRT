$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(25)(0..GNBDERNIER)
If clalev([F:YSLC9])>0 : Close Local File [YSLC9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(25)
& ) From Variable A_BUFI,A_BUFC,A_BUF1
& As [YSLC9]
Return

$LEC_DERLU
Return

$CHARGE_DERLU
If clalev([F:YSLC9])>0
 Read [YSLC9] = [F:YSLC9]A_TABI
 [L]CLE = [F:YSLC9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:YSLC9])>0
 Filter [YSLC9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [YSLC1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:YSLC9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:YSLC]YSLCID+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:YSLC9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:YSLC9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
