SELFMT(NBSEL) = "K:25X"
SELFOR(NBSEL) = "K:25X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:YSLC]YSLCID"
TRILIB(NBTRI) = SELLIB(NBSEL)
Return

