#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1ASYRAUS
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "ASYRAUS"
REP.CLA = "ASYRAUS"
REP.ICL = "AUS"
REP.AFCRIGHT = ""
REP.NCL = 485 : # Users
REP.DES = 18017 : # User
REP.TIT = "INTUSR"
REP.ANX = "CODUSR"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Code
     REP.CLE(1).PAR = 'USR' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'USR' : REP.CLE(1).VPR= 'AUS.USR' : REP.CLE(1).DES = 5296
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'ASYRAUS_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 2 : # Detail
REP.AENAFAC(2) = 2 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 2 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "DETAIL" : REP.SEC(1).NSC = 5562 : # Detail
     REP.SEC(1).BLC(1)="DETAIL" : REP.SEC(1).NBC(1)=5562 : # Detail
Return

