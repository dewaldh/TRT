####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "C_LED"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_LED",this)
 When "C_LINE"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_LINE",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBLIG -= 1 : Endif
 When "C_PPKH"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_PPKH",this)
 When "C_REP"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_REP",this)
 When "FMTCOLREM"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"FMTCOLREM",this)
 When "PLI"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"PLI",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# ADDLINE
####################################################################
$ADDLINE
Case _ACOLLECTION
 When "C_LED"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_LED","C_PFINDPRICE_C_LED",this)
 When "C_LINE"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_LINE","C_PFINDPRICED",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBLIG += 1 : Endif
 When "C_PPKH"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_PPKH","C_PPRICLINKH",this)
 When "C_REP"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_REP","C_PFINDPRICE_C_REP",this)
 When "FMTCOLREM"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"FMTCOLREM","C_FMTCOLREM",this)
 When "PLI"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"PLI","C_PLI",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
