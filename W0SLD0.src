#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management SLD0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone STRTYP
mkstat = 0
zoncou = "STRTYP"
indice = 0
IMPENT = [M]STRTYP
If varinit([M]STRTYP,indice)=0
 IMPENT = 4
Endif
[M]STRTYP = IMPENT

## Zone STRNUM
mkstat = 0
zoncou = "STRNUM"
indice = 0
If varmode([M]STRNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "STRNUM" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]STRNUM,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "STRNUM" : GZNUM = 3211 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "SLD"
  Local Char    VALEUR(250)
  VALEUR = [M]STRNUM
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]STRNUM = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "STRNUM" : GZNUM = 3211 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DESAXX
mkstat = 0
zoncou = "DESAXX"
indice = 0
If varinit([M]DESAXX,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DESAXX,"SALTRS","DESAXX","4",[M:SLD0]STRNUM) From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DESAXX,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DESAXX" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
SAVACT = GACTION : GACTION = "STD"
Call AV_ENAFLG(IMPENT) From SUBSLD
GACTION = SAVACT
If mkstat=1 : Raz IMPENT : Endif
If !mkstat
 If varinit([M]ENAFLG,indice)=0
  IMPENT = 1
 Endif
Endif
If varmode([M]ENAFLG,indice)=0
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
Endif
[M]ENAFLG = IMPENT

## Zone ACSCOD
mkstat = 0
zoncou = "ACSCOD"
indice = 0
If varmode([M]ACSCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ACSCOD" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ACS","!",[M]ACSCOD,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "ACSCOD" : GZNUM = 729 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CTLACS"
  Call CTLACS([M]ACSCOD,"E") From CONTX3
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ACSCOD" : GZNUM = 729 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone GFY
mkstat = 0
zoncou = "GFY"
indice = 0
If varmode([M]GFY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "GFY" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("AGF","!",[M]GFY,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "GFY" : GZNUM = 616 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SLD0"
Return

