############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(YSALCTR,ASTATUS)
Variable Instance YSALCTR Using C_YSALCTR
Variable Integer ASTATUS

If YSALCTR = null : End : Endif

Gosub _AVARIABLES From WMC00YSALCTR

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_YSALCTR

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YSALCTR,ASTATUS)
Variable Instance YSALCTR Using C_YSALCTR
Variable Integer ASTATUS

If YSALCTR = null : End : Endif

Gosub AVARIABLES From WMC00YSALCTR
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
YSALCTR.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_YSALCTR
Else
 Gosub AUPDATE_YSALCTR
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YSALCTR.freeSnapshot
YSALCTR.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YSALCTR._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YSALCTR._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YSALCTR._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If YSALCTR.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YSALCTR.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(YSALCTR.ASTALIN)<>""
 [L]A_ASTALIN = YSALCTR.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YSALCTR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

If [V]CST_ASTALIN(YSALCTR.ASTALIN)<>""
 [L]A_ASTALIN = YSALCTR.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YSALCTR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_YSALCTR
Trbegin [F:YSLC]
 Gosub AUPDATE_YSALCTR
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_YSALCTR
[L]A_ASTALIN = YSALCTR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YSALCTR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YSALCTR._MAJSQL_YSALCTR(YSALCTR))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif


[L]A_ASTALIN = YSALCTR.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YSALCTR.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If YSALCTR.snapshotEnabled=1
 [L]A_RETIK = YSALCTR.revertUpdTick
Endif

[L]ASTATUS = max(fmet YSALCTR.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(YSALCTR.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YSALCTR.ASTALIN = [V]CST_ALL
Endif
Return



