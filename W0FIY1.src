#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management FIY1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
WCHAR = [M]CPY
If varmode([M]CPY,indice)=0
 SAVACT = GACTION : GACTION = "INICPY"
 Call INICPY([M]CPY) From EXEFNC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CPY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]CPY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTCPY"
   Call CONTSOC([M]CPY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("CPY","",[M]CPY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTCPY"
     Call CONTCPY([M]CPY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "STD"
      Call C_CPY([M]CPY) From SUBFIY
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       If WCHAR<>[M]CPY or IMPMOD
        SAVACT = GACTION : GACTION = "STD"
        Call AM_CPY([M]CPY) From SUBFIY
        GACTION = SAVACT
        If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
        Gosub AFF From GSAISIE
        If mkstat : End : Endif
       Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone LEDTYP
mkstat = 0
zoncou = "LEDTYP"
indice = 0
IMPENT = [M]LEDTYP
If varinit([M]LEDTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]LEDTYP,indice)=0
 Call INICOMBO(2644) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_LEDTYP(IMPENT) From SUBFIY
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2644,1))))
 Endif
 [M]LEDTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LEDTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LEDTYP(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
    mkstat = 1 : GMESSAGE = mess(99,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]LEDTYP = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone BASTAB
mkstat = 0
zoncou = "BASTAB"
indice = 0
status = 1
zoncou = "BASTAB"
If varmode([M]BASTAB,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BASTAB" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]BASTAB
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]BASTAB = nolign
   status = 75
   Call SAI_BASTAB(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  SAVACT = GACTION : GACTION = "STD"
  Call C_BASTAB From SUBFIY
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "BASTAB" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_BASTAB(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone BASTAB
mkstat = 0
zoncou = "BASTAB"
If !mkstat
 SAVACT = GACTION : GACTION = "STD"
 Call C_BASTAB From SUBFIY
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "BASTAB" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone NO
mkstat = 0
zoncou = "NO"
[M]NO(indice) = nolign

## Zone FIYNUM
mkstat = 0
zoncou = "FIYNUM"
[M]FIYNUM(indice) = nolign

## Zone DES
mkstat = 0
zoncou = "DES"
If varmode([M]DES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone DESSHO
mkstat = 0
zoncou = "DESSHO"
If varmode([M]DESSHO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DESSHO" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone FIYEND
mkstat = 0
zoncou = "FIYEND"
If varmode([M]FIYEND,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FIYEND([M]FIYEND(indice)) From SUBFIY
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FIYEND(indice) : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "FIYEND" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "DATDEBFIN"
  Call DATDEBFIN([M]FIYEND(indice),[M]FIYSTR(nolign-1)) From EXEFNC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FIYEND" : GZNUM = 829 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_FIYEND([M]FIYEND(indice)) From SUBFIY
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FIYEND" : GZNUM = 829 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone FIYSTA
mkstat = 0
zoncou = "FIYSTA"
IMPENT = [M]FIYSTA(indice)
If varinit([M]FIYSTA,indice)=0
 IMPENT = 1
Endif
[M]FIYSTA(indice) = IMPENT

## Zone BASTAB
mkstat = 0
zoncou = "BASTAB"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "FIY1"
Return

