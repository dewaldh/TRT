############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(YSLL,ASTATUS)
Variable Instance YSLL Using C_YSLL
Variable Integer ASTATUS

If YSLL = null : End : Endif

Gosub _AVARIABLES From WMC00YSLL

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_YSLL

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YSLL,ASTATUS)
Variable Instance YSLL Using C_YSLL
Variable Integer ASTATUS

If YSLL = null : End : Endif

Gosub AVARIABLES From WMC00YSLL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
YSLL.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_YSLL
Else
 Gosub AUPDATE_YSLL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YSLL.freeSnapshot
YSLL.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YSLL._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YSLL._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YSLL._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If YSLL.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YSLL.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(YSLL.ASTALIN)<>""
 [L]A_ASTALIN = YSLL.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YSLL.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

If [V]CST_ASTALIN(YSLL.ASTALIN)<>""
 [L]A_ASTALIN = YSLL.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YSLL.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_YSLL
Trbegin [F:YSLC]
 Gosub AUPDATE_YSLL
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_YSLL
[L]A_ASTALIN = YSLL.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YSLL.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YSLL._MAJSQL_YSALCTR(YSLL))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif


[L]A_ASTALIN = YSLL.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YSLL.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If YSLL.snapshotEnabled=1
 [L]A_RETIK = YSLL.revertUpdTick
Endif

[L]ASTATUS = max(fmet YSLL.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(YSLL.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YSLL.ASTALIN = [V]CST_ALL
Endif
Return



