Gosub INIT_TCHREPDIECCE From WMC2PJMACOPY : Gosub TESTALLSTAT
Gosub AINIT From WMC0PJMACOPY : Gosub TESTALLSTAT
If this.snapshotEnabled=1 & this.APARENT=null
 [L]ASTASNAPSHOT = this.freeSnapshot
Endif
Return

####################################################################
# _ALOADATTRIBUTE
####################################################################

$_ALOADATTRIBUTE
Gosub DECLARE_VAR From WMC0PJMACOPY
Gosub _ALOADATTRIBUTE_SALFCY From WMC1PJMACOPY : Gosub TESTALLSTAT
Gosub _ALOADATTRIBUTE_OPPNUM From WMC1PJMACOPY : Gosub TESTALLSTAT
Return

####################################################################
# _ALNKPRO
####################################################################

$_ALNKPRO
Gosub DECLARE_VAR From WMC0PJMACOPY
Gosub LNKPRO_SALFCY From WMC1PJMACOPY : Gosub TESTALLSTAT
For AINDCLA=1 To maxtab(this.BUDGET)
 If this.BUDGET(AINDCLA)<>null& find(this.BUDGET(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.BUDGET(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
For AINDCLA=1 To maxtab(this.TASK)
 If this.TASK(AINDCLA)<>null& find(this.TASK(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.TASK(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



