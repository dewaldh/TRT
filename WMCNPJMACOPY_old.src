Gosub INIT_BLIGNES From WMC1PJMACOPY : Gosub TESTALLSTAT
Gosub INIT_TACALL From WMC1PJMACOPY : Gosub TESTALLSTAT
Gosub INIT_TASPRL From WMC1PJMACOPY : Gosub TESTALLSTAT
