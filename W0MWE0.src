Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
indice = 0
IMPENT = [M]VCRTYP
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone ITMSTA
mkstat = 0
zoncou = "ITMSTA"
indice = 0
IMPENT = [M]ITMSTA
If varinit([M]ITMSTA,indice)=0
 IMPENT = 1
Endif
[M]ITMSTA = IMPENT

## Zone SCITRKFLG
mkstat = 0
zoncou = "SCITRKFLG"
indice = 0
IMPENT = [M]SCITRKFLG
If varinit([M]SCITRKFLG,indice)=0
 IMPENT = 1
Endif
[M]SCITRKFLG = IMPENT

####################################
## Bloc liste 5
####################################
IMPTBL = 0

## Zone TOTAMT
mkstat = 0
zoncou = "TOTAMT"
indice = 0
SAVACT = GACTION : GACTION = "FMTFM3DEV"
Call FMTFM3DEV(GLOCALDEV) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]TOTAMT : Endif

## Zone TOTPOSTED
mkstat = 0
zoncou = "TOTPOSTED"
indice = 0
SAVACT = GACTION : GACTION = "FMTFM3DEV"
Call FMTFM3DEV(GLOCALDEV) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]TOTPOSTED : Endif

## Zone TOTNOTPST
mkstat = 0
zoncou = "TOTNOTPST"
indice = 0
SAVACT = GACTION : GACTION = "FMTFM3DEV"
Call FMTFM3DEV(GLOCALDEV) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]TOTNOTPST : Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
