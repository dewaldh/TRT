#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1ABANK
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "ABANK"
REP.CLA = "ABANK"
REP.ICL = "ABN"
REP.AFCRIGHT = "GESABN"
REP.NCL = 23741 : # Bank sort codes
REP.DES = 23741 : # Bank sort codes
REP.TIT = "PAB"
REP.ANX = "ABN0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Country
     REP.CLE(1).PAR = 'CRY' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'CRY' : REP.CLE(1).VPR= 'ABN.CRY' : REP.CLE(1).DES = 263
REP.CLE(2) = NewInstance C_AREPPAR AllocGroup REP : # Bank
     REP.CLE(2).PAR = 'BAN' : REP.CLE(2).TYP = 7 : REP.CLE(2).PRO= 'BAN' : REP.CLE(2).VPR= 'ABN.BAN' : REP.CLE(2).DES = 7475
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'ABANK_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 2 : # Detail
REP.AENAFAC(2) = 2 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 2 : # Summary
Return

