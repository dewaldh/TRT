$LEC_DERLU
Read [AFF]AFF0 = [F:CMG]FCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Return

$CHARGE_DERLU
If clalev([F:CMG9])>0
 Read [CMG9] = [F:CMG9]A_TABI
 [L]CLE = [F:CMG9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:CMG9])>0
 Filter [CMG9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [CMG1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:CMG9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:CMG]NUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:CMG9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:CMG9]
 Endif
Endif
Return

