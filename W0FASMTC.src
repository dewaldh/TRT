Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG1
mkstat = 0
zoncou = "NBLIG1"
indice = 0
status = 1
zoncou = "NBLIG1"
If varmode([M]NBLIG1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG1" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG1
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG1 = nolign
   status = 75
   Call SAI_NBLIG1(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

mkstat = 0

End

