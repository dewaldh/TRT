$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "1"
  FLGEXE = 1
  GACTION = "STD"
 When "2"
  FLGEXE = 1
  GACTION = "STD"
 When "3"
  FLGEXE = 1
  GACTION = "STD"
 When "4"
  FLGEXE = 1
  GACTION = "STD"
 When "5"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$(1)
  PARBOUT(2) = [M]STOFCY(nolign-1)
  PARBOUT(3) = [M]ITMREF
  PARBOUT(4) = ""
  FLGEXE = 1
  GACTION = "CONSSFC"
  Call CONSULT_CHAR(VALBOUT,"SFC",PARBOUT) From GCONSULT
Endcase
GACTION = SAVACT
Return

