####################################################################
# 1 : PCOLDES Description ( Field A 20)
####################################################################

$SET_PCOLDES
If [L]_AOLDVAL<>this.PCOLDES
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_PCOLDES
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.PCOLDES
 Gosub PROPAGATE_PCOLDES
Endif
Return

$INIT_PCOLDES
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_PRICEDET
[L]CURPRO = "PCOLDES"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_PCOLDES
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_PRICEDET
[L]CURPRO = "PCOLDES"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_PCOLDES               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_PCOLDES               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_PCOLDES
[L]CURPRO = "PCOLDES"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.PCOLDES,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_PCOLDES
[L]CURPRO = "PCOLDES"
Return

$_AOTHERS_PCOLDES
[L]CURPRO = "PCOLDES"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_PCOLDES
[L]CURPRO = "PCOLDES"
Return

$PROPAGATE_PCOLDES
this.isReadonly = 0
[L]CURPRO = "PCOLDES"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_PCOLDES
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_PRICEDET
[L]CURPRO = "PCOLDES"
$_LNKPRO_PCOLDES
Return


####################################################################
# 2 : PVALUE Amount ( Field MD1 0)
####################################################################

$SET_PVALUE
If [L]_AOLDVAL<>this.PVALUE
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_PVALUE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.PVALUE
 Gosub PROPAGATE_PVALUE
Endif
Return

$INIT_PVALUE
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_PRICEDET
[L]CURPRO = "PVALUE"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_PVALUE
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_PRICEDET
[L]CURPRO = "PVALUE"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_PVALUE                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_PVALUE                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_PVALUE
[L]CURPRO = "PVALUE"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.PVALUE=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_PVALUE
[L]CURPRO = "PVALUE"
Return

$_AOTHERS_PVALUE
[L]CURPRO = "PVALUE"
[L]ASTATUS =  func ASYRTRTATY.CONTROLMD1(this,"PVALUE",this.PVALUE,this.APARENT.CURREF)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_PVALUE
[L]CURPRO = "PVALUE"
Return

$PROPAGATE_PVALUE
this.isReadonly = 0
[L]CURPRO = "PVALUE"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_PVALUE
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_PRICEDET
[L]CURPRO = "PVALUE"
$_LNKPRO_PVALUE
Local Char AFORMAT(50)
[L]ASTATUS =  func ASYRTRTATY.GETFORMATMD1(this,"PVALUE",this.PVALUE,[L]AFORMAT,this.APARENT.CURREF)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]AFORMAT<>''
[L]AFORMAT='"'+[L]AFORMAT+'"'
[L]ASTATUS = fmet this.ASETATTRIBUTE("PVALUE","$x3Format",[L]AFORMAT)
Endif
Return




