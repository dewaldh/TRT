Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBOBJ
mkstat = 0
zoncou = "NBOBJ"
indice = 0
status = 1
zoncou = "NBOBJ"
SAVACT = GACTION : GACTION = "STD"
Call AV_NBOBJ From SUBMAS
GACTION = SAVACT
If mkstat=1 : Raz [M]NBOBJ : Endif
If !mkstat
 If varmode([M]NBOBJ,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "NBOBJ" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   NBLIG = [M]NBOBJ
   For nolign=1 To NBLIG
    indice = nolign-1
    [M]NBOBJ = nolign
    status = 75
    Call SAI_NBOBJ(IMPFIC,ACTION,IMPTRT)
    If mkstat : Break : Endif
   Next nolign
   If mkstat : End : Endif
   indice = 0
  Endif
 Endif
Endif

