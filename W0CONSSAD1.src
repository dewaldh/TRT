#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CONSSAD1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
WCHAR = [M]FCY
If varmode([M]FCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FCY([M]FCY) From CNSSADSTD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STKASFCY"
  Call STKASFCY([M]FCY) From STKACT
  GACTION = SAVACT
  If mkstat=1 : Raz [M]FCY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   If vireblc([M]FCY,2)=""
    mkstat = 1 : GMESSAGE = mess(10,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "ACONTFCY"
    Call CONTSITE([M]FCY) From SUBAGF
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     Call CONTROLE("FCY","",[M]FCY,"",mkstat) From CONTOBJ
     If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "STKCTRFCY"
      Call STKCTRFCY([M]FCY) From STKACT
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       If WCHAR<>[M]FCY or IMPMOD
        SAVACT = GACTION : GACTION = "STD"
        Call AM_FCY([M]FCY) From CNSSADSTD
        GACTION = SAVACT
        If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
        Gosub AFF From GSAISIE
        If mkstat : End : Endif
       Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
indice = 0
WCHAR = [M]ITMREF
If varmode([M]ITMREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREF" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]ITMREF,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "VXADCUPC"
  Call ADCCTRUPC([M]ITMREF) From VXADCUPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ITM","",[M]ITMREF,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "CONTITF"
    Call CONTITF([M]ITMREF,[M]FCY) From STKLIB
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call AP_ITMREF([M]ITMREF) From CNSSADSTD
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]ITMREF or IMPMOD
       SAVACT = GACTION : GACTION = "STD"
       Call AM_ITMREF([M]ITMREF) From CNSSADSTD
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
indice = 0
IMPENT = [M]VCRTYP
WNUM = IMPENT
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]VCRTYP,indice)=0
 Call INICOMBO(725) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_VCRTYP(IMPENT) From CNSSADSTD
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,725,1))))
 Endif
 [M]VCRTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "VCRTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]VCRTYP(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "VCRTYP" : GZNUM = 4969 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WNUM<>IMPENT or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_VCRTYP(IMPENT) From CNSSADSTD
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "VCRTYP" : GZNUM = 4969 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif
[M]VCRTYP = IMPENT

## Zone VCRNUM
mkstat = 0
zoncou = "VCRNUM"
indice = 0
WCHAR = [M]VCRNUM
If varmode([M]VCRNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "VCRNUM" : Gosub ACTION From =IMPTRT
 Endif
 If WCHAR<>[M]VCRNUM or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_VCRNUM([M]VCRNUM) From CNSSADSTD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 3170 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone AGGFLG
mkstat = 0
zoncou = "AGGFLG"
indice = 0
IMPENT = [M]AGGFLG
If varinit([M]AGGFLG,indice)=0
 IMPENT = 1
Endif
[M]AGGFLG = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CONSSAD1"
Return

