# Supervisor
# Mask management TWMSTS3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG1
mkstat = 0
zoncou = "NBLIG1"
indice = 0
status = 1
zoncou = "NBLIG1"
If varmode([M]NBLIG1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG1" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG1
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG1 = nolign
   status = 75
   Call SAI_NBLIG1(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone TWMCOD
mkstat = 0
zoncou = "TWMCOD"
If varmode([M]TWMCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "TWMCOD" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("MAT","!",[M]TWMCOD(indice),"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "TWMCOD" : GZNUM = 44032 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG1(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG1
mkstat = 0
zoncou = "NBLIG1"

## Zone TWMLINSTA
mkstat = 0
zoncou = "TWMLINSTA"
IMPENT = [M]TWMLINSTA(indice)
If varinit([M]TWMLINSTA,indice)=0
 IMPENT = 1
Endif
[M]TWMLINSTA(indice) = IMPENT

## Zone PODNOTLIN
mkstat = 0
zoncou = "PODNOTLIN"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:TWS1]CUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]PODNOTLIN(indice) : Endif

## Zone PIDNOTLIN
mkstat = 0
zoncou = "PIDNOTLIN"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:TWS1]CUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]PIDNOTLIN(indice) : Endif

## Zone NBLIG1
mkstat = 0
zoncou = "NBLIG1"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "TWMSTS3"
Return

