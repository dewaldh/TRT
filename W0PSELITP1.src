# Supervisor
# Mask management PSELITP1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBITP
mkstat = 0
zoncou = "NBITP"
indice = 0
status = 1
zoncou = "NBITP"
If varmode([M]NBITP,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBITP From SUBPSI1
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBITP : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBITP" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBITP
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBITP = nolign
   status = 75
   Call SAI_NBITP(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBITP(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBITP
mkstat = 0
zoncou = "NBITP"

## Zone NOITP
mkstat = 0
zoncou = "NOITP"
[M]NOITP(indice) = nolign

## Zone NBITP
mkstat = 0
zoncou = "NBITP"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "PSELITP1"
Return

