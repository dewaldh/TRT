####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0SFTINDREF
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_INDCOD From WMC1SFTINDREF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_INDCOD From WMC1SFTINDREF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_INDESAXX From WMC1SFTINDREF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_INDESAXX From WMC1SFTINDREF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_INDTYP From WMC1SFTINDREF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_INDTYP From WMC1SFTINDREF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREDATTIM From WMC1SFTINDREF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREDATTIM From WMC1SFTINDREF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDATTIM From WMC1SFTINDREF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDATTIM From WMC1SFTINDREF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AUUID From WMC1SFTINDREF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AUUID From WMC1SFTINDREF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREUSR From WMC1SFTINDREF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREUSR From WMC1SFTINDREF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDUSR From WMC1SFTINDREF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDUSR From WMC1SFTINDREF : Gosub TESTALLSTAT :  Endif
[L]ASTATUS = fmet this.ACONTROL
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



