# Supervisor
# Mask management CRMAGD1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone AGDBAP
mkstat = 0
zoncou = "AGDBAP"
indice = 0
IMPENT = [M]AGDBAP
If varinit([M]AGDBAP,indice)=0
 IMPENT = 1
Endif
If varmode([M]AGDBAP,indice)=0
 [M]AGDBAP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "AGDBAP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]AGDBAP(indice)
 Endif
Endif
[M]AGDBAP = IMPENT

## Zone AGDTSK
mkstat = 0
zoncou = "AGDTSK"
indice = 0
IMPENT = [M]AGDTSK
If varinit([M]AGDTSK,indice)=0
 IMPENT = 1
Endif
If varmode([M]AGDTSK,indice)=0
 [M]AGDTSK(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "AGDTSK" : Gosub ACTION From =IMPTRT
  IMPENT = [M]AGDTSK(indice)
 Endif
Endif
[M]AGDTSK = IMPENT

## Zone AGDCLL
mkstat = 0
zoncou = "AGDCLL"
indice = 0
IMPENT = [M]AGDCLL
If varinit([M]AGDCLL,indice)=0
 IMPENT = 1
Endif
If varmode([M]AGDCLL,indice)=0
 [M]AGDCLL(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "AGDCLL" : Gosub ACTION From =IMPTRT
  IMPENT = [M]AGDCLL(indice)
 Endif
Endif
[M]AGDCLL = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIGAGD
mkstat = 0
zoncou = "NBLIGAGD"
indice = 0
status = 1
zoncou = "NBLIGAGD"
If varmode([M]NBLIGAGD,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBLIGAGD From SUBCRMAGD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBLIGAGD : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIGAGD" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIGAGD
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIGAGD = nolign
   status = 75
   Call SAI_NBLIGAGD(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIGAGD(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIGAGD
mkstat = 0
zoncou = "NBLIGAGD"

## Zone TYPACT
mkstat = 0
zoncou = "TYPACT"
IMPENT = [M]TYPACT(indice)
If varinit([M]TYPACT,indice)=0
 IMPENT = 1
Endif
[M]TYPACT(indice) = IMPENT

## Zone EXE
mkstat = 0
zoncou = "EXE"
IMPENT = [M]EXE(indice)
If varinit([M]EXE,indice)=0
 IMPENT = 1
Endif
[M]EXE(indice) = IMPENT

## Zone NBLIGAGD
mkstat = 0
zoncou = "NBLIGAGD"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CRMAGD1"
Return

