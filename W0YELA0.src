# Supervisor
# Mask management YELA0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YLONAGR
mkstat = 0
zoncou = "YLONAGR"
indice = 0
If varmode([M]YLONAGR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLONAGR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("YELA","!$",[M]YLONAGR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YLONAGR" : GZNUM = 100118 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YLONAGRDES
mkstat = 0
zoncou = "YLONAGRDES"
indice = 0
If varmode([M]YLONAGRDES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLONAGRDES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YENAFLG
mkstat = 0
zoncou = "YENAFLG"
indice = 0
IMPENT = [M]YENAFLG
If varinit([M]YENAFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]YENAFLG,indice)=0
 [M]YENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YENAFLG(indice)
 Endif
Endif
[M]YENAFLG = IMPENT

## Zone YSRCLONAGR
mkstat = 0
zoncou = "YSRCLONAGR"
indice = 0
If varmode([M]YSRCLONAGR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSRCLONAGR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("YELA","!$",[M]YSRCLONAGR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YSRCLONAGR" : GZNUM = 100680 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YELA0"
Return

