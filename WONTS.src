$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(20)(0..GNBDERNIER)
Local Char     A_BUF2(10)(0..GNBDERNIER)
Local Char     A_BUF3(80)(0..GNBDERNIER)
Local Libelle  A_BUF4(0..GNBDERNIER)
Local Char     A_BUF5(5)(0..GNBDERNIER)
Local Char     A_BUF6(5)(0..GNBDERNIER)
Local Date     A_BUF7(0..GNBDERNIER)
Local Date     A_BUF8(0..GNBDERNIER)
If clalev([F:NTS9])>0 : Close Local File [NTS9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(20),
& Char     A_TAB2(10),
& Char     A_TAB3(80),
& Libelle  A_TAB4,
& Char     A_TAB5(5),
& Char     A_TAB6(5),
& Date     A_TAB7,
& Date     A_TAB8
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5,A_BUF6,A_BUF7,A_BUF8
& As [NTS9]
Return

$LEC_DERLU
Read [AXX]AXX0="NOTE";"NTSDESAXX";GLANGUE;[F:NTS]CODE;[F:NTS]NTSCOD2
If fstat : Raz [F:AXX] : Endif
Read [AX3]AXX0="NOTE";"NTSSHOAXX";GLANGUE;[F:NTS]CODE;[F:NTS]NTSCOD2
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:NTS9])>0
 Read [NTS9] = [F:NTS9]A_TABI
 [L]CLE = [F:NTS9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:NTS9])>0
 Filter [NTS9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [NTS0] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:NTS9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:NTS]NTSCOD2+"~"
 A_CLEDERLU += [F:NTS]CODE+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:NTS9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:NTS9]
 Endif
Endif
Return

