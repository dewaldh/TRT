# Supervisor
# Mask management ASHW0 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:50:09"
A_USER  = "ADMIN"
A_STAMP = "20180827235009"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask ASHW0 [ASW0]
Gosub HORODAT
Local Inpbox "ASHW0" From GFONCTION At A_STAMP With A_WINDPREV Mask [ASW0] As [SIM_]
Grizo [ASW0]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [ASW0]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=2
Return

$ON_CODREP
Onevent CONTROL Call C_CODREP(zc) From W1ASHW0
Onevent BEFORE_INPUT Call AS_CODREP(zc) From W1ASHW0
Return

Subprog AS_CODREP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_CODREP(zc) From W1ASHW0
Onkey 144 Titled GBOUTP Call P_CODREP(zc) From W1ASHW0
Onkey 146 Titled GBOUTA Call SA_CODREP(zc) From W1ASHW0
Gosub _AFF
End

Subprog C_CODREP(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "CODREP" : GZNUM = 48741 : Endif
If !mkstat
 ## Action CONTOBJW
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONTOBJW"
 PARMSK(1) = "ASW"
 Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJW","CONTOBJ","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CODREP" : GZNUM = 48741 : Endif
Endif
Gosub _AFF
End

Subprog S_CODREP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "ASW"
PARMSK(2) = "$"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_CODREP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ASW"
PARMSK(3) = "$"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CODREP(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ASW",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_AZINTREP
Onevent BEFORE_INPUT Call AS_AZINTREP(zc) From W1ASHW0
Onevent AFTER_AREA Call AP_AZINTREP(zc) From W1ASHW0
Onevent CONTROL Call C_AZINTREP(zc) From W1ASHW0
Return

Subprog AS_AZINTREP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
If !GWEBSERV
 ## Action ABRINI
 Call INIABR From SUBAAB
 If mkstat=2
  [M]AZINTREP(INDIC) = VALEUR
  Affzo AZINTREP(INDIC) : mkstat = 2
 Endif
Endif
Gosub _AFF
End

Subprog C_AZINTREP(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "AZINTREP" : GZNUM = 8 : Endif
Gosub _AFF
End

Subprog AP_AZINTREP(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action TXTCONT
 Call TXTCONT(VALEUR,[M]AZINTREP,[M]INTREP,30,1,[M]CODREP,"INTREP") From OBJDIV
 If mkstat : GERR = 1 : GZONE = "AZINTREP" : GZNUM = 8 : Endif
Endif
Gosub _AFF
End

$ON_MODIFLNK
Onevent INIT_AREA Call D_MODIFLNK(zc) From W1ASHW0
Return

Subprog D_MODIFLNK(VALEUR)
Variable Integer VALEUR
If varinit([M]MODIFLNK,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_MODIFPAR
Onevent INIT_AREA Call D_MODIFPAR(zc) From W1ASHW0
Return

Subprog D_MODIFPAR(VALEUR)
Variable Integer VALEUR
If varinit([M]MODIFPAR,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$CHG_STY
Return


