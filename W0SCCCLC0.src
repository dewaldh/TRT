  IMPENT = [M]MFCTYP(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_MFCTYP(IMPENT) From SUBSCC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "MFCTYP" : GZNUM = 20138 : Endif
 Gosub AFF From GSAISIE
