 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_MFGTYP(IMPENT) From SUBSCC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "MFGTYP" : GZNUM = 15296 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
