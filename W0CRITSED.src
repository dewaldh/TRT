# Supervisor
# Mask management CRITSED 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ITMREFSTR
mkstat = 0
zoncou = "ITMREFSTR"
indice = 0
WCHAR = [M]ITMREFSTR
If varmode([M]ITMREFSTR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREFSTR" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "VXADCUPC"
 Call ADCCTRUPC([M]ITMREFSTR) From VXADCUPC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("ITM","!",[M]ITMREFSTR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTITF"
   Call CONTITF([M]ITMREFSTR,[M]FCY) From STKLIB
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WCHAR<>[M]ITMREFSTR or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_ITMREFSTR([M]ITMREFSTR) From CNSSEDSTD
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "ITMREFSTR" : GZNUM = 12930 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone ITMREFEND
mkstat = 0
zoncou = "ITMREFEND"
indice = 0
WCHAR = [M]ITMREFEND
SAVACT = GACTION : GACTION = "STD"
Call AV_ITMREFEND([M]ITMREFEND) From CNSSEDSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]ITMREFEND : Endif
If !mkstat
 If varmode([M]ITMREFEND,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "ITMREFEND" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "VXADCUPC"
  Call ADCCTRUPC([M]ITMREFEND) From VXADCUPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ITM","!",[M]ITMREFEND,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "CTLALPV"
    Call CTLALPV([M]ITMREFEND,[M]ITMREFSTR) From CONTDIV
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTITF"
     Call CONTITF([M]ITMREFEND,[M]FCY) From STKLIB
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]ITMREFEND or IMPMOD
       SAVACT = GACTION : GACTION = "STD"
       Call AM_ITMREFEND([M]ITMREFEND) From CNSSEDSTD
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "ITMREFEND" : GZNUM = 12931 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone ECRAN
mkstat = 0
zoncou = "ECRAN"
indice = 0
If varmode([M]ECRAN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ECRAN" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("GTC","!",[M]ECRAN,"SED",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "ECRAN" : GZNUM = 46 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CRITSED"
Return

