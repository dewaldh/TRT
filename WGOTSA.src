$BOUTON
STDBOUT = "ECSAN09<>RHILJMP"
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "a" : LBOUT(NBTEX) = 1801 : Call TEXTE(10072,TEXTE(NBTEX)) From OBJDIV
