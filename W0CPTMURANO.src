#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CPTMURANO 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ALLCPY
mkstat = 0
zoncou = "ALLCPY"
indice = 0
IMPENT = [M]ALLCPY
If varinit([M]ALLCPY,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLCPY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_ALLCPY(IMPENT) From TRTMURANO
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]ALLCPY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLCPY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLCPY(indice)
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_ALLCPY(IMPENT) From TRTMURANO
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ALLCPY" : GZNUM = 6052 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]ALLCPY = IMPENT

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CPY([M]CPY) From TRTMURANO
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CPY : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "INICPY"
  Call INICPY([M]CPY) From EXEFNC
  GACTION = SAVACT
  If mkstat=1 : Raz [M]CPY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTCPY"
   Call CONTSOC([M]CPY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("CPY","!A",[M]CPY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTCPY"
     Call CONTCPY([M]CPY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "STD"
      Call C_CPY([M]CPY) From TRTMURANO
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone ALLFCY
mkstat = 0
zoncou = "ALLFCY"
indice = 0
IMPENT = [M]ALLFCY
If varinit([M]ALLFCY,indice)=0
 IMPENT = 1
Endif
If varmode([M]ALLFCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_ALLFCY(IMPENT) From TRTMURANO
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]ALLFCY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLFCY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLFCY(indice)
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_ALLFCY(IMPENT) From TRTMURANO
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ALLFCY" : GZNUM = 6053 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]ALLFCY = IMPENT

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FCY([M]FCY) From TRTMURANO
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "INIFCY"
  Call INIFCY([M]FCY,[M]CPY) From EXEFNC
  GACTION = SAVACT
  If mkstat=1 : Raz [M]FCY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTFCY"
   Call CONTSITE([M]FCY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("FCY","!",[M]FCY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTFCYCPY"
     Call CONTFCYCPY([M]FCY,[M]CPY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "CONTFCY"
      Call CONTFCY([M]FCY) From EXEFNC
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       SAVACT = GACTION : GACTION = "STD"
       Call C_FCY([M]FCY) From TRTMURANO
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone STRDAT
mkstat = 0
zoncou = "STRDAT"
indice = 0
If varinit([M]STRDAT,indice)=0
 [M]STRDAT = date$
Endif
If varmode([M]STRDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "STRDAT" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_STRDAT([M]STRDAT) From TRTMURANO
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 7288 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone ENDDAT
mkstat = 0
zoncou = "ENDDAT"
indice = 0
If varinit([M]ENDDAT,indice)=0
 [M]ENDDAT = [M]STRDAT
Endif
If varmode([M]ENDDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENDDAT" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "CTLDATVAL"
 Call CTLDATVAL([M]ENDDAT,[M]STRDAT) From CONTX3
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ENDDAT" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone LOT
mkstat = 0
zoncou = "LOT"
indice = 0
If varmode([M]LOT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LOT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone STALOT
mkstat = 0
zoncou = "STALOT"
indice = 0
IMPENT = [M]STALOT
If varinit([M]STALOT,indice)=0
 IMPENT = 1
Endif
If varmode([M]STALOT,indice)=0
 Call INICOMBO(617) From COMBOS
 [M]STALOT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "STALOT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]STALOT(indice)
 Endif
 If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
  mkstat = 1 : GMESSAGE = mess(99,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "STALOT" : GZNUM = 1287 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]STALOT = IMPENT

## Zone ENTDAT
mkstat = 0
zoncou = "ENTDAT"
indice = 0
If varmode([M]ENTDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENTDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone TRC
mkstat = 0
zoncou = "TRC"
indice = 0
IMPENT = [M]TRC
If varinit([M]TRC,indice)=0
 IMPENT = 2
Endif
If varmode([M]TRC,indice)=0
 [M]TRC(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TRC" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TRC(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "TRC" : GZNUM = 3342 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]TRC = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CPTMURANO"
Return

