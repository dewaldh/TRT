Endif
Return

$CLE_DERLU
If clalev([F:YEST9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = num$([F:YEST]YESTID)+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:YEST9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:YEST9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 1
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:YEST]YESTID"
Call TEXTE(100395,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 2
SELFMT(NBSEL) = "N:3F"
SELFOR(NBSEL) = "N:3#"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:YEST]YESTID"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:YEST]YESTNBR"
Call TEXTE(1120,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 2
SELFMT(NBSEL) = "N:3F"
SELFOR(NBSEL) = "N:3#"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:YEST]YESTNBR"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:YEST]YESTDES"
Call TEXTE(1470,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:50X"
SELFOR(NBSEL) = "K:50X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:YEST]YESTDES"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:YEST_])=0
 Gosub OUVFIC_LISTE
Endif
Return

