$BOUTON
STDBOUT = ""
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "E" : LBOUT(NBTEX) = 1101 : Call TEXTE(12413,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "A" : LBOUT(NBTEX) = 1802 : Call TEXTE(12409,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
NBMEN += 1
NMEN(NBMEN) = 10 : Call TEXTE(34,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "a" : LMEN(NBMEN) = 2001 : Call TEXTE(3531,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "b" : LMEN(NBMEN) = 2002 : Call TEXTE(29670,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 20 : Call TEXTE(6084,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 20 : CMEN(NBMEN) = "N" : LMEN(NBMEN) = 2003 : Call TEXTE(12817,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 20 : CMEN(NBMEN) = "O" : LMEN(NBMEN) = 2004 : Call TEXTE(23197,TMEN(NBMEN)) From OBJDIV
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "a"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "ENT"
  FLGEXE = 1
  GACTION = "SAISTOANA"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISTOANA","TRTSTOANA","XYTRTSTOANA") From GSAISIE
 When "b"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "SAISTOENTS"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISTOENTS","TRTSTOENTS","XYTRTSTOENTS") From GSAISIE
 When "N"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$(1)
  PARBOUT(2) = [M:ENT]STOFCY
  PARBOUT(3) = [M:ENT]ITMREF
  PARBOUT(4) = ""
  FLGEXE = 1
  GACTION = "CONSSFC"
  Call CONSULT_CHAR(VALBOUT,"SFC",PARBOUT) From GCONSULT
 When "O"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:ENT]STOFCY
  PARBOUT(2) = [M:ENT]ITMREF
  PARBOUT(3) = num$([M:ENT]TRSTYP)
  PARBOUT(4) = [M:ENT]TRSCOD
  FLGEXE = 1
  GACTION = "VISUSRUV"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISRUV","VISUSRUV","XYVISUSRUV") From GSAISIE
  [M:ENT]STOFCY = PARBOUT(1)
  [M:ENT]ITMREF = PARBOUT(2)
  [M:ENT]TRSTYP = val(PARBOUT(3))
  [M:ENT]TRSCOD = PARBOUT(4)
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


