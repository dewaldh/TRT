# Supervisor
# Mask management POO0 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:08:40"
A_USER  = "ADMIN"
A_STAMP = "20180828000840"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBL"
  If [M]NBL=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask POO0 [POO0]
Gosub HORODAT
Local Inpbox "POO0" From GFONCTION At A_STAMP With A_WINDPREV Mask [POO0] As [SIM_]
Grizo [POO0]5
Grizo [POO0]10
Grizo [POO0]15
Grizo [POO0]20
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [POO0]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=3
X1(1)=1 : X2(1)=74 : Y1(1)=4 : Y2(1)=5
X1(2)=1 : X2(2)=74 : Y1(2)=6 : Y2(2)=8
X1(3)=1 : X2(3)=74 : Y1(3)=9 : Y2(3)=28
Return

$ON_POO
Onevent CONTROL Call C_POO(zc) From W1POO0
Onevent BEFORE_INPUT Call AS_POO(zc) From W1POO0
Return

Subprog AS_POO(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_POO(zc) From W1POO0
Gosub _AFF
End

Subprog C_POO(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "POO" : GZNUM = 5296 : Endif
If !mkstat
 ## Action CONTOBJW
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONTOBJW"
 PARMSK(1) = "POO"
 Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJW","CONTOBJ","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "POO" : GZNUM = 5296 : Endif
Endif
Gosub _AFF
End

Subprog S_POO(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELOBJET
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELOBJET"
 PARMSK(1) = "POO"
 PARMSK(2) = "POO"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJET","SELOBJ","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

