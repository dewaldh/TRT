# Supervisor
## CRUD-CLASSE ## PJMCOSTCTR
# 2018-08-27 13:23:34 047


############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(PJMCOSTCTR,ASTATUS)
Variable Instance PJMCOSTCTR Using R_PJMCOSTCTR
Variable Integer ASTATUS

If PJMCOSTCTR = null : End : Endif

Gosub AVARIABLES From WMR00PJMCOSTCTR
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If fmet PJMCOSTCTR.ACTX.AGETAFCRIGHT(PJMCOSTCTR,"GESPJCC",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END : Endif
If fmet PJMCOSTCTR.ACTX.AGETAFCRIGHTC(PJMCOSTCTR,"GESPJCC",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END
Endif

Gosub AREPCONTROL_AINSERT
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If !adxlog
 Gosub AINSERT_TR_PJMCOSTCTR
Else
 Gosub AINSERT_PJMCOSTCTR
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_END : Return : Endif

Gosub _AINSERT_AFTER
$AINSERT_END
[L]A_I = PJMCOSTCTR.freeSnapshot
PJMCOSTCTR.snapshotEnabled = 1
End

#-----------------------------------------------------#
$AREPCONTROL_AINSERT
PJMCOSTCTR.ASTALIN = [V]CST_ANEW
PJMCOSTCTR.PJCC.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PJMCOSTCTR._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PJMCOSTCTR._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PJMCOSTCTR._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If PJMCOSTCTR.PJCC.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PJMCOSTCTR.PJCC.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.PJCC.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(PJMCOSTCTR.PJCC.CDIE)
 If PJMCOSTCTR.PJCC.CDIE([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.PJCC.CDIE([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(PJMCOSTCTR.PJCC.DATRATE) : # PJMCOSTDAT -- 2
 If PJMCOSTCTR.PJCC.DATRATE(A_N2) <> null
& and !find(PJMCOSTCTR.PJCC.DATRATE(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.PJCC.DATRATE(A_N2)._AINSERT_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
For A_N2=1 To maxtab(PJMCOSTCTR.PJCC.DATRATE) : # PJMCOSTDAT -- 2
 If PJMCOSTCTR.PJCC.DATRATE(A_N2) <> null
& and !find(PJMCOSTCTR.PJCC.DATRATE(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.PJCC.DATRATE(A_N2)._AINSERT_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(PJMCOSTCTR.PJCC.CDIE)
 If PJMCOSTCTR.PJCC.CDIE([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.PJCC.CDIE([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.PJCC.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_PJMCOSTCTR
Trbegin [F:PJCC]
 Gosub AINSERT_PJMCOSTCTR
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AINSERT_AFTER
If find(PJMCOSTCTR.PJCC.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PJMCOSTCTR.PJCC.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(PJMCOSTCTR.PJCC.CDIE)
 If PJMCOSTCTR.PJCC.CDIE([L]A_I)<>null
  If PJMCOSTCTR.PJCC.CDIE([L]A_I).ASTALIN=[V]CST_ADEL or PJMCOSTCTR.PJCC.CDIE([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PJMCOSTCTR.PJCC.CDIE([L]A_I)
   PJMCOSTCTR.PJCC.CDIE([L]A_I) = null
  Elsif PJMCOSTCTR.PJCC.CDIE([L]A_I).ASTALIN <> [V]CST_ALL
   PJMCOSTCTR.PJCC.CDIE([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PJMCOSTCTR.PJCC.DATRATE)
 If PJMCOSTCTR.PJCC.DATRATE(A_N2) <> null
  [L]ASTATUS2 = fmet PJMCOSTCTR.PJCC.DATRATE(A_N2)._AINSERT_AFTER
  If PJMCOSTCTR.PJCC.DATRATE(A_N2).ASTALIN=[V]CST_ADEL or PJMCOSTCTR.PJCC.DATRATE(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PJMCOSTCTR.PJCC.DATRATE(A_N2)
   PJMCOSTCTR.PJCC.DATRATE(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AINSERT_PJMCOSTCTR
[L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.PJCC._AINSERT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PJMCOSTCTR.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If PJMCOSTCTR.snapshotEnabled=1
 [L]A_RETIK = PJMCOSTCTR.revertUpdTick
Endif
[L]ASTATUS = max(fmet PJMCOSTCTR.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



