# Supervisor
# Mask management ARC0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CODARC
mkstat = 0
zoncou = "CODARC"
indice = 0
If varmode([M]CODARC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CODARC" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CODARC,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CODARC" : GZNUM = 42339 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "ARC"
  Local Char    VALEUR(250)
  VALEUR = [M]CODARC
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]CODARC = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CODARC" : GZNUM = 42339 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone INTIT
mkstat = 0
zoncou = "INTIT"
indice = 0
If varinit([M]INTIT,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]INTIT,"ARCHPAR","INTIT",CODARC,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]INTIT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "INTIT" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
If varinit([M]ENAFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]ENAFLG,indice)=0
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
Endif
[M]ENAFLG = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ARC0"
Return

