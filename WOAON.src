$LEC_DERLU
Read [AXX]AXX0="AOBJBURMOD";"INTIT";GLANGUE;[F:AON]ABREV;[F:AON]TRNCPYMOD
If fstat : Raz [F:AXX] : Endif
Return

$CHARGE_DERLU
If clalev([F:AON9])>0
 Read [AON9] = [F:AON9]A_TABI
 [L]CLE = [F:AON9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:AON9])>0
 Filter [AON9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AON1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AON9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AON]TRN+"~"
 A_CLEDERLU += [F:AON]ABREV+"~"
 A_CLEDERLU += [F:AON]CPY+"~"
 A_CLEDERLU += [F:AON]MODELE+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AON9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AON9]
 Endif
Endif
Return

