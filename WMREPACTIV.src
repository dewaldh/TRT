          Call ASET_AREPLNK(REP.LNK(2).LNK(1),'$query',1,'','ACTIV',5,'ACTIV',49616,4,'','',2,2,1,1,1) From ASYRSUBREP
     
     REP.LNK(2).LNK(2) = NewInstance C_AREPLNK AllocGroup REP : # Display
          Call ASET_AREPLNK(REP.LNK(2).LNK(2),'$details',1,'','ACTIV',2,'ACTIV',49838,4,'','',1,2,2,1,1) From ASYRSUBREP
          REP.LNK(2).LNK(2).CLE(1)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(2).LNK(2).CLE(1),'CODACT',7,'ACV.CODACT','CODACT',0,0,0,0,'CODACT',1,1) From ASYRSUBREP
     

