#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1ACTIV
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "ACTIV"
REP.CLA = "ACTIV"
REP.ICL = "ACV"
REP.AFCRIGHT = "GESACV"
REP.NCL = 186 : # Activity codes
REP.DES = 4 : # Activity code
REP.TIT = "LIBACT"
REP.ANX = "CODACT"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Activity code
     REP.CLE(1).PAR = 'CODACT' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'CODACT' : REP.CLE(1).VPR= 'ACV.CODACT' : REP.CLE(1).DES = 4
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'ACTIV_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 2 : # Detail
REP.AENAFAC(2) = 2 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 2 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "MAIN" : REP.SEC(1).NSC = 186 : # Activity codes
     REP.SEC(1).BLC(1)="ID" : REP.SEC(1).NBC(1)=1377 : # Identifier
     REP.SEC(1).BLC(2)="CHAR" : REP.SEC(1).NBC(2)=3965 : # Characteristics
Return

#-------------------------------------------------------#
# Liens de la représentation
#-------------------------------------------------------#
$LINK_REP
REP.LNK(1) = NewInstance C_AREPLNK AllocGroup REP : # Save
     Call ASET_AREPLNK(REP.LNK(1),'$save',1,'','',17,'ACTIV',12413,4,'','',1,2,1,1,1) From ASYRSUBREP
     REP.LNK(1).ATT(1)= NewInstance C_AREPATT AllocGroup REP
          REP.LNK(1).ATT(1).COD = "$isHidden" : REP.LNK(1).ATT(1).ATT = "true"
     REP.LNK(1).ATT(2)= NewInstance C_AREPATT AllocGroup REP
          REP.LNK(1).ATT(2).COD = "$isDisabled" : REP.LNK(1).ATT(2).ATT = "false"
     REP.LNK(1).LNK(1) = NewInstance C_AREPLNK AllocGroup REP : # List
          Call ASET_AREPLNK(REP.LNK(1).LNK(1),'$query',1,'','ACTIV',5,'ACTIV',49616,4,'','',2,2,1,1,1) From ASYRSUBREP
     
     REP.LNK(1).LNK(2) = NewInstance C_AREPLNK AllocGroup REP : # Display
          Call ASET_AREPLNK(REP.LNK(1).LNK(2),'$details',1,'','ACTIV',2,'ACTIV',49838,4,'','',1,2,2,1,1) From ASYRSUBREP
          REP.LNK(1).LNK(2).CLE(1)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(1).LNK(2).CLE(1),'CODACT',7,'ACV.CODACT','CODACT',0,0,0,0,'CODACT',1,1) From ASYRSUBREP
     
     REP.LNK(1).LNK(3) = NewInstance C_AREPLNK AllocGroup REP : # Edit
          Call ASET_AREPLNK(REP.LNK(1).LNK(3),'$edit',1,'','ACTIV',3,'ACTIV',12964,4,'','',2,2,1,1,1) From ASYRSUBREP
          REP.LNK(1).LNK(3).CLE(1)  = NewInstance C_AREPPAR AllocGroup REP
               Call ASET_AREPPAR(REP.LNK(1).LNK(3).CLE(1),'CODACT',7,'ACV.CODACT','CODACT',0,0,0,0,'CODACT',1,1) From ASYRSUBREP
     

REP.LNK(2) = NewInstance C_AREPLNK AllocGroup REP : # Cancel
     Call ASET_AREPLNK(REP.LNK(2),'$abort',1,'','',18,'ACTIV',12409,4,'','',1,2,1,1,1) From ASYRSUBREP
