Raz mkstat
Call TEXTE(1515,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_BPSINV(zc) From W1WE3ALL1
Onkey   2 Titled GBOUTS Call S_BPSINV(zc) From W1WE3ALL1
Onkey 144 Titled GBOUTP Call P_BPSINV(zc) From W1WE3ALL1
Onkey 146 Titled GBOUTA Call SA_BPSINV(zc) From W1WE3ALL1
Gosub _AFF
End

Subprog C_BPSINV(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "BPSINV" : GZNUM = 8472 : Endif
If !GWEBSERV
 If !mkstat
  ## Action BPSCHOICE
  Local Char    PARMSK(250)(1..20) , SAVACT(20)
  SAVACT = GACTION : GACTION = "BPSCHOICE"
  Call S_TAB_CHAR(VALEUR,PARMSK,"BPSCHOICE","TRTSEL","") From GACTION
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "BPSINV" : GZNUM = 8472 : Endif
 Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("BPS","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "BPSINV" : GZNUM = 8472 : Endif
 If !mkstat
  ## Action STD
  Call C_BPSINV(VALEUR) From SUBPOH
  If mkstat : GERR = 1 : GZONE = "BPSINV" : GZNUM = 8472 : Endif
  If !mkstat
   ## Action CONTBPRCPY
   Call CONTBPRCPY(VALEUR,[M:POH0]POHFCY,"","","F") From CPTSUB
   If mkstat : GERR = 1 : GZONE = "BPSINV" : GZNUM = 8472 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

$AMM_BPSINV
Call AM_BPSINV(zc)
If !mkstat & GOBJET
 zoncou = "BPSINV" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

