Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone SCDMOD
mkstat = 0
zoncou = "SCDMOD"
indice = 0
