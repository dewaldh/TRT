# Supervisor
# Mask management SHIPDOC 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
IMPENT = [M]VCRTYP(indice)
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP(indice) = IMPENT

## Zone SDDOC
mkstat = 0
zoncou = "SDDOC"
If varmode([M]SDDOC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SDDOC" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone SDTYPE
mkstat = 0
zoncou = "SDTYPE"
If varinit([M]SDTYPE,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]SDTYPE(indice),"107","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]SDTYPE,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "SDTYPE" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]SDTYPE(indice),2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "SDTYPE" : GZNUM = 865 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTADI"
   Call CONTADI([M]SDTYPE(indice),"107","") From SUBADI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "SDTYPE" : GZNUM = 865 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("ADI","",[M]SDTYPE(indice),"107",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "SDTYPE" : GZNUM = 865 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone SDORI
mkstat = 0
zoncou = "SDORI"
IMPENT = [M]SDORI(indice)
If varinit([M]SDORI,indice)=0
 IMPENT = 1
Endif
If varmode([M]SDORI,indice)=0
 [M]SDORI(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "SDORI" : Gosub ACTION From =IMPTRT
  IMPENT = [M]SDORI(indice)
 Endif
Endif
[M]SDORI(indice) = IMPENT

## Zone SDDATE
mkstat = 0
zoncou = "SDDATE"
If varmode([M]SDDATE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SDDATE" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone SDREF1
mkstat = 0
zoncou = "SDREF1"
If varmode([M]SDREF1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SDREF1" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone SDREF2
mkstat = 0
zoncou = "SDREF2"
If varmode([M]SDREF2,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SDREF2" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone SDBP
mkstat = 0
zoncou = "SDBP"
If varmode([M]SDBP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SDBP" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("BPR","!",[M]SDBP(indice),"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "SDBP" : GZNUM = 1500 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone SDNTYP
mkstat = 0
zoncou = "SDNTYP"
If varinit([M]SDNTYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]SDNTYP(indice),"902","") From SUBADI
 GACTION = SAVACT
Endif

## Zone SDFILNAM
mkstat = 0
zoncou = "SDFILNAM"
WCHAR = [M]SDFILNAM(indice)
If varmode([M]SDFILNAM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SDFILNAM" : Gosub ACTION From =IMPTRT
 Endif
 If WCHAR<>[M]SDFILNAM(indice) or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_SDFILNAM([M]SDFILNAM(indice)) From SUBSHIP
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "SDFILNAM" : GZNUM = 12749 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone SDCMT
mkstat = 0
zoncou = "SDCMT"
If varmode([M]SDCMT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "SDCMT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SHIPDOC"
Return

