$ON_TYPPCE
Onevent CONTROL Call C_TYPPCE(zc) From W1CNVECAR
Onevent BEFORE_INPUT Call AS_TYPPCE(zc) From W1CNVECAR
Onevent AFTER_CHANGE Gosub AMM_TYPPCE From W1CNVECAR
Return

Subprog AS_TYPPCE(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(5152,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_TYPPCE(VALEUR) From CNVECAR
If mkstat=2
 [M]TYPPCE(INDIC) = VALEUR
 Affzo TYPPCE(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_TYPPCE(zc) From W1CNVECAR
Onkey   2 Titled GBOUTS Call S_TYPPCE(zc) From W1CNVECAR
Onkey 144 Titled GBOUTP Call P_TYPPCE(zc) From W1CNVECAR
Onkey 146 Titled GBOUTA Call SA_TYPPCE(zc) From W1CNVECAR
Gosub _AFF
End

Subprog C_TYPPCE(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "TYPPCE" : GZNUM = 6087 : Endif
If !mkstat
 ## Action CONTGTE
 Call CONTGTE(VALEUR,"","") From SUBGTE
 If mkstat : GERR = 1 : GZONE = "TYPPCE" : GZNUM = 6087 : Endif
 If !mkstat
  ## Action CONTOBJN
  Local Char    PARMSK(250)(1..20) , SAVACT(20)
  SAVACT = GACTION : GACTION = "CONTOBJN"
  Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJN","CONTOBJ","") From GACTION
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "TYPPCE" : GZNUM = 6087 : Endif
  If !mkstat
   ## Action STD
   Call C_TYPPCE(VALEUR) From CNVECAR
   If mkstat : GERR = 1 : GZONE = "TYPPCE" : GZNUM = 6087 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

$AMM_TYPPCE
Call AM_TYPPCE(zc)
If !mkstat & GOBJET
 zoncou = "TYPPCE" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_TYPPCE(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZTYPPCE)")>indice
 [M]TYPPCE(indice) = VALEUR
 Affzo ZTYPPCE(indice)
Endif
Gosub _AFF
End

Subprog S_TYPPCE(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELGTE
 Call SELGTE(VALEUR,"",[M]LEDTYP,"") From SUBGTE
Endif
Gosub _AFF
End

Subprog B1_TYPPCE(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action TUNGTE
 Call TUNGTE(VALEUR,"") From SUBGTE
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog SA_TYPPCE(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "GTE"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_TYPPCE(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("GTE",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

