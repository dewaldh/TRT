Subprog D_TYPECART(VALEUR)
Variable Integer VALEUR
If varinit([M]TYPECART,indice)=0
 VALEUR = 2
Endif
Gosub _AFF
End

Subprog AS_TYPECART(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(3666) From COMBOS
## Action STD
Call AS_TYPECART(VALEUR) From CNVECAR
If mkstat=2
 [M]TYPECART(INDIC) = VALEUR
 Affzo TYPECART(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,3666,1))))
Endif
If !mkstat
 Discombo 0 From TYPECART(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From TYPECART(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_TYPECART(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
## Action STD
Call C_TYPECART(VALEUR) From CNVECAR
If mkstat : GERR = 1 : GZONE = "TYPECART" : GZNUM = 54894 : Endif
Gosub _AFF
End

