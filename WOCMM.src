$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(20)(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
Local Char     A_BUF3(15)(0..GNBDERNIER)
If clalev([F:CMM9])>0 : Close Local File [CMM9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(20),
& Char     A_TAB2(30),
& Char     A_TAB3(15)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3
& As [CMM9]
Return

$LEC_DERLU
Read [AFF]AFF0 = [F:CMM]FCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Return

$CHARGE_DERLU
If clalev([F:CMM9])>0
 Read [CMM9] = [F:CMM9]A_TABI
 [L]CLE = [F:CMM9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:CMM9])>0
 Filter [CMM9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [CMM1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:CMM9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:CMM]NUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:CMM9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:CMM9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 0
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:CMM]NUM"
Call TEXTE(5649,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:20X"
SELFOR(NBSEL) = "K:20X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:CMM]NUM"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:CMM]DESVCR"
Call TEXTE(8,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = "K:30X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:CMM]DESVCR"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:CMM]BPR"
Call TEXTE(19569,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:15X"
SELFOR(NBSEL) = "K:15c"
SELMAJ(NBSEL) = 1
NBTRI += 1
TRIZON(NBTRI) = "[F:CMM]BPR"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:CMM_])=0
 Gosub OUVFIC_LISTE
Endif
Return

