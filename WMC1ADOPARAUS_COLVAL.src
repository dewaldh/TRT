####################################################################
# Labels of the main class and methods 
####################################################################

####################################################################
# 1 : VALUES Values ( Field A 10)
####################################################################

$SET_VALUES
If [L]_AOLDVAL<>this.VALUES
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_VALUES
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.VALUES
 Gosub PROPAGATE_VALUES
Endif
Return

$INIT_VALUES
Gosub DECLARE_VAR From WMC0ADOPARAUS_COLVAL
[L]CURPRO = "VALUES"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_VALUES
Gosub DECLARE_VAR From WMC0ADOPARAUS_COLVAL
[L]CURPRO = "VALUES"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_VALUES                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_VALUES                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_VALUES
[L]CURPRO = "VALUES"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.VALUES,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_VALUES
[L]CURPRO = "VALUES"
Return

$_AOTHERS_VALUES
[L]CURPRO = "VALUES"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_VALUES
[L]CURPRO = "VALUES"
Return

$PROPAGATE_VALUES
this.isReadonly = 0
[L]CURPRO = "VALUES"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_VALUES
Gosub DECLARE_VAR From WMC0ADOPARAUS_COLVAL
[L]CURPRO = "VALUES"
$_LNKPRO_VALUES
Return




