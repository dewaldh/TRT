For AINDCLA=1 To maxtab(this.MFI)
 If this.MFI(AINDCLA)<>null& find(this.MFI(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.MFI(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
For AINDCLA=1 To maxtab(this.MFM)
 If this.MFM(AINDCLA)<>null& find(this.MFM(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.MFM(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
For AINDCLA=1 To maxtab(this.MFO)
 If this.MFO(AINDCLA)<>null& find(this.MFO(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.MFO(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
