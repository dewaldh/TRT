# Sage X3 supervisor
# Mask management WR1STD1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_ROUNUM
Onevent CONTROL Call C_ROUNUM(zc) From W4WR1STD1
Onevent BEFORE_AREA Gosub AV_ROUNUM From W4WR1STD1
Onevent BEFORE_INPUT Call AS_ROUNUM(zc) From W4WR1STD1
Onevent AFTER_CHANGE Gosub AMM_ROUNUM From W4WR1STD1
Return

Subprog IB_ROUNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action IBPRDINF
 Call IBPRDINF(VALEUR) From PRDINFLIB
Endif
Gosub _AFF
End

$AV_ROUNUM
## Action STD
Call AV_ROUNUM(zc) From SUBMFG
Gosub _AFF
Return

Subprog AS_ROUNUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3817,GBOUT1) From OBJDIV
Call TEXTE(12837,GBOUT13) From OBJDIV
Call TEXTE(8255,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_ROUNUM(VALEUR)
If !([M]MFGTRKFLG = 1)
 Affzo ROUNUM : mkstat = 2
Endif
If !mkstat
 ## Action STD
 Call AS_ROUNUM(VALEUR) From SUBMFG
 If mkstat=2
  [M]ROUNUM(INDIC) = VALEUR
  Affzo ROUNUM(INDIC) : mkstat = 2
 Endif
 Onkey 129 Titled GBOUT1 Call B1_ROUNUM(zc) From W4WR1STD1
 Onkey 170 Titled GBOUT13 Call B13_ROUNUM(zc) From W4WR1STD1
 Onkey   2 Titled GBOUTS Call S_ROUNUM(zc) From W4WR1STD1
 Onkey 144 Titled GBOUTP Call P_ROUNUM(zc) From W4WR1STD1
 Onkey 146 Titled GBOUTA Call SA_ROUNUM(zc) From W4WR1STD1
Endif
Gosub _AFF
End

Subprog C_ROUNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ITMCHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ITMCHOICE"
 Call S_TAB_CHAR(VALEUR,PARMSK,"ITMCHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ROUNUM" : GZNUM = 171 : Endif
Endif
If !mkstat
 ## Action VXADCUPC
 Call ADCCTRUPC(VALEUR) From VXADCUPC
 If mkstat : GERR = 1 : GZONE = "ROUNUM" : GZNUM = 171 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ITM","!",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ROUNUM" : GZNUM = 171 : Endif
  If !mkstat
   ## Action STD
   Call C_ROUNUM(VALEUR) From SUBMFG
   If mkstat : GERR = 1 : GZONE = "ROUNUM" : GZNUM = 171 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

$AMM_ROUNUM
Call AM_ROUNUM(zc)
If !mkstat & GOBJET
 zoncou = "ROUNUM" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_ROUNUM(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_ROUNUM(VALEUR) From SUBMFG
If mkstat : GERR = 1 : GZONE = "ROUNUM" : GZNUM = 171 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZROUNUM)")>indice
 [M]ROUNUM(indice) = VALEUR
 Affzo ZROUNUM(indice)
Endif
Gosub _AFF
End

Subprog S_ROUNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELGAM
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELGAM"
 PARMSK(1) = [M:MFG0]MFGFCY
 PARMSK(2) = ""
 PARMSK(3) = ""
 PARMSK(4) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELGAM","MFGSELLIB","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_ROUNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action GOBJETC0A
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GOBJETC0A"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_ROUNUM" : PROGSUB = "SUBMFG" : PROGSPE = "SPEMFG" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 Local Char    OBJCLE(30)
 OBJCLE = WCLE2
 Call OBJET_CHAR(OBJCLE,"ROU",WCLE1) From GOBJET
 ACTION = "BB1_ROUNUM" : PROGSUB = "SUBMFG" : PROGSPE = "SPEMFG" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog B13_ROUNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
