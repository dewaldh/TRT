# Supervisor
# Mask management CRMAGD6 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone INFBAP
mkstat = 0
zoncou = "INFBAP"
indice = 0
IMPENT = [M]INFBAP
If varinit([M]INFBAP,indice)=0
 IMPENT = 1
Endif
If varmode([M]INFBAP,indice)=0
 [M]INFBAP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INFBAP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INFBAP(indice)
 Endif
Endif
[M]INFBAP = IMPENT

## Zone INFCLL
mkstat = 0
zoncou = "INFCLL"
indice = 0
IMPENT = [M]INFCLL
If varinit([M]INFCLL,indice)=0
 IMPENT = 1
Endif
If varmode([M]INFCLL,indice)=0
 [M]INFCLL(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INFCLL" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INFCLL(indice)
 Endif
Endif
[M]INFCLL = IMPENT

## Zone INFTSK
mkstat = 0
zoncou = "INFTSK"
indice = 0
IMPENT = [M]INFTSK
If varinit([M]INFTSK,indice)=0
 IMPENT = 1
Endif
If varmode([M]INFTSK,indice)=0
 [M]INFTSK(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INFTSK" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INFTSK(indice)
 Endif
Endif
[M]INFTSK = IMPENT

## Zone INFOPP
mkstat = 0
zoncou = "INFOPP"
indice = 0
IMPENT = [M]INFOPP
If varinit([M]INFOPP,indice)=0
 IMPENT = 1
Endif
If varmode([M]INFOPP,indice)=0
 [M]INFOPP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INFOPP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INFOPP(indice)
 Endif
Endif
[M]INFOPP = IMPENT

## Zone INFCON
mkstat = 0
zoncou = "INFCON"
indice = 0
IMPENT = [M]INFCON
If varinit([M]INFCON,indice)=0
 IMPENT = 1
Endif
If varmode([M]INFCON,indice)=0
 [M]INFCON(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INFCON" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INFCON(indice)
 Endif
Endif
[M]INFCON = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIGINF
mkstat = 0
zoncou = "NBLIGINF"
indice = 0
status = 1
zoncou = "NBLIGINF"
If varmode([M]NBLIGINF,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBLIGINF From SUBCRMAGD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBLIGINF : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIGINF" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIGINF
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIGINF = nolign
   status = 75
   Call SAI_NBLIGINF(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIGINF(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIGINF
mkstat = 0
zoncou = "NBLIGINF"

## Zone INFTYPE
mkstat = 0
zoncou = "INFTYPE"
IMPENT = [M]INFTYPE(indice)
If varinit([M]INFTYPE,indice)=0
 IMPENT = 1
Endif
[M]INFTYPE(indice) = IMPENT

## Zone NBLIGINF
mkstat = 0
zoncou = "NBLIGINF"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CRMAGD6"
Return

