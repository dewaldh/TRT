# Supervisor
# Window management OTES

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [28/08/2018]
A_HEURE = "00:44:39"
A_USER  = "ADMIN"
Call ISWINVALID(nomap,GLANGUE,"OTES",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
Local Char NAVMSK(200)
NAVMSK="TES0;"
Return

$OUVRE_BOITE
Gosub INIT_BOITE
Gosub DEF_BOITE
Gosub AFF_BOITE
Return

$INIT_BOITE
Local Char    OBJLIS(GLONAOB)(0..10), DESLIS(80)(0..10), BOX1(10)
Local Integer CHGLIS(0..10), BROLIS(0..10), TRELIS(0..10)
Local Char    SAIRAP(50)(1..16)
Local Integer DERLU , AFOLDEF
Local Char    ABRENA(GLONABR)
DERLU = 1
AFOLDEF = 1
ABRENA = ""
BOX1 = "GAU_CHE"
OBJLIS(0) = "TES"
CHGLIS(0) = 2
BROLIS(0) = 2
TRELIS(0) = 1
DESLIS(0) = "CODEXP"
If clalev([M:TES0])=0 : Local Mask TES0 [TES0] : Endif
Gosub TIT
nolign = 1
If clalev([F:ACC])=0 : Local File ACCES  [ACC] : Endif
If clalev([F:AXX])=0 : Local file ATEXTRA [AXX] : Endif
If clalev([F:AX3])=0 : Local file ATEXTRA [AX3] : Endif
Link [TES] With
& [ACC]CODACC=GUSER;[F:TES]ACS,
& [AXX]AXX0="TABEXPENS";"DESTRA";GLANGUE;[F:TES]CODEXP;"",
& [AX3]AXX0="TABEXPENS";"SHOTRA";GLANGUE;CODEXP;""
& As [TES_]
Return

$DEF_BOITE
Gosub HORODAT From WGOTES
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "OTES" From GFONCTION At A_STAMP With A_WINDPREV Mask [TES0]
&  Listbox [TES_] GAU_CHE
&   [L]SAIRAP(1) = [F:TES]CODEXP Using "K:10c" Titled TITSEL(1) ,
&   [L]SAIRAP(2) = [F:AXX]TEXTE Using "K:80X" Titled TITSEL(2)
&  Listbox [TES9] GAU_CHE9
&   [L]SAIRAP(1) = [F:TES9]A_TAB1 Using "K:10c" Titled TITSEL9(1)  ,
&   [L]SAIRAP(2) = [F:TES9]A_TAB2 Using "K:80X" Titled TITSEL9(2) 
& As [TES0]
Titled [TES0] TIT
Return

$VISU_BOITE
Gosub INIT_BOITE
Gosub HORODAT From WGOTES
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "OTES" From GFONCTION At A_STAMP With A_WINDPREV Mask [TES0]
& As [TES0]
Titled [TES0] TIT
Gosub AFF_BOITE
Return

$AFF_BOITE
Boxact [TES0] 
Return

$SAISIE_BOITE
Boxinp [TES0] Using REPONSE
Return

$FERME_BOITE
Onerrgo RIEN
Close Local Inpbox [TES0]
Onerrgo
Return

$RIEN
Resume

$TIT
Local Char TITLIS(30)
Call TEXTE(23220,TITLIS) From OBJDIV
Local Char TITLIS9(30)
TITLIS9 = mess(196,123,1)
Local Char TITSEL(30)(1..2)
Call TEXTE(41193,TITSEL(1)) From OBJDIV
Call TEXTE(8,TITSEL(2)) From OBJDIV
Local Char TITSEL9(30)(1..2)
Call TEXTE(41193,TITSEL9(1)) From OBJDIV
Call TEXTE(8,TITSEL9(2)) From OBJDIV
Local Char TIT(60) , TITTRN(60)
Call TEXTE(41193,TIT) From OBJDIV
If GPROGRAMME<>""
 If dim([L]ACTION)<0 : Local Char ACTION(20) : Endif
 [L]ACTION = "TITRE" : Gosub ACTION From =GPROGRAMME
Endif
Return

$REMPLIT_
Gosub SETCLE_
If dim(FILTREG)<=0 or FILTREG
 If CRITERE=""  : CRITERE  = "1=1" : Endif
 If FILTSUP=""  : FILTSUP  = "1=1" : Endif
 If FILROLE=""  : FILROLE  = "1=1" : Endif
 If FILSUITE="" : FILSUITE = "1=1" : Endif
 Filter [TES_] Where evalue(CRITERE) & evalue(FILTSUP) & evalue(FILROLE) & evalue(FILSUITE)
&       Order With CLELISTE
Endif
GPOINT = "AV_FILLBOX" : Gosub ENTREE From EXEFNC
ACTION = "HINT" : Gosub ACTION From =GPROGRAMME
If HINTLIS(0)<>'' or NOHLIS(0)<>0
 Fillbox [TES0] GAU_CHE Hint Key =HINTLIS(0)
Else
 Gosub CALNOHOBJ From GOBJET
 If A_NOH
  Fillbox [TES0] GAU_CHE
 Else
  Fillbox [TES0] GAU_CHE Hint Key =""
 Endif
Endif
GPOINT = "AP_FILLBOX" : Gosub ENTREE From EXEFNC
Filter [TES_]
Return

$SETCLE_
Local Char    CLELISTE(250)
Local Integer INDLIS , SNSLIS , LENLIS, GAUTRELIS
INDLIS = 1
SNSLIS = 1
LENLIS = 1
GAUTRELIS = 1
CLELISTE = "CODEXP"
ACTION = "CLE_GAUCHE" : Gosub ACTION From =GPROGRAMME
Return

