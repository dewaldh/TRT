# Supervisor
# Window management AACHOOSE

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [28/08/2018]
A_HEURE = "00:32:48"
A_USER  = "ADMIN"
Call ISWINVALID(nomap,GLANGUE,"AACHOOSE",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
Local Char NAVMSK(200)
NAVMSK="AAS;"
Return

$OUVRE_BOITE
Gosub INIT_BOITE
Gosub DEF_BOITE
Gosub AFF_BOITE
Return

$INIT_BOITE
Local Char    OBJLIS(GLONAOB)(0..10), DESLIS(80)(0..10), BOX1(10)
Local Integer CHGLIS(0..10), BROLIS(0..10), TRELIS(0..10)
Local Char    SAIRAP(50)(1..16)
Local Integer DERLU , AFOLDEF
Local Char    ABRENA(GLONABR)
DERLU = 0
AFOLDEF = 1
ABRENA = ""
BOX1 = "GAU_CHE1"
OBJLIS(1) = "AAA"
CHGLIS(1) = 2
BROLIS(1) = 1
TRELIS(1) = 1
DESLIS(1) = "ABREV"
If clalev([M:AAS])=0 : Local Mask AASEL [AAS] : Endif
Gosub TIT
nolign = 1
Return

$DEF_BOITE
Gosub HORODAT From WGAACHOOSE
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mesbox "AACHOOSE" From GFONCTION At A_STAMP With A_WINDPREV Mask [AAS]
&  Listbox [AAB] GAU_CHE1
&   [L]SAIRAP(1) = AA Using "K:80X" Titled TITSEL1(1)
& As [AAS]
Titled [AAS] TIT
Return

$VISU_BOITE
Gosub INIT_BOITE
Gosub HORODAT From WGAACHOOSE
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mesbox "AACHOOSE" From GFONCTION At A_STAMP With A_WINDPREV Mask [AAS]
& As [AAS]
Titled [AAS] TIT
Gosub AFF_BOITE
Return

$AFF_BOITE
Boxact [AAS] 
Return

$SAISIE_BOITE
Boxinp [AAS] Using REPONSE
Return

$FERME_BOITE
Onerrgo RIEN
Close Local Inpbox [AAS]
Onerrgo
Return

$RIEN
Resume

$TIT
Local Char TITLIS1(30)
Call TEXTE(7359,TITLIS1) From OBJDIV
Local Char TITSEL1(30)(1..1)
Call TEXTE(6028,TITSEL1(1)) From OBJDIV
Local Char TIT(60) , TITTRN(60)
Call TEXTE(6028,TIT) From OBJDIV
If GPROGRAMME<>""
 If dim([L]ACTION)<0 : Local Char ACTION(20) : Endif
 [L]ACTION = "TITRE" : Gosub ACTION From =GPROGRAMME
Endif
Return

$REMPLIT_
Return

$REMPLIT_1
Gosub SETCLE_1
If dim(FILTREG)<=0 or FILTREG
 If CRITERE=""   : CRITERE   = "1=1" : Endif
 If FILGAUSUP="" : FILGAUSUP = "1=1" : Endif
 If FILSUITE=""  : FILSUITE  = "1=1" : Endif
 Filter [AAB] Where evalue(CRITERE) & evalue(FILGAUSUP) & evalue(FILSUITE)
&       Order With CLELISTE
Endif
GPOINT = "AV_FILLBOX" : Gosub ENTREE From EXEFNC
ACTION = "HINT" : Gosub ACTION From =GPROGRAMME
If HINTLIS(1)<>'' or NOHLIS(1)<>0
 Fillbox [AAS] GAU_CHE1 Hint Key =HINTLIS(1)
Else
 Fillbox [AAS] GAU_CHE1
Endif
GPOINT = "AP_FILLBOX" : Gosub ENTREE From EXEFNC
Filter [AAB]
Return

$SETCLE_1
Local Char    CLELISTE(250)
Local Integer INDLIS , SNSLIS , LENLIS, GAUTRELIS
INDLIS = 1
SNSLIS = 1
LENLIS = 1
GAUTRELIS = 1
CLELISTE = "ABREV"
ACTION = "CLE_GAUCHE" : Gosub ACTION From =GPROGRAMME
Return

$DEFAUT_BRO
Case ILIS
 When 1 : Default file [AAB]
Endcase
Return

$SETBOX
Return

$SETORDER
Case currbox
 When "GAU_CHE1"
  Gosub SETCLE_1
  Filter [AAB] Order With CLELISTE
Endcase
Return

$ENDORDER
Case currbox
 When "GAU_CHE1"
  Gosub SETCLE_1
  Filter [AAB]
Endcase
Return

$PICKING
Return

$DEPICK
Return

$REPICK
Return

$SETCUR
Return

$DEFVAR
