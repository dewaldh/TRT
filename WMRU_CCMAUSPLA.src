# Supervisor
## CRUD-CLASSE ## CCMAUSPLA
# 2018-08-27 13:09:43 476


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(CCMAUSPLA,ASTATUS)
Variable Instance CCMAUSPLA Using R_CCMAUSPLA
Variable Integer ASTATUS

If CCMAUSPLA = null : End : Endif

Gosub AVARIABLES From WMR00CCMAUSPLA
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet CCMAUSPLA.ACTX.AGETAFCRIGHT(CCMAUSPLA,"GESAUS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet CCMAUSPLA.ACTX.AGETAFCRIGHTU(CCMAUSPLA,"GESAUS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif
If fmet CCMAUSPLA.ACTX.AGETACSRIGHTM(CCMAUSPLA,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AUPDATE_END
  Endif
  
  Gosub AREPCONTROL_AUPDATE
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  
If !adxlog
 Gosub AUPDATE_TR_CCMAUSPLA
Else
 Gosub AUPDATE_CCMAUSPLA
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = CCMAUSPLA.freeSnapshot
CCMAUSPLA.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
CCMAUSPLA.ASTALIN = [V]CST_AUPD
CCMAUSPLA.AUS.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = CCMAUSPLA.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = CCMAUSPLA.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSPLA._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSPLA._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSPLA._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If CCMAUSPLA.AUS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
CCMAUSPLA.AUS.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(CCMAUSPLA.AUS.ASTALIN)<>""
 [L]A_ASTALIN = CCMAUSPLA.AUS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.FNCCOL)
 If CCMAUSPLA.AUS.FNCCOL([L]A_I)<>null
  [L]A_ASTALIN = CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS.FNCCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If CCMAUSPLA.AUS.BPA(A_N2) <> null
& and CCMAUSPLA.AUS.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUSPLA.AUS.BID) : # BID -- 3
 If CCMAUSPLA.AUS.BID(A_N2) <> null
& and CCMAUSPLA.AUS.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
A_N2=1
If CCMAUSPLA.AUS.BPA(A_N2) <> null
& and CCMAUSPLA.AUS.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUSPLA.AUS.BID) : # BID -- 3
 If CCMAUSPLA.AUS.BID(A_N2) <> null
& and CCMAUSPLA.AUS.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.FNCCOL)
 If CCMAUSPLA.AUS.FNCCOL([L]A_I)<>null
  [L]A_ASTALIN = CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS.FNCCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(CCMAUSPLA.AUS.ASTALIN)<>""
 [L]A_ASTALIN = CCMAUSPLA.AUS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_CCMAUSPLA
Trbegin [F:AUS]
 Gosub AUPDATE_CCMAUSPLA
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(CCMAUSPLA.AUS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 CCMAUSPLA.AUS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.FNCCOL)
 If CCMAUSPLA.AUS.FNCCOL([L]A_I)<>null
  If CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSPLA.AUS.FNCCOL([L]A_I)
   CCMAUSPLA.AUS.FNCCOL([L]A_I) = null
  Elsif CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN <> [V]CST_ALL
   CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
A_N2=1
If CCMAUSPLA.AUS.BPA(A_N2) <> null
 If CCMAUSPLA.AUS.BPA(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance CCMAUSPLA.AUS.BPA(A_N2)
  CCMAUSPLA.AUS.BPA(A_N2) = null
 Elsif CCMAUSPLA.AUS.BPA(A_N2).ASTALIN <> [V]CST_ALL
  CCMAUSPLA.AUS.BPA(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.BPA(A_N2).COLTEL)
   If CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I)
     CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I) = null
    Elsif CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.BPA(A_N2).COLWEB)
   If CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I)
     CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I) = null
    Elsif CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(CCMAUSPLA.AUS.BID)
 If CCMAUSPLA.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet CCMAUSPLA.AUS.BID(A_N2)._AUPDATE_AFTER
  If CCMAUSPLA.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSPLA.AUS.BID(A_N2)
   CCMAUSPLA.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_CCMAUSPLA
[L]A_ASTALIN = CCMAUSPLA.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = CCMAUSPLA.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If CCMAUSPLA.snapshotEnabled=1
 [L]A_RETIK = CCMAUSPLA.revertUpdTick
Endif
Local Integer ASTATUS2
A_N2=1
If CCMAUSPLA.AUS.BPA(A_N2) <> null
 If CCMAUSPLA.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  CCMAUSPLA.AUS.BPA(A_N2).ASTALIN = [V]CST_ANEW
  For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.BPA(A_N2).COLTEL)
   If CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ANEW
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.BPA(A_N2).COLWEB)
   If CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ANEW
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(CCMAUSPLA.AUS.BID)
 If CCMAUSPLA.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet CCMAUSPLA.AUS.BID(A_N2)._AUPDATE_ABORT
  If CCMAUSPLA.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSPLA.AUS.BID(A_N2)
   CCMAUSPLA.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet CCMAUSPLA.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



