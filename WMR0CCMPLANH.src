$IS_PLANNER_USER
Gosub DECLARE_VAR
[L]AISMET = [V]CST_AYES
[L]AMETHOD = "IS_PLANNER_USER" : [L]ACTION = "IS_PLANNER_USER" : [L]CURPRO = "" : Gosub ACTION_MET From ASYRSUB
