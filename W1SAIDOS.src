#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management SAIDOS 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:11:08"
A_USER  = "ADMIN"
A_STAMP = "20180828001108"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask SAIDOS [DIA]
Gosub HORODAT
Local Inpbox "SAIDOS" From GFONCTION At A_STAMP With A_WINDPREV Mask [DIA] As [SIM_]
Grizo [DIA]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [DIA]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=37 : X2(0)=76 : Y1(0)=14 : Y2(0)=15
Return

$ON_DOSSIER
Onevent AFTER_CHANGE Gosub AMM_DOSSIER From W1SAIDOS
Onevent CONTROL Call C_DOSSIER(zc) From W1SAIDOS
Onevent BEFORE_INPUT Call AS_DOSSIER(zc) From W1SAIDOS
Return

Subprog AS_DOSSIER(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_DOSSIER(zc) From W1SAIDOS
Onkey 144 Titled GBOUTP Call P_DOSSIER(zc) From W1SAIDOS
Onkey 146 Titled GBOUTA Call SA_DOSSIER(zc) From W1SAIDOS
Gosub _AFF
End

Subprog C_DOSSIER(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("ADS","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "DOSSIER" : GZNUM = 10035 : Endif
If !mkstat
 ## Action STD
 Call C_DOSSIER(VALEUR) From SAIDOS
 If mkstat : GERR = 1 : GZONE = "DOSSIER" : GZNUM = 10035 : Endif
Endif
Gosub _AFF
End

$AMM_DOSSIER
Call AM_DOSSIER(zc)
If !mkstat & GOBJET
 zoncou = "DOSSIER" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_DOSSIER(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action APASSWD
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "APASSWD"
 PARMSK(1) = ""
 PARMSK(2) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"APASSWD","APASSWD","SPEPASSWD") From GSAISIE
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DOSSIER" : GZNUM = 10035 : Endif
Endif
Gosub _AFF
End

Subprog S_DOSSIER(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "ADS"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_DOSSIER(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADS"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_DOSSIER(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ADS",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$CHG_STY
Return


