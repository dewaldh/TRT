#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management RECUPBAM 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
WCHAR = [M]CPY
If varmode([M]CPY,indice)=0
 SAVACT = GACTION : GACTION = "INICPY"
 Call INICPY([M]CPY) From EXEFNC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CPY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]CPY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTCPY"
   Call CONTSOC([M]CPY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("CPY","",[M]CPY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTCPY"
     Call CONTCPY([M]CPY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]CPY or IMPMOD
       SAVACT = GACTION : GACTION = "STD"
       Call AM_CPY([M]CPY) From RECUPBAM
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone ALLFCY
mkstat = 0
zoncou = "ALLFCY"
indice = 0
IMPENT = [M]ALLFCY
If varinit([M]ALLFCY,indice)=0
 IMPENT = 2
Endif
If varmode([M]ALLFCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_ALLFCY(IMPENT) From RECUPBAM
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]ALLFCY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ALLFCY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ALLFCY(indice)
 Endif
Endif
[M]ALLFCY = IMPENT

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FCY([M]FCY) From RECUPBAM
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "INIFCY"
  Call INIFCY([M]FCY,[M]CPY) From EXEFNC
  GACTION = SAVACT
  If mkstat=1 : Raz [M]FCY : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTFCY"
   Call CONTSITE([M]FCY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("FCY","!C",[M]FCY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTFCY"
     Call CONTFCY([M]FCY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "CONTFCYCPY"
      Call CONTFCYCPY([M]FCY,[M]CPY) From EXEFNC
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone LEDTYP
mkstat = 0
zoncou = "LEDTYP"
indice = 0
IMPENT = [M]LEDTYP
If varinit([M]LEDTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]LEDTYP,indice)=0
 Call INICOMBO(2644) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_LEDTYP(IMPENT) From RECUPBAM
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2644,1))))
 Endif
 [M]LEDTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LEDTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LEDTYP(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
    mkstat = 1 : GMESSAGE = mess(99,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_LEDTYP(IMPENT) From RECUPBAM
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif
[M]LEDTYP = IMPENT

## Zone FIY
mkstat = 0
zoncou = "FIY"
indice = 0
If varmode([M]FIY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FIY([M]FIY) From RECUPBAM
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FIY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "FIY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If [M]FIY=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "FIY" : GZNUM = 3565 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_FIY([M]FIY) From RECUPBAM
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FIY" : GZNUM = 3565 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone STRDAT
mkstat = 0
zoncou = "STRDAT"
indice = 0
If varmode([M]STRDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "STRDAT" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_STRDAT([M]STRDAT) From RECUPBAM
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 1805 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone ENDDAT
mkstat = 0
zoncou = "ENDDAT"
indice = 0
If varmode([M]ENDDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENDDAT" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_ENDDAT([M]ENDDAT) From RECUPBAM
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ENDDAT" : GZNUM = 1806 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone CUR
mkstat = 0
zoncou = "CUR"
indice = 0
If varmode([M]CUR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CUR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("TCU","!",[M]CUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CUR" : GZNUM = 323 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone TYP
mkstat = 0
zoncou = "TYP"
indice = 0
IMPENT = [M]TYP
If varinit([M]TYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]TYP,indice)=0
 [M]TYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TYP(indice)
 Endif
Endif
[M]TYP = IMPENT

## Zone ACC
mkstat = 0
zoncou = "ACC"
indice = 0
SAVACT = GACTION : GACTION = "INIACCFMT"
Call INIACCFMT(num$([M]COA)) From SETGLBCOA
GACTION = SAVACT
If mkstat=1 : Raz [M]ACC : Endif
If !mkstat
 If varmode([M]ACC,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "ACC" : Gosub ACTION From =IMPTRT
  Endif
  Call CONTROLE("GAC","!",[M]ACC,num$([M]COA),mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ACC" : GZNUM = 6127 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "RECUPBAM"
Return

