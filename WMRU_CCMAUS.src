############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(CCMAUS,ASTATUS)
Variable Instance CCMAUS Using R_CCMAUS
Variable Integer ASTATUS

If CCMAUS = null : End : Endif

Gosub AVARIABLES From WMR00CCMAUS
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet CCMAUS.ACTX.AGETAFCRIGHT(CCMAUS,"GESAUS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet CCMAUS.ACTX.AGETAFCRIGHTU(CCMAUS,"GESAUS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif
If fmet CCMAUS.ACTX.AGETACSRIGHTM(CCMAUS,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AUPDATE_END
  Endif
  
  Gosub AREPCONTROL_AUPDATE
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  
If !adxlog
 Gosub AUPDATE_TR_CCMAUS
Else
 Gosub AUPDATE_CCMAUS
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = CCMAUS.freeSnapshot
CCMAUS.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
CCMAUS.ASTALIN = [V]CST_AUPD
CCMAUS.AUS.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = CCMAUS.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = CCMAUS.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUS._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUS._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUS._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If CCMAUS.AUS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
CCMAUS.AUS.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(CCMAUS.AUS.ASTALIN)<>""
 [L]A_ASTALIN = CCMAUS.AUS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.AUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(CCMAUS.AUS.FNCCOL)
 If CCMAUS.AUS.FNCCOL([L]A_I)<>null
  [L]A_ASTALIN = CCMAUS.AUS.FNCCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.AUS.FNCCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If CCMAUS.AUS.BPA(A_N2) <> null
& and CCMAUS.AUS.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUS.AUS.BID) : # BID -- 3
 If CCMAUS.AUS.BID(A_N2) <> null
& and CCMAUS.AUS.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
A_N2=1
If CCMAUS.AUS.BPA(A_N2) <> null
& and CCMAUS.AUS.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUS.AUS.BID) : # BID -- 3
 If CCMAUS.AUS.BID(A_N2) <> null
& and CCMAUS.AUS.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(CCMAUS.AUS.FNCCOL)
 If CCMAUS.AUS.FNCCOL([L]A_I)<>null
  [L]A_ASTALIN = CCMAUS.AUS.FNCCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.AUS.FNCCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(CCMAUS.AUS.ASTALIN)<>""
 [L]A_ASTALIN = CCMAUS.AUS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.AUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_CCMAUS
Trbegin [F:AUS]
 Gosub AUPDATE_CCMAUS
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(CCMAUS.AUS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 CCMAUS.AUS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(CCMAUS.AUS.FNCCOL)
 If CCMAUS.AUS.FNCCOL([L]A_I)<>null
  If CCMAUS.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUS.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUS.AUS.FNCCOL([L]A_I)
   CCMAUS.AUS.FNCCOL([L]A_I) = null
  Elsif CCMAUS.AUS.FNCCOL([L]A_I).ASTALIN <> [V]CST_ALL
   CCMAUS.AUS.FNCCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
A_N2=1
If CCMAUS.AUS.BPA(A_N2) <> null
 If CCMAUS.AUS.BPA(A_N2).ASTALIN=[V]CST_ADEL or CCMAUS.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance CCMAUS.AUS.BPA(A_N2)
  CCMAUS.AUS.BPA(A_N2) = null
 Elsif CCMAUS.AUS.BPA(A_N2).ASTALIN <> [V]CST_ALL
  CCMAUS.AUS.BPA(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(CCMAUS.AUS.BPA(A_N2).COLTEL)
   If CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I)
     CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I) = null
    Elsif CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  For [L]A_I=1 To maxtab(CCMAUS.AUS.BPA(A_N2).COLWEB)
   If CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I)
     CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I) = null
    Elsif CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(CCMAUS.AUS.BID)
 If CCMAUS.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet CCMAUS.AUS.BID(A_N2)._AUPDATE_AFTER
  If CCMAUS.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or CCMAUS.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUS.AUS.BID(A_N2)
   CCMAUS.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_CCMAUS
[L]A_ASTALIN = CCMAUS.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.AUS._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = CCMAUS.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If CCMAUS.snapshotEnabled=1
 [L]A_RETIK = CCMAUS.revertUpdTick
Endif
Local Integer ASTATUS2
A_N2=1
If CCMAUS.AUS.BPA(A_N2) <> null
 If CCMAUS.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  CCMAUS.AUS.BPA(A_N2).ASTALIN = [V]CST_ANEW
  For [L]A_I=1 To maxtab(CCMAUS.AUS.BPA(A_N2).COLTEL)
   If CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     CCMAUS.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ANEW
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  For [L]A_I=1 To maxtab(CCMAUS.AUS.BPA(A_N2).COLWEB)
   If CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     CCMAUS.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ANEW
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(CCMAUS.AUS.BID)
 If CCMAUS.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet CCMAUS.AUS.BID(A_N2)._AUPDATE_ABORT
  If CCMAUS.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or CCMAUS.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUS.AUS.BID(A_N2)
   CCMAUS.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet CCMAUS.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



