#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management AWA0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CODE
mkstat = 0
zoncou = "CODE"
indice = 0
If varmode([M]CODE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CODE" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CODE,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CODE" : GZNUM = 5296 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "AWA"
  Local Char    VALEUR(250)
  VALEUR = [M]CODE
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]CODE = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CODE" : GZNUM = 5296 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone INTIT
mkstat = 0
zoncou = "INTIT"
indice = 0
If varinit([M]INTIT,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]INTIT,"AWRKPAR","INTIT",[M]CODE,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]INTIT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "INTIT" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone CATEG
mkstat = 0
zoncou = "CATEG"
indice = 0
If varinit([M]CATEG,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]CATEG,"51","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]CATEG,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "CATEG" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]CATEG,"51","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CATEG" : GZNUM = 707 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!",[M]CATEG,"51",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "CATEG" : GZNUM = 707 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
If varinit([M]ENAFLG,indice)=0
 IMPENT = 2
Endif
If varmode([M]ENAFLG,indice)=0
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
Endif
[M]ENAFLG = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "AWA0"
Return

