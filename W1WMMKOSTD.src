# Sage X3 supervisor
# Mask management WMMKOSTD 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [26/03/2018]
A_HEURE = "13:13:02"
A_USER  = "ADMIN"
A_STAMP = "20180326131302"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIG"
  If [M]NBLIG=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask WMMKOSTD [MKO2]
Gosub HORODAT
Local Inpbox "WMMKOSTD" From GFONCTION At A_STAMP With A_WINDPREV Mask [MKO2] As [SIM_]
Grizo [MKO2]15
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [MKO2]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=112 : Y1(0)=1 : Y2(0)=22
Return

$ON_NBLIG
Onevent BEFORE_INPUT Call AS_NBLIG From W1WMMKOSTD
Return

Subprog IB_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action STD
 Call IB_NBLIG From CNSMKOSTD
Endif
Gosub _AFF
End

Subprog AS_NBLIG
Local Decimal BASPAG
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(735,GBOUT1) From OBJDIV
Call TEXTE(735,GBOUT2) From OBJDIV
Call TEXTE(17883,GBOUT3) From OBJDIV
Call TEXTE(17927,GBOUT4) From OBJDIV
Call IB_NBLIG
Onkey 129 Titled GBOUT1 Call B1_NBLIG From W1WMMKOSTD
Onkey 130 Titled GBOUT2 Call B2_NBLIG From W1WMMKOSTD
Onkey 131 Titled GBOUT3 Call B3_NBLIG From W1WMMKOSTD
Onkey 132 Titled GBOUT4 Call B4_NBLIG From W1WMMKOSTD
Gosub _AFF
End

Subprog B1_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action GOBJETC0
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GOBJETC0"
 Local Char    OBJCLE(30)
 OBJCLE = [M:MKO2]MFGNUM(nolign-1)
 Call OBJET_CHAR(OBJCLE,"MFG","") From GOBJET
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog B2_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action GOBJETC1
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GOBJETC1"
 Call OBJET_CHAR([M:MKO2]MFGTRKNUM(nolign-1),"MTK","") From GOBJET
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog B3_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action CONSMKM
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONSMKM"
 PARMSK(1) = [M:MKO1]MFGFCY
 PARMSK(2) = ""
 PARMSK(3) = [M:MKO2]MFGTRKDAT(nolign-1)
 PARMSK(4) = [M:MKO2]MFGTRKNUM(nolign-1)
 PARMSK(5) = [M:MKO2]MFGNUM(nolign-1)
 PARMSK(6) = ""
 PARMSK(7) = ""
 PARMSK(8) = ""
 PARMSK(9) = ""
 PARMSK(10) = ""
 PARMSK(11) = ""
 PARMSK(12) = ""
 Call CONSULT_NUM(BASPAG,"MKM",PARMSK) From GCONSULT
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog B4_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action CONSMKI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONSMKI"
 PARMSK(1) = [M:MKO1]MFGFCY
 PARMSK(2) = [M:MKO2]RELITM(nolign-1)
 PARMSK(3) = [M:MKO2]MFGTRKDAT(nolign-1)
 PARMSK(4) = [M:MKO2]MFGTRKNUM(nolign-1)
 PARMSK(5) = [M:MKO2]MFGNUM(nolign-1)
 PARMSK(6) = ""
 PARMSK(7) = ""
 PARMSK(8) = ""
 PARMSK(9) = ""
 PARMSK(10) = ""
 Call CONSULT_NUM(BASPAG,"MKI",PARMSK) From GCONSULT
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

