####################################################################
# 1 : YRULID Settlement Rule ID ( Field YSRID 7)
####################################################################

$SET_YRULID
If [L]_AOLDVAL<>this.YRULID
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_YRULID
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.YRULID
 Gosub PROPAGATE_YRULID
Endif
Return

$INIT_YRULID
Gosub DECLARE_VAR From WMC0YSETRUL
[L]CURPRO = "YRULID"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_YRULID
Gosub DECLARE_VAR From WMC0YSETRUL
[L]CURPRO = "YRULID"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_YRULID                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_YRULID                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_YRULID
[L]CURPRO = "YRULID"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.YRULID,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_YRULID
[L]CURPRO = "YRULID"
If vireblc(this.YRULID,2)<>vireblc(format$("K:7X",this.YRULID),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_YRULID
[L]CURPRO = "YRULID"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_YRULID
[L]CURPRO = "YRULID"
Return

$PROPAGATE_YRULID
this.isReadonly = 0
[L]CURPRO = "YRULID"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_YRULID
Gosub DECLARE_VAR From WMC0YSETRUL
[L]CURPRO = "YRULID"
$_LNKPRO_YRULID
Return

$_ALOADATTRIBUTE_YRULID
[L]ASTATUS = fmet this.ASETATTRIBUTE("YRULID","$isMandatory","true")
Return


