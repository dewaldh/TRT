$BOUTON
STDBOUT = "C"
NBTEX  = 0
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
NBMEN += 1
NMEN(NBMEN) = 10 : Call TEXTE(4685,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "f" : LMEN(NBMEN) = 2001 : Call TEXTE(2697,TMEN(NBMEN)) From OBJDIV
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "f"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$(9)
  PARBOUT(2) = WNUMTEX
  FLGEXE = 1
  GACTION = "SAITEX"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ITEXMOD","TRTX3TEX","XWITEXMOD") From GSAISIE
  WNUMTEX = PARBOUT(2)
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


