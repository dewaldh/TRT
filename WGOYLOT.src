$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [09/11/2018]
A_HEURE = "14:00:22"
A_USER  = "MAJID"
Call ISWINVALID(nomap,GLANGUE,"OYLOT",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

