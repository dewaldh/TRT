############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(BPCUSTOMER,ASTATUS)
Variable Instance BPCUSTOMER Using R_BPCUSTOMER
Variable Integer ASTATUS

If BPCUSTOMER = null : End : Endif

Gosub AVARIABLES From WMR00BPCUSTOMER
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet BPCUSTOMER.ACTX.AGETAFCRIGHT(BPCUSTOMER,"GESBPC",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet BPCUSTOMER.ACTX.AGETAFCRIGHTU(BPCUSTOMER,"GESBPC",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_BPCUSTOMER
Else
 Gosub AUPDATE_BPCUSTOMER
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = BPCUSTOMER.freeSnapshot
BPCUSTOMER.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
BPCUSTOMER.ASTALIN = [V]CST_AUPD
BPCUSTOMER.BPC.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = BPCUSTOMER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BPCUSTOMER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If BPCUSTOMER.BPC.ASTALIN = [V]CST_ANEWDEL : Return : Endif
BPCUSTOMER.BPC.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(BPCUSTOMER.BPC.ASTALIN)<>""
 [L]A_ASTALIN = BPCUSTOMER.BPC.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_DAYMON)
 If BPCUSTOMER.BPC.C_DAYMON([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_DAYMON([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_DAYMON([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_DIE)
 If BPCUSTOMER.BPC.C_DIE([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_DIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_DIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_INVDTAAMT)
 If BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_REP)
 If BPCUSTOMER.BPC.C_REP([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_REP([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_REP([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_TSCCOD)
 If BPCUSTOMER.BPC.C_TSCCOD([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_TSCCOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_TSCCOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If BPCUSTOMER.BPC.BPR(A_N2) <> null
& and BPCUSTOMER.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If BPCUSTOMER.BPC.BPR(A_N2).ASTALIN<>[V]CST_ADEL & BPCUSTOMER.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(BPCUSTOMER.BPC,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPARTNER:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BPCUSTOMER.BPC.BPD) : # BPDLVCUST -- 3
 If BPCUSTOMER.BPC.BPD(A_N2) <> null
& and BPCUSTOMER.BPC.BPD(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
A_N2=1
If BPCUSTOMER.BPC.BPR(A_N2) <> null
& and BPCUSTOMER.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If BPCUSTOMER.BPC.BPR(A_N2).ASTALIN<>[V]CST_ADEL & BPCUSTOMER.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(BPCUSTOMER.BPC,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPARTNER:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BPCUSTOMER.BPC.BPD) : # BPDLVCUST -- 3
 If BPCUSTOMER.BPC.BPD(A_N2) <> null
& and BPCUSTOMER.BPC.BPD(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_DAYMON)
 If BPCUSTOMER.BPC.C_DAYMON([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_DAYMON([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_DAYMON([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_DIE)
 If BPCUSTOMER.BPC.C_DIE([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_DIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_DIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_INVDTAAMT)
 If BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_REP)
 If BPCUSTOMER.BPC.C_REP([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_REP([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_REP([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_TSCCOD)
 If BPCUSTOMER.BPC.C_TSCCOD([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.BPC.C_TSCCOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.C_TSCCOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(BPCUSTOMER.BPC.ASTALIN)<>""
 [L]A_ASTALIN = BPCUSTOMER.BPC.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_BPCUSTOMER
Trbegin [F:BPC]
 Gosub AUPDATE_BPCUSTOMER
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(BPCUSTOMER.BPC.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 BPCUSTOMER.BPC.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_DAYMON)
 If BPCUSTOMER.BPC.C_DAYMON([L]A_I)<>null
  If BPCUSTOMER.BPC.C_DAYMON([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.C_DAYMON([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPC.C_DAYMON([L]A_I)
   BPCUSTOMER.BPC.C_DAYMON([L]A_I) = null
  Elsif BPCUSTOMER.BPC.C_DAYMON([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.BPC.C_DAYMON([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_DIE)
 If BPCUSTOMER.BPC.C_DIE([L]A_I)<>null
  If BPCUSTOMER.BPC.C_DIE([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.C_DIE([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPC.C_DIE([L]A_I)
   BPCUSTOMER.BPC.C_DIE([L]A_I) = null
  Elsif BPCUSTOMER.BPC.C_DIE([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.BPC.C_DIE([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_INVDTAAMT)
 If BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I)<>null
  If BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I)
   BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I) = null
  Elsif BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.BPC.C_INVDTAAMT([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_REP)
 If BPCUSTOMER.BPC.C_REP([L]A_I)<>null
  If BPCUSTOMER.BPC.C_REP([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.C_REP([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPC.C_REP([L]A_I)
   BPCUSTOMER.BPC.C_REP([L]A_I) = null
  Elsif BPCUSTOMER.BPC.C_REP([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.BPC.C_REP([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.BPC.C_TSCCOD)
 If BPCUSTOMER.BPC.C_TSCCOD([L]A_I)<>null
  If BPCUSTOMER.BPC.C_TSCCOD([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.C_TSCCOD([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPC.C_TSCCOD([L]A_I)
   BPCUSTOMER.BPC.C_TSCCOD([L]A_I) = null
  Elsif BPCUSTOMER.BPC.C_TSCCOD([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.BPC.C_TSCCOD([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
A_N2=1
If BPCUSTOMER.BPC.BPR(A_N2) <> null
 [L]ASTATUS2 = fmet BPCUSTOMER.BPC.BPR(A_N2)._AUPDATE_AFTER
 If BPCUSTOMER.BPC.BPR(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.BPR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance BPCUSTOMER.BPC.BPR(A_N2)
  BPCUSTOMER.BPC.BPR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BPCUSTOMER.BPC.BPD)
 If BPCUSTOMER.BPC.BPD(A_N2) <> null
  [L]ASTATUS2 = fmet BPCUSTOMER.BPC.BPD(A_N2)._AUPDATE_AFTER
  If BPCUSTOMER.BPC.BPD(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.BPD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPC.BPD(A_N2)
   BPCUSTOMER.BPC.BPD(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_BPCUSTOMER
[L]A_ASTALIN = BPCUSTOMER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.BPC._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BPCUSTOMER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If BPCUSTOMER.snapshotEnabled=1
 [L]A_RETIK = BPCUSTOMER.revertUpdTick
Endif
Local Integer ASTATUS2
A_N2=1
If BPCUSTOMER.BPC.BPR(A_N2) <> null
 [L]ASTATUS2 = fmet BPCUSTOMER.BPC.BPR(A_N2)._AUPDATE_ABORT
 If BPCUSTOMER.BPC.BPR(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.BPR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance BPCUSTOMER.BPC.BPR(A_N2)
  BPCUSTOMER.BPC.BPR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BPCUSTOMER.BPC.BPD)
 If BPCUSTOMER.BPC.BPD(A_N2) <> null
  [L]ASTATUS2 = fmet BPCUSTOMER.BPC.BPD(A_N2)._AUPDATE_ABORT
  If BPCUSTOMER.BPC.BPD(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPC.BPD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPC.BPD(A_N2)
   BPCUSTOMER.BPC.BPD(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet BPCUSTOMER.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



