####################################################################
# AINIT
####################################################################

$AINIT
Gosub DECLARE_VAR From WMC0PARSTA1
Local Integer ALINE,TBLINE,ASTASNAPSHOT,ALLSTAT
If this.APARENT=null & this.snapshotEnabled=0
 this.snapshotEnabled = 1
Elsif this.APARENT<>null
 this.snapshotEnabled = this.APARENT.snapshotEnabled
Endif
If this.ASTALIN=[V]CST_ALL : this.ASTALIN = [V]CST_ANEW : Endif
Gosub INIT_COD From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_INTIT From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_INTSHO From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_MODULE From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_RLGTBL From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_NBRTBL From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_DATFLD From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_DATTBL From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_CPYFLD From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_CPYTBL From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_FCYFLD From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_FCYTBL From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_NBRCRI From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_EXPNUM From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_NBRVAR From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_CODACT From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_CREDAT From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_CREUSR From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_UPDDAT From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_UPDUSR From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_CREDATTIM From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_UPDDATTIM From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub INIT_AUUID From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub AINIT From WMC0PARSTA1 : Gosub TESTALLSTAT
If this.snapshotEnabled=1 & this.APARENT=null
 [L]ASTASNAPSHOT = this.freeSnapshot
Endif
Return

####################################################################
# _ALOADATTRIBUTE
####################################################################

$_ALOADATTRIBUTE
Gosub DECLARE_VAR From WMC0PARSTA1
Gosub _ALOADATTRIBUTE_COD From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub _ALOADATTRIBUTE_RLGTBL From WMC1PARSTA1 : Gosub TESTALLSTAT
Return

####################################################################
# _ALNKPRO
####################################################################

$_ALNKPRO
Gosub DECLARE_VAR From WMC0PARSTA1
Gosub LNKPRO_RLGTBL From WMC1PARSTA1 : Gosub TESTALLSTAT
For AINDCLA=1 To 10
 If this.TBLCOL(AINDCLA)<>null& find(this.TBLCOL(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.TBLCOL(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.CRICOL(AINDCLA)<>null& find(this.CRICOL(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.CRICOL(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
For AINDCLA=1 To 10
 If this.VARCOL(AINDCLA)<>null& find(this.VARCOL(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.VARCOL(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
Gosub LNKPRO_CODACT From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub LNKPRO_CREUSR From WMC1PARSTA1 : Gosub TESTALLSTAT
Gosub LNKPRO_UPDUSR From WMC1PARSTA1 : Gosub TESTALLSTAT
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



