####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "YEPR"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"YEPR",this)
 When "YLDP"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"YLDP",this)
 When "YREL"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"YREL",this)
 When "YSTAGE"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"YSTAGE",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

