# Supervisor
# Mask management VISUBOM 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone BOMALTTYP
mkstat = 0
zoncou = "BOMALTTYP"
indice = 0
IMPENT = [M]BOMALTTYP
If varinit([M]BOMALTTYP,indice)=0
 IMPENT = 2
Endif
If varmode([M]BOMALTTYP,indice)=0
 [M]BOMALTTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "BOMALTTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]BOMALTTYP(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "BOMALTTYP" : GZNUM = 4281 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]BOMALTTYP = IMPENT

## Zone BOMALT
mkstat = 0
zoncou = "BOMALT"
indice = 0
If varmode([M]BOMALT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BOMALT" : Gosub ACTION From =IMPTRT
 Endif
 If [M]BOMALT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 3964 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If [M]BOMALT<0
   mkstat = 1 : GMESSAGE = mess(180,141,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 3964 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTNUM("TBO","",[M]BOMALT,num$([M]BOMALTTYP),mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 3964 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone MSTOFCY
mkstat = 0
zoncou = "MSTOFCY"
indice = 0
SAVACT = GACTION : GACTION = "STD"
Call AV_MSTOFCY([M]MSTOFCY) From SUBVBOM
GACTION = SAVACT
If mkstat=1 : Raz [M]MSTOFCY : Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone DATREF
mkstat = 0
zoncou = "DATREF"
indice = 0
If varmode([M]DATREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATREF" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone QTYCOD
mkstat = 0
zoncou = "QTYCOD"
indice = 0
IMPENT = [M]QTYCOD
If varinit([M]QTYCOD,indice)=0
 IMPENT = 1
Endif
[M]QTYCOD = IMPENT

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone USESTA
mkstat = 0
zoncou = "USESTA"
indice = 0
IMPENT = [M]USESTA
If varinit([M]USESTA,indice)=0
 IMPENT = 1
Endif
[M]USESTA = IMPENT

####################################
## Bloc tableau 5
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 6
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 5
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone CPNTYP
mkstat = 0
zoncou = "CPNTYP"
IMPENT = [M]CPNTYP(indice)
If varinit([M]CPNTYP,indice)=0
 IMPENT = 1
Endif
[M]CPNTYP(indice) = IMPENT

## Zone LIKQTYCOD
mkstat = 0
zoncou = "LIKQTYCOD"
IMPENT = [M]LIKQTYCOD(indice)
If varinit([M]LIKQTYCOD,indice)=0
 IMPENT = 1
Endif
[M]LIKQTYCOD(indice) = IMPENT

## Zone QTYRND
mkstat = 0
zoncou = "QTYRND"
IMPENT = [M]QTYRND(indice)
If varinit([M]QTYRND,indice)=0
 IMPENT = 1
Endif
[M]QTYRND(indice) = IMPENT

## Zone PICPRN
mkstat = 0
zoncou = "PICPRN"
IMPENT = [M]PICPRN(indice)
If varinit([M]PICPRN,indice)=0
 IMPENT = 1
Endif
[M]PICPRN(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "VISUBOM"
Return

