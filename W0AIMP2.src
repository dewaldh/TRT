#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management AIMP2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone DIFFERE
mkstat = 0
zoncou = "DIFFERE"
indice = 0
IMPENT = [M]DIFFERE
If varinit([M]DIFFERE,indice)=0
 IMPENT = 2
Endif
If varmode([M]DIFFERE,indice)=0
 [M]DIFFERE(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIFFERE" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DIFFERE(indice)
 Endif
Endif
[M]DIFFERE = IMPENT

## Zone DIFDAT
mkstat = 0
zoncou = "DIFDAT"
indice = 0
If varmode([M]DIFDAT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_DIFDAT([M]DIFDAT) From AIMP2
 GACTION = SAVACT
 If mkstat=1 : Raz [M]DIFDAT : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIFDAT" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If [M]DIFDAT=[0/0/0]
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "DIFDAT" : GZNUM = 212 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_DIFDAT([M]DIFDAT) From AIMP2
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DIFDAT" : GZNUM = 212 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone DIFTIM
mkstat = 0
zoncou = "DIFTIM"
indice = 0
If varmode([M]DIFTIM,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_DIFTIM([M]DIFTIM) From AIMP2
 GACTION = SAVACT
 If mkstat=1 : Raz [M]DIFTIM : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIFTIM" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]DIFTIM,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "DIFTIM" : GZNUM = 847 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_DIFTIM([M]DIFTIM) From AIMP2
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DIFTIM" : GZNUM = 847 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "AIMP2"
Return

