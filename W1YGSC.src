$ON_YJOBCOD
Onevent BEFORE_AREA Gosub AV_YJOBCOD From W1YGSC
Onevent CONTROL Call C_YJOBCOD(zc) From W1YGSC
Onevent BEFORE_INPUT Call AS_YJOBCOD(zc) From W1YGSC
Return

$AV_YJOBCOD
## Action INICCEFMT
Call INICCEFMT(num$(func AFNC.PARAM ("YDEVDTY",""))) From SETGLBDIE
If GCCEFMT<>""
 Chgfmt [M]YJOBCOD(indice) With "K:"+GCCEFMT
Else
 Chgfmt [M]YJOBCOD(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog AS_YJOBCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YJOBCOD(zc) From W1YGSC
Onkey   2 Titled GBOUTS Call S_YJOBCOD(zc) From W1YGSC
Onkey 144 Titled GBOUTP Call P_YJOBCOD(zc) From W1YGSC
Onkey 146 Titled GBOUTA Call SA_YJOBCOD(zc) From W1YGSC
Gosub _AFF
End

Subprog C_YJOBCOD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$(func AFNC.PARAM ("YDEVDTY",""))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "YJOBCOD" : GZNUM = 21780 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$(func AFNC.PARAM ("YDEVDTY","")),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YJOBCOD" : GZNUM = 21780 : Endif
Endif
Gosub _AFF
End

Subprog S_YJOBCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "CCE"
PARMSK(2) = "!"
PARMSK(3) = num$(func AFNC.PARAM ("YDEVDTY",""))
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_YJOBCOD(VALEUR)
Variable Char    VALEUR()
Call OBJET("CCE",VALEUR,num$(func AFNC.PARAM ("YDEVDTY",""))) From GOBJET
Gosub _AFF
End

Subprog SA_YJOBCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "CCE"
PARMSK(2) = num$(func AFNC.PARAM ("YDEVDTY",""))
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YJOBCOD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = num$(func AFNC.PARAM ("YDEVDTY",""))
Call VISU("CCE",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$CHG_STY
Return


