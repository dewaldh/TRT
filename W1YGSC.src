#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management YGSC 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [17/10/2018]
A_HEURE = "12:15:36"
A_USER  = "MAJID"
A_STAMP = "20181017121536"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask YGSC [YGSC]
Gosub HORODAT
Local Inpbox "YGSC" From GFONCTION At A_STAMP With A_WINDPREV Mask [YGSC] As [SIM_]
Grizo [YGSC]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [YGSC]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=120 : Y1(0)=1 : Y2(0)=25
Return

$ON_YGSCID
Return

$ON_YNUM
Onevent CONTROL Call C_YNUM(zc) From W1YGSC
Return

Subprog C_YNUM(VALEUR)
Variable Decimal VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YNUM" : GZNUM = 1120 : Endif
Gosub _AFF
End

$ON_YDES
Onevent CONTROL Call C_YDES(zc) From W1YGSC
Return

Subprog C_YDES(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YDES" : GZNUM = 1470 : Endif
Gosub _AFF
End

$ON_YSTAATV
Onevent INIT_AREA Call D_YSTAATV(zc) From W1YGSC
Onevent BEFORE_INPUT Call AS_YSTAATV(zc) From W1YGSC
Return

Subprog D_YSTAATV(VALEUR)
Variable Integer VALEUR
If varinit([M]YSTAATV,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YSTAATV(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_YJOBACTFLG
Onevent AFTER_CHANGE Gosub AMM_YJOBACTFLG From W1YGSC
Onevent INIT_AREA Call D_YJOBACTFLG(zc) From W1YGSC
Onevent BEFORE_INPUT Call AS_YJOBACTFLG(zc) From W1YGSC
Onevent CONTROL Call C_YJOBACTFLG(zc) From W1YGSC
Return

Subprog D_YJOBACTFLG(VALEUR)
Variable Integer VALEUR
If varinit([M]YJOBACTFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YJOBACTFLG(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog C_YJOBACTFLG(VALEUR)
Variable Integer VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YJOBACTFLG" : GZNUM = 100380 : Endif
Gosub _AFF
End

$AMM_YJOBACTFLG
Call AM_YJOBACTFLG(zc)
If !mkstat & GOBJET
 zoncou = "YJOBACTFLG" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_YJOBACTFLG(VALEUR)
Variable Integer VALEUR
## Action SPE
Call AM_YJOBACTFLG(VALEUR) From SPEYGSC
If mkstat : GERR = 1 : GZONE = "YJOBACTFLG" : GZNUM = 100380 : Endif
Gosub _AFF
End

$ON_YJOBCOD
Onevent BEFORE_AREA Gosub AV_YJOBCOD From W1YGSC
Return

$AV_YJOBCOD
## Action INICCEFMT
Call INICCEFMT(num$(func AFNC.PARAM ("YDEVDTY",""))) From SETGLBDIE
If GCCEFMT<>""
 Chgfmt [M]YJOBCOD(indice) With "KP:"+GCCEFMT
Else
 Chgfmt [M]YJOBCOD(indice) With "-K:1X"
Endif
Gosub _AFF
Return

$CHG_STY
Return


