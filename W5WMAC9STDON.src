# Sage X3 supervisor
# Mask management WMAC9STDON 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_TOTDEB
Onevent BEFORE_AREA Gosub AV_TOTDEB From W5WMAC9STDON
Return

$AV_TOTDEB
Chgfmt [M]TOTDEB(indice) With "NPz3:"+GDEVFM1(1)
Gosub _AFF
Return

$ON_CURAFF
Return

$ON_TOTCDT
Onevent BEFORE_AREA Gosub AV_TOTCDT From W5WMAC9STDON
Return

$AV_TOTCDT
Chgfmt [M]TOTCDT(indice) With "NPz3:"+GDEVFM1(1)
Gosub _AFF
Return

$ON_CURAFF2
Return

$ON_SOLDE
Onevent BEFORE_AREA Gosub AV_SOLDE From W5WMAC9STDON
Return

$AV_SOLDE
## Action STD
Call AV_SOLDE(zc) From SUBGA0
Chgfmt [M]SOLDE(indice) With "NPz3:"+GDEVFM1(1)
Gosub _AFF
Return

$ON_LIGRND
Onevent INIT_AREA Call D_LIGRND(zc) From W5WMAC9STDON
Return

Subprog D_LIGRND(VALEUR)
Variable Integer VALEUR
If varinit([M]LIGRND,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_DIE
Return

$ON_COA
Return

$TIT_COL
Local Char TITCL(200)(1..21)
Raz TITCL
Case BASPAG
 When "NBLIG"
  TITCL(1) = [M:HAEF]INTCOA(7)
  TITCL(2) = [M:HAEF]INTDIE(find([M:HAE9]DIE(0),[M:HAEF]DIE(0..19))-1)
  TITCL(3) = [M:HAEF]INTDIE(find([M:HAE9]DIE(1),[M:HAEF]DIE(0..19))-1)
  TITCL(4) = [M:HAEF]INTDIE(find([M:HAE9]DIE(2),[M:HAEF]DIE(0..19))-1)
  TITCL(5) = [M:HAEF]INTDIE(find([M:HAE9]DIE(3),[M:HAEF]DIE(0..19))-1)
  TITCL(6) = [M:HAEF]INTDIE(find([M:HAE9]DIE(4),[M:HAEF]DIE(0..19))-1)
  TITCL(7) = [M:HAEF]INTDIE(find([M:HAE9]DIE(5),[M:HAEF]DIE(0..19))-1)
  TITCL(8) = [M:HAEF]INTDIE(find([M:HAE9]DIE(6),[M:HAEF]DIE(0..19))-1)
  TITCL(9) = [M:HAEF]INTDIE(find([M:HAE9]DIE(7),[M:HAEF]DIE(0..19))-1)
  TITCL(10) = [M:HAEF]INTDIE(find([M:HAE9]DIE(8),[M:HAEF]DIE(0..19))-1)
  TITCL(11) = [M:HAEF]INTDIE(9)
  TITCL(12) = [M:HAEF]INTDIE(10)
  TITCL(13) = [M:HAEF]INTDIE(11)
  TITCL(14) = [M:HAEF]INTDIE(12)
  TITCL(15) = [M:HAEF]INTDIE(13)
  TITCL(16) = [M:HAEF]INTDIE(14)
  TITCL(17) = [M:HAEF]INTDIE(15)
  TITCL(18) = [M:HAEF]INTDIE(16)
  TITCL(19) = [M:HAEF]INTDIE(17)
  TITCL(20) = [M:HAEF]INTDIE(18)
  TITCL(21) = [M:HAEF]INTDIE(19)
  If !GORITITCOL
   Titcol [M]NBLIG TITCL(1) For ACC8
   Titcol [M]NBLIG TITCL(2) For CCE1
   Titcol [M]NBLIG TITCL(3) For CCE2
   Titcol [M]NBLIG TITCL(4) For CCE3
   Titcol [M]NBLIG TITCL(5) For CCE4
   Titcol [M]NBLIG TITCL(6) For CCE5
   Titcol [M]NBLIG TITCL(7) For CCE6
   Titcol [M]NBLIG TITCL(8) For CCE7
   Titcol [M]NBLIG TITCL(9) For CCE8
   Titcol [M]NBLIG TITCL(10) For CCE9
   Titcol [M]NBLIG TITCL(11) For CCE10
   Titcol [M]NBLIG TITCL(12) For CCE11
   Titcol [M]NBLIG TITCL(13) For CCE12
   Titcol [M]NBLIG TITCL(14) For CCE13
   Titcol [M]NBLIG TITCL(15) For CCE14
   Titcol [M]NBLIG TITCL(16) For CCE15
   Titcol [M]NBLIG TITCL(17) For CCE16
   Titcol [M]NBLIG TITCL(18) For CCE17
   Titcol [M]NBLIG TITCL(19) For CCE18
   Titcol [M]NBLIG TITCL(20) For CCE19
   Titcol [M]NBLIG TITCL(21) For CCE20
  Else
   Titcol [M:HAE9]NBLIG TITCL(1) For ACC8
   Titcol [M:HAE9]NBLIG TITCL(2) For CCE1
   Titcol [M:HAE9]NBLIG TITCL(3) For CCE2
   Titcol [M:HAE9]NBLIG TITCL(4) For CCE3
   Titcol [M:HAE9]NBLIG TITCL(5) For CCE4
   Titcol [M:HAE9]NBLIG TITCL(6) For CCE5
   Titcol [M:HAE9]NBLIG TITCL(7) For CCE6
   Titcol [M:HAE9]NBLIG TITCL(8) For CCE7
   Titcol [M:HAE9]NBLIG TITCL(9) For CCE8
   Titcol [M:HAE9]NBLIG TITCL(10) For CCE9
   Titcol [M:HAE9]NBLIG TITCL(11) For CCE10
   Titcol [M:HAE9]NBLIG TITCL(12) For CCE11
   Titcol [M:HAE9]NBLIG TITCL(13) For CCE12
   Titcol [M:HAE9]NBLIG TITCL(14) For CCE13
   Titcol [M:HAE9]NBLIG TITCL(15) For CCE14
   Titcol [M:HAE9]NBLIG TITCL(16) For CCE15
   Titcol [M:HAE9]NBLIG TITCL(17) For CCE16
   Titcol [M:HAE9]NBLIG TITCL(18) For CCE17
   Titcol [M:HAE9]NBLIG TITCL(19) For CCE18
   Titcol [M:HAE9]NBLIG TITCL(20) For CCE19
   Titcol [M:HAE9]NBLIG TITCL(21) For CCE20
  Endif
Endcase
Return

$CHG_STY
Return


