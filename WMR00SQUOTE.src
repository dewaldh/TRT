############################################################################################
## R : Subprog AREAD(INSTANCE,SQHNUM)
############################################################################################
# SQUOTE : SQUOTE
# SQUOTED : SQUOTE SQUOTED
# SVCRVAT : SQUOTE SVCRVAT
# SVCRFOOT : SQUOTE SVCRFOOT
# CPTANALIN : SQUOTED CPTANALIN
# CPTANALIN : SVCRFOOT CPTANALIN
############################################################################################

############################################################################################
### AVARIABLES
############################################################################################
$AVARIABLES
[L]ASTATUS =max([L]ASTATUS,fmet SQUOTE._ADELERRORS([V]CST_ALLERRORS))
$_AVARIABLES
Local    Integer     A_LEVEL,A_I,A_NEWCOL,A_N1,A_N2,A_S2,A_N3,A_S3,A_N4,A_S4,A_N5,A_S5,A_N6,A_S6
Local    Integer    ASTATUS,A_RETIK,A_TYPMAJ,A_ASTALIN
Local    Char        A_STRSITE(GLONFCY),A_STRACC(GLONACS),CURPRO(50),A_PARENT(50),A_PATH(200),A_ABR(GLONABR),A_ERROR(250)
Local    Uuident    A_UUID

[L]A_LEVEL    = SQUOTE.ACTX.LEVWML
[L]ASTATUS  = 0

Onerrgo ERR_OPENTAB
If find(SQUOTE.ACTX.AFOLDER,nomap,"")>0
 Local File SQUOTE [SQH]
 Local File SQUOTED [SQD]
 Local File SVCRVAT [SVV]
 Local File SVCRFOOT [SVF]
 Local File CPTANALIN [CALSQD]
 Local File CPTANALIN [CALSVF]
 Local File ATEXTRA [AXX]
Else
 Local File =SQUOTE.ACTX.AFOLDER+".SQUOTE" [SQH]
 Local File =SQUOTE.ACTX.AFOLDER+".SQUOTED" [SQD]
 Local File =SQUOTE.ACTX.AFOLDER+".SVCRVAT" [SVV]
 Local File =SQUOTE.ACTX.AFOLDER+".SVCRFOOT" [SVF]
 Local File =SQUOTE.ACTX.AFOLDER+".CPTANALIN" [CALSQD]
 Local File =SQUOTE.ACTX.AFOLDER+".CPTANALIN" [CALSVF]
 Local File =SQUOTE.ACTX.AFOLDER+".ATEXTRA" [AXX]
Endif
Onerrgo
Return

$ERR_OPENTAB
[L]ASTATUS = max([L]ASTATUS,fmet SQUOTE.ASETERROR("",errp+"("+num$(errl)+")"-mess(6,123,1)-num$(errn)-errmes$(errn)-errm,[V]CST_AERROR))
Resume

############################################################################################
### Link
############################################################################################
$AOPEN_LINK
Gosub AVARIABLES
Link [SQH] With
& [SQD]SQD0 ~= [F:SQH]SQHNUM,
& [SVV]SVV0 = 1;[F:SQH]SQHNUM,
& [SVF]SVF1 = 1;[F:SQH]SQHNUM,
& [CALSQD]CAL0 = "SQD";0;[F:SQD]SQHNUM;[F:SQD]SQDLIN;0;"";1,
& [CALSVF]CAL0 = "SVF";[F:SVF]VCRTYP;[F:SVF]VCRNUM;0;[F:SVF]DTA;"";1
& As [LNK_]
Return



