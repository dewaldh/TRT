#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management STOLTI 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone STOFCY
mkstat = 0
zoncou = "STOFCY"
indice = 0
If varmode([M]STOFCY,indice)=0
 SAVACT = GACTION : GACTION = "STKASFCY"
 Call STKASFCY([M]STOFCY) From STKACT
 GACTION = SAVACT
 If mkstat=1 : Raz [M]STOFCY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "STOFCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]STOFCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","!",[M]STOFCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STKCTRFCY"
    Call STKCTRFCY([M]STOFCY) From STKACT
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "STOFCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone REFDAT
mkstat = 0
zoncou = "REFDAT"
indice = 0
If varmode([M]REFDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "REFDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone DATJOUR
mkstat = 0
zoncou = "DATJOUR"
indice = 0
IMPENT = [M]DATJOUR
If varinit([M]DATJOUR,indice)=0
 IMPENT = 1
Endif
If varmode([M]DATJOUR,indice)=0
 [M]DATJOUR(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATJOUR" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DATJOUR(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_DATJOUR(IMPENT) From SUBLTI
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DATJOUR" : GZNUM = 8531 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]DATJOUR = IMPENT

## Zone IPTDAT
mkstat = 0
zoncou = "IPTDAT"
indice = 0
If varmode([M]IPTDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "IPTDAT" : Gosub ACTION From =IMPTRT
 Endif
 If [M]IPTDAT=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "IPTDAT" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STKCTRIPT"
  Call STKCTRIPT([M]IPTDAT,[M]STOFCY,"1") From STKLIB
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "IPTDAT" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone DOCITMFLG
mkstat = 0
zoncou = "DOCITMFLG"
indice = 0
IMPENT = [M]DOCITMFLG
If varinit([M]DOCITMFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]DOCITMFLG,indice)=0
 [M]DOCITMFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DOCITMFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DOCITMFLG(indice)
 Endif
Endif
[M]DOCITMFLG = IMPENT

## Zone SIMFLG
mkstat = 0
zoncou = "SIMFLG"
indice = 0
IMPENT = [M]SIMFLG
If varinit([M]SIMFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]SIMFLG,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_SIMFLG(IMPENT) From SUBLTI
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]SIMFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "SIMFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]SIMFLG(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "SIMFLG" : GZNUM = 4973 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_SIMFLG(IMPENT) From SUBLTI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "SIMFLG" : GZNUM = 4973 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]SIMFLG = IMPENT

####################################
## Bloc tableau 5
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 5
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone STAAXX
mkstat = 0
zoncou = "STAAXX"
If varinit([M]STAAXX,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]STAAXX(indice),"TABSTASTO","STAAXX",[M]STASTO(nolign-1),"") From ATEXTRA
 GACTION = SAVACT
Endif

## Zone NEWSTAQQQ
mkstat = 0
zoncou = "NEWSTAQQQ"
WCHAR = [M]NEWSTAQQQ(indice)
If varmode([M]NEWSTAQQQ,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NEWSTAQQQ" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STKCTRSTA"
 Call STKCTRSTA([M]NEWSTAQQQ(indice),"Q*",0,1,0,mkstat) From STKACT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "NEWSTAQQQ" : GZNUM = 8538 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WCHAR<>[M]NEWSTAQQQ(indice) or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_NEWSTAQQQ([M]NEWSTAQQQ(indice)) From SUBLTI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NEWSTAQQQ" : GZNUM = 8538 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone SHOAXXQQQ
mkstat = 0
zoncou = "SHOAXXQQQ"
If varinit([M]SHOAXXQQQ,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]SHOAXXQQQ(indice),"TABSTASTO","STASHOAXX",[M]NEWSTAQQQ(nolign-1),"") From ATEXTRA
 GACTION = SAVACT
Endif

## Zone NEWSTARRR
mkstat = 0
zoncou = "NEWSTARRR"
WCHAR = [M]NEWSTARRR(indice)
If varmode([M]NEWSTARRR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NEWSTARRR" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STKCTRSTA"
 Call STKCTRSTA([M]NEWSTARRR(indice),"R*",0,1,0,mkstat) From STKACT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "NEWSTARRR" : GZNUM = 8539 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WCHAR<>[M]NEWSTARRR(indice) or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_NEWSTARRR([M]NEWSTARRR(indice)) From SUBLTI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NEWSTARRR" : GZNUM = 8539 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone SHOAXXRRR
mkstat = 0
zoncou = "SHOAXXRRR"
If varinit([M]SHOAXXRRR,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]SHOAXXRRR(indice),"TABSTASTO","STASHOAXX",[M]NEWSTARRR(nolign-1),"") From ATEXTRA
 GACTION = SAVACT
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "STOLTI"
Return

