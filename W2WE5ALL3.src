Subprog B1_OCNNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action GOBJETC0A
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GOBJETC0A"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_OCNNUM" : PROGSUB = "SUBPOC" : PROGSPE = "SPEPOC" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 Local Char    OBJCLE(30)
 OBJCLE = VALEUR
 Call OBJET_CHAR(OBJCLE,"SOR","") From GOBJET
 ACTION = "BB1_OCNNUM" : PROGSUB = "SUBPOC" : PROGSPE = "SPEPOC" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$TIT_COL
Local Char TITCL(200)(1..30)
Raz TITCL
Case BASPAG
 When "NBFAC"
  TITCL(1) = GCOLCOA(1)
  TITCL(2) = GCOLCOA(2)
  TITCL(3) = GCOLCOA(3)
