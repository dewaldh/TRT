Endif
Gosub _AFF
End

Subprog AS_PFICCE17(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_PFICCE17(VALEUR) From SUBPOC
If mkstat=2
 [M]PFICCE17(INDIC) = VALEUR
 Affzo PFICCE17(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_PFICCE17(zc) From W2WE5ALL3
Onkey   2 Titled GBOUTS Call S_PFICCE17(zc) From W2WE5ALL3
Onkey 144 Titled GBOUTP Call P_PFICCE17(zc) From W2WE5ALL3
Onkey 146 Titled GBOUTA Call SA_PFICCE17(zc) From W2WE5ALL3
Gosub _AFF
End

Subprog C_PFICCE17(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$(GDIE(17))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "PFICCE17" : GZNUM = 0 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$(GDIE(17)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "PFICCE17" : GZNUM = 0 : Endif
 If !mkstat
  ## Action STD
  Call C_PFICCE17(VALEUR) From SUBPOC
  If mkstat : GERR = 1 : GZONE = "PFICCE17" : GZNUM = 0 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_PFICCE17(VALEUR)
