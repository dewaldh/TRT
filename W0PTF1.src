# Supervisor
# Mask management PTF1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone DOCNAM
mkstat = 0
zoncou = "DOCNAM"
indice = 0
If varmode([M]DOCNAM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DOCNAM" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ARP","!A",[M]DOCNAM,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "DOCNAM" : GZNUM = 5169 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone DOCFLG
mkstat = 0
zoncou = "DOCFLG"
indice = 0
IMPENT = [M]DOCFLG
If varinit([M]DOCFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]DOCFLG,indice)=0
 [M]DOCFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DOCFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DOCFLG(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DOCFLG" : GZNUM = 6364 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]DOCFLG = IMPENT

####################################
## Bloc liste 6
####################################
IMPTBL = 0

## Zone CSTCOD
mkstat = 0
zoncou = "CSTCOD"
indice = 0
IMPENT = [M]CSTCOD
If varinit([M]CSTCOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]CSTCOD,indice)=0
 Call INICOMBO(35) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CSTCOD(IMPENT) From SUBPTF
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,35,1))))
 Endif
 [M]CSTCOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "CSTCOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]CSTCOD(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "CSTCOD" : GZNUM = 50529 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
    mkstat = 1 : GMESSAGE = mess(99,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "CSTCOD" : GZNUM = 50529 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]CSTCOD = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone LEDTYPSCR
mkstat = 0
zoncou = "LEDTYPSCR"
indice = 0
IMPENT = [M]LEDTYPSCR
If varinit([M]LEDTYPSCR,indice)=0
 IMPENT = 1
Endif
If varmode([M]LEDTYPSCR,indice)=0
 [M]LEDTYPSCR(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LEDTYPSCR" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LEDTYPSCR(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "LEDTYPSCR" : GZNUM = 662 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]LEDTYPSCR = IMPENT

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBLEDTYP
mkstat = 0
zoncou = "NBLEDTYP"
indice = 0
status = 1
zoncou = "NBLEDTYP"
If varmode([M]NBLEDTYP,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBLEDTYP From SUBPTF
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBLEDTYP : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLEDTYP" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLEDTYP
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLEDTYP = nolign
   status = 75
   Call SAI_NBLEDTYP(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone NBRCOL
mkstat = 0
zoncou = "NBRCOL"
indice = 0
If varmode([M]NBRCOL,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRCOL" : Gosub ACTION From =IMPTRT
 Endif
 If [M]NBRCOL<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "NBRCOL" : GZNUM = 11966 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc liste 5
####################################
IMPTBL = 0

## Zone MATFLG
mkstat = 0
zoncou = "MATFLG"
indice = 0
IMPENT = [M]MATFLG
If varinit([M]MATFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]MATFLG,indice)=0
 [M]MATFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "MATFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]MATFLG(indice)
 Endif
Endif
[M]MATFLG = IMPENT

## Zone DIFQTYFLG
mkstat = 0
zoncou = "DIFQTYFLG"
indice = 0
IMPENT = [M]DIFQTYFLG
WNUM = IMPENT
If varinit([M]DIFQTYFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]DIFQTYFLG,indice)=0
 [M]DIFQTYFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIFQTYFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DIFQTYFLG(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DIFQTYFLG" : GZNUM = 53159 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "DUMMY"
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DIFQTYFLG" : GZNUM = 53159 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]DIFQTYFLG = IMPENT

## Zone DIFQTYSCR
mkstat = 0
zoncou = "DIFQTYSCR"
indice = 0
IMPENT = [M]DIFQTYSCR
If varinit([M]DIFQTYSCR,indice)=0
 IMPENT = 1
Endif
If varmode([M]DIFQTYSCR,indice)=0
 [M]DIFQTYSCR(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIFQTYSCR" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DIFQTYSCR(indice)
 Endif
Endif
[M]DIFQTYSCR = IMPENT

## Zone DIFPRIFLG
mkstat = 0
zoncou = "DIFPRIFLG"
indice = 0
IMPENT = [M]DIFPRIFLG
WNUM = IMPENT
If varinit([M]DIFPRIFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]DIFPRIFLG,indice)=0
 [M]DIFPRIFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIFPRIFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DIFPRIFLG(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DIFPRIFLG" : GZNUM = 14901 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "DUMMY"
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DIFPRIFLG" : GZNUM = 14901 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]DIFPRIFLG = IMPENT

## Zone DIFPRISCR
mkstat = 0
zoncou = "DIFPRISCR"
indice = 0
IMPENT = [M]DIFPRISCR
If varinit([M]DIFPRISCR,indice)=0
 IMPENT = 1
Endif
If varmode([M]DIFPRISCR,indice)=0
 [M]DIFPRISCR(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIFPRISCR" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DIFPRISCR(indice)
 Endif
Endif
[M]DIFPRISCR = IMPENT

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBLEDTYP(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLEDTYP
mkstat = 0
zoncou = "NBLEDTYP"

## Zone LEDTYPINT
mkstat = 0
zoncou = "LEDTYPINT"
IMPENT = [M]LEDTYPINT(indice)
IMPENT = nolign
[M]LEDTYPINT(indice) = IMPENT
If varinit([M]LEDTYPINT,indice)=0
 IMPENT = 1
Endif
[M]LEDTYPINT(indice) = IMPENT

## Zone LEDTYPCOD
mkstat = 0
zoncou = "LEDTYPCOD"
IMPENT = [M]LEDTYPCOD(indice)
WNUM = IMPENT
If varinit([M]LEDTYPCOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]LEDTYPCOD,indice)=0
 [M]LEDTYPCOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LEDTYPCOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LEDTYPCOD(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "LEDTYPCOD" : GZNUM = 1220 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_LEDTYPCOD(IMPENT) From SUBPTF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "LEDTYPCOD" : GZNUM = 1220 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]LEDTYPCOD(indice) = IMPENT

## Zone NBLEDTYP
mkstat = 0
zoncou = "NBLEDTYP"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "PTF1"
Return

