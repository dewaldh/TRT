$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [28/08/2018]
A_HEURE = "00:43:20"
A_USER  = "ADMIN"
Call ISWINVALID(nomap,GLANGUE,"OMMS",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
Local Char NAVMSK(200)
NAVMSK="MMS1;"
Return

$OUVRE_BOITE
Gosub INIT_BOITE
Gosub DEF_BOITE
Gosub AFF_BOITE
Return

$INIT_BOITE
Local Char    OBJLIS(GLONAOB)(0..10), DESLIS(80)(0..10), BOX1(10)
Local Integer CHGLIS(0..10), BROLIS(0..10), TRELIS(0..10)
Local Char    SAIRAP(50)(1..16)
Local Integer DERLU , AFOLDEF
Local Char    ABRENA(GLONABR)
DERLU = 1
AFOLDEF = 1
ABRENA = ""
BOX1 = "GAU_CHE"
OBJLIS(0) = "MMS"
CHGLIS(0) = 2
BROLIS(0) = 2
TRELIS(0) = 1
DESLIS(0) = "ID"
If clalev([M:MMS1])=0 : Local Mask MMS1 [MMS1] : Endif
Gosub TIT
nolign = 1
Return

$DEF_BOITE
Gosub HORODAT From WGOMMS
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "OMMS" From GFONCTION At A_STAMP With A_WINDPREV Mask [MMS1]
&  Listbox [MPA] GAU_CHE
&   [L]SAIRAP(1) = [F:MPA]ID Using "K:10X" Titled TITSEL(1) ,
&   [L]SAIRAP(2) = [F:MPA]INTIT Using "K:30X" Titled TITSEL(2)
&  Listbox [MPA9] GAU_CHE9
&   [L]SAIRAP(1) = [F:MPA9]A_TAB1 Using "K:10X" Titled TITSEL9(1)  ,
&   [L]SAIRAP(2) = [F:MPA9]A_TAB2 Using "K:30X" Titled TITSEL9(2) 
& As [MMS1]
Titled [MMS1] TIT
Return

$VISU_BOITE
Gosub INIT_BOITE
Gosub HORODAT From WGOMMS
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "OMMS" From GFONCTION At A_STAMP With A_WINDPREV Mask [MMS1]
& As [MMS1]
Titled [MMS1] TIT
Gosub AFF_BOITE
Return

$AFF_BOITE
Boxact [MMS1] 
Return

$SAISIE_BOITE
Boxinp [MMS1] Using REPONSE
Return

$FERME_BOITE
Onerrgo RIEN
Close Local Inpbox [MMS1]
Onerrgo
Return

$RIEN
Resume

$TIT
Local Char TITLIS(30)
Call TEXTE(40786,TITLIS) From OBJDIV
Local Char TITLIS9(30)
TITLIS9 = mess(196,123,1)
Local Char TITSEL(30)(1..2)
Call TEXTE(1377,TITSEL(1)) From OBJDIV
Call TEXTE(8,TITSEL(2)) From OBJDIV
Local Char TITSEL9(30)(1..2)
Call TEXTE(1377,TITSEL9(1)) From OBJDIV
Call TEXTE(8,TITSEL9(2)) From OBJDIV
Local Char TIT(60) , TITTRN(60)
Call TEXTE(40786,TIT) From OBJDIV
If GPROGRAMME<>""
 If dim([L]ACTION)<0 : Local Char ACTION(20) : Endif
 [L]ACTION = "TITRE" : Gosub ACTION From =GPROGRAMME
Endif
Return

$REMPLIT_
Gosub SETCLE_
If dim(FILTREG)<=0 or FILTREG
 If CRITERE=""  : CRITERE  = "1=1" : Endif
 If FILTSUP=""  : FILTSUP  = "1=1" : Endif
 If FILROLE=""  : FILROLE  = "1=1" : Endif
 If FILSUITE="" : FILSUITE = "1=1" : Endif
 Filter [MPA] Where evalue(CRITERE) & evalue(FILTSUP) & evalue(FILROLE) & evalue(FILSUITE)
&       Order With CLELISTE
Endif
GPOINT = "AV_FILLBOX" : Gosub ENTREE From EXEFNC
ACTION = "HINT" : Gosub ACTION From =GPROGRAMME
If HINTLIS(0)<>'' or NOHLIS(0)<>0
 Fillbox [MMS1] GAU_CHE Hint Key =HINTLIS(0)
Else
 Gosub CALNOHOBJ From GOBJET
 If A_NOH
  Fillbox [MMS1] GAU_CHE
 Else
  Fillbox [MMS1] GAU_CHE Hint Key =""
 Endif
Endif
GPOINT = "AP_FILLBOX" : Gosub ENTREE From EXEFNC
Filter [MPA]
Return

$SETCLE_
Local Char    CLELISTE(250)
Local Integer INDLIS , SNSLIS , LENLIS, GAUTRELIS
INDLIS = 1
SNSLIS = 1
LENLIS = 1
GAUTRELIS = 1
CLELISTE = "ID"
ACTION = "CLE_GAUCHE" : Gosub ACTION From =GPROGRAMME
Return

