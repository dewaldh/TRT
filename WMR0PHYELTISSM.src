####################################################################
# EXEC OPERATION
####################################################################
Subprog EXE_OPERATION(WCLA,_OPERATION,ACLBRETURN,ACLE,ATBPAR,TBVAL)
Variable Instance WCLA Using OBJECT
Value    Char _OPERATION
Variable Clbfile ACLBRETURN
Value    Char ACLE
Variable Char ATBPAR()(1..)
Variable Char TBVAL()(1..)
Local Integer ASTATUS
Case [L]_OPERATION
 When Default
Endcase 
End

####################################################################
# EXEC METHOD
####################################################################
Subprog EXE_METHOD(WCLA,_METHOD,NAME_INSTANCE,ATBPAR,TBVAL)
Variable Instance WCLA Using OBJECT
Value    Char  NAME_INSTANCE
Value    Char _METHOD
Variable Char ATBPAR()(1..)
Variable Char TBVAL()(1..)
Local Integer ASTATUS
Case [L]_METHOD
 When "_PHYELT_ISSUE_MANAGEMENT"
  # Parameters value
  Local Tinyint _P_ISSACT
  [L]_P_ISSACT = func ASYRSUB.PARAM_NUM("P_ISSACT",ATBPAR,TBVAL)
  Local Date _P_ISSDAT
  [L]_P_ISSDAT = func ASYRSUB.PARAM_DAT("P_ISSDAT",ATBPAR,TBVAL)
  Local Tinyint _P_ISSTYP
  [L]_P_ISSTYP = func ASYRSUB.PARAM_NUM("P_ISSTYP",ATBPAR,TBVAL)
  # Call method
  Local Integer ARETURN
  If [L]NAME_INSTANCE=""
   [L]NAME_INSTANCE = "WCLA"
  Else
   [L]NAME_INSTANCE = "WCLA."+[L]NAME_INSTANCE
  Endif
  [L]ARETURN = evalue('fmet'-NAME_INSTANCE+'._PHYELT_ISSUE_MANAGEMENT([L]_P_ISSACT,[L]_P_ISSDAT,[L]_P_ISSTYP)')
 When "_CANCEL"
  # Parameters value
  # Call method
  Local Integer ARETURN
  If [L]NAME_INSTANCE=""
   [L]NAME_INSTANCE = "WCLA"
  Else
   [L]NAME_INSTANCE = "WCLA."+[L]NAME_INSTANCE
  Endif
  [L]ARETURN = evalue('fmet'-NAME_INSTANCE+'._CANCEL()')
 When Default
Endcase 
End

