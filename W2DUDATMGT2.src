$ON_UMRNUM
Onevent BEFORE_INPUT Call AS_UMRNUM(zc) From W2DUDATMGT2
Onevent CONTROL Call C_UMRNUM(zc) From W2DUDATMGT2
Return

Subprog AS_UMRNUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(50749,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_UMRNUM(VALEUR) From SUBDDM2
If mkstat=2
 [M]UMRNUM(INDIC) = VALEUR
 Affzo UMRNUM(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_UMRNUM(zc) From W2DUDATMGT2
Onkey   2 Titled GBOUTS Call S_UMRNUM(zc) From W2DUDATMGT2
Onkey 144 Titled GBOUTP Call P_UMRNUM(zc) From W2DUDATMGT2
Onkey 146 Titled GBOUTA Call SA_UMRNUM(zc) From W2DUDATMGT2
Gosub _AFF
End

Subprog C_UMRNUM(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("MDT","!A",VALEUR,num$(GSOCIETE),mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "UMRNUM" : GZNUM = 50913 : Endif
If !mkstat
 ## Action STD
 Call C_UMRNUM(VALEUR) From SUBDDM2
 If mkstat : GERR = 1 : GZONE = "UMRNUM" : GZNUM = 50913 : Endif
Endif
Gosub _AFF
End

Subprog S_UMRNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELMDT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELMDT"
 PARMSK(1) = [M:SAI1]CPY(nolign-1)
 PARMSK(2) = [M:SAI1]BPRPAY(nolign-1)
 PARMSK(3) = "2"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELMDT","CPTSEL","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_UMRNUM(VALEUR)
Variable Char    VALEUR()
Call OBJET("MDT",VALEUR,num$(GSOCIETE)) From GOBJET
Gosub _AFF
End

Subprog SA_UMRNUM(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "MDT"
PARMSK(2) = num$(GSOCIETE)
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_UMRNUM(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = num$(GSOCIETE)
Call VISU("MDT",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

