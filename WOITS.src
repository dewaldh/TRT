$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(20)(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
Local Libelle  A_BUF3(0..GNBDERNIER)
Local Decimal  A_BUF4(0..GNBDERNIER)
Local Char     A_BUF5(3)(0..GNBDERNIER)
Local Char     A_BUF6(20)(0..GNBDERNIER)
If clalev([F:ITS9])>0 : Close Local File [ITS9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(20),
& Char     A_TAB2(30),
& Libelle  A_TAB3,
& Decimal  A_TAB4,
& Char     A_TAB5(3),
& Char     A_TAB6(20)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5,A_BUF6
& As [ITS9]
Return

$LEC_DERLU
Read [ITM]ITM0=[F:ITS]ITMREF
If fstat : Raz [F:ITM] : Endif
Read [AXX]AXX0="ITMMASTER";"DES1AXX";GLANGUE;[F:ITM]ITMREF;""
If fstat : Raz [F:AXX] : Endif
Return

$CHARGE_DERLU
If clalev([F:ITS9])>0
 Read [ITS9] = [F:ITS9]A_TABI
 [L]CLE = [F:ITS9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:ITS9])>0
 Filter [ITS9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [ITS] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:ITS9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:ITS]ITMREF+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:ITS9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:ITS9]
 Endif
Endif
Return

