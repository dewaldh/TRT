  If mkstat : GERR = 1 : GZONE = "REFTYP" : GZNUM = 6320 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTOBJN"
   Local Char    VALEUR(250)
   VALEUR = [M]REFTYP
   Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJN","CONTOBJ","") From GACTION
   [M]REFTYP = VALEUR
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "REFTYP" : GZNUM = 6320 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

