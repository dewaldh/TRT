Subprog D_MVTDAT(VALEUR)
Variable Date    VALEUR
If varinit([M]MVTDAT,indice)=0
 VALEUR = date$
Endif
Gosub _AFF
End

Subprog C_MVTDAT(VALEUR)
Variable Date    VALEUR
## Action %OBLIG
If VALEUR=[0/0/0]
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "MVTDAT" : GZNUM = 34986 : Endif
If !mkstat
 ## Action STD
 Call C_MVTDAT(VALEUR) From TRTFASSPL
 If mkstat : GERR = 1 : GZONE = "MVTDAT" : GZNUM = 34986 : Endif
Endif
Gosub _AFF
End

$ON_MVTTYP
Onevent INIT_AREA Call D_MVTTYP(zc) From W1FASSPL1B
Onevent CONTROL Call C_MVTTYP(zc) From W1FASSPL1B
Onevent BEFORE_INPUT Call AS_MVTTYP(zc) From W1FASSPL1B
Onevent AFTER_CHANGE Gosub AMM_MVTTYP From W1FASSPL1B
Return

Subprog D_MVTTYP(VALEUR)
Variable Char    VALEUR()
If varinit([M]MVTTYP,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"626","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_MVTTYP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_MVTTYP(zc) From W1FASSPL1B
Onkey 144 Titled GBOUTP Call P_MVTTYP(zc) From W1FASSPL1B
Onkey 146 Titled GBOUTA Call SA_MVTTYP(zc) From W1FASSPL1B
If VALEUR="" : Call INIADI(VALEUR,"626") From TRTDIV : Endif
Gosub _AFF
End

Subprog C_MVTTYP(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "MVTTYP" : GZNUM = 2675 : Endif
If !mkstat
 ## Action CONTADI
 Call CONTADI(VALEUR,"626","") From SUBADI
 If mkstat : GERR = 1 : GZONE = "MVTTYP" : GZNUM = 2675 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ADI","",VALEUR,"626",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "MVTTYP" : GZNUM = 2675 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_MVTTYP
Call AM_MVTTYP(zc)
If !mkstat & GOBJET
 zoncou = "MVTTYP" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_MVTTYP(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZMVTTYP)")>indice
 [M]MVTTYP(indice) = VALEUR
 Affzo ZMVTTYP(indice)
Endif
Gosub _AFF
End

Subprog S_MVTTYP(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = "626"
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog SA_MVTTYP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(2) = "626"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_MVTTYP(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "626"
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

