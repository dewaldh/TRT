Subprog D_LABCCE4(VALEUR)
Variable Char    VALEUR()
If varinit([M]LABCCE4,indice)=0
 ## Action STD
 Call D_LABCCE4(VALEUR) From SUBMTK
Endif
Gosub _AFF
End

Subprog AS_LABCCE4(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(8255,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_LABCCE4(VALEUR) From SUBMTK
If mkstat=2
 [M]LABCCE4(INDIC) = VALEUR
 Affzo LABCCE4(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_LABCCE4(zc) From W3MTK1
Onkey   2 Titled GBOUTS Call S_LABCCE4(zc) From W3MTK1
Onkey 144 Titled GBOUTP Call P_LABCCE4(zc) From W3MTK1
Onkey 146 Titled GBOUTA Call SA_LABCCE4(zc) From W3MTK1
Gosub _AFF
End

Subprog C_LABCCE4(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$(GDIE(4))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "LABCCE4" : GZNUM = 0 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$(GDIE(4)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "LABCCE4" : GZNUM = 0 : Endif
 If !mkstat
  ## Action STD
  Call C_LABCCE4(VALEUR) From SUBMTK
  If mkstat : GERR = 1 : GZONE = "LABCCE4" : GZNUM = 0 : Endif
 Endif
Endif
Gosub _AFF
End

