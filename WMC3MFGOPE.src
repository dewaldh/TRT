Gosub _AFORMAT_SCOWST                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_SCOWST                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_SCOWST
[L]CURPRO = "SCOWST"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.SCOWST,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_SCOWST
[L]CURPRO = "SCOWST"
If vireblc(this.SCOWST,2)<>vireblc(format$("K:8c",this.SCOWST),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_SCOWST
[L]CURPRO = "SCOWST"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_SCOWST
[L]CURPRO = "SCOWST"
If vireblc(this.SCOWST,2)<>""
 If fmet this.AGETATTRIBUTE([L]CURPRO,"$namProParent")=""
  [L]ASTATUS =  func WMTYPWST.ACTRLREF(this,"SCOWST",this.SCOWST,this.SCOWST,this.MFGFCY,"")
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Else
  If this.SCOWST<>evalue("this.APARENT."+fmet this.AGETATTRIBUTE([L]CURPRO,"$namProParent"))
   [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(83,123,1)) :  Return : # Invalid value
  Endif
 Endif
Endif
Return

$PROPAGATE_SCOWST
this.isReadonly = 0
[L]CURPRO = "SCOWST"
Gosub _LNKPRO_SCOWST
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_SCOWST
Gosub DECLARE_VAR From WMC0MFGOPE
[L]CURPRO = "SCOWST"
$_LNKPRO_SCOWST
If vireblc(this.SCOWST,2)<>""
 # Link WORKSTATIO
 Local File WORKSTATIO [WMWS]
 Read [F:WMWS]WST0 = this.SCOWST;this.MFGFCY
 If !fstat
 Endif
 LogicClose File [WMWS]
Endif
