####################################################################
# 61 : WAITIM Waiting time ( Field TIH 0)
####################################################################

$SET_WAITIM
If [L]_AOLDVAL<>this.WAITIM
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_WAITIM
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.WAITIM
 Gosub PROPAGATE_WAITIM
Endif
Return

$INIT_WAITIM
Gosub DECLARE_VAR From WMC0MFGOPE
[L]CURPRO = "WAITIM"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_WAITIM
Gosub DECLARE_VAR From WMC0MFGOPE
[L]CURPRO = "WAITIM"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_WAITIM                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_WAITIM                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_WAITIM
[L]CURPRO = "WAITIM"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.WAITIM=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

