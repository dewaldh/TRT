############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PBDUICONF,ASTATUS)
Variable Instance PBDUICONF Using R_PBDUICONF
Variable Integer ASTATUS

If PBDUICONF = null : End : Endif

Gosub AVARIABLES From WMR00PBDUICONF
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PBDUICONF.ACTX.AGETAFCRIGHT(PBDUICONF,"GESPBDECO",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PBDUICONF.ACTX.AGETAFCRIGHTU(PBDUICONF,"GESPBDECO",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PBDUICONF
Else
 Gosub AUPDATE_PBDUICONF
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PBDUICONF.freeSnapshot
PBDUICONF.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PBDUICONF.ASTALIN = [V]CST_AUPD
PBDUICONF.PBDUIC.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PBDUICONF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PBDUICONF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PBDUICONF._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PBDUICONF._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PBDUICONF._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PBDUICONF.PBDUIC.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PBDUICONF.PBDUIC.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PBDUICONF.PBDUIC.ASTALIN)<>""
 [L]A_ASTALIN = PBDUICONF.PBDUIC.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.PBDUIC.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PBDUICONF.PBDUIC.LINES) : # PBDUICONFD -- 2
 If PBDUICONF.PBDUIC.LINES(A_N2) <> null
& and PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN<>[V]CST_ANEW & PBDUICONF.PBDUIC.ASTALIN=[V]CST_ADEL
   PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN=[V]CST_ANEW & find(PBDUICONF.PBDUIC.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.PBDUIC.LINES(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PBDUICONF.PBDUIC.LINES) : # PBDUICONFD -- 2
 If PBDUICONF.PBDUIC.LINES(A_N2) <> null
& and PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.PBDUIC.LINES(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PBDUICONF.PBDUIC.ASTALIN)<>""
 [L]A_ASTALIN = PBDUICONF.PBDUIC.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.PBDUIC.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PBDUICONF
Trbegin [F:PBDUIC]
 Gosub AUPDATE_PBDUICONF
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PBDUICONF.PBDUIC.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PBDUICONF.PBDUIC.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PBDUICONF.PBDUIC.LINES)
 If PBDUICONF.PBDUIC.LINES(A_N2) <> null
  If PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN=[V]CST_ADEL or PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PBDUICONF.PBDUIC.LINES(A_N2)
   PBDUICONF.PBDUIC.LINES(A_N2) = null
  Elsif PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN <> [V]CST_ALL
   PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN = [V]CST_ALL
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_PBDUICONF
[L]A_ASTALIN = PBDUICONF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.PBDUIC._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PBDUICONF.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PBDUICONF.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PBDUICONF.snapshotEnabled=1
 [L]A_RETIK = PBDUICONF.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PBDUICONF.PBDUIC.LINES)
 If PBDUICONF.PBDUIC.LINES(A_N2) <> null
  If PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN=[V]CST_ANEWDEL
   PBDUICONF.PBDUIC.LINES(A_N2).ASTALIN = [V]CST_ANEW
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PBDUICONF.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



