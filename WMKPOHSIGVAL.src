$EXEACT_SIG
CHAINE = ""
OK = 1
FORMULE = "([L]LEVSIG=S_NBRUSR) & (S_NBRUSR>0) & (val(RESULT) >2)"
Onerrgo ERR
OK = evalue(FORMULE)
Onerrgo
If ERN : OK = 0 : Endif
If OK
 FORMULE = "" : ERN = 0
 Onerrgo ERR
 CHAINE -= "DIVGENSOH (During signature)"
 Call DIVGENSOH([F:POH]POHNUM,"C",3) From TRTACHISO1
 Onerrgo
Endif
Return

