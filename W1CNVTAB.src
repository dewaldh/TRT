# Supervisor
# Mask management CNVTAB 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:55:13"
A_USER  = "ADMIN"
A_STAMP = "20180827235513"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIG"
  If [M]NBLIG=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask CNVTAB [CNV]
Gosub HORODAT
Local Inpbox "CNVTAB" From GFONCTION At A_STAMP With A_WINDPREV Mask [CNV] As [SIM_]
Grizo [CNV]5
Grizo [CNV]10
Grizo [CNV]15
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [CNV]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=21 : X2(0)=92 : Y1(0)=7 : Y2(0)=11
X1(1)=21 : X2(1)=92 : Y1(1)=12 : Y2(1)=19
X1(2)=21 : X2(2)=92 : Y1(2)=20 : Y2(2)=21
Return

$ON_DEVCUR
Onevent CONTROL Call C_DEVCUR(zc) From W1CNVTAB
Onevent BEFORE_INPUT Call AS_DEVCUR(zc) From W1CNVTAB
Onevent AFTER_CHANGE Gosub AMM_DEVCUR From W1CNVTAB
Onevent INIT_AREA Call D_DEVCUR(zc) From W1CNVTAB
Return

Subprog D_DEVCUR(VALEUR)
Variable Char    VALEUR()
If varinit([M]DEVCUR,indice)=0
 VALEUR = GLOCALDEV
Endif
Gosub _AFF
End

Subprog AS_DEVCUR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3837,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_DEVCUR(zc) From W1CNVTAB
Onkey   2 Titled GBOUTS Call S_DEVCUR(zc) From W1CNVTAB
Onkey 144 Titled GBOUTP Call P_DEVCUR(zc) From W1CNVTAB
Onkey 146 Titled GBOUTA Call SA_DEVCUR(zc) From W1CNVTAB
Gosub _AFF
End

Subprog C_DEVCUR(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "DEVCUR" : GZNUM = 5270 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("TCU","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "DEVCUR" : GZNUM = 5270 : Endif
Endif
Gosub _AFF
End

$AMM_DEVCUR
Call AM_DEVCUR(zc)
If !mkstat & GOBJET
 zoncou = "DEVCUR" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_DEVCUR(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZDEVCUR)")>indice
 [M]DEVCUR(indice) = VALEUR
 Affzo ZDEVCUR(indice)
Endif
Gosub _AFF
End

Subprog S_DEVCUR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "TCU"
PARMSK(2) = ""
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_DEVCUR(VALEUR)
Variable Char    VALEUR()
Call OBJET("TCU",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_DEVCUR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TCU"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_DEVCUR(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TCU",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZDEVCUR
Onevent BEFORE_AREA Gosub AV_ZDEVCUR From W1CNVTAB
Return

$AV_ZDEVCUR
Call LECTEXTRA(zc,"TABCUR","INTDES",num$([M]DEVCUR(indice)),"") From ATEXTRA
Gosub _AFF
Return

