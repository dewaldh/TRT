  ACTION = "AVANT_SUITE" : Gosub ACTION From =GPROGRAMME
  If OK
   Local Char VALBOUT(250) , PARBOUT(250)(1..20)
   PARBOUT(1) = [L]VALC2
   PARBOUT(2) = [L]VALC1
   PARBOUT(3) = [L]VALC3
   FLGEXE = 1
   GACTION = "GOBJETC0"
   Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  Endif
 When "O"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M:PIH0]PIVTYP)
  PARBOUT(2) = [M:PIH0]NUM
  FLGEXE = 1
  GACTION = "CPTTRS"
  Call S_TAB_CHAR(VALBOUT,PARBOUT,"","CPTTRS","") From GACTION
  OK = 1
  ACTION = "AVANT_SUITE" : Gosub ACTION From =GPROGRAMME
  If OK
   Local Char VALBOUT(250) , PARBOUT(250)(1..20)
   PARBOUT(1) = [L]VALC2
   PARBOUT(2) = [L]VALC1
   PARBOUT(3) = [L]VALC3
   FLGEXE = 1
   GACTION = "GOBJETC0"
   Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  Endif
 When "P"
  FLGEXE = 1
  GACTION = "ANUBIS"
  Call DEVALID([M:PIH0]NUM) From ANUBIS
 When "U"
  FLGEXE = 1
  GACTION = "TWMUNBLK"
  Call PIUNBLK([M:PIH0]NUM) From TWMLIB
Endcase
GACTION = SAVACT
Return

