Subprog AS_CRIT1(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_CRIT1(zc) From W1CAZS
Onkey 144 Titled GBOUTP Call P_CRIT1(zc) From W1CAZS
Onkey 146 Titled GBOUTA Call SA_CRIT1(zc) From W1CAZS
Gosub _AFF
End

Subprog C_CRIT1(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "CRIT1" : GZNUM = 5460 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("DIE","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CRIT1" : GZNUM = 5460 : Endif
Endif
Gosub _AFF
End

$AMM_CRIT1
Call AM_CRIT1(zc)
If !mkstat & GOBJET
 zoncou = "CRIT1" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_CRIT1(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_CRIT1(VALEUR) From SUBCAZ
If mkstat : GERR = 1 : GZONE = "CRIT1" : GZNUM = 5460 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZCRIT1)")>indice
 [M]CRIT1(indice) = VALEUR
 Affzo ZCRIT1(indice)
Endif
Gosub _AFF
End

Subprog S_CRIT1(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "DIE"
PARMSK(2) = ""
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_CRIT1(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "DIE"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CRIT1(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("DIE",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

