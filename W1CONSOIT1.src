Subprog AS_ITMREF(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(12972,GBOUT2) From OBJDIV
Call TEXTE(11651,GBOUT3) From OBJDIV
Call TEXTE(13005,GBOUT4) From OBJDIV
Call TEXTE(12837,GBOUT13) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_ITMREF(VALEUR)
Onkey 130 Titled GBOUT2 Call B2_ITMREF(zc) From W1CONSOIT1
Onkey 131 Titled GBOUT3 Call B3_ITMREF(zc) From W1CONSOIT1
Onkey 132 Titled GBOUT4 Call B4_ITMREF(zc) From W1CONSOIT1
Onkey 170 Titled GBOUT13 Call B13_ITMREF(zc) From W1CONSOIT1
Onkey   2 Titled GBOUTS Call S_ITMREF(zc) From W1CONSOIT1
Onkey 144 Titled GBOUTP Call P_ITMREF(zc) From W1CONSOIT1
Onkey 146 Titled GBOUTA Call SA_ITMREF(zc) From W1CONSOIT1
Gosub _AFF
End

Subprog C_ITMREF(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
If !GWEBSERV
 If !mkstat
  ## Action ITMCHOICE
  Local Char    PARMSK(250)(1..20) , SAVACT(20)
  SAVACT = GACTION : GACTION = "ITMCHOICE"
  Call S_TAB_CHAR(VALEUR,PARMSK,"ITMCHOICE","TRTSEL","") From GACTION
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
 Endif
Endif
If !mkstat
 ## Action VXADCUPC
 Call ADCCTRUPC(VALEUR) From VXADCUPC
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ITM","",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
  If !mkstat
   ## Action CONTITF
   Call CONTITF(VALEUR,[M]FCY) From STKLIB
   If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

