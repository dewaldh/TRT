# Supervisor
# Mask management CONSSTK2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone LEV
mkstat = 0
zoncou = "LEV"
SAVACT = GACTION : GACTION = "STD"
Call AV_LEV([M]LEV(indice)) From CNSSTKSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]LEV(indice) : Endif

## Zone SERMGTCOD
mkstat = 0
zoncou = "SERMGTCOD"
IMPENT = [M]SERMGTCOD(indice)
If varinit([M]SERMGTCOD,indice)=0
 IMPENT = 1
Endif
[M]SERMGTCOD(indice) = IMPENT

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
IMPENT = [M]VCRTYP(indice)
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP(indice) = IMPENT

## Zone TRSTYP
mkstat = 0
zoncou = "TRSTYP"
IMPENT = [M]TRSTYP(indice)
If varinit([M]TRSTYP,indice)=0
 IMPENT = 1
Endif
[M]TRSTYP(indice) = IMPENT

## Zone VCRTYPORI
mkstat = 0
zoncou = "VCRTYPORI"
IMPENT = [M]VCRTYPORI(indice)
If varinit([M]VCRTYPORI,indice)=0
 IMPENT = 1
Endif
[M]VCRTYPORI(indice) = IMPENT

## Zone FLG
mkstat = 0
zoncou = "FLG"
IMPENT = [M]FLG(indice)
If varinit([M]FLG,indice)=0
 IMPENT = 1
Endif
[M]FLG(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CONSSTK2"
Return

