#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1TABMODELIV
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "TABMODELIV"
REP.CLA = "TABMODELIV"
REP.ICL = "TMD"
REP.AFCRIGHT = "GESTMD"
REP.NCL = 3435 : # Delivery methods
REP.DES = 1722 : # Delivery mode
REP.TIT = "DESAXX"
REP.ANX = "TMD0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Delivery mode
     REP.CLE(1).PAR = 'MDL' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'MDL' : REP.CLE(1).VPR= 'TMD.MDL' : REP.CLE(1).DES = 1722
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'TABMODELIV_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 1 : # Detail
REP.AENAFAC(2) = 1 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

