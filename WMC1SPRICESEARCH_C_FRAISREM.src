####################################################################
# 1 : INDEXFRAIS Index ( Field C 2)
####################################################################

$SET_INDEXFRAIS
If [L]_AOLDVAL<>this.INDEXFRAIS
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_INDEXFRAIS
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.INDEXFRAIS
 Gosub PROPAGATE_INDEXFRAIS
Endif
Return

$INIT_INDEXFRAIS
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_FRAISREM
[L]CURPRO = "INDEXFRAIS"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_INDEXFRAIS
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_FRAISREM
[L]CURPRO = "INDEXFRAIS"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_INDEXFRAIS            : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_INDEXFRAIS            : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_INDEXFRAIS
[L]CURPRO = "INDEXFRAIS"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.INDEXFRAIS=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_INDEXFRAIS
[L]CURPRO = "INDEXFRAIS"
Return

$_AOTHERS_INDEXFRAIS
[L]CURPRO = "INDEXFRAIS"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_INDEXFRAIS
[L]CURPRO = "INDEXFRAIS"
Return

$PROPAGATE_INDEXFRAIS
this.isReadonly = 0
[L]CURPRO = "INDEXFRAIS"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_INDEXFRAIS
Gosub DECLARE_VAR From WMC0SPRICESEARCH_C_FRAISREM
[L]CURPRO = "INDEXFRAIS"
$_LNKPRO_INDEXFRAIS
Return


