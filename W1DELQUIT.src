#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management DELQUIT 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:59:46"
A_USER  = "ADMIN"
A_STAMP = "20180827235946"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask DELQUIT [DIA]
Gosub HORODAT
Local Inpbox "DELQUIT" From GFONCTION At A_STAMP With A_WINDPREV Mask [DIA] As [SIM_]
Grizo [DIA]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [DIA]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=33 : X2(0)=80 : Y1(0)=12 : Y2(0)=17
Return

$ON_BPRFCT
Onevent AFTER_CHANGE Gosub AMM_BPRFCT From W1DELQUIT
Onevent CONTROL Call C_BPRFCT(zc) From W1DELQUIT
Onevent BEFORE_INPUT Call AS_BPRFCT(zc) From W1DELQUIT
Return

Subprog AS_BPRFCT(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_BPRFCT(zc) From W1DELQUIT
Onkey 144 Titled GBOUTP Call P_BPRFCT(zc) From W1DELQUIT
Onkey 146 Titled GBOUTA Call SA_BPRFCT(zc) From W1DELQUIT
Gosub _AFF
End

Subprog C_BPRFCT(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "BPRFCT" : GZNUM = 3669 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("FCT","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "BPRFCT" : GZNUM = 3669 : Endif
Endif
Gosub _AFF
End

$AMM_BPRFCT
Call AM_BPRFCT(zc)
If !mkstat & GOBJET
 zoncou = "BPRFCT" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_BPRFCT(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_BPRFCT(VALEUR) From DELQUIT
If mkstat : GERR = 1 : GZONE = "BPRFCT" : GZNUM = 3669 : Endif
Gosub _AFF
End

Subprog S_BPRFCT(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "FCT"
PARMSK(2) = ""
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_BPRFCT(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "FCT"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_BPRFCT(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("FCT",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_FCTVCR
Onevent CONTROL Call C_FCTVCR(zc) From W1DELQUIT
Onevent BEFORE_INPUT Call AS_FCTVCR(zc) From W1DELQUIT
Return

Subprog AS_FCTVCR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_FCTVCR(zc) From W1DELQUIT
Gosub _AFF
End

Subprog C_FCTVCR(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "FCTVCR" : GZNUM = 2553 : Endif
If !GWEBSERV
 If !mkstat
  ## Action CONTVCR
  Call CONTVCR(VALEUR) From TRTDIV
  If mkstat : GERR = 1 : GZONE = "FCTVCR" : GZNUM = 2553 : Endif
 Endif
Endif
If !mkstat
 ## Action STD
 Call C_FCTVCR(VALEUR) From DELQUIT
 If mkstat : GERR = 1 : GZONE = "FCTVCR" : GZNUM = 2553 : Endif
Endif
Gosub _AFF
End

Subprog S_FCTVCR(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELFCTVCR
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELFCTVCR"
 PARMSK(1) = [M]BPRFCT
 PARMSK(2) = "0"
 Call S_LIS_CHAR(VALEUR,PARMSK,"SELFCTVCR","NOTRGLT","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

$ON_TRACE
Onevent INIT_AREA Call D_TRACE(zc) From W1DELQUIT
Onevent BEFORE_INPUT Call AS_TRACE(zc) From W1DELQUIT
Onevent CONTROL Call C_TRACE(zc) From W1DELQUIT
Return

Subprog D_TRACE(VALEUR)
Variable Integer VALEUR
If varinit([M]TRACE,indice)=0
 VALEUR = 2
Endif
Gosub _AFF
End

Subprog AS_TRACE(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog C_TRACE(VALEUR)
Variable Integer VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "TRACE" : GZNUM = 6058 : Endif
Gosub _AFF
End

$CHG_STY
Return


