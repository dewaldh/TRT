Gosub _AFF
End

Subprog AS_CCE17(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action FLDAVTCAR
Call FLDAVTCAR(VALEUR,"CCE17."+[M:HAEF]DIE(16)) From TRTGAS
If mkstat=2
 [M]CCE17(INDIC) = VALEUR
 Affzo CCE17(INDIC) : mkstat = 2
Endif
If !mkstat
 ## Action STD
 Call AS_CCE17(VALEUR) From SUBGA0
 If mkstat=2
  [M]CCE17(INDIC) = VALEUR
  Affzo CCE17(INDIC) : mkstat = 2
 Endif
 Onkey 129 Titled GBOUT1 Call B1_CCE17(zc) From W2WMAC9STDON
 Onkey   2 Titled GBOUTS Call S_CCE17(zc) From W2WMAC9STDON
 Onkey 144 Titled GBOUTP Call P_CCE17(zc) From W2WMAC9STDON
 Onkey 146 Titled GBOUTA Call SA_CCE17(zc) From W2WMAC9STDON
Endif
Gosub _AFF
End

Subprog C_CCE17(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$([M]DIE(16))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CCE17" : GZNUM = 0 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$([M]DIE(16)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CCE17" : GZNUM = 0 : Endif
 If !mkstat
  ## Action FLDCTLCAR
  Call FLDCTLCAR(VALEUR,"CCE17."+[M:HAEF]DIE(16)) From TRTGAS
  If mkstat : GERR = 1 : GZONE = "CCE17" : GZNUM = 0 : Endif
  If !mkstat
   ## Action STD
   Call C_CCE17(VALEUR) From SUBGA0
   If mkstat : GERR = 1 : GZONE = "CCE17" : GZNUM = 0 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

