####################################################################
# 31 : ORIDAT Source date ( Field D)
####################################################################

$SET_ORIDAT
If [L]_AOLDVAL<>this.ORIDAT
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_ORIDAT
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.ORIDAT
 Gosub PROPAGATE_ORIDAT
Endif
Return

$INIT_ORIDAT
Gosub DECLARE_VAR From WMC0PYHINQ
[L]CURPRO = "ORIDAT"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_ORIDAT
Gosub DECLARE_VAR From WMC0PYHINQ
[L]CURPRO = "ORIDAT"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_ORIDAT                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_ORIDAT                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_ORIDAT
[L]CURPRO = "ORIDAT"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.ORIDAT=[0/0/0]
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_ORIDAT
[L]CURPRO = "ORIDAT"
Return

$_AOTHERS_ORIDAT
[L]CURPRO = "ORIDAT"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_ORIDAT
[L]CURPRO = "ORIDAT"
Return

$PROPAGATE_ORIDAT
this.isReadonly = 0
[L]CURPRO = "ORIDAT"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_ORIDAT
Gosub DECLARE_VAR From WMC0PYHINQ
[L]CURPRO = "ORIDAT"
$_LNKPRO_ORIDAT
Return


