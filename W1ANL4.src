Onkey 146 Titled GBOUTA Call SA_FCY001(zc) From W1ANL4
Gosub _AFF
End

Subprog C_FCY001(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "FCY001" : GZNUM = 9540 : Endif
If !mkstat
 ## Action ACONTFCY
 Call CONTSITE(VALEUR) From SUBAGF
 If mkstat : GERR = 1 : GZONE = "FCY001" : GZNUM = 9540 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("FCY","E",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "FCY001" : GZNUM = 9540 : Endif
  If !mkstat
   ## Action STD
   Call C_FCY001(VALEUR) From SUBANL4
   If mkstat : GERR = 1 : GZONE = "FCY001" : GZNUM = 9540 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

