REP.REP = "YPLA"
REP.CLA = "YPLA"
REP.ICL = "YPLN"
REP.AFCRIGHT = "GESYPLA"
REP.NCL = 100862 : # Planning App
REP.DES = 100865 : # Planning Applications
REP.TIT = ""
