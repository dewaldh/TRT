Subprog D_CSTCOD(VALEUR)
Variable Integer VALEUR
If varinit([M]CSTCOD,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_CSTCOD(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(480) From COMBOS
## Action STD
Call AS_CSTCOD(VALEUR) From FUNSTVA
If mkstat=2
 [M]CSTCOD(INDIC) = VALEUR
 Affzo CSTCOD(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,480,1))))
Endif
If !mkstat
 Discombo 0 From CSTCOD(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From CSTCOD(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_CSTCOD(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
Gosub _AFF
End

