Subprog C_YVALTO(VALEUR)
Variable Date    VALEUR

#  Valid to cannot be before valid from date
If VALEUR < [M:YPRE4]YVALFRM(nolign-1)
  GMESSAGE = mess(30,6255,1)    #  Valid to cannot be before valid from date
  GERR = 1
  mkstat = 1
Endif

End


