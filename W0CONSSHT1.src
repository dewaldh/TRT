## Zone WVCRTYP
mkstat = 0
zoncou = "WVCRTYP"
indice = 0
IMPENT = [M]WVCRTYP
WNUM = IMPENT
If varinit([M]WVCRTYP,indice)=0
 IMPENT = 10
Endif
If varmode([M]WVCRTYP,indice)=0
 Call INICOMBO(701) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_WVCRTYP(IMPENT) From CNSSHTSTD
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,701,1))))
 Endif
 [M]WVCRTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "WVCRTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]WVCRTYP(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "WVCRTYP" : GZNUM = 4969 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
    mkstat = 1 : GMESSAGE = mess(99,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "WVCRTYP" : GZNUM = 4969 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WNUM<>IMPENT or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_WVCRTYP(IMPENT) From CNSSHTSTD
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "WVCRTYP" : GZNUM = 4969 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif
[M]WVCRTYP = IMPENT

