#-------------------------------------------------------------------#
# Tables list to open
#-------------------------------------------------------------------#
$AQUERY_OPEN
If clalev([F:AWS])=0 : Local File AWRKHISSUI [AWS] : Endif
If clalev([F:A_])=0 : Local File AWRKHISSUI [A_] : Endif
ACTION = "AQUERY_OPEN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Tables list to close
#-------------------------------------------------------------------#
$AQUERY_CLOSE
LogicClose File [AWS]
LogicClose File [A_]
ACTION = "AQUERY_CLOSE_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the query
#-------------------------------------------------------------------#
$AQUERY_JOIN
Link [AWS] With
& [A_]AWS2~=[AWS]DEST;[AWS]CHRONO
& As [LNK_]
& Where ( (1=1)
& and (([F:AWS]CODEVT='POCSIG') & find([F:AWS]CODWRK,'POCSIGNOT','POCSIGREJ','POCSIGNCR')<>0 & [F:AWS]FLGSIG=2 & [F:AWS]DEST=GACTX.USER)
&  )
ACTION = "AQUERY_JOIN_AFTER" : Gosub AQUERY_ACTION
Return
 
#-------------------------------------------------------------------#
# Link for the lookup
#-------------------------------------------------------------------#
$AQUERY_JOIN_LOOKUP
Link [AWS] With
& [A_]AWS2~=[AWS]DEST;[AWS]CHRONO
& As [LNK_]
& Where ( (1=1)
& and (([F:AWS]CODEVT='POCSIG') & find([F:AWS]CODWRK,'POCSIGNOT','POCSIGREJ','POCSIGNCR')<>0 & [F:AWS]FLGSIG=2 & [F:AWS]DEST=GACTX.USER)
&  )
Return
 
#-------------------------------------------------------------------#
# Default filter for the query
#-------------------------------------------------------------------#
$AQUERY_FILTER_DEF
Return
 
#-------------------------------------------------------------------#
# Transclass of fields of query
#-------------------------------------------------------------------#
$AQUERY_TRANS
# AWS --- AWRKHISSUI(AWS)
QRY(NBREP) = NewInstance R_POCSIG AllocGroup PQRY
QRY(NBREP).setAccessorEnabled = 0
If dim([F:AWS]AUUID)>0 & [F:AWS]AUUID=NullUuid
 [F:AWS]AUUID = getUuid
Endif
SetInstance QRY(NBREP).AWS With [F:AWS]
[L]ASTATUS = fmet QRY(NBREP).ACTION("AQUERY_TRANS_AFTER")
 
QRY(NBREP).setAccessorEnabled = 1
Gosub AQUERY_LNKPRO
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_CREATEINSTANCE
# AWS --- AWRKHISSUI(AWS)
QRY = NewInstance R_POCSIG AllocGroup PQRY
QRY.setAccessorEnabled = 0
Return

#-------------------------------------------------------------------#
# Create instance
#-------------------------------------------------------------------#
$AQUERY_SETINSTANCE
If dim([F:AWS]AUUID)>0 & [F:AWS]AUUID=NullUuid
 [F:AWS]AUUID = getUuid
Endif
SetInstance QRY.AWS With [F:AWS]
[L]ASTATUS = fmet QRY.ACTION("AQUERY_TRANS_AFTER")
Return

