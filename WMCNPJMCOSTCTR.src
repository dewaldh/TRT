 If this.DATRATE(AINDCLA)<>null& find(this.DATRATE(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.DATRATE(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
