# Supervisor
# Mask management SPC1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone BASTAB
mkstat = 0
zoncou = "BASTAB"
indice = 0
status = 1
zoncou = "BASTAB"
If varmode([M]BASTAB,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BASTAB" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]BASTAB
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]BASTAB = nolign
   status = 75
   Call SAI_BASTAB(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  SAVACT = GACTION : GACTION = "STD"
  Call C_BASTAB From SUBSPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "BASTAB" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_BASTAB(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone BASTAB
mkstat = 0
zoncou = "BASTAB"
If !mkstat
 SAVACT = GACTION : GACTION = "STD"
 Call C_BASTAB From SUBSPC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "BASTAB" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone FIL
mkstat = 0
zoncou = "FIL"
WCHAR = [M]FIL(indice)
SAVACT = GACTION : GACTION = "STD"
Call AV_FIL([M]FIL(indice)) From SUBSPC
GACTION = SAVACT
If mkstat=1 : Raz [M]FIL(indice) : Endif
If !mkstat
 If varmode([M]FIL,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "FIL" : Gosub ACTION From =IMPTRT
  Endif
  Call CONTROLE("ATB","!",[M]FIL(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "FIL" : GZNUM = 1176 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_FIL([M]FIL(indice)) From SUBSPC
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FIL" : GZNUM = 1176 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WCHAR<>[M]FIL(indice) or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_FIL([M]FIL(indice)) From SUBSPC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "FIL" : GZNUM = 1176 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone FLD
mkstat = 0
zoncou = "FLD"
WCHAR = [M]FLD(indice)
If varmode([M]FLD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]FLD(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "FLD" : GZNUM = 1261 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_FLD([M]FLD(indice)) From SUBSPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FLD" : GZNUM = 1261 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WCHAR<>[M]FLD(indice) or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_FLD([M]FLD(indice)) From SUBSPC
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "FLD" : GZNUM = 1261 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone CRIDIE
mkstat = 0
zoncou = "CRIDIE"
If varmode([M]CRIDIE,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CRIDIE([M]CRIDIE(indice)) From SUBSPC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CRIDIE(indice) : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "CRIDIE" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  Call CONTROLE("DIE","!",[M]CRIDIE(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CRIDIE" : GZNUM = 29327 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_CRIDIE([M]CRIDIE(indice)) From SUBSPC
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CRIDIE" : GZNUM = 29327 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone CRIIND
mkstat = 0
zoncou = "CRIIND"
If varmode([M]CRIIND,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CRIIND([M]CRIIND(indice)) From SUBSPC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CRIIND(indice) : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "CRIIND" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_CRIIND([M]CRIIND(indice)) From SUBSPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CRIIND" : GZNUM = 1390 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone LIEN
mkstat = 0
zoncou = "LIEN"
IMPENT = [M]LIEN(indice)
If varinit([M]LIEN,indice)=0
 IMPENT = 1
Endif
If varmode([M]LIEN,indice)=0
 Call INICOMBO(49) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_LIEN(IMPENT) From SUBSPC
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,49,1))))
 Endif
 [M]LIEN(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LIEN" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LIEN(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "LIEN" : GZNUM = 75 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]LIEN(indice) = IMPENT

## Zone BASTAB
mkstat = 0
zoncou = "BASTAB"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SPC1"
Return

