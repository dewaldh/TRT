 SAVACT = GACTION : GACTION = "INFPARCPY"
 PARMSK(1) = [M:FAS0]CPY
 PARMSK(2) = ""
 PARMSK(3) = num$(G_NON)
 Call SAISIE_CHAR(VALEUR,PARMSK,"INFPARCPY","INFPARCPY","") From GSAISIE
