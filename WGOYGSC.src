$BOUTON
STDBOUT = "ECSAN09<>RHILJMP"
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "1" : LBOUT(NBTEX) = 1101 : Call TEXTE(100894,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "2" : LBOUT(NBTEX) = 1102 : Call TEXTE(100943,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "1"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$("YGSC")
  PARBOUT(2) = num$("DV")
  FLGEXE = 1
  GACTION = "YPJMCRE"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","","",1,"SPEYPJMCRE") From GTRAITE
 When "2"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$("YGSC")
  PARBOUT(2) = num$("PM")
  FLGEXE = 1
  GACTION = "YPJMCRE"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","","",1,"SPEYPJMCRE") From GTRAITE
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


