# Supervisor
# Mask management GDIAENTRY3 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:02:20"
A_USER  = "ADMIN"
A_STAMP = "20180828000220"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBRFLD"
  If [M]NBRFLD=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask GDIAENTRY3 [GDE3]
Gosub HORODAT
Local Inpbox "GDIAENTRY3" From GFONCTION At A_STAMP With A_WINDPREV Mask [GDE3] As [SIM_]
Grizo [GDE3]20
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [GDE3]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=80 : Y1(0)=1 : Y2(0)=20
Return

$ON_DAC
Onevent INIT_AREA Call D_DAC(zc) From W1GDIAENTRY3
Onevent BEFORE_INPUT Call AS_DAC(zc) From W1GDIAENTRY3
Onevent CONTROL Call C_DAC(zc) From W1GDIAENTRY3
Return

Subprog D_DAC(VALEUR)
Variable Integer VALEUR
If varinit([M]DAC,indice)=0
 VALEUR = 1
 ## Action STD
 Call D_DAC(VALEUR) From SUBGDE
Endif
Gosub _AFF
End

Subprog AS_DAC(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(35) From COMBOS
## Action STD
Call AS_DAC(VALEUR) From SUBGDE
If mkstat=2
 [M]DAC(INDIC) = VALEUR
 Affzo DAC(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,35,1))))
Endif
If !mkstat
 Discombo 0 From DAC(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From DAC(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_DAC(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
## Action STD
Call C_DAC(VALEUR) From SUBGDE
If mkstat : GERR = 1 : GZONE = "DAC" : GZNUM = 6377 : Endif
Gosub _AFF
End

$ON_CRD
Onevent INIT_AREA Call D_CRD(zc) From W1GDIAENTRY3
Onevent BEFORE_INPUT Call AS_CRD(zc) From W1GDIAENTRY3
Return

Subprog D_CRD(VALEUR)
Variable Integer VALEUR
If varinit([M]CRD,indice)=0
 VALEUR = 1
 ## Action STD
 Call D_CRD(VALEUR) From SUBGDE
Endif
Gosub _AFF
End

Subprog AS_CRD(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action STD
Call AS_CRD(VALEUR) From SUBGDE
If mkstat=2
 [M]CRD(INDIC) = VALEUR
 Affzo CRD(INDIC) : mkstat = 2
Endif
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_DEFVAL
Onevent BEFORE_INPUT Call AS_DEFVAL(zc) From W1GDIAENTRY3
Onevent CONTROL Call C_DEFVAL(zc) From W1GDIAENTRY3
Return

Subprog AS_DEFVAL(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(1484,GBOUT1) From OBJDIV
Call TEXTE(27108,GBOUT2) From OBJDIV
Call TEXTE(29795,GBOUT3) From OBJDIV
If !GWEBSERV
 ## Action AFRIB
 Call IB_FORMULE From AFORMULE
 If mkstat=2
  [M]DEFVAL(INDIC) = VALEUR
  Affzo DEFVAL(INDIC) : mkstat = 2
 Endif
Endif
If !mkstat
 ## Action STD
 Call AS_DEFVAL(VALEUR) From SUBGDE
 If mkstat=2
  [M]DEFVAL(INDIC) = VALEUR
  Affzo DEFVAL(INDIC) : mkstat = 2
 Endif
 Onkey 129 Titled GBOUT1 Call B1_DEFVAL(zc) From W1GDIAENTRY3
 Onkey 130 Titled GBOUT2 Call B2_DEFVAL(zc) From W1GDIAENTRY3
 Onkey 131 Titled GBOUT3 Call B3_DEFVAL(zc) From W1GDIAENTRY3
Endif
Gosub _AFF
End

Subprog C_DEFVAL(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action AFRCONT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "AFRCONT"
 PARMSK(1) = ""
 Call S_AFF_CHAR(VALEUR,PARMSK,"AFRCONT","AFORMULE","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DEFVAL" : GZNUM = 88 : Endif
Endif
If !mkstat
 ## Action CONTEXP
 Call CONTEXP(VALEUR) From OBJDIV
 If mkstat : GERR = 1 : GZONE = "DEFVAL" : GZNUM = 88 : Endif
Endif
Gosub _AFF
End

Subprog B1_DEFVAL(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action AFRB1
 Call B1_FORMULE(VALEUR,"GJOURNAL","GTYPPCE","GACCOUNT","BPARTNER","GDIAENTRY") From AFORMULE
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog B2_DEFVAL(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action AFFMESS
 Call AFFMESS From AFORMULE
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog B3_DEFVAL(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ANAFOR
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ANAFOR"
 PARMSK(1) = "GJOURNAL"
 PARMSK(2) = "GTYPPCE"
 PARMSK(3) = "GACCOUNT"
 PARMSK(4) = "BPARTNER"
 PARMSK(5) = "GDIAENTRY"
 Call TRAITE_CHAR(VALEUR,PARMSK,"","ANAFOR","ANAFOR",3,"") From GTRAITE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_CTL
Onevent BEFORE_INPUT Call AS_CTL(zc) From W1GDIAENTRY3
Onevent CONTROL Call C_CTL(zc) From W1GDIAENTRY3
Onevent AFTER_CHANGE Gosub AMM_CTL From W1GDIAENTRY3
Return

Subprog AS_CTL(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(1404,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_CTL(VALEUR) From SUBGDE
If mkstat=2
 [M]CTL(INDIC) = VALEUR
 Affzo CTL(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_CTL(zc) From W1GDIAENTRY3
Onkey   2 Titled GBOUTS Call S_CTL(zc) From W1GDIAENTRY3
Onkey 144 Titled GBOUTP Call P_CTL(zc) From W1GDIAENTRY3
Onkey 146 Titled GBOUTA Call SA_CTL(zc) From W1GDIAENTRY3
Gosub _AFF
End

Subprog C_CTL(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("ACL","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "CTL" : GZNUM = 178 : Endif
Gosub _AFF
End

$AMM_CTL
Call AM_CTL(zc)
If !mkstat & GOBJET
 zoncou = "CTL" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_CTL(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZCTL)")>indice
 [M]CTL(indice) = VALEUR
 Affzo ZCTL(indice)
Endif
Gosub _AFF
End

Subprog S_CTL(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "ACL"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_CTL(VALEUR)
Variable Char    VALEUR()
Call OBJET("ACL",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_CTL(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ACL"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CTL(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ACL",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

