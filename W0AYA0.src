# Supervisor
# Mask management AYA0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ACTCOD
mkstat = 0
zoncou = "ACTCOD"
indice = 0
If varmode([M]ACTCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ACTCOD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]ACTCOD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ACTCOD" : GZNUM = 89 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "AYA"
  Local Char    VALEUR(250)
  VALEUR = [M]ACTCOD
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]ACTCOD = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ACTCOD" : GZNUM = 89 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone FCYCOD
mkstat = 0
zoncou = "FCYCOD"
indice = 0
If varinit([M]FCYCOD,indice)=0
 [M]FCYCOD = GAYTFCYWRK
Endif

## Zone INTIT
mkstat = 0
zoncou = "INTIT"
indice = 0
If varinit([M]INTIT,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]INTIT,"AYTACT","INTIT",FCYCOD,ACTCOD) From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]INTIT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "INTIT" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "AYA0"
Return

