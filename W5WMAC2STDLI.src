  TITCL(3) = [M:HAEF]INTDIE(0)
  TITCL(4) = [M:HAEF]INTDIE(1)
  TITCL(5) = [M:HAEF]INTDIE(2)
  TITCL(6) = [M:HAEF]INTDIE(3)
  TITCL(7) = [M:HAEF]INTDIE(4)
  TITCL(8) = [M:HAEF]INTDIE(5)
  TITCL(9) = [M:HAEF]INTDIE(6)
  TITCL(10) = [M:HAEF]INTDIE(7)
  TITCL(11) = [M:HAEF]INTDIE(8)
  TITCL(12) = [M:HAEF]INTDIE(9)
  TITCL(13) = [M:HAEF]INTDIE(10)
  TITCL(14) = [M:HAEF]INTDIE(11)
  TITCL(15) = [M:HAEF]INTDIE(12)
  TITCL(16) = [M:HAEF]INTDIE(13)
