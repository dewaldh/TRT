############################################################################################
### Subprog _ADELETE
############################################################################################
Subprog _ADELETE(APLSTD,ASTATUS)
Variable Instance APLSTD Using C_APLSTD
Variable Integer ASTATUS

If APLSTD = null : End : Endif

Gosub _AVARIABLES From WMC00APLSTD

[L]A_STRSITE = ""
[L]A_STRACC = ""
Local Instance ALNKDELTAB Using C_ALNKDELTAB
Gosub ADELETE_APLSTD

End

############################################################################################
### Subprog ADELETE
############################################################################################
Subprog ADELETE(APLSTD,ASTATUS)
Variable Instance APLSTD Using C_APLSTD
Variable Integer ASTATUS

If APLSTD = null : End : Endif

Gosub AVARIABLES From WMC00APLSTD
If [L]ASTATUS>=[V]CST_AERROR : Goto ADELETE_END : Endif

Local Instance ALNKDELTAB Using C_ALNKDELTAB

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub ADELETE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto ADELETE_END : Endif

If !adxlog
 Gosub ADELETE_TR_APLSTD
Else
 Gosub ADELETE_APLSTD
Endif

$ADELETE_END
End


$ADELETE_CONTROL
Gosub _ADELETE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Verification delete
[L]ASTATUS = max([L]ASTATUS,func ASYRSUB.VERIFY_INSTANCE(APLSTD,ALNKDELTAB))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _ADELETE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_ADELETE_CONTROL_BEFORE
If APLSTD.ASTALIN = [V]CST_ANEWDEL : Return : Endif
APLSTD.ASTALIN = [V]CST_ADEL
[L]ASTATUS = max([L]ASTATUS,fmet APLSTD.ACTION("ADELETE_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_ADELETE_CONTROL_AFTER

[L]ASTATUS = max([L]ASTATUS,fmet APLSTD.ACTION("ADELETE_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$ADELETE_TR_APLSTD
Trbegin [F:AST]
 Gosub ADELETE_APLSTD
 If [L]ASTATUS>=[V]CST_AERROR : Gosub ADELETE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$ADELETE_APLSTD
[L]ASTATUS = max([L]ASTATUS,fmet APLSTD.ACTION("ADELETE_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = func ASYRSUB.DELETE_LINKS(APLSTD,ALNKDELTAB)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00APLSTD._MAJSQL_APLSTD(APLSTD))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet APLSTD.ACTION("ADELETE_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$ADELETE_ABORT
Rollback
$_ADELETE_ABORT
If APLSTD.snapshotEnabled=1
 [L]A_RETIK = APLSTD.revertUpdTick
Endif
[L]ASTATUS = max(fmet APLSTD.ACTION("ADELETE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#

#-----------------------------------------------------#
$_ADELETE_AFTER
Return



