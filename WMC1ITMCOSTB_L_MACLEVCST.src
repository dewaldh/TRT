####################################################################
# 1 : MACLEVCST Machine cost level ( Field MD8 0)
####################################################################

$SET_MACLEVCST
If [L]_AOLDVAL<>this.MACLEVCST
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_MACLEVCST
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.MACLEVCST
 Gosub PROPAGATE_MACLEVCST
Endif
Return

$INIT_MACLEVCST
Gosub DECLARE_VAR From WMC0ITMCOSTB_L_MACLEVCST
[L]CURPRO = "MACLEVCST"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_MACLEVCST
Gosub DECLARE_VAR From WMC0ITMCOSTB_L_MACLEVCST
[L]CURPRO = "MACLEVCST"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_MACLEVCST             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_MACLEVCST             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_MACLEVCST
[L]CURPRO = "MACLEVCST"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.MACLEVCST=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_MACLEVCST
[L]CURPRO = "MACLEVCST"
Return

$_AOTHERS_MACLEVCST
[L]CURPRO = "MACLEVCST"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_MACLEVCST
[L]CURPRO = "MACLEVCST"
Return

$PROPAGATE_MACLEVCST
this.isReadonly = 0
[L]CURPRO = "MACLEVCST"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_MACLEVCST
Gosub DECLARE_VAR From WMC0ITMCOSTB_L_MACLEVCST
[L]CURPRO = "MACLEVCST"
$_LNKPRO_MACLEVCST
Return


