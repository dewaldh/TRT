#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management GRHPN4 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone DATDEB
mkstat = 0
zoncou = "DATDEB"
indice = 0
WDATE = [M]DATDEB
If varmode([M]DATDEB,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATDEB" : Gosub ACTION From =IMPTRT
 Endif
 If [M]DATDEB=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WDATE<>[M]DATDEB or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_DATDEB([M]DATDEB) From TRTACHGRH1
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone DATFIN
mkstat = 0
zoncou = "DATFIN"
indice = 0
WDATE = [M]DATFIN
If varmode([M]DATFIN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATFIN" : Gosub ACTION From =IMPTRT
 Endif
 If [M]DATFIN=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "DATDEBFIN"
  Call DATDEBFIN([M]DATFIN,[M:GPN4]DATDEB) From EXEFNC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WDATE<>[M]DATFIN or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_DATFIN([M]DATFIN) From TRTACHGRH1
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone NBRTOP
mkstat = 0
zoncou = "NBRTOP"
indice = 0
If varinit([M]NBRTOP,indice)=0
 [M]NBRTOP = 10
Endif
If varmode([M]NBRTOP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRTOP" : Gosub ACTION From =IMPTRT
 Endif
 If [M]NBRTOP=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "NBRTOP" : GZNUM = 31256 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_NBRTOP([M]NBRTOP) From TRTACHGRH1
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NBRTOP" : GZNUM = 31256 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone CUMRTNAMT
mkstat = 0
zoncou = "CUMRTNAMT"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]CUMRTNAMT(indice) : Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GRHPN4"
Return

