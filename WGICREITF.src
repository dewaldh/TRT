$BOUTON
STDBOUT = ""
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "C" : LBOUT(NBTEX) = 1801 : Call TEXTE(873,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "I" : LBOUT(NBTEX) = 1802 : Call TEXTE(20395,TEXTE(NBTEX)) From OBJDIV
