$BOUTON
STDBOUT = "RC-+D"
NBTEX  = 0
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
NBMEN += 1
NMEN(NBMEN) = 10 : Call TEXTE(30750,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "B" : LMEN(NBMEN) = 2001 : Call TEXTE(31208,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "C" : LMEN(NBMEN) = 2002 : Call TEXTE(31194,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "D" : LMEN(NBMEN) = 2003 : Call TEXTE(31183,TMEN(NBMEN)) From OBJDIV
NBMEN += 1
NMEN(NBMEN) = 10 : CMEN(NBMEN) = "E" : LMEN(NBMEN) = 2004 : Call TEXTE(54388,TMEN(NBMEN)) From OBJDIV
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "B"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "CCSO"
  PARBOUT(2) = num$(1)
  PARBOUT(3) = [M:CCSO]ORDDATDEB
  PARBOUT(4) = [M:CCSO]ORDDATFIN
  PARBOUT(5) = num$("0")
  PARBOUT(6) = num$("0")
  FLGEXE = 1
  GACTION = "VISUGRHSO1"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRHSOH1","FUNGRHSOH","XYFUNGRHSOH") From GSAISIE
  [M:CCSO]ORDDATDEB = PARBOUT(3)
  [M:CCSO]ORDDATFIN = PARBOUT(4)
 When "C"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "CCSO"
  PARBOUT(2) = num$(2)
  PARBOUT(3) = [M:CCSO]ORDDATDEB
  PARBOUT(4) = [M:CCSO]ORDDATFIN
  PARBOUT(5) = num$("0")
  PARBOUT(6) = num$("0")
  FLGEXE = 1
  GACTION = "VISUGRHSO3"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRHSOH3","FUNGRHSOH","XYFUNGRHSOH") From GSAISIE
  [M:CCSO]ORDDATDEB = PARBOUT(3)
  [M:CCSO]ORDDATFIN = PARBOUT(4)
 When "D"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "CCSO"
  PARBOUT(2) = num$(1)
  PARBOUT(3) = [M:CCSO]ORDDATDEB
  PARBOUT(4) = [M:CCSO]ORDDATFIN
  FLGEXE = 1
  GACTION = "VISUGRHSO2"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRHSOH2","FUNGRHSOH","XYFUNGRHSOH") From GSAISIE
  [M:CCSO]ORDDATDEB = PARBOUT(3)
  [M:CCSO]ORDDATFIN = PARBOUT(4)
 When "E"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "CCSO"
  PARBOUT(2) = num$(2)
  PARBOUT(3) = [M:CCSO]ORDDATDEB
  PARBOUT(4) = [M:CCSO]ORDDATFIN
  FLGEXE = 1
  GACTION = "VISUGRHSO2"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRHSOH2","FUNGRHSOH","XYFUNGRHSOH") From GSAISIE
  [M:CCSO]ORDDATDEB = PARBOUT(3)
  [M:CCSO]ORDDATFIN = PARBOUT(4)
Endcase
GACTION = SAVACT
Return

$EXEACT
