$DEFVAR
Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "CONSCSO1"     : ABMSK(0)  = "CSO1"     : AMA(0) = 1
NOMMSK(1) = "WMCSOALL"     : ABMSK(1)  = "CSO2"     : AMA(1) = 1
NBMASK = 1
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 0 : If clalev([M:CSO1])>0 : Default Mask [CSO1] : Endif
 When 1 : If clalev([M:CSO2])>0 : Default Mask [CSO2] : Endif
Endcase
Return

$TITMASK
Case MA
 When 0 : If clalev([M:CSO1])>0 : Gosub TITRES From W1CONSCSO1 : Endif
 When 1 : If clalev([M:CSO2])>0 : Gosub TITRES From W1WMCSOALL : Endif
Endcase
Return

$FERME_MASK
If clalev([M:CSO1])>0 : Close Local Mask [CSO1] : Endif
If clalev([M:CSO2])>0 : Close Local Mask [CSO2] : Endif
Return

$RAZMASK
Case MA
 When 0 : If clalev([M:CSO1])>0 : Raz [M:CSO1] : Endif
 When 1 : If clalev([M:CSO2])>0 : Raz [M:CSO2] : Endif
Endcase
Return

$AFFMASK
Case MA
 When 0 : If clalev([M:CSO1])>0 : Affzo [M:CSO1] : Endif
 When 1 : If clalev([M:CSO2])>0 : Affzo [M:CSO2] : Endif
Endcase
Return

