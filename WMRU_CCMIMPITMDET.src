############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(CCMIMPITMDET,ASTATUS)
Variable Instance CCMIMPITMDET Using R_CCMIMPITMDET
Variable Integer ASTATUS

If CCMIMPITMDET = null : End : Endif

Gosub AVARIABLES From WMR00CCMIMPITMDET
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet CCMIMPITMDET.ACTX.AGETAFCRIGHT(CCMIMPITMDET,"GESCCM",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet CCMIMPITMDET.ACTX.AGETAFCRIGHTU(CCMIMPITMDET,"GESCCM",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_CCMIMPITMDET
Else
 Gosub AUPDATE_CCMIMPITMDET
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = CCMIMPITMDET.freeSnapshot
CCMIMPITMDET.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
CCMIMPITMDET.ASTALIN = [V]CST_AUPD
CCMIMPITMDET.CCMITMD.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = CCMIMPITMDET.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMIMPITMDET.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = CCMIMPITMDET.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMIMPITMDET.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet CCMIMPITMDET._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet CCMIMPITMDET._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet CCMIMPITMDET._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If CCMIMPITMDET.CCMITMD.ASTALIN = [V]CST_ANEWDEL : Return : Endif
CCMIMPITMDET.CCMITMD.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(CCMIMPITMDET.CCMITMD.ASTALIN)<>""
 [L]A_ASTALIN = CCMIMPITMDET.CCMITMD.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet CCMIMPITMDET.CCMITMD.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

If [V]CST_ASTALIN(CCMIMPITMDET.CCMITMD.ASTALIN)<>""
 [L]A_ASTALIN = CCMIMPITMDET.CCMITMD.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet CCMIMPITMDET.CCMITMD.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_CCMIMPITMDET
Trbegin [F:CCMITMD]
 Gosub AUPDATE_CCMIMPITMDET
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(CCMIMPITMDET.CCMITMD.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 CCMIMPITMDET.CCMITMD.ASTALIN = [V]CST_ALL
Endif
Return

#-----------------------------------------------------#
$AUPDATE_CCMIMPITMDET
[L]A_ASTALIN = CCMIMPITMDET.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMIMPITMDET.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMIMPITMDET.CCMITMD._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = CCMIMPITMDET.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet CCMIMPITMDET.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If CCMIMPITMDET.snapshotEnabled=1
 [L]A_RETIK = CCMIMPITMDET.revertUpdTick
Endif

[L]ASTATUS = max(fmet CCMIMPITMDET.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



