# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "ATABTAB"
REP.CLA = "ATABTAB"
REP.ICL = "ADI"
REP.AFCRIGHT = "GESADI"
REP.NCL = 11495 : # Miscellaneous tables
REP.DES = 22436 : # Definition of misc tables
REP.TIT = "LNGDES"
REP.ANX = "CODE"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Table number
     REP.CLE(1).PAR = 'NUMTAB' : REP.CLE(1).TYP = 2 : REP.CLE(1).PRO= 'NUMTAB' : REP.CLE(1).VPR= 'ADI.NUMTAB' : REP.CLE(1).DES = 1236
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'ATABTAB_RSTD' : REP.TRT(1).TYP = 1
