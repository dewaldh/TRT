#<AdxTL>@(#)0.0.0.0 $Revision$
# Sage X3 supervisor
## LISTE ## VEN037 Expected Returns

$PARAM
Local Integer PARTYP(1..99)
PARTYP(1) = 7
PARTYP(2) = 8
PARTYP(3) = 7
PARTYP(4) = 7
PARTYP(5) = 7
PARTYP(6) = 7
PARTYP(7) = 7
Return

$OUVRE
If clalev([F:SRD])=0 : Local File SRETURND [SRD] : Endif
If clalev([F:SRH])=0 : Local File SRETURN [SRH] : Endif
If clalev([F:BPC])=0 : Local File BPCUSTOMER [BPC] : Endif

Local Char AFILOPE(250)
AFILOPE = ""
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[SRD]"
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[SRH]"
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[BPC]"

CRITSUP(1) = "([F:SRD]EXTQTY > 0)"
CRITSUP(2) = " & ([F:SRD]QTY = 0)"
Gosub REMPLACE_SUP From SUBALH9

Link [SRD] With
&   [SRH]SRH0 ~= [SRD]SRHNUM
& , [BPC]BPC0 ~= [SRH]BPCORD
& As [LNK]
& Where evalue(CRITERE) & evalue(CRITSUP)
& Order by Key CLE = [F:SRH]SALFCY;[F:SRD]RTNDAT;[F:SRD]SRHNUM;[F:SRH]BPCORD

Columns [LNK] ([F:SRH]SALFCY,[F:SRD]RTNDAT,[F:SRD]SRHNUM,[F:SRH]BPCORD,
& [F:BPC]BPCNAM,[F:SRD]ITMREF,[F:SRD]ITMDES1,[F:SRD]EXTQTY,[F:SRD]QTY,
& [F:SRD]SAU,[F:SRD]SRDLIN)
Return

$LIENS
If dim(AFILOPE)>0 & AFILOPE<>"" : Default File AFILOPE : Endif
Default [F],[L],[S],[V],[M]
Default [L],[S],[V],[M],[F]
Return

$CONT
OKCONT = 1
Return

$DEFVAR
[L]NBGRP = 2
[L]NBCOL = 11
[L]NBLIG = 100
[L]GRP(1) = 1
[L]COLGRP = 1
[L]SNSGRP = 2
[L]GRP(2) = 2
[L]CUM(11) = 1
[L]TRI(1) = 11
[L]TRI(2) = 11
[L]TRI(3) = 8
[L]AOBSIT = ""
[L]AOBACC = ""
[L]AOBFIC = ""
Return

$TUNLIEN
Local Char    TLIEN(30)(1..16) , TTYP(GLONATY)(1..16)
Local Decimal TLONG(1..16)
Local Integer TMEN(1..16)
Case NOBOUT
 When 1
  TTYP(1)  = "FCY"      : TLONG(1)  = 5     : TMEN(1)  = 0    : TLIEN(1)  = "01"
 When 2
  TTYP(1)  = "VCR"      : TLONG(1)  = 20    : TMEN(1)  = 0    : TLIEN(1)  = "03"
 When 3
  TTYP(1)  = "BPR"      : TLONG(1)  = 15    : TMEN(1)  = 0    : TLIEN(1)  = "04"
 When 4
  TTYP(1)  = "ITM"      : TLONG(1)  = 20    : TMEN(1)  = 0    : TLIEN(1)  = "06"
 When 5
  TTYP(1)  = "UOM"      : TLONG(1)  = 3     : TMEN(1)  = 0    : TLIEN(1)  = "10"
 When Default
Endcase
Return

$BOUTONS
Raz GBOUT1, GBOUT2, GBOUT3, GBOUT4, GBOUT5, GBOUT6, GBOUT7, GBOUT8
Raz GBOUT9, GBOUT10, GBOUT11, GBOUT12, GBOUT13, GBOUT14, GBOUT15, GBOUT16
If [M:LST]C01(nolign-1) <> "" 
 If [M:LST]NIV >= 1
  Call TEXTE(1427,GBOUT1) From OBJDIV
  [M:LST]TUNNEL(1) = "FCY"
 Endif
Endif
If [M:LST]C03(nolign-1) <> "" 
 Call TEXTE(8586,GBOUT2) From OBJDIV
 [M:LST]TUNNEL(2) = "SRH"
Endif
If [M:LST]C04(nolign-1) <> "" 
 Call TEXTE(1500,GBOUT3) From OBJDIV
 [M:LST]TUNNEL(3) = "BPR"
Endif
If [M:LST]C06(nolign-1) <> "" 
 Call TEXTE(2061,GBOUT4) From OBJDIV
 [M:LST]TUNNEL(4) = "ITM"
Endif
If [M:LST]C10(nolign-1) <> "" 
 Call TEXTE(3612,GBOUT5) From OBJDIV
 [M:LST]TUNNEL(5) = "TUN"
Endif
Return

$LECTURE
If I=1
 Default File "[SRD] , [SRH] , [BPC]"
Endif
Case I
 When 1: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:SRH]SALFCY
 When 2: [F:ALR]TYP = 8 : [F:ALR]DAT = [F:SRD]RTNDAT
 When 3: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:SRD]SRHNUM
 When 4: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:SRH]BPCORD
 When 5: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:BPC]BPCNAM
 When 6: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:SRD]ITMREF
 When 7: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:SRD]ITMDES1
 When 8: [F:ALR]TYP = 4 : [F:ALR]NUM = [F:SRD]EXTQTY
 When 9: [F:ALR]TYP = 4 : [F:ALR]NUM = [F:SRD]QTY
 When 10: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:SRD]SAU
 When 11: [F:ALR]TYP = 2 : [F:ALR]NUM = 1
Endcase
Return


