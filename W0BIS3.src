      Call C_FCYLIN([M]FCYLIN(indice)) From SUBBIS2
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "FCYLIN" : GZNUM = 1344 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       SAVACT = GACTION : GACTION = "CONTFCY"
       Call CONTFCY([M]FCYLIN(indice)) From EXEFNC
       GACTION = SAVACT
       If mkstat : GERR = 1 : GZONE = "FCYLIN" : GZNUM = 1344 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
       If !mkstat
        If WCHAR<>[M]FCYLIN(indice) or IMPMOD
         SAVACT = GACTION : GACTION = "STD"
         Call AM_FCYLIN([M]FCYLIN(indice)) From SUBBIS2
         GACTION = SAVACT
         If mkstat : GERR = 1 : GZONE = "FCYLIN" : GZNUM = 1344 : Endif
         Gosub AFF From GSAISIE
         If mkstat : End : Endif
        Endif
       Endif
