     Call CONTROLE("FCY","C",[M]FCYLIN(indice),"",mkstat) From CONTOBJ
     If mkstat : GERR = 1 : GZONE = "FCYLIN" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
