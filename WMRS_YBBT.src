# Supervisor
## ACLA-CLASSE ## YBBT
# 2018-10-14 23:34:53 226

#-------------------------------------------------------#
$METHODS
$EVENTS
Case [L]AEVENT
 When "AREAD_AFTER"
  Case [L]CURPTH
   When "YBBH" : Gosub ADDACS1
   When "YBBH.YBBD" : Gosub ADDACS2
   When "" : Gosub ADDACSREP
  Endcase
 When "AINIT"
  Case [L]CURPTH
   When "YBBH" : Gosub ADDACS1
   When "YBBH.YBBD" : Gosub ADDACS2
   When "" : Gosub ADDACSREP
  Endcase
 When Default
Endcase
Return

#-------------------------------------------------------#
$PROPERTIES
Case [L]ARULE
 When "CONTROL"
  Case [L]CURPRO
   When "YBBH.YBATFCY"
    If this.YBATFCY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTFCY(this,"GESYBB",this.YBATFCY,[V]CST_ATRUE)
    Endif
   When "YBBH.YBBD.YFCY"
    If this.YFCY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTFCY(this,"GESYBB",this.YFCY,[V]CST_ATRUE)
    Endif
   When "YBBH.YBBD.YCPY"
    If this.YCPY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTCPY(this,"GESYBB",this.YCPY,[V]CST_ATRUE)
    Endif
   When Default
  Endcase
 When "PROPAGATE"
  Case [L]CURPRO
   When Default
  Endcase
 Endcase
 Return
 
 #-------------------------------------------------------#
 # YBBH
 $ADDACS1
 Return
 
 # YBBH.YBBD
 $ADDACS2
 Return
 
 $ADDACSREP
 Return
 


