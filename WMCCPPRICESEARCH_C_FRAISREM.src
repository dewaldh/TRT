####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0PPRICESEARCH_C_FRAISREM
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_INDEXFRAIS From WMC1PPRICESEARCH_C_FRAISREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_INDEXFRAIS From WMC1PPRICESEARCH_C_FRAISREM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_COLDES From WMC1PPRICESEARCH_C_FRAISREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_COLDES From WMC1PPRICESEARCH_C_FRAISREM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_RAT From WMC1PPRICESEARCH_C_FRAISREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_RAT From WMC1PPRICESEARCH_C_FRAISREM : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AMT From WMC1PPRICESEARCH_C_FRAISREM : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AMT From WMC1PPRICESEARCH_C_FRAISREM : Gosub TESTALLSTAT :  Endif
