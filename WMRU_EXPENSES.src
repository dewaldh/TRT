# Supervisor
## CRUD-CLASSE ## EXPENSES
# 2018-08-27 13:14:37 565


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(EXPENSES,ASTATUS)
Variable Instance EXPENSES Using R_EXPENSES
Variable Integer ASTATUS

If EXPENSES = null : End : Endif

Gosub AVARIABLES From WMR00EXPENSES
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet EXPENSES.ACTX.AGETAFCRIGHT(EXPENSES,"GESEXS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet EXPENSES.ACTX.AGETAFCRIGHTU(EXPENSES,"GESEXS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_EXPENSES
Else
 Gosub AUPDATE_EXPENSES
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = EXPENSES.freeSnapshot
EXPENSES.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
EXPENSES.ASTALIN = [V]CST_AUPD
EXPENSES.EXS.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = EXPENSES.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = EXPENSES.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet EXPENSES._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet EXPENSES._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet EXPENSES._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If EXPENSES.EXS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
EXPENSES.EXS.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(EXPENSES.EXS.ASTALIN)<>""
 [L]A_ASTALIN = EXPENSES.EXS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.CODEXPENS)
 If EXPENSES.EXS.CODEXPENS([L]A_I)<>null
  [L]A_ASTALIN = EXPENSES.EXS.CODEXPENS([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.CODEXPENS([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.DIMCEN)
 If EXPENSES.EXS.DIMCEN([L]A_I)<>null
  [L]A_ASTALIN = EXPENSES.EXS.DIMCEN([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.DIMCEN([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.LNKRATLED)
 If EXPENSES.EXS.LNKRATLED([L]A_I)<>null
  [L]A_ASTALIN = EXPENSES.EXS.LNKRATLED([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.LNKRATLED([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.RATEXPENS)
 If EXPENSES.EXS.RATEXPENS([L]A_I)<>null
  [L]A_ASTALIN = EXPENSES.EXS.RATEXPENS([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.RATEXPENS([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

For [L]A_I=1 To maxtab(EXPENSES.EXS.CODEXPENS)
 If EXPENSES.EXS.CODEXPENS([L]A_I)<>null
  [L]A_ASTALIN = EXPENSES.EXS.CODEXPENS([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.CODEXPENS([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.DIMCEN)
 If EXPENSES.EXS.DIMCEN([L]A_I)<>null
  [L]A_ASTALIN = EXPENSES.EXS.DIMCEN([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.DIMCEN([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.LNKRATLED)
 If EXPENSES.EXS.LNKRATLED([L]A_I)<>null
  [L]A_ASTALIN = EXPENSES.EXS.LNKRATLED([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.LNKRATLED([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.RATEXPENS)
 If EXPENSES.EXS.RATEXPENS([L]A_I)<>null
  [L]A_ASTALIN = EXPENSES.EXS.RATEXPENS([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.RATEXPENS([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(EXPENSES.EXS.ASTALIN)<>""
 [L]A_ASTALIN = EXPENSES.EXS.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_EXPENSES
Trbegin [F:EXS]
 Gosub AUPDATE_EXPENSES
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(EXPENSES.EXS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 EXPENSES.EXS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.CODEXPENS)
 If EXPENSES.EXS.CODEXPENS([L]A_I)<>null
  If EXPENSES.EXS.CODEXPENS([L]A_I).ASTALIN=[V]CST_ADEL or EXPENSES.EXS.CODEXPENS([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance EXPENSES.EXS.CODEXPENS([L]A_I)
   EXPENSES.EXS.CODEXPENS([L]A_I) = null
  Elsif EXPENSES.EXS.CODEXPENS([L]A_I).ASTALIN <> [V]CST_ALL
   EXPENSES.EXS.CODEXPENS([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.DIMCEN)
 If EXPENSES.EXS.DIMCEN([L]A_I)<>null
  If EXPENSES.EXS.DIMCEN([L]A_I).ASTALIN=[V]CST_ADEL or EXPENSES.EXS.DIMCEN([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance EXPENSES.EXS.DIMCEN([L]A_I)
   EXPENSES.EXS.DIMCEN([L]A_I) = null
  Elsif EXPENSES.EXS.DIMCEN([L]A_I).ASTALIN <> [V]CST_ALL
   EXPENSES.EXS.DIMCEN([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.LNKRATLED)
 If EXPENSES.EXS.LNKRATLED([L]A_I)<>null
  If EXPENSES.EXS.LNKRATLED([L]A_I).ASTALIN=[V]CST_ADEL or EXPENSES.EXS.LNKRATLED([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance EXPENSES.EXS.LNKRATLED([L]A_I)
   EXPENSES.EXS.LNKRATLED([L]A_I) = null
  Elsif EXPENSES.EXS.LNKRATLED([L]A_I).ASTALIN <> [V]CST_ALL
   EXPENSES.EXS.LNKRATLED([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(EXPENSES.EXS.RATEXPENS)
 If EXPENSES.EXS.RATEXPENS([L]A_I)<>null
  If EXPENSES.EXS.RATEXPENS([L]A_I).ASTALIN=[V]CST_ADEL or EXPENSES.EXS.RATEXPENS([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance EXPENSES.EXS.RATEXPENS([L]A_I)
   EXPENSES.EXS.RATEXPENS([L]A_I) = null
  Elsif EXPENSES.EXS.RATEXPENS([L]A_I).ASTALIN <> [V]CST_ALL
   EXPENSES.EXS.RATEXPENS([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Return

#-----------------------------------------------------#
$AUPDATE_EXPENSES
[L]A_ASTALIN = EXPENSES.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.EXS._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = EXPENSES.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet EXPENSES.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If EXPENSES.snapshotEnabled=1
 [L]A_RETIK = EXPENSES.revertUpdTick
Endif

[L]ASTATUS = max(fmet EXPENSES.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



