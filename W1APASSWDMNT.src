#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management APASSWDMNT 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:49:30"
A_USER  = "ADMIN"
A_STAMP = "20180827234930"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask APASSWDMNT [PWD]
Gosub HORODAT
Local Inpbox "APASSWDMNT" From GFONCTION At A_STAMP With A_WINDPREV Mask [PWD] As [SIM_]
Grizo [PWD]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [PWD]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=32 : X2(0)=81 : Y1(0)=12 : Y2(0)=16
Return

$ON_DOSSIER
Return

$ON_USER
Return

$ON_PASSE
Onevent CONTROL Call C_PASSE(zc) From W1APASSWDMNT
Return

Subprog C_PASSE(VALEUR)
Variable Char    VALEUR()
## Action STD
Call C_PASSE(VALEUR) From APASSWDMNT
If mkstat : GERR = 1 : GZONE = "PASSE" : GZNUM = 967 : Endif
Gosub _AFF
End

$ON_MOTIF
Onevent INIT_AREA Call D_MOTIF(zc) From W1APASSWDMNT
Onevent CONTROL Call C_MOTIF(zc) From W1APASSWDMNT
Onevent BEFORE_INPUT Call AS_MOTIF(zc) From W1APASSWDMNT
Onevent AFTER_CHANGE Gosub AMM_MOTIF From W1APASSWDMNT
Return

Subprog D_MOTIF(VALEUR)
Variable Char    VALEUR()
If varinit([M]MOTIF,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"82","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_MOTIF(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"82") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_MOTIF(zc) From W1APASSWDMNT
Onkey   2 Titled GBOUTS Call S_MOTIF(zc) From W1APASSWDMNT
Onkey 144 Titled GBOUTP Call P_MOTIF(zc) From W1APASSWDMNT
Onkey 146 Titled GBOUTA Call SA_MOTIF(zc) From W1APASSWDMNT
If VALEUR="" : Call INIADI(VALEUR,"82") From TRTDIV : Endif
Gosub _AFF
End

Subprog C_MOTIF(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "MOTIF" : GZNUM = 2675 : Endif
If !mkstat
 ## Action CONTADI
 Call CONTADI(VALEUR,"82","") From SUBADI
 If mkstat : GERR = 1 : GZONE = "MOTIF" : GZNUM = 2675 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ADI","",VALEUR,"82",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "MOTIF" : GZNUM = 2675 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_MOTIF
Call AM_MOTIF(zc)
If !mkstat & GOBJET
 zoncou = "MOTIF" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_MOTIF(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZMOTIF)")>indice
 [M]MOTIF(indice) = VALEUR
 Affzo ZMOTIF(indice)
Endif
Gosub _AFF
End

Subprog S_MOTIF(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = "82"
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_MOTIF(VALEUR)
Variable Char    VALEUR()
Call OBJET("ADI",VALEUR,"82") From GOBJET
Gosub _AFF
End

Subprog SA_MOTIF(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(2) = "82"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_MOTIF(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "82"
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZMOTIF
Onevent BEFORE_AREA Gosub AV_ZMOTIF From W1APASSWDMNT
Return

$AV_ZMOTIF
Call LECTEXTRA(zc,"ATABDIV","LNGDES","82",num$([M]MOTIF(indice))) From ATEXTRA
Gosub _AFF
Return

$CHG_STY
Return


