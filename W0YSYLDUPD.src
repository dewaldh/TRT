# Supervisor
# Mask management YSYLDUPD 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YNEWYLD
mkstat = 0
zoncou = "YNEWYLD"
indice = 0
If varmode([M]YNEWYLD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YNEWYLD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YNEWYLD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YNEWYLD" : GZNUM = 100433 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YREA
mkstat = 0
zoncou = "YREA"
indice = 0
If varmode([M]YREA,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YREA" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YREA,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YREA" : GZNUM = 47587 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YUSR
mkstat = 0
zoncou = "YUSR"
indice = 0
If varmode([M]YUSR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YUSR" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YUSR,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YUSR" : GZNUM = 250 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("AUS","",[M]YUSR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YUSR" : GZNUM = 250 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YSYLDUPD"
Return

