## Action %CNTOBJ
Call CONTSEL("TCY","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "XBPACRY" : GZNUM = 263 : Endif
If !mkstat
 ## Action STD
 Call C_XBPACRY(VALEUR) From SUBBPA
 If mkstat : GERR = 1 : GZONE = "XBPACRY" : GZNUM = 263 : Endif
Endif
Gosub _AFF
End

