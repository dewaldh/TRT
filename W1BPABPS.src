 If mkstat : GERR = 1 : GZONE = "XBPACRY" : GZNUM = 263 : Endif
 If !mkstat & evalue("dim("+clanam([M])+"ZXBPACRY)")>indice
  [M]XBPACRY(indice) = VALEUR
