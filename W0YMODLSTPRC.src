# Supervisor
# Mask management YMODLSTPRC 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YLANDP
mkstat = 0
zoncou = "YLANDP"
indice = 0
If varmode([M]YLANDP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLANDP" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YLANDP=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YLANDP" : GZNUM = 100454 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YHOUP
mkstat = 0
zoncou = "YHOUP"
indice = 0
If varmode([M]YHOUP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YHOUP" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YHOUP=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YHOUP" : GZNUM = 100455 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YCOM
mkstat = 0
zoncou = "YCOM"
indice = 0
If varmode([M]YCOM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCOM" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YCOM,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YCOM" : GZNUM = 821 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YMODLSTPRC"
Return

