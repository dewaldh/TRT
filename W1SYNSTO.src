$ON_AUTCOR
Onevent INIT_AREA Call D_AUTCOR(zc) From W1SYNSTO
Onevent BEFORE_INPUT Call AS_AUTCOR(zc) From W1SYNSTO
Return

Subprog D_AUTCOR(VALEUR)
Variable Integer VALEUR
If varinit([M]AUTCOR,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_AUTCOR(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$CHG_STY
Return


