$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "A"
  FLGEXE = 1
  GACTION = "STD"
 When "B"
  FLGEXE = 1
  GACTION = "STD"
 When "C"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "SAIWX3"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"FWX3","TRTWX3","",1,"XYTRTWX3") From GTRAITE
Endcase
GACTION = SAVACT
Return

