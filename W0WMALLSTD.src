# Supervisor
# Mask management WMALLSTD 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
SAVACT = GACTION : GACTION = "STD"
Call AV_ITMREF([M]ITMREF(indice)) From CNSALLSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]ITMREF(indice) : Endif

## Zone ITMDES1
mkstat = 0
zoncou = "ITMDES1"
SAVACT = GACTION : GACTION = "STD"
Call AV_ITMDES1([M]ITMDES1(indice)) From CNSALLSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]ITMDES1(indice) : Endif

## Zone ALLTYP
mkstat = 0
zoncou = "ALLTYP"
IMPENT = [M]ALLTYP(indice)
SAVACT = GACTION : GACTION = "STD"
Call AV_ALLTYP(IMPENT) From CNSALLSTD
GACTION = SAVACT
If mkstat=1 : Raz IMPENT : Endif
If !mkstat
 If varinit([M]ALLTYP,indice)=0
  IMPENT = 1
 Endif
Endif
[M]ALLTYP(indice) = IMPENT

## Zone STU
mkstat = 0
zoncou = "STU"
SAVACT = GACTION : GACTION = "STD"
Call AV_STU([M]STU(indice)) From CNSALLSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]STU(indice) : Endif

## Zone ALLQTY
mkstat = 0
zoncou = "ALLQTY"
SAVACT = GACTION : GACTION = "STD"
Call AV_ALLQTY([M]ALLQTY(indice)) From CNSALLSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]ALLQTY(indice) : Endif

## Zone QTYSTU
mkstat = 0
zoncou = "QTYSTU"
SAVACT = GACTION : GACTION = "STD"
Call AV_QTYSTU([M]QTYSTU(indice)) From CNSALLSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]QTYSTU(indice) : Endif

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
IMPENT = [M]VCRTYP(indice)
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP(indice) = IMPENT

## Zone SERMGTCOD
mkstat = 0
zoncou = "SERMGTCOD"
IMPENT = [M]SERMGTCOD(indice)
If varinit([M]SERMGTCOD,indice)=0
 IMPENT = 1
Endif
[M]SERMGTCOD(indice) = IMPENT

## Zone TRSTYP
mkstat = 0
zoncou = "TRSTYP"
IMPENT = [M]TRSTYP(indice)
If varinit([M]TRSTYP,indice)=0
 IMPENT = 1
Endif
[M]TRSTYP(indice) = IMPENT

## Zone PIO
mkstat = 0
zoncou = "PIO"
IMPENT = [M]PIO(indice)
If varinit([M]PIO,indice)=0
 IMPENT = 1
Endif
[M]PIO(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "WMALLSTD"
Return

