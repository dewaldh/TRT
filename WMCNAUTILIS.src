If this.BPA<>null
 [L]ASTATUS = fmet this.BPA._ALNKPRO : Gosub TESTALLSTAT
Endif
Gosub LNKPRO_AUSPIC From WMC2AUTILIS : Gosub TESTALLSTAT
For AINDCLA=1 To maxtab(this.BID)
 If this.BID(AINDCLA)<>null& find(this.BID(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.BID(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
For AINDCLA=1 To maxtab(this.ADP)
 If this.ADP(AINDCLA)<>null& find(this.ADP(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.ADP(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
Gosub LNKPRO_CRY From WMC2AUTILIS : Gosub TESTALLSTAT
For AINDCLA=1 To maxtab(this.FNCCOL)
 If this.FNCCOL(AINDCLA)<>null& find(this.FNCCOL(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.FNCCOL(AINDCLA)._ALNKPRO : Gosub TESTALLSTAT
 Endif
Next AINDCLA
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



