####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0TIMEZONEINFO
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_COD From WMC1TIMEZONEINFO : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_COD From WMC1TIMEZONEINFO : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_DES From WMC1TIMEZONEINFO : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_DES From WMC1TIMEZONEINFO : Gosub TESTALLSTAT :  Endif
