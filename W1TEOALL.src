Raz mkstat
Call TEXTE(12837,GBOUT13) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_CPNITMDEB(VALEUR)
Onkey 170 Titled GBOUT13 Call B13_CPNITMDEB(zc) From W1TEOALL
Onkey   2 Titled GBOUTS Call S_CPNITMDEB(zc) From W1TEOALL
Onkey 144 Titled GBOUTP Call P_CPNITMDEB(zc) From W1TEOALL
Onkey 146 Titled GBOUTA Call SA_CPNITMDEB(zc) From W1TEOALL
Gosub _AFF
End

Subprog C_CPNITMDEB(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ITMCHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ITMCHOICE"
 Call S_TAB_CHAR(VALEUR,PARMSK,"ITMCHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CPNITMDEB" : GZNUM = 51374 : Endif
Endif
If !mkstat
 ## Action VXADCUPC
 Call ADCCTRUPC(VALEUR) From VXADCUPC
 If mkstat : GERR = 1 : GZONE = "CPNITMDEB" : GZNUM = 51374 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ITM","!",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CPNITMDEB" : GZNUM = 51374 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_CPNITMDEB
Call AM_CPNITMDEB(zc)
If !mkstat & GOBJET
 zoncou = "CPNITMDEB" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

