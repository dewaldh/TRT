############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(PJMSOLITMH,ASTATUS)
Variable Instance PJMSOLITMH Using C_PJMSOLITMH
Variable Integer ASTATUS

If PJMSOLITMH = null : End : Endif

Gosub _AVARIABLES From WMC00PJMSOLITMH

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_PJMSOLITMH

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PJMSOLITMH,ASTATUS)
Variable Instance PJMSOLITMH Using C_PJMSOLITMH
Variable Integer ASTATUS

If PJMSOLITMH = null : End : Endif

Gosub AVARIABLES From WMC00PJMSOLITMH
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
PJMSOLITMH.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PJMSOLITMH
Else
 Gosub AUPDATE_PJMSOLITMH
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PJMSOLITMH.freeSnapshot
PJMSOLITMH.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PJMSOLITMH._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PJMSOLITMH._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PJMSOLITMH._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PJMSOLITMH.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PJMSOLITMH.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PJMSOLITMH.ASTALIN)<>""
 [L]A_ASTALIN = PJMSOLITMH.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PJMSOLITMH.SPLD) : # PJMSOLITMD -- 2
 If PJMSOLITMH.SPLD(A_N2) <> null
& and PJMSOLITMH.SPLD(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PJMSOLITMH.SPLD(A_N2).ASTALIN<>[V]CST_ANEW & PJMSOLITMH.ASTALIN=[V]CST_ADEL
   PJMSOLITMH.SPLD(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PJMSOLITMH.SPLD(A_N2).ASTALIN=[V]CST_ANEW & find(PJMSOLITMH.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PJMSOLITMH.SPLD(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PJMSOLITMH.SPLD(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PJMSOLITMH.SPLD(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(PJMSOLITMH.SPLO) : # PJMSOLITMO -- 3
 If PJMSOLITMH.SPLO(A_N2) <> null
& and PJMSOLITMH.SPLO(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PJMSOLITMH.SPLO(A_N2).ASTALIN<>[V]CST_ANEW & PJMSOLITMH.ASTALIN=[V]CST_ADEL
   PJMSOLITMH.SPLO(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PJMSOLITMH.SPLO(A_N2).ASTALIN=[V]CST_ANEW & find(PJMSOLITMH.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PJMSOLITMH.SPLO(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PJMSOLITMH.SPLO(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PJMSOLITMH.SPLO(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PJMSOLITMH.SPLD) : # PJMSOLITMD -- 2
 If PJMSOLITMH.SPLD(A_N2) <> null
& and PJMSOLITMH.SPLD(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PJMSOLITMH.SPLD(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PJMSOLITMH.SPLD(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(PJMSOLITMH.SPLO) : # PJMSOLITMO -- 3
 If PJMSOLITMH.SPLO(A_N2) <> null
& and PJMSOLITMH.SPLO(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PJMSOLITMH.SPLO(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PJMSOLITMH.SPLO(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PJMSOLITMH.ASTALIN)<>""
 [L]A_ASTALIN = PJMSOLITMH.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PJMSOLITMH
Trbegin [F:PSOH]
 Gosub AUPDATE_PJMSOLITMH
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_PJMSOLITMH
[L]A_ASTALIN = PJMSOLITMH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00PJMSOLITMH._MAJSQL_PJMSOLITMH(PJMSOLITMH))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 For A_N2=1 To maxtab(PJMSOLITMH.SPLD)
  If PJMSOLITMH.SPLD(A_N2) <> null and
& (PJMSOLITMH.SPLD(A_N2).ASTALIN=[L]A_TYPMAJ or
& PJMSOLITMH.SPLD(A_N2).ASTALIN=[V]CST_ALL)
   If PJMSOLITMH.SPLD(A_N2).ASTALIN=[L]A_TYPMAJ
    Case PJMSOLITMH.SPLD(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLD(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(PJMSOLITMH.SPLO)
  If PJMSOLITMH.SPLO(A_N2) <> null and
& (PJMSOLITMH.SPLO(A_N2).ASTALIN=[L]A_TYPMAJ or
& PJMSOLITMH.SPLO(A_N2).ASTALIN=[V]CST_ALL)
   If PJMSOLITMH.SPLO(A_N2).ASTALIN=[L]A_TYPMAJ
    Case PJMSOLITMH.SPLO(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.SPLO(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PJMSOLITMH.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PJMSOLITMH.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PJMSOLITMH.snapshotEnabled=1
 [L]A_RETIK = PJMSOLITMH.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PJMSOLITMH.SPLD)
 If PJMSOLITMH.SPLD(A_N2) <> null
  [L]ASTATUS2 = fmet PJMSOLITMH.SPLD(A_N2)._AUPDATE_ABORT
  If PJMSOLITMH.SPLD(A_N2).ASTALIN=[V]CST_ADEL or PJMSOLITMH.SPLD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PJMSOLITMH.SPLD(A_N2)
   PJMSOLITMH.SPLD(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(PJMSOLITMH.SPLO)
 If PJMSOLITMH.SPLO(A_N2) <> null
  [L]ASTATUS2 = fmet PJMSOLITMH.SPLO(A_N2)._AUPDATE_ABORT
  If PJMSOLITMH.SPLO(A_N2).ASTALIN=[V]CST_ADEL or PJMSOLITMH.SPLO(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PJMSOLITMH.SPLO(A_N2)
   PJMSOLITMH.SPLO(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PJMSOLITMH.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PJMSOLITMH.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PJMSOLITMH.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PJMSOLITMH.SPLD)
 If PJMSOLITMH.SPLD(A_N2) <> null
  [L]ASTATUS2 = fmet PJMSOLITMH.SPLD(A_N2)._AUPDATE_AFTER
  If PJMSOLITMH.SPLD(A_N2).ASTALIN=[V]CST_ADEL or PJMSOLITMH.SPLD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PJMSOLITMH.SPLD(A_N2)
   PJMSOLITMH.SPLD(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(PJMSOLITMH.SPLO)
 If PJMSOLITMH.SPLO(A_N2) <> null
  [L]ASTATUS2 = fmet PJMSOLITMH.SPLO(A_N2)._AUPDATE_AFTER
  If PJMSOLITMH.SPLO(A_N2).ASTALIN=[V]CST_ADEL or PJMSOLITMH.SPLO(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PJMSOLITMH.SPLO(A_N2)
   PJMSOLITMH.SPLO(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



