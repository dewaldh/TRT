#<AdxTL>@(#)0.0.0.0 $Revision$
# Sage X3 supervisor
## LISTE ## ACH042 Subcontracting Prod 

$PARAM
Local Integer PARTYP(1..99)
PARTYP(1) = 7
PARTYP(2) = 7
PARTYP(3) = 7
PARTYP(4) = 8
PARTYP(5) = 7
PARTYP(6) = 7
Return

$OUVRE
If clalev([F:ORD])=0 : Local File ORDERS [ORD] : Endif
If clalev([F:ITM])=0 : Local File ITMMASTER [ITM] : Endif
If clalev([F:BPS])=0 : Local File BPSUPPLIER [BPS] : Endif
If clalev([F:AY13])=0 : Local File ATEXTRA [AY13] : Endif

Local Char AFILOPE(250)
AFILOPE = ""
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[ORD]"
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[ITM]"
If AFILOPE<>"" : AFILOPE += "," : Endif
AFILOPE += "[BPS]"

CRITSUP(1) = "(([F:ORD]WIPSTA=1 & find([F:ORD]WIPTYP,3,4,13)))"
Gosub REMPLACE_SUP From SUBALH9

Link [ORD] With
&   [ITM]ITM0 ~= [ORD]ITMREF
& , [BPS]BPS0 ~= [ORD]BPRNUM
& , [AY13]AXX0   = "ITMMASTER";"DES1AXX";GLANGUE;[F:ITM]ITMREF;""
& As [LNK]
& Where evalue(CRITERE) & evalue(CRITSUP)
& Order by Key CLE = [F:ORD]ORIFCY

Columns [LNK] ([F:ORD]ORIFCY,[F:ORD]ITMREF,[F:ORD]ENDDAT,[F:ORD]BPRNUM,
& [F:BPS]BPSNAM,[F:ORD]PJT,[F:ORD]RMNEXTQTY,[F:ORD]VCRTYP,[F:ORD]VCRNUM,
& [F:ORD]WIPTYP,[F:ORD]WIPNUM,[F:AY13]TEXTE)
Return

$LIENS
If dim(AFILOPE)>0 & AFILOPE<>"" : Default File AFILOPE : Endif
Default [F],[L],[S],[V],[M]
Default [L],[S],[V],[M],[F]
Return

$CONT
OKCONT = 1
Return

$DEFVAR
[L]NBGRP = 1
[L]NBCOL = 11
[L]NBLIG = 100
[L]GRP(1) = 1
[L]COLGRP = 1
[L]SNSGRP = 2
[L]CUM(11) = 1
[L]TRI(1) = 11
[L]TRI(2) = 8
[L]AOBSIT = ""
[L]AOBACC = ""
[L]AOBFIC = ""
Return

$TUNLIEN
Local Char    TLIEN(30)(1..16) , TTYP(GLONATY)(1..16)
Local Decimal TLONG(1..16)
Local Integer TMEN(1..16)
Case NOBOUT
 When 1
  TTYP(1)  = "FCY"      : TLONG(1)  = 5     : TMEN(1)  = 0    : TLIEN(1)  = "01"
 When 2
  TTYP(1)  = "ITF"      : TLONG(1)  = 20    : TMEN(1)  = 0    : TLIEN(1)  = "02"
 When 3
  TTYP(1)  = "BPR"      : TLONG(1)  = 15    : TMEN(1)  = 0    : TLIEN(1)  = "05"
 When Default
Endcase
Return

$BOUTONS
Raz GBOUT1, GBOUT2, GBOUT3, GBOUT4, GBOUT5, GBOUT6, GBOUT7, GBOUT8
Raz GBOUT9, GBOUT10, GBOUT11, GBOUT12, GBOUT13, GBOUT14, GBOUT15, GBOUT16
If [M:LST]C01(nolign-1) <> "" 
 If [M:LST]NIV >= 1
  Call TEXTE(1427,GBOUT1) From OBJDIV
  [M:LST]TUNNEL(1) = "FCY"
 Endif
Endif
If [M:LST]C02(nolign-1) <> "" 
 Call TEXTE(3549,GBOUT2) From OBJDIV
 [M:LST]TUNNEL(2) = "ITF"
Endif
If [M:LST]C05(nolign-1) <> "" 
 Call TEXTE(1500,GBOUT3) From OBJDIV
 [M:LST]TUNNEL(3) = "BPR"
Endif
Return

$LECTURE
If I=1
 Default File "[ORD] , [ITM] , [BPS]"
Endif
Case I
 When 1: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:ORD]ORIFCY
 When 2: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:ORD]ITMREF
 When 3: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:AY13]TEXTE
 When 4: [F:ALR]TYP = 8 : [F:ALR]DAT = [F:ORD]ENDDAT
 When 5: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:ORD]BPRNUM
 When 6: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:BPS]BPSNAM
 When 7: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:ORD]PJT
 When 8: [F:ALR]TYP = 4 : [F:ALR]NUM = [F:ORD]RMNEXTQTY
 When 9: [F:ALR]TYP = 1 : [F:ALR]NUM = [F:ORD]VCRTYP
 When 10: [F:ALR]TYP = 7 : [F:ALR]VLR = [F:ORD]VCRNUM
 When 11: [F:ALR]TYP = 2 : [F:ALR]NUM = 1
Endcase
Return


