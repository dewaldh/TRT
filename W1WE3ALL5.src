$ON_STCD
Onevent BEFORE_INPUT Call AS_STCD(zc) From W1WE3ALL5
Onevent INIT_AREA Call D_STCD(zc) From W1WE3ALL5
Return

Subprog D_STCD(VALEUR)
Variable Char    VALEUR()
If varinit([M]STCD,indice)=0
 VALEUR = "21"
Endif
Gosub _AFF
End

Subprog AS_STCD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(48553,GBOUTI) From OBJDIV
## Action STD
Call AS_STCD(VALEUR) From TRTSTCDOC
If mkstat=2
 [M]STCD(INDIC) = VALEUR
 Affzo STCD(INDIC) : mkstat = 2
Endif
Onkey 178 Titled GBOUTI Call CL_STCD(zc) From W1WE3ALL5
Gosub _AFF
End

Subprog CL_STCD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action STCDOC
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "STCDOC"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "ACL_STCD" : PROGSUB = "TRTSTCDOC" : PROGSPE = "SPECSTDOC" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = [M]WLABR
 PARMSK(2) = [M]WVCRNUM
 PARMSK(3) = num$([M]WVCRTYP)
 PARMSK(4) = num$([M]WCHGCOE)
 Call SAISIE_CHAR(VALEUR,PARMSK,"ISTCDOC","TRTSTCDOC","SPESTCDOC") From GSAISIE
 ACTION = "BCL_STCD" : PROGSUB = "TRTSTCDOC" : PROGSPE = "SPECSTDOC" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_WVCRTYP
Onevent INIT_AREA Call D_WVCRTYP(zc) From W1WE3ALL5
Return

Subprog D_WVCRTYP(VALEUR)
Variable Integer VALEUR
If varinit([M]WVCRTYP,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$TIT_COL
Local Char TITCL(200)(1..2)
Raz TITCL
Case BASPAG
 When "NBCST"
  TITCL(1) = mess(796,197,1)
  TITCL(2) = mess(797,197,1)+ " (" + GLOCALDEV + ")"
  If !GORITITCOL
   Titcol [M]NBCST TITCL(1) For TOTDOC
   Titcol [M]NBCST TITCL(2) For TOTSOC
  Else
   Titcol [M:CSTO]NBCST TITCL(1) For TOTDOC
   Titcol [M:CSTO]NBCST TITCL(2) For TOTSOC
  Endif
Endcase
Return

