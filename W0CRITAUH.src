#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CRITAUH 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ECRAN
mkstat = 0
zoncou = "ECRAN"
indice = 0
If varmode([M]ECRAN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ECRAN" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("GTC","!",[M]ECRAN,"AUH",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "ECRAN" : GZNUM = 46 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone STRDAT
mkstat = 0
zoncou = "STRDAT"
indice = 0
WDATE = [M]STRDAT
SAVACT = GACTION : GACTION = "ADATRELI"
Call INIDAT([M]STRDAT,[M]DATFRMD) From ADATREL
GACTION = SAVACT
If mkstat=1 : Raz [M]STRDAT : Endif
If !mkstat
 If varmode([M]STRDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "STRDAT" : Gosub ACTION From =IMPTRT
  Endif
  If WDATE<>[M]STRDAT or IMPMOD
   SAVACT = GACTION : GACTION = "ADATRELM"
   Call MODIFDAT([M]STRDAT,[M]DATFRMD) From ADATREL
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 1355 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone ENDDAT
mkstat = 0
zoncou = "ENDDAT"
indice = 0
WDATE = [M]ENDDAT
SAVACT = GACTION : GACTION = "ADATRELI"
Call INIDAT([M]ENDDAT,[M]DATFRMF) From ADATREL
GACTION = SAVACT
If mkstat=1 : Raz [M]ENDDAT : Endif
If !mkstat
 If varmode([M]ENDDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "ENDDAT" : Gosub ACTION From =IMPTRT
  Endif
  If WDATE<>[M]ENDDAT or IMPMOD
   SAVACT = GACTION : GACTION = "ADATRELM"
   Call MODIFDAT([M]ENDDAT,[M]DATFRMF) From ADATREL
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "ENDDAT" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone TABLE
mkstat = 0
zoncou = "TABLE"
indice = 0
If varmode([M]TABLE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "TABLE" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ATB","!",[M]TABLE,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "TABLE" : GZNUM = 1176 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone USER
mkstat = 0
zoncou = "USER"
indice = 0
If varmode([M]USER,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "USER" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("AUS","!",[M]USER,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "USER" : GZNUM = 250 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone LOGIN
mkstat = 0
zoncou = "LOGIN"
indice = 0
If varmode([M]LOGIN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LOGIN" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone EVENEMENT
mkstat = 0
zoncou = "EVENEMENT"
indice = 0
If varmode([M]EVENEMENT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "EVENEMENT" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_EVENEMENT([M]EVENEMENT) From CNSAUHSTD
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "EVENEMENT" : GZNUM = 1360 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone AUDBI
mkstat = 0
zoncou = "AUDBI"
indice = 0
IMPENT = [M]AUDBI
If varinit([M]AUDBI,indice)=0
 IMPENT = 1
Endif
If varmode([M]AUDBI,indice)=0
 [M]AUDBI(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "AUDBI" : Gosub ACTION From =IMPTRT
  IMPENT = [M]AUDBI(indice)
 Endif
Endif
[M]AUDBI = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CRITAUH"
Return

