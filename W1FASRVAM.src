If mkstat : GERR = 1 : GZONE = "RVACOEREF" : GZNUM = 34971 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZRVACOEREF)")>indice
 [M]RVACOEREF(indice) = VALEUR
 Affzo ZRVACOEREF(indice)
Endif
Gosub _AFF
End

Subprog S_RVACOEREF(VALEUR)
