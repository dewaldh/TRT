$ON_DPRPLN
Onevent BEFORE_INPUT Call AS_DPRPLN(zc) From W1FASRVAM
Onevent AFTER_AREA Call AP_DPRPLN(zc) From W1FASRVAM
Onevent AFTER_CHANGE Gosub AMM_DPRPLN From W1FASRVAM
Onevent INIT_AREA Call D_DPRPLN(zc) From W1FASRVAM
Onevent CONTROL Call C_DPRPLN(zc) From W1FASRVAM
Return

Subprog D_DPRPLN(VALEUR)
Variable Integer VALEUR
If varinit([M]DPRPLN,indice)=0
 VALEUR = 0
Endif
Gosub _AFF
End

Subprog AS_DPRPLN(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(3101) From COMBOS
## Action STD
Call AS_DPRPLN(VALEUR) From TRTFASRVA
If mkstat=2
 [M]DPRPLN(INDIC) = VALEUR
 Affzo DPRPLN(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,3101,1))))
Endif
If !mkstat
 Discombo 0 From DPRPLN(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From DPRPLN(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_DPRPLN(VALEUR)
Variable Integer VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "DPRPLN" : GZNUM = 33972 : Endif
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
Gosub _AFF
End

Subprog AP_DPRPLN(VALEUR)
Variable Integer VALEUR
## Action STD
Call AP_DPRPLN(VALEUR) From TRTFASRVA
If mkstat : GERR = 1 : GZONE = "DPRPLN" : GZNUM = 33972 : Endif
Gosub _AFF
End

