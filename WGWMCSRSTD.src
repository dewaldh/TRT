# Supervisor
# Window management WMCSRSTD

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [28/08/2018]
A_HEURE = "00:46:45"
A_USER  = "ADMIN"
Call ISWINVALID(nomap,GLANGUE,"WMCSRSTD",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
Local Char NAVMSK(200)
NAVMSK="CSR1;CSR2;"
Return

$OUVRE_BOITE
Gosub INIT_BOITE
Gosub DEF_BOITE
Gosub AFF_BOITE
Return

$INIT_BOITE
Local Char    OBJLIS(GLONAOB)(0..10), DESLIS(80)(0..10), BOX1(10)
Local Integer CHGLIS(0..10), BROLIS(0..10), TRELIS(0..10)
Local Char    SAIRAP(50)(1..16)
Local Integer DERLU , AFOLDEF
Local Char    ABRENA(GLONABR)
DERLU = 0
AFOLDEF = 1
ABRENA = ""
BOX1 = "GAU_CHE"
If clalev([M:CSR1])=0 : Local Mask CONSCSR1 [CSR1] : Endif
If clalev([M:CSR2])=0 : Local Mask WMCSRSTD [CSR2] : Endif
Gosub TIT
nolign = 1
Return

$DEF_BOITE
Gosub HORODAT From WGWMCSRSTD
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
If GFUSION=1
  A_WINDPREV=";;;FCNSCSR"
Endif
Local Inpbox "WMCSRSTD" From GFONCTION At A_STAMP With A_WINDPREV Mask [CSR1]
&   Folder Mask [CSR2]
& As [CSR2]
Titled [CSR2] TIT
Return

$VISU_BOITE
Gosub INIT_BOITE
Gosub HORODAT From WGWMCSRSTD
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "WMCSRSTD" From GFONCTION At A_STAMP With A_WINDPREV Mask [CSR1]
&   Folder Mask [CSR2]
& As [CSR2]
Titled [CSR2] TIT
Gosub AFF_BOITE
Return

$AFF_BOITE
Boxact [CSR2] Folder AFOLDEF
Return

$SAISIE_BOITE
Boxinp [CSR2] Using REPONSE
Return

$FERME_BOITE
Onerrgo RIEN
Close Local Inpbox [CSR2]
Onerrgo
Return

$RIEN
Resume

$TIT
Local Char TIT(60) , TITTRN(60)
Call TEXTE(33677,TIT) From OBJDIV
If GPROGRAMME<>""
 If dim([L]ACTION)<0 : Local Char ACTION(20) : Endif
 [L]ACTION = "TITRE" : Gosub ACTION From =GPROGRAMME
Endif
Return

$REMPLIT_
Return

$DEFAUT_BRO
Return

$SETBOX
Return

$SETORDER
Return

$ENDORDER
Return

$PICKING
Return

$DEPICK
Return

$REPICK
Return

$SETCUR
Return

$DEFVAR
Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "CONSCSR1"     : ABMSK(0)  = "CSR1"     : AMA(0) = 1
NOMMSK(1) = "WMCSRSTD"     : ABMSK(1)  = "CSR2"     : AMA(1) = 1
NBMASK = 1
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 0 : If clalev([M:CSR1])>0 : Default Mask [CSR1] : Endif
 When 1 : If clalev([M:CSR2])>0 : Default Mask [CSR2] : Endif
Endcase
Return

$TITMASK
Case MA
 When 0 : If clalev([M:CSR1])>0 : Gosub TITRES From W1CONSCSR1 : Endif
 When 1 : If clalev([M:CSR2])>0 : Gosub TITRES From W1WMCSRSTD : Endif
Endcase
Return

$FERME_MASK
If clalev([M:CSR1])>0 : Close Local Mask [CSR1] : Endif
If clalev([M:CSR2])>0 : Close Local Mask [CSR2] : Endif
Return

$RAZMASK
Case MA
 When 0 : If clalev([M:CSR1])>0 : Raz [M:CSR1] : Endif
 When 1 : If clalev([M:CSR2])>0 : Raz [M:CSR2] : Endif
Endcase
Return

$AFFMASK
Case MA
 When 0 : If clalev([M:CSR1])>0 : Affzo [M:CSR1] : Endif
 When 1 : If clalev([M:CSR2])>0 : Affzo [M:CSR2] : Endif
Endcase
Return

$OUVMSKCMP
Return

$FERMSKCMP
Return

$RAZMSKCMP
