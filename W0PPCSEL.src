# Supervisor
# Mask management PPCSEL 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBTAR
mkstat = 0
zoncou = "NBTAR"
indice = 0
status = 1
zoncou = "NBTAR"
If varmode([M]NBTAR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBTAR" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBTAR
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBTAR = nolign
   status = 75
   Call SAI_NBTAR(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBTAR(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBTAR
mkstat = 0
zoncou = "NBTAR"

## Zone NOLIG
mkstat = 0
zoncou = "NOLIG"
[M]NOLIG(indice) = nolign

## Zone PLITYP
mkstat = 0
zoncou = "PLITYP"
IMPENT = [M]PLITYP(indice)
If varinit([M]PLITYP,indice)=0
 IMPENT = 1
Endif
[M]PLITYP(indice) = IMPENT

## Zone PLIENAFLG
mkstat = 0
zoncou = "PLIENAFLG"
IMPENT = [M]PLIENAFLG(indice)
If varinit([M]PLIENAFLG,indice)=0
 IMPENT = 1
Endif
[M]PLIENAFLG(indice) = IMPENT

## Zone NBTAR
mkstat = 0
zoncou = "NBTAR"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "PPCSEL"
Return

