#---------------------------------------------------------------#
$ATRT_TEST
Local Integer ASTATUS
If !SILENCE : Call TRACE("1 --> Open main table" ) From ASYRCOM : Endif
Local File AUTILIS [AUS_]

If !SILENCE : Call TRACE("2 --> Local Instance R_AUTILIS") From ASYRCOM : Endif
Local Instance AUTILIS Using R_AUTILIS

If !SILENCE : Call TRACE("3 --> New Instance R_AUTILIS") From ASYRCOM : Endif
AUTILIS = NewInstance R_AUTILIS AllocGroup null

If !SILENCE : Call TRACE("4 --> Reading main table, Read First") From ASYRCOM : Endif
Read [AUS_]CODUSR First
If !fstat
 If !SILENCE : Call TRACE("Call program read of AUTILIS") From ASYRCOM : Endif
 [L]ASTATUS = max([L]ASTATUS,fmet AUTILIS.AREAD([AUS_]USR))
Endif

If !SILENCE
 Call TRACE("5 --> Check :"-num$([L]ASTATUS)) From ASYRCOM
 Call TRACE("AUTILIS.AUS.USR ="-num$(AUTILIS.AUS.USR)) From ASYRCOM
 Call TRACE("6 --> List of keys :") From ASYRCOM
 For I=1 To maxtab(AUTILIS.AUS.AKEYS)
  Call TRACE("-"-num$(I)-":"-AUTILIS.AUS.AKEYS(I)) From ASYRCOM
 Next
Endif
Close Local File [AUS_]
FreeGroup AUTILIS
Return

############################################################################################
### Funprog _AREADETAG
############################################################################################
Funprog _AREADETAG(AUTILIS,ACLE,ASTATUS,_ETAG)
Variable Instance AUTILIS Using R_AUTILIS
Value Char ACLE
Variable Integer ASTATUS
Variable Char _ETAG
[L]ASTATUS = [V]CST_AOK
Local Char _USR(5)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_USR = num$([L]CLE(1)) : Endif
Raz [L]_ETAG
Local File AUTILIS [AUS]
Default File [F:AUS]
Read [F:AUS]CODUSR = [L]_USR
If fstat
 [L]ASTATUS = [V]CST_AERROR
Endif
If [L]ASTATUS=[V]CST_AOK and dim([F:AUS]UPDTICK)>0
 [L]_ETAG = num$([F:AUS]UPDTICK)
Endif
End [L]ASTATUS

############################################################################################
### Subprog _AREADSUP
############################################################################################
Subprog _AREADSUP(AUTILIS,ACLE,ASTATUS)
Variable Instance AUTILIS Using R_AUTILIS
Value Char ACLE
Variable Integer ASTATUS
Local Char _USR(5)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_USR = num$([L]CLE(1)) : Endif
[L]ASTATUS = max([L]ASTATUS,fmet AUTILIS.AREAD([L]_USR))
End

############################################################################################
### Subprog AREAD
############################################################################################
Subprog AREAD(AUTILIS,_USR,ASTATUS)
Variable Instance AUTILIS Using R_AUTILIS
Value Char _USR
Variable Integer ASTATUS

If AUTILIS = null : End : Endif

Gosub AVARIABLES From WMR00AUTILIS
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If AUTILIS.APARENT=null
 [L]A_I = AUTILIS.freeSnapshot
 AUTILIS.snapshotEnabled = 0
 AUTILIS.setAccessorEnabled = 0
Endif

If fmet AUTILIS.ACTX.AGETAFCRIGHT(AUTILIS,"GESAUS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END : Endif
Default File [F:AUS]
Read [F:AUS]CODUSR = [L]_USR
If fstat
[L]A_ERROR = [L]_USR
& -":"-mess(23,100,1)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(AUTILIS,"",0,[L]ASTATUS,[V]CST_AHTTP_NOT_FOUND,[L]A_ERROR) : Goto AREAD_END
Endif
[L]A_STRSITE = ""
[L]A_STRACC = [F:AUS]ACSUSR
If fmet AUTILIS.ACTX.AGETAFCRIGHTR(AUTILIS,"GESAUS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END
Endif
If fmet AUTILIS.ACTX.AGETACSRIGHTC(AUTILIS,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AREAD_END
  Endif
If fmet AUTILIS.ACTX.ASTACRITROLE(AUTILIS,[L]_USR
& ,"AUS","AUS",[V]CST_ATRUE)>=[V]CST_AERROR
   [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END 
  Endif
  
  [L]ASTATUS = max([L]ASTATUS,fmet AUTILIS.ACTION("AREAD_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
  
  AUTILIS.AUS.setAccessorEnabled = 0
  [L]ASTATUS = max([L]ASTATUS,fmet AUTILIS.AUS.AREAD([L]_USR))
  If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
  
  [L]ASTATUS = max([L]ASTATUS,fmet AUTILIS.ACTION("AREAD_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
  
  If [L]ASTATUS<[V]CST_AERROR & AUTILIS.APARENT=null
   # Load Link
   [L]ASTATUS = max([L]ASTATUS,fmet AUTILIS._ALNKPRO)
   If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
  Endif
  
  $AREAD_END
  If AUTILIS.APARENT=null
   AUTILIS.snapshotEnabled = 1
   AUTILIS.setAccessorEnabled = 1
  Endif
  LogicClose File [AUS] ,[BPA] ,[BID]
  End
  


