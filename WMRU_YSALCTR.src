############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YSALCTR,ASTATUS)
Variable Instance YSALCTR Using R_YSALCTR
Variable Integer ASTATUS

If YSALCTR = null : End : Endif

Gosub AVARIABLES From WMR00YSALCTR
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet YSALCTR.ACTX.AGETAFCRIGHT(YSALCTR,"GESYSLC",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet YSALCTR.ACTX.AGETAFCRIGHTU(YSALCTR,"GESYSLC",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif
If fmet YSALCTR.ACTX.AGETACSRIGHTM(YSALCTR,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AUPDATE_END
  Endif
  
  Gosub AREPCONTROL_AUPDATE
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  
If !adxlog
 Gosub AUPDATE_TR_YSALCTR
Else
 Gosub AUPDATE_YSALCTR
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YSALCTR.freeSnapshot
YSALCTR.snapshotEnabled = 1
$AUPDATE_END
End

