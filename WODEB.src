$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(5)(0..GNBDERNIER)
Local Shortint A_BUF2(0..GNBDERNIER)
Local Date     A_BUF3(0..GNBDERNIER)
Local Libelle  A_BUF4(0..GNBDERNIER)
Local Integer  A_BUF5(0..GNBDERNIER)
Local Libelle  A_BUF6(0..GNBDERNIER)
Local Date     A_BUF7(0..GNBDERNIER)
Local Char     A_BUF8(20)(0..GNBDERNIER)
Local Integer  A_BUF9(0..GNBDERNIER)
If clalev([F:DEB9])>0 : Close Local File [DEB9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(5),
& Shortint A_TAB2,
& Date     A_TAB3,
& Libelle  A_TAB4,
& Integer  A_TAB5,
& Libelle  A_TAB6,
& Date     A_TAB7,
& Char     A_TAB8(20),
& Integer  A_TAB9
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5,A_BUF6,A_BUF7,A_BUF8,A_BUF9
& As [DEB9]
Return

$LEC_DERLU
Return

$CHARGE_DERLU
If clalev([F:DEB9])>0
 Read [DEB9] = [F:DEB9]A_TABI
 [L]CLE = [F:DEB9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:DEB9])>0
 Filter [DEB9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [DEB] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:DEB9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:DEB]DEBDAT+"~"
 A_CLEDERLU += [F:DEB]CPY+"~"
 A_CLEDERLU += num$([F:DEB]FLO)+"~"
 A_CLEDERLU += num$([F:DEB]VCRTYP)+"~"
 A_CLEDERLU += [F:DEB]VCRNUM+"~"
 A_CLEDERLU += num$([F:DEB]VCRLIN)+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:DEB9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:DEB9]
 Endif
Endif
Return

