#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CMD0 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:54:42"
A_USER  = "ADMIN"
A_STAMP = "20180827235442"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask CMD0 [CMD0]
Gosub HORODAT
Local Inpbox "CMD0" From GFONCTION At A_STAMP With A_WINDPREV Mask [CMD0] As [SIM_]
Grizo [CMD0]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [CMD0]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=86 : Y1(0)=1 : Y2(0)=3
Return

$ON_HTRTYP
Onevent INIT_AREA Call D_HTRTYP(zc) From W1CMD0
Return

Subprog D_HTRTYP(VALEUR)
Variable Integer VALEUR
If varinit([M]HTRTYP,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_HTRNUM
Onevent BEFORE_INPUT Call AS_HTRNUM(zc) From W1CMD0
Return

Subprog AS_HTRNUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action STD
Call AS_HTRNUM(VALEUR) From SUBCMD
If mkstat=2
 [M]HTRNUM(INDIC) = VALEUR
 Affzo HTRNUM(INDIC) : mkstat = 2
Endif
Gosub _AFF
End

$ON_DESAXX
Onevent INIT_AREA Call D_DESAXX(zc) From W1CMD0
Onevent BEFORE_INPUT Call AS_DESAXX(zc) From W1CMD0
Onevent CONTROL Call C_DESAXX(zc) From W1CMD0
Return

Subprog IB_DESAXX(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action BOUTTEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "BOUTTEXTRA"
 Call S_AFF_CHAR(VALEUR,PARMSK,"","ATEXTRA","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog D_DESAXX(VALEUR)
Variable Char    VALEUR()
If varinit([M]DESAXX,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"HDKTRS","DESAXX",num$(HTRTYP),HTRNUM) From ATEXTRA
Endif
Gosub _AFF
End

Subprog AS_DESAXX(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(9637,GBOUT1) From OBJDIV
Call IB_DESAXX(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_DESAXX(zc) From W1CMD0
Gosub _AFF
End

Subprog C_DESAXX(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "DESAXX" : GZNUM = 6468 : Endif
Gosub _AFF
End

Subprog B1_DESAXX(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ATEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ATEXTRA"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_DESAXX" : PROGSUB = "SUBCMD" : PROGSPE = "SPECMD" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "HDKTRS"
 PARMSK(2) = "DESAXX"
 PARMSK(3) = num$(HTRTYP)
 PARMSK(4) = HTRNUM
 Call SAISIE_CHAR(VALEUR,PARMSK,"ATEXTRA","ATEXTRA","") From GSAISIE
 ACTION = "BB1_DESAXX" : PROGSUB = "SUBCMD" : PROGSPE = "SPECMD" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_ENAFLG
Onevent INIT_AREA Call D_ENAFLG(zc) From W1CMD0
Onevent BEFORE_INPUT Call AS_ENAFLG(zc) From W1CMD0
Return

Subprog D_ENAFLG(VALEUR)
Variable Integer VALEUR
If varinit([M]ENAFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_ENAFLG(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_ACSCOD
Onevent CONTROL Call C_ACSCOD(zc) From W1CMD0
Onevent BEFORE_INPUT Call AS_ACSCOD(zc) From W1CMD0
Onevent AFTER_CHANGE Gosub AMM_ACSCOD From W1CMD0
Return

Subprog AS_ACSCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_ACSCOD(zc) From W1CMD0
Onkey 144 Titled GBOUTP Call P_ACSCOD(zc) From W1CMD0
Onkey 146 Titled GBOUTA Call SA_ACSCOD(zc) From W1CMD0
Gosub _AFF
End

Subprog C_ACSCOD(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("ACS","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "ACSCOD" : GZNUM = 729 : Endif
If !mkstat
 ## Action CTLACS
 Call CTLACS(VALEUR,"E") From CONTX3
 If mkstat : GERR = 1 : GZONE = "ACSCOD" : GZNUM = 729 : Endif
Endif
Gosub _AFF
End

$AMM_ACSCOD
Call AM_ACSCOD(zc)
If !mkstat & GOBJET
 zoncou = "ACSCOD" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_ACSCOD(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZACSCOD)")>indice
 [M]ACSCOD(indice) = VALEUR
 Affzo ZACSCOD(indice)
Endif
Gosub _AFF
End

Subprog S_ACSCOD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELACS
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELACS"
 PARMSK(1) = "E"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELACS","TRTX3","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog SA_ACSCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ACS"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_ACSCOD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("ACS",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZACSCOD
Onevent BEFORE_AREA Gosub AV_ZACSCOD From W1CMD0
Return

$AV_ZACSCOD
Call LECTEXTRA(zc,"ACCCOD","DESACC",num$([M]ACSCOD(indice)),"") From ATEXTRA
Gosub _AFF
Return

$ON_GFY
Onevent CONTROL Call C_GFY(zc) From W1CMD0
Onevent BEFORE_INPUT Call AS_GFY(zc) From W1CMD0
Onevent AFTER_CHANGE Gosub AMM_GFY From W1CMD0
Return

Subprog AS_GFY(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(8582,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_GFY(zc) From W1CMD0
Onkey   2 Titled GBOUTS Call S_GFY(zc) From W1CMD0
Onkey 144 Titled GBOUTP Call P_GFY(zc) From W1CMD0
Onkey 146 Titled GBOUTA Call SA_GFY(zc) From W1CMD0
Gosub _AFF
End

Subprog C_GFY(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("AGF","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "GFY" : GZNUM = 616 : Endif
Gosub _AFF
End

$AMM_GFY
Call AM_GFY(zc)
If !mkstat & GOBJET
 zoncou = "GFY" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_GFY(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZGFY)")>indice
 [M]GFY(indice) = VALEUR
 Affzo ZGFY(indice)
Endif
Gosub _AFF
End

Subprog S_GFY(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "AGF"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_GFY(VALEUR)
Variable Char    VALEUR()
Call OBJET("AGF",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_GFY(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AGF"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_GFY(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AGF",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZGFY
Onevent BEFORE_AREA Gosub AV_ZGFY From W1CMD0
Return

$AV_ZGFY
If clalev([F:AGF])=0 : Local File AGRPFCY [AGF] : Endif
Call LECTURE("AGF",num$([M]GFY(indice)),"") From CONTOBJ
zc = [F:AGF]DES
Gosub _AFF
Return

$CHG_STY
Return


