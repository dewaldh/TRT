Subprog AS_QUAFLG(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action PASLIN
If GMODIF<>2 mkstat=2 : Endif
If mkstat=2
 [M]QUAFLG(INDIC) = VALEUR
 Affzo QUAFLG(INDIC) : mkstat = 2
Endif
If !mkstat
 ## Action STD
 Call AS_QUAFLG(VALEUR) From SUBPOH
 If mkstat=2
  [M]QUAFLG(INDIC) = VALEUR
  Affzo QUAFLG(INDIC) : mkstat = 2
 Endif
 If VALEUR<1
  VALEUR = 1
 Endif
Endif
Gosub _AFF
End

