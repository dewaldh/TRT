  Case [L]CURPRO
   When "QRY_CPY"
    If this.QRY_CPY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTCPY(this,"GESSOH",this.QRY_CPY,[V]CST_ATRUE)
    Endif
   When "QRY_SALFCY"
    If this.QRY_SALFCY<>""
