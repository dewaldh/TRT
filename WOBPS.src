$CHARGE_DERLU
If clalev([F:BPS9])>0
 Read [BPS9] = [F:BPS9]A_TABI
 [L]CLE = [F:BPS9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:BPS9])>0
 Filter [BPS9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [BPRS] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:BPS9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:BPS]BPSNUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:BPS9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:BPS9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 2
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:BPS]BPSNUM"
Call TEXTE(3680,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:15X"
SELFOR(NBSEL) = "K:15c"
SELMAJ(NBSEL) = 1
NBTRI += 1
TRIZON(NBTRI) = "[F:BPS]BPSNUM"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:BPS]BPSNAM"
Call TEXTE(1644,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:35X"
SELFOR(NBSEL) = "K:35X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:BPS]BPSNAM"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:BPS]BPSSHO"
Call TEXTE(4010,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:10X"
SELFOR(NBSEL) = "K:10X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:BPS]BPSSHO"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:BPS_])=0
 Gosub OUVFIC_LISTE
Endif
Return

