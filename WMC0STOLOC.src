$ADELLINE
Case _ACOLLECTION
 When "STF"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"STF",this)
 When "STO"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"STO",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

