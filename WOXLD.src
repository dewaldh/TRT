$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(10)(0..GNBDERNIER)
Local Date     A_BUF2(0..GNBDERNIER)
If clalev([F:Y08XLE9])>0 : Close Local File [Y08XLE9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(10),
& Date     A_TAB2
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2
& As [Y08XLE9]
Return

$LEC_DERLU
Return

$CHARGE_DERLU
If clalev([F:Y08XLE9])>0
 Read [Y08XLE9] = [F:Y08XLE9]A_TABI
 [L]CLE = [F:Y08XLE9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:Y08XLE9])>0
 Filter [Y08XLE9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [XLD1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:Y08XLE9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:Y08XLE]CODLIV+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:Y08XLE9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:Y08XLE9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 1
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:Y08XLE]CODLIV"
Call TEXTE(5296,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:10X"
SELFOR(NBSEL) = "K:10X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:Y08XLE]CODLIV"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:Y08XLE]DATELIV"
Call TEXTE(2956,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 8
SELFMT(NBSEL) = "DZz:"+GFMDAT4+""
SELFOR(NBSEL) = "DZz:"+GFMDAT4
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:Y08XLE]DATELIV"
TRILIB(NBTRI) = SELLIB(NBSEL)
Return

