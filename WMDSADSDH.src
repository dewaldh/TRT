# Supervisor
## BI-DIMENSION ## Synchronization rule SADSDH

$OUVRE
Local Char FILTRE(250)
OUVTAB = GDOSSIER+".SDELIVERY" : If clalev([F:SDH])=0 : Local File =OUVTAB [SDH] : Endif
OUVTAB = GDOSSIER+".COMPANY" : If clalev([F:CPY])=0 : Local File =OUVTAB [CPY] : Endif
OUVTAB = "Link [SDH]"
Link [SDH] With
& [CPY]CPY0 = [F:SDH]CPY
& As [LNK_]
& Where [F :SDH]CFMFLG=2
& Order By Key CLE = [SDH]SDHNUM
FILTRE = ""
Return

$FERME
If clalev([F:SDH]) : Close local File [SDH] : Endif
If clalev([F:CPY]) : Close local File [CPY] : Endif
Return

$DCLE
Local Char    DCLE(GLONAVA)(1..1)
Local Integer DTYP(1..1)
Local Integer NBDCLE
NBDCLE = 1
DCLE(1) = "SDHNUM"
DTYP(1) =  7
Return

$CHARGE_CLE
If dim([F:AAA_]SDHNUM)>0
 [F:AAA_]SDHNUM = [F:SDH]SDHNUM
Endif
Return

########################################################################
# Return the filter necessary for incremental delete in synchroonization
Subprog GET_FLTDELINC(PFLT)
Variable Clbfile PFLT
PFLT = "1=1"
Append PFLT, " & [F:AAA_]SDHNUM=[F:SDH]SDHNUM"

End

