####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0YSCRC
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YSLCID From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YSLCID From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YCOMMENTID From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YCOMMENTID From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YDAT From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YDAT From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YSTFFMEM From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YSTFFMEM From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YCOVTYP From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YCOVTYP From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_YCMT From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_YCMT From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREDATTIM From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREDATTIM From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDATTIM From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDATTIM From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AUUID From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AUUID From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREUSR From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREUSR From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDUSR From WMC1YSCRC : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDUSR From WMC1YSCRC : Gosub TESTALLSTAT :  Endif
[L]ASTATUS = fmet this.ACONTROL
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



