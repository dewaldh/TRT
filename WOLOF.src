$LEC_DERLU
Read [AFF]AFF0 = [F:LOF]FCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Return

$CHARGE_DERLU
If clalev([F:LOF9])>0
 Read [LOF9] = [F:LOF9]A_TABI
 [L]CLE = [F:LOF9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:LOF9])>0
 Filter [LOF9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [LOF1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:LOF9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = num$([F:LOF]LINLOF)+"~"
 A_CLEDERLU += [F:LOF]CODLOF+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:LOF9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:LOF9]
 Endif
Endif
Return

