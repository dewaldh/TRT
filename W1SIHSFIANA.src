#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management SIHSFIANA 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:12:29"
A_USER  = "ADMIN"
A_STAMP = "20180828001229"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBAXE"
  If [M]NBAXE=0
  Endif
 When "NBACC"
  If [M]NBACC=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask SIHSFIANA [SIHS]
Gosub HORODAT
Local Inpbox "SIHSFIANA" From GFONCTION At A_STAMP With A_WINDPREV Mask [SIHS] As [SIM_]
Grizo [SIHS]5
Grizo [SIHS]10
Grizo [SIHS]20
Grizo [SIHS]25
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [SIHS]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=32 : X2(0)=81 : Y1(0)=7 : Y2(0)=8
X1(1)=32 : X2(1)=81 : Y1(1)=9 : Y2(1)=10
X1(2)=32 : X2(2)=81 : Y1(2)=11 : Y2(2)=21
X1(3)=32 : X2(3)=31 : Y1(3)=22 : Y2(3)=21
Return

$ON_SFINUM
Return

$ON_ZSFINUM
Onevent BEFORE_AREA Gosub AV_ZSFINUM From W1SIHSFIANA
Return

$AV_ZSFINUM
Call LECTEXTRA(zc,"SFOOTINV","DESAXX",num$([M]SFINUM(indice)),"") From ATEXTRA
Gosub _AFF
Return

$ON_DSP
Onevent CONTROL Call C_DSP(zc) From W1SIHSFIANA
Onevent AFTER_CHANGE Gosub AMM_DSP From W1SIHSFIANA
Onevent BEFORE_INPUT Call AS_DSP(zc) From W1SIHSFIANA
Return

Subprog AS_DSP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(4805,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_DSP(zc) From W1SIHSFIANA
Onkey   2 Titled GBOUTS Call S_DSP(zc) From W1SIHSFIANA
Onkey 144 Titled GBOUTP Call P_DSP(zc) From W1SIHSFIANA
Onkey 146 Titled GBOUTA Call SA_DSP(zc) From W1SIHSFIANA
Gosub _AFF
End

Subprog C_DSP(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("DSP","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 4043 : Endif
If !mkstat
 ## Action CTLDSP
 Call CTRL_DSP(VALEUR,[M:SIHV]FCY,[M:SIHV]DAT) From TRTX3CPT
 If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 4043 : Endif
Endif
Gosub _AFF
End

$AMM_DSP
Call AM_DSP(zc)
If !mkstat & GOBJET
 zoncou = "DSP" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_DSP(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_DSP(VALEUR) From SUBSIHV
If mkstat : GERR = 1 : GZONE = "DSP" : GZNUM = 4043 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZDSP)")>indice
 [M]DSP(indice) = VALEUR
 Affzo ZDSP(indice)
Endif
Gosub _AFF
End

Subprog S_DSP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "DSP"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_DSP(VALEUR)
Variable Char    VALEUR()
Call OBJET("DSP",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_DSP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "DSP"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_DSP(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("DSP",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZDSP
Onevent BEFORE_AREA Gosub AV_ZDSP From W1SIHSFIANA
Return

$AV_ZDSP
Call LECTEXTRA(zc,"CADSP","DESTRA",num$([M]DSP(indice)),"") From ATEXTRA
Gosub _AFF
Return

$ON_DIE
Return

$ON_NAMDIE
Onevent BEFORE_AREA Gosub AV_NAMDIE From W1SIHSFIANA
Return

$AV_NAMDIE
zc = GCOLDIE(nolign)
Gosub _AFF
Return

$ON_CCE
Onevent BEFORE_AREA Gosub AV_CCE From W1SIHSFIANA
Onevent CONTROL Call C_CCE(zc) From W1SIHSFIANA
Onevent BEFORE_INPUT Call AS_CCE(zc) From W1SIHSFIANA
Return

$AV_CCE
## Action INICCEFMT
Call INICCEFMT(num$(GDIE(nolign))) From SETGLBDIE
If GCCEFMT<>""
 Chgfmt [M]CCE(indice) With "K:"+GCCEFMT
Else
 Chgfmt [M]CCE(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog AS_CCE(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_CCE(zc) From W1SIHSFIANA
Onkey   2 Titled GBOUTS Call S_CCE(zc) From W1SIHSFIANA
Onkey 144 Titled GBOUTP Call P_CCE(zc) From W1SIHSFIANA
Onkey 146 Titled GBOUTA Call SA_CCE(zc) From W1SIHSFIANA
Gosub _AFF
End

Subprog C_CCE(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$(GDIE(nolign))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CCE" : GZNUM = 1688 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$(GDIE(nolign)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CCE" : GZNUM = 1688 : Endif
 If !mkstat
  ## Action CTLCCEH
  Call CTRL_CCEH(VALEUR,nolign,GFINRSP,[M:SIHV]DAT) From TRTX3CPT
  If mkstat : GERR = 1 : GZONE = "CCE" : GZNUM = 1688 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_CCE(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "CCE"
PARMSK(2) = "!"
PARMSK(3) = num$(GDIE(nolign))
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_CCE(VALEUR)
Variable Char    VALEUR()
Call OBJET("CCE",VALEUR,num$(GDIE(nolign))) From GOBJET
Gosub _AFF
End

Subprog SA_CCE(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "CCE"
PARMSK(2) = num$(GDIE(nolign))
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CCE(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = num$(GDIE(nolign))
Call VISU("CCE",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ACC
Onevent BEFORE_AREA Gosub AV_ACC From W1SIHSFIANA
Return

$AV_ACC
## Action INIACCFMT
Call INIACCFMT("") From SETGLBCOA
Gosub _AFF
Return

$CHG_STY
Return


