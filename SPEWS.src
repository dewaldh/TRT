#<AdxTL>@(#)0.0.0.0 $Revision$
#The ALIAS is the web service name
#If it can change, use the object variable
#Ex: WW_OBJET = "BPS"


#--------------------------------------------------------------------
#Action
#--------------------------------------------------------------------
ACTION
  GWEBSERV = 1

  Case ACTION
    When "AV_WS" : Gosub AV_WS
    When "AP_WS" : Gosub AP_WS
  Endcase
Return

#--------------------------------------------------------------------
#After execution
#--------------------------------------------------------------------
$AP_WS

  If WW_OBJET = "BPS"   #ALIAS = "YBPS_V001" #WW_OBJET = "BPS"
    Gosub AP_WS From YBPSWS
  Endif

  If WW_OBJET = "PJM"   #ALIAS = "YBPS_V001" #WW_OBJET = "BPS"
    Gosub AP_WS From YPJMWS
  Endif

  If WW_OBJET = "BIS"   #ALIAS = "YBPS_V001" #WW_OBJET = "BPS"
    Gosub AP_WS From YBISWS
  Endif

Kill GYOK
 GWEBSERV = 0
Return

#--------------------------------------------------------------------
#Before execution
#--------------------------------------------------------------------
$AV_WS

Global Integer GYOK
GYOK = 1
  If WW_OBJET = "BPS"     #ALIAS = "YBPS_V001" #WW_OBJET = "BPS"
    Gosub AV_WS From YBPSWS
  Endif
Return

