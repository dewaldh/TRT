# Supervisor
# Mask management PDSYSCO0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
indice = 0
IMPENT = [M]VCRTYP
If varinit([M]VCRTYP,indice)=0
 IMPENT = 36
Endif
[M]VCRTYP = IMPENT

## Zone ITMSTA
mkstat = 0
zoncou = "ITMSTA"
indice = 0
IMPENT = [M]ITMSTA
If varinit([M]ITMSTA,indice)=0
 IMPENT = 1
Endif
[M]ITMSTA = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

indice = 0
status = 1
zoncou = "NBLIG"
If !mkstat
 NBLIG = [M]NBLIG
 For nolign=1 To NBLIG
  indice = nolign-1
  [M]NBLIG = nolign
  status = 75
  Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1


## Zone LINAMT
mkstat = 0
zoncou = "LINAMT"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]LINAMT(indice) : Endif

## Zone LINAMTL
mkstat = 0
zoncou = "LINAMTL"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CPRCUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]LINAMTL(indice) : Endif

## Zone CPRCUR
mkstat = 0
zoncou = "CPRCUR"
If varinit([M]CPRCUR,indice)=0
 [M]CPRCUR(indice) = GLOCALDEV
Endif


End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "PDSYSCO0"
Return

