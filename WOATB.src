$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(12)(0..GNBDERNIER)
Local Char     A_BUF2(80)(0..GNBDERNIER)
Local Char     A_BUF3(8)(0..GNBDERNIER)
Local Libelle  A_BUF4(0..GNBDERNIER)
If clalev([F:ATB9])>0 : Close Local File [ATB9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(12),
& Char     A_TAB2(80),
& Char     A_TAB3(8),
& Libelle  A_TAB4
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4
& As [ATB9]
Return

$LEC_DERLU
Read [ATX]NUMERO = GLANGUE ; [F:ATB]INTITFIC
If fstat : Raz [F:ATX] : Endif
Return

$CHARGE_DERLU
If clalev([F:ATB9])>0
 Read [ATB9] = [F:ATB9]A_TABI
 [L]CLE = [F:ATB9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:ATB9])>0
 Filter [ATB9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [ATB1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:ATB9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:ATB]CODFIC+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:ATB9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:ATB9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
