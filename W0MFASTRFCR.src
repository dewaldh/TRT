    indice = nolign-1
    [M]NBOBJ = nolign
    status = 75
    Call SAI_NBOBJ(IMPFIC,ACTION,IMPTRT)
    If mkstat : Break : Endif
   Next nolign
   If mkstat : End : Endif
   indice = 0
  Endif
