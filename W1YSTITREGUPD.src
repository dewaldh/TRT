#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management YSTITREGUPD 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [06/09/2018]
A_HEURE = "10:27:49"
A_USER  = "MB"
A_STAMP = "20180906102749"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask YSTITREGUPD [YSRU]
Gosub HORODAT
Local Inpbox "YSTITREGUPD" From GFONCTION At A_STAMP With A_WINDPREV Mask [YSRU] As [SIM_]
Grizo [YSRU]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [YSRU]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=25
Return

$ON_YTITREGDAT
Onevent BEFORE_AREA Gosub AV_YTITREGDAT From W1YSTITREGUPD
Return

$AV_YTITREGDAT
Chgfmt [M]YTITREGDAT(indice) With "DZ:"+GFMDAT
Gosub _AFF
Return

$ON_YOLDSTA
Onevent INIT_AREA Call D_YOLDSTA(zc) From W1YSTITREGUPD
Onevent BEFORE_INPUT Call AS_YOLDSTA(zc) From W1YSTITREGUPD
Return

Subprog D_YOLDSTA(VALEUR)
Variable Integer VALEUR
If varinit([M]YOLDSTA,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YOLDSTA(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_YUSRREG
Onevent CONTROL Call C_YUSRREG(zc) From W1YSTITREGUPD
Onevent BEFORE_INPUT Call AS_YUSRREG(zc) From W1YSTITREGUPD
Return

Subprog AS_YUSRREG(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(485,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YUSRREG(zc) From W1YSTITREGUPD
Onkey   2 Titled GBOUTS Call S_YUSRREG(zc) From W1YSTITREGUPD
Onkey 144 Titled GBOUTP Call P_YUSRREG(zc) From W1YSTITREGUPD
Onkey 146 Titled GBOUTA Call SA_YUSRREG(zc) From W1YSTITREGUPD
Gosub _AFF
End

Subprog C_YUSRREG(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("AUS","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "YUSRREG" : GZNUM = 3976 : Endif
Gosub _AFF
End

Subprog S_YUSRREG(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "AUS"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_YUSRREG(VALEUR)
Variable Char    VALEUR()
Call OBJET("AUS",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_YUSRREG(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AUS"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YUSRREG(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AUS",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$CHG_STY
Return


