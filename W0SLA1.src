# Supervisor
# Mask management SLA1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone DOCNAM
mkstat = 0
zoncou = "DOCNAM"
indice = 0
If varmode([M]DOCNAM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DOCNAM" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ARP","!A",[M]DOCNAM,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "DOCNAM" : GZNUM = 5169 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone DOCFLG
mkstat = 0
zoncou = "DOCFLG"
indice = 0
IMPENT = [M]DOCFLG
If varinit([M]DOCFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]DOCFLG,indice)=0
 [M]DOCFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DOCFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DOCFLG(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DOCFLG" : GZNUM = 6364 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]DOCFLG = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone TRSFAM
mkstat = 0
zoncou = "TRSFAM"
indice = 0
If varinit([M]TRSFAM,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TRSFAM,"9","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]TRSFAM,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "TRSFAM" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]TRSFAM,"9","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "TRSFAM" : GZNUM = 53175 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!",[M]TRSFAM,"9",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "TRSFAM" : GZNUM = 53175 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone WRHCOD
mkstat = 0
zoncou = "WRHCOD"
indice = 0
IMPENT = [M]WRHCOD
If varinit([M]WRHCOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]WRHCOD,indice)=0
 [M]WRHCOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "WRHCOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]WRHCOD(indice)
 Endif
Endif
[M]WRHCOD = IMPENT

## Zone WRHOBY
mkstat = 0
zoncou = "WRHOBY"
indice = 0
IMPENT = [M]WRHOBY
WNUM = IMPENT
If varinit([M]WRHOBY,indice)=0
 IMPENT = 1
Endif
If varmode([M]WRHOBY,indice)=0
 [M]WRHOBY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "WRHOBY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]WRHOBY(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_WRHOBY(IMPENT) From SUBSLA
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "WRHOBY" : GZNUM = 40968 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]WRHOBY = IMPENT

## Zone INVTYP
mkstat = 0
zoncou = "INVTYP"
indice = 0
IMPENT = [M]INVTYP
If varinit([M]INVTYP,indice)=0
 IMPENT = 1
Endif
[M]INVTYP = IMPENT

## Zone INVCAN
mkstat = 0
zoncou = "INVCAN"
indice = 0
IMPENT = [M]INVCAN
WNUM = IMPENT
If varinit([M]INVCAN,indice)=0
 IMPENT = 1
 SAVACT = GACTION : GACTION = "STD"
 Call D_INVCAN(IMPENT) From SUBSLA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]INVCAN,indice)=0
  [M]INVCAN(indice) = IMPENT
  If !mkstat
   IMPMOD = 0 : IMPZON = "INVCAN" : Gosub ACTION From =IMPTRT
   IMPENT = [M]INVCAN(indice)
  Endif
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_INVCAN(IMPENT) From SUBSLA
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "INVCAN" : GZNUM = 54023 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]INVCAN = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone INVDTACOD
mkstat = 0
zoncou = "INVDTACOD"
indice = 0
IMPENT = [M]INVDTACOD
If varinit([M]INVDTACOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]INVDTACOD,indice)=0
 Call INICOMBO(35) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_INVDTACOD(IMPENT) From SUBSLA
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,35,1))))
 Endif
 [M]INVDTACOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "INVDTACOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]INVDTACOD(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "INVDTACOD" : GZNUM = 13994 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]INVDTACOD = IMPENT

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone TRSCOD
mkstat = 0
zoncou = "TRSCOD"
indice = 0
If varinit([M]TRSCOD,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TRSCOD,"14","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]TRSCOD,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "TRSCOD" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]TRSCOD,"14","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "TRSCOD" : GZNUM = 18946 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!C",[M]TRSCOD,"14",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "TRSCOD" : GZNUM = 18946 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone ENTCOD
mkstat = 0
zoncou = "ENTCOD"
indice = 0
If varmode([M]ENTCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENTCOD" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("GAU","!A",[M]ENTCOD,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "ENTCOD" : GZNUM = 13044 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STKCTRGAU"
  Call STKCTRGAU([M]ENTCOD) From STKACT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ENTCOD" : GZNUM = 13044 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 5
####################################
IMPTBL = 0

## Zone SRGWAIFLG
mkstat = 0
zoncou = "SRGWAIFLG"
indice = 0
IMPENT = [M]SRGWAIFLG
If varinit([M]SRGWAIFLG,indice)=0
 IMPENT = 2
Endif
If varmode([M]SRGWAIFLG,indice)=0
 [M]SRGWAIFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "SRGWAIFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]SRGWAIFLG(indice)
 Endif
Endif
[M]SRGWAIFLG = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SLA1"
Return

