# Supervisor
# Mask management YSLC3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIT
mkstat = 0
zoncou = "NBLIT"
indice = 0
status = 1
zoncou = "NBLIT"
If varmode([M]NBLIT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIT" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIT
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIT = nolign
   status = 75
   Call SAI_NBLIT(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIA
mkstat = 0
zoncou = "NBLIA"
indice = 0
status = 1
zoncou = "NBLIA"
If varmode([M]NBLIA,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIA" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIA
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIA = nolign
   status = 75
   Call SAI_NBLIA(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIT(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIT
mkstat = 0
zoncou = "NBLIT"

## Zone YSALROL
mkstat = 0
zoncou = "YSALROL"
If varinit([M]YSALROL,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]YSALROL(indice),"6007","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]YSALROL,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "YSALROL" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]YSALROL(indice),2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "YSALROL" : GZNUM = 100536 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTADI"
   Call CONTADI([M]YSALROL(indice),"6007","") From SUBADI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "YSALROL" : GZNUM = 100536 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("ADI","",[M]YSALROL(indice),"6007",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "YSALROL" : GZNUM = 100536 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone YTOTPAY
mkstat = 0
zoncou = "YTOTPAY"
If varmode([M]YTOTPAY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YTOTPAY" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YTOTPAY(indice)=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YTOTPAY" : GZNUM = 100538 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone NBLIT
mkstat = 0
zoncou = "NBLIT"

End

