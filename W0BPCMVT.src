#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management BPCMVT 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone TOUSOC
mkstat = 0
zoncou = "TOUSOC"
indice = 0
IMPENT = [M]TOUSOC
WNUM = IMPENT
If varinit([M]TOUSOC,indice)=0
 IMPENT = 1
Endif
If varmode([M]TOUSOC,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_TOUSOC(IMPENT) From FUNBPCMVT
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]TOUSOC(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TOUSOC" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TOUSOC(indice)
 Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_TOUSOC(IMPENT) From FUNBPCMVT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "TOUSOC" : GZNUM = 6052 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]TOUSOC = IMPENT

## Zone SOC
mkstat = 0
zoncou = "SOC"
indice = 0
WCHAR = [M]SOC
If varmode([M]SOC,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_SOC([M]SOC) From FUNBPCMVT
 GACTION = SAVACT
 If mkstat=1 : Raz [M]SOC : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "INICPY"
  Call INICPY([M]SOC) From EXEFNC
  GACTION = SAVACT
  If mkstat=1 : Raz [M]SOC : Endif
  If !mkstat
   IMPMOD = 0 : IMPZON = "SOC" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTCPY"
   Call CONTSOC([M]SOC) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "SOC" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("CPY","!",[M]SOC,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "SOC" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTCPY"
     Call CONTCPY([M]SOC) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "SOC" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      If WCHAR<>[M]SOC or IMPMOD
       SAVACT = GACTION : GACTION = "SETSOC"
       GACTION = SAVACT
       GSOCIETE = [M]SOC
       If mkstat : GERR = 1 : GZONE = "SOC" : GZNUM = 1343 : Endif
       Gosub AFF From GSAISIE
       If mkstat : End : Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone TOUCLIENT
mkstat = 0
zoncou = "TOUCLIENT"
indice = 0
IMPENT = [M]TOUCLIENT
WNUM = IMPENT
If varinit([M]TOUCLIENT,indice)=0
 IMPENT = 1
Endif
If varmode([M]TOUCLIENT,indice)=0
 [M]TOUCLIENT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TOUCLIENT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TOUCLIENT(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_TOUCLIENT(IMPENT) From FUNBPCMVT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "TOUCLIENT" : GZNUM = 15112 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]TOUCLIENT = IMPENT

## Zone CLIENT
mkstat = 0
zoncou = "CLIENT"
indice = 0
If varmode([M]CLIENT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CLIENT([M]CLIENT) From FUNBPCMVT
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CLIENT : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "CLIENT" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  Call CONTROLE("BPC","!",[M]CLIENT,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CLIENT" : GZNUM = 3662 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone TRACE
mkstat = 0
zoncou = "TRACE"
indice = 0
IMPENT = [M]TRACE
If varinit([M]TRACE,indice)=0
 IMPENT = 1
Endif
If varmode([M]TRACE,indice)=0
 [M]TRACE(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TRACE" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TRACE(indice)
 Endif
Endif
[M]TRACE = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "BPCMVT"
Return

