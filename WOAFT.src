Endif
Return

$REMP_DERLU
If clalev([F:AFT9])>0
 Filter [AFT9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AFT1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AFT9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AFT]PRFCOD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AFT9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AFT9]
 Endif
Endif
Return

