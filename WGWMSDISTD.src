$BOUTON
STDBOUT = "RC-+D"
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "b" : LBOUT(NBTEX) = 1801 : Call TEXTE(8035,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "g" : LBOUT(NBTEX) = 1802 : Call TEXTE(12708,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "b"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$(2)
  PARBOUT(2) = [M]FCY
  PARBOUT(3) = ""
  PARBOUT(4) = [M]ITMREF
  PARBOUT(5) = ""
  PARBOUT(6) = ""
  PARBOUT(7) = num$("")
  PARBOUT(8) = ""
  PARBOUT(9) = num$(1)
  PARBOUT(10) = num$("")
  PARBOUT(11) = ""
  PARBOUT(12) = num$("")
  PARBOUT(13) = ""
  PARBOUT(14) = ""
  PARBOUT(15) = ""
  FLGEXE = 1
  GACTION = "CONSORD"
  Call CONSULT_CHAR(VALBOUT,"ORD",PARBOUT) From GCONSULT
 When "g"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "FGRAPHSDI"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IGRAPHSDI","CNSSDISTD","YCNSSDI") From GSAISIE
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


