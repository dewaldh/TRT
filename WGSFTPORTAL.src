$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "L"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M]EMPNUM)
  PARBOUT(2) = [M]FCY
  FLGEXE = 1
  GACTION = "TRTSFTL"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"SFTL","TRTSFTL","") From GSAISIE
 When "B"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M]EMPNUM)
  PARBOUT(2) = [M]FCY
  PARBOUT(3) = "BRK"
  FLGEXE = 1
  GACTION = "TRTSFTBRK"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"SFTBREAK","TRTSFTB","XYTRTSFTB") From GSAISIE
 When "I"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M]EMPNUM)
  PARBOUT(2) = [M]FCY
  PARBOUT(3) = "IND"
  FLGEXE = 1
  GACTION = "TRTSFTBRK"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"SFTBREAK","TRTSFTB","XYTRTSFTB") From GSAISIE
 When "T"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M]EMPNUM)
  PARBOUT(2) = [M]FCY
  FLGEXE = 1
  GACTION = "TRTSFTEL"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"SFTEL","TRTSFTEL","") From GSAISIE
 When "D"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M]EMPNUM)
  PARBOUT(2) = "SFTPORTAL"
  FLGEXE = 1
  GACTION = "TRTSFTDEF"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"SFTDEFAULT","TRTSFTD","XYTRTSFTD") From GSAISIE
 When "A"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M]EMPNUM)
  FLGEXE = 1
  GACTION = "TRTSFTAT"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"SFTADJTEAM","TRTSFTAT","XYTRTSFTAT") From GSAISIE
Endcase
GACTION = SAVACT
Return

