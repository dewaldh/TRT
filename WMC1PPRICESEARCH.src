 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_CLE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.CLE
 Gosub PROPAGATE_CLE
Endif
Return

$INIT_CLE
Gosub DECLARE_VAR From WMC0PPRICESEARCH
[L]CURPRO = "CLE"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
