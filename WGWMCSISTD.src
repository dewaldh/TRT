Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "CONSCSI1"     : ABMSK(0)  = "CSI1"     : AMA(0) = 1
NOMMSK(1) = "WMCSISTD"     : ABMSK(1)  = "CSI2"     : AMA(1) = 1
NBMASK = 1
NBMSKCMP = 0
