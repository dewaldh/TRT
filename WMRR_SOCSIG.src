$TEST
Local Integer SILENCE
Call OUVRE_TRACE("Test program")From LECFIC

Gosub ATRT_TEST

Call LEC_TRACE From LECFIC
Call SUPP_TRACE From LECFIC


Return

#---------------------------------------------------------------#
$ATRT_TEST
Local Integer ASTATUS
If !SILENCE : Call TRACE("1 --> Open main table" ) From ASYRCOM : Endif
Local File AWRKHISSUI [AWS_]

If !SILENCE : Call TRACE("2 --> Local Instance R_SOCSIG") From ASYRCOM : Endif
Local Instance SOCSIG Using R_SOCSIG

If !SILENCE : Call TRACE("3 --> New Instance R_SOCSIG") From ASYRCOM : Endif
SOCSIG = NewInstance R_SOCSIG AllocGroup null

If !SILENCE : Call TRACE("4 --> Reading main table, Read First") From ASYRCOM : Endif
Read [AWS_]AWS2 First
If !fstat
 If !SILENCE : Call TRACE("Call program read of TCAWRKHISSUI") From ASYRCOM : Endif
 [L]ASTATUS = max([L]ASTATUS,fmet SOCSIG.AREAD([AWS_]DEST,[AWS_]CHRONO))
Endif

If !SILENCE
 Call TRACE("5 --> Check :"-num$([L]ASTATUS)) From ASYRCOM
 Call TRACE("SOCSIG.AWS.DEST ="-num$(SOCSIG.AWS.DEST)) From ASYRCOM
 Call TRACE("SOCSIG.AWS.CHRONO ="-num$(SOCSIG.AWS.CHRONO)) From ASYRCOM
 Call TRACE("6 --> List of keys :") From ASYRCOM
 For I=1 To maxtab(SOCSIG.AWS.AKEYS)
  Call TRACE("-"-num$(I)-":"-SOCSIG.AWS.AKEYS(I)) From ASYRCOM
 Next
Endif
Close Local File [AWS_]
FreeGroup SOCSIG
Return

############################################################################################
### Funprog _AREADETAG
############################################################################################
Funprog _AREADETAG(SOCSIG,ACLE,ASTATUS,_ETAG)
Variable Instance SOCSIG Using R_SOCSIG
Value Char ACLE
Variable Integer ASTATUS
Variable Char _ETAG
[L]ASTATUS = [V]CST_AOK
Local Char _DEST(5)
Local Char _CHRONO(20)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_DEST = num$([L]CLE(1)) : Endif
If [L]CLE(2)<>"" : [L]_CHRONO = num$([L]CLE(2)) : Endif
Raz [L]_ETAG
Local File AWRKHISSUI [AWS]
Default File [F:AWS]
Read [F:AWS]AWS2 = [L]_DEST;[L]_CHRONO
If fstat
 [L]ASTATUS = [V]CST_AERROR
Endif
If [L]ASTATUS=[V]CST_AOK and dim([F:AWS]UPDTICK)>0
 [L]_ETAG = num$([F:AWS]UPDTICK)
Endif
End [L]ASTATUS

############################################################################################
### Subprog _AREADSUP
############################################################################################
Subprog _AREADSUP(SOCSIG,ACLE,ASTATUS)
Variable Instance SOCSIG Using R_SOCSIG
Value Char ACLE
Variable Integer ASTATUS
Local Char _DEST(5)
Local Char _CHRONO(20)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_DEST = num$([L]CLE(1)) : Endif
If [L]CLE(2)<>"" : [L]_CHRONO = num$([L]CLE(2)) : Endif
[L]ASTATUS = max([L]ASTATUS,fmet SOCSIG.AREAD([L]_DEST,[L]_CHRONO))
End

############################################################################################
### Subprog AREAD
############################################################################################
Subprog AREAD(SOCSIG,_DEST,_CHRONO,ASTATUS)
Variable Instance SOCSIG Using R_SOCSIG
Value Char _DEST
Value Char _CHRONO
Variable Integer ASTATUS

If SOCSIG = null : End : Endif

Gosub AVARIABLES From WMR00SOCSIG
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If SOCSIG.APARENT=null
 [L]A_I = SOCSIG.freeSnapshot
 SOCSIG.snapshotEnabled = 0
 SOCSIG.setAccessorEnabled = 0
Endif

If fmet SOCSIG.ACTX.AGETAFCRIGHT(SOCSIG,"GESSOR",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END : Endif
Default File [F:AWS]
Read [F:AWS]AWS2 = [L]_DEST;[L]_CHRONO
If fstat
[L]A_ERROR = [L]_DEST+"~"+[L]_CHRONO
& -":"-mess(23,100,1)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(SOCSIG,"",0,[L]ASTATUS,[V]CST_AHTTP_NOT_FOUND,[L]A_ERROR) : Goto AREAD_END
Endif
[L]A_STRSITE = ""
[L]A_STRACC = ""
If fmet SOCSIG.ACTX.AGETAFCRIGHTR(SOCSIG,"GESSOR",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END
Endif
If fmet SOCSIG.ACTX.ASTACRITROLE(SOCSIG,[L]_DEST+"~"+[L]_CHRONO
& ,"SOR","AWS",[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END 
Endif

[L]ASTATUS = max([L]ASTATUS,fmet SOCSIG.ACTION("AREAD_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

SOCSIG.AWS.setAccessorEnabled = 0
[L]ASTATUS = max([L]ASTATUS,fmet SOCSIG.AWS.AREAD([L]_DEST,[L]_CHRONO))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

[L]ASTATUS = max([L]ASTATUS,fmet SOCSIG.ACTION("AREAD_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If [L]ASTATUS<[V]CST_AERROR & SOCSIG.APARENT=null
 # Load Link
 [L]ASTATUS = max([L]ASTATUS,fmet SOCSIG._ALNKPRO)
 If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
Endif

$AREAD_END
If SOCSIG.APARENT=null
 SOCSIG.snapshotEnabled = 1
 SOCSIG.setAccessorEnabled = 1
Endif
LogicClose File [AWS]
End



