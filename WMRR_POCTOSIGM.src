############################################################################################
# Program TEST
############################################################################################
$TEST
Local Integer SILENCE
Call OUVRE_TRACE("Test program")From LECFIC

Gosub ATRT_TEST

Call LEC_TRACE From LECFIC
Call SUPP_TRACE From LECFIC


Return

#---------------------------------------------------------------#
$ATRT_TEST
Local Integer ASTATUS
If !SILENCE : Call TRACE("1 --> Open main table" ) From ASYRCOM : Endif
Local File AWRKHISSUI [AWS_]

If !SILENCE : Call TRACE("2 --> Local Instance R_POCTOSIGM") From ASYRCOM : Endif
Local Instance POCTOSIGM Using R_POCTOSIGM

If !SILENCE : Call TRACE("3 --> New Instance R_POCTOSIGM") From ASYRCOM : Endif
POCTOSIGM = NewInstance R_POCTOSIGM AllocGroup null

If !SILENCE : Call TRACE("4 --> Reading main table, Read First") From ASYRCOM : Endif
Read [AWS_]AWS2 First
If !fstat
 If !SILENCE : Call TRACE("Call program read of TCAWRKHISSUI") From ASYRCOM : Endif
 [L]ASTATUS = max([L]ASTATUS,fmet POCTOSIGM.AREAD([AWS_]DEST,[AWS_]CHRONO))
Endif

If !SILENCE
 Call TRACE("5 --> Check :"-num$([L]ASTATUS)) From ASYRCOM
 Call TRACE("POCTOSIGM.AWS.DEST ="-num$(POCTOSIGM.AWS.DEST)) From ASYRCOM
 Call TRACE("POCTOSIGM.AWS.CHRONO ="-num$(POCTOSIGM.AWS.CHRONO)) From ASYRCOM
 Call TRACE("6 --> List of keys :") From ASYRCOM
 For I=1 To maxtab(POCTOSIGM.AWS.AKEYS)
  Call TRACE("-"-num$(I)-":"-POCTOSIGM.AWS.AKEYS(I)) From ASYRCOM
 Next
Endif
Close Local File [AWS_]
FreeGroup POCTOSIGM
Return

############################################################################################
### Funprog _AREADETAG
############################################################################################
Funprog _AREADETAG(POCTOSIGM,ACLE,ASTATUS,_ETAG)
Variable Instance POCTOSIGM Using R_POCTOSIGM
Value Char ACLE
Variable Integer ASTATUS
Variable Char _ETAG
[L]ASTATUS = [V]CST_AOK
Local Char _DEST(5)
Local Char _CHRONO(20)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_DEST = num$([L]CLE(1)) : Endif
If [L]CLE(2)<>"" : [L]_CHRONO = num$([L]CLE(2)) : Endif
Raz [L]_ETAG
Local File AWRKHISSUI [AWS]
Default File [F:AWS]
Read [F:AWS]AWS2 = [L]_DEST;[L]_CHRONO
If fstat
 [L]ASTATUS = [V]CST_AERROR
Endif
If [L]ASTATUS=[V]CST_AOK and dim([F:AWS]UPDTICK)>0
 [L]_ETAG = num$([F:AWS]UPDTICK)
Endif
End [L]ASTATUS

############################################################################################
### Subprog _AREADSUP
############################################################################################
Subprog _AREADSUP(POCTOSIGM,ACLE,ASTATUS)
Variable Instance POCTOSIGM Using R_POCTOSIGM
Value Char ACLE
Variable Integer ASTATUS
Local Char _DEST(5)
Local Char _CHRONO(20)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_DEST = num$([L]CLE(1)) : Endif
If [L]CLE(2)<>"" : [L]_CHRONO = num$([L]CLE(2)) : Endif
[L]ASTATUS = max([L]ASTATUS,fmet POCTOSIGM.AREAD([L]_DEST,[L]_CHRONO))
End

############################################################################################
### Subprog AREAD
############################################################################################
Subprog AREAD(POCTOSIGM,_DEST,_CHRONO,ASTATUS)
Variable Instance POCTOSIGM Using R_POCTOSIGM
Value Char _DEST
Value Char _CHRONO
Variable Integer ASTATUS

If POCTOSIGM = null : End : Endif

Gosub AVARIABLES From WMR00POCTOSIGM
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If POCTOSIGM.APARENT=null
 [L]A_I = POCTOSIGM.freeSnapshot
 POCTOSIGM.snapshotEnabled = 0
 POCTOSIGM.setAccessorEnabled = 0
Endif

If fmet POCTOSIGM.ACTX.AGETAFCRIGHT(POCTOSIGM,"SYRWRKPLN",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END : Endif
Default File [F:AWS]
Read [F:AWS]AWS2 = [L]_DEST;[L]_CHRONO
If fstat
[L]A_ERROR = [L]_DEST+"~"+[L]_CHRONO
& -":"-mess(23,100,1)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(POCTOSIGM,"",0,[L]ASTATUS,[V]CST_AHTTP_NOT_FOUND,[L]A_ERROR) : Goto AREAD_END
Endif
[L]A_STRSITE = ""
[L]A_STRACC = ""
If fmet POCTOSIGM.ACTX.AGETAFCRIGHTR(POCTOSIGM,"SYRWRKPLN",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END
Endif

[L]ASTATUS = max([L]ASTATUS,fmet POCTOSIGM.ACTION("AREAD_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

POCTOSIGM.AWS.setAccessorEnabled = 0
[L]ASTATUS = max([L]ASTATUS,fmet POCTOSIGM.AWS.AREAD([L]_DEST,[L]_CHRONO))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

[L]ASTATUS = max([L]ASTATUS,fmet POCTOSIGM.ACTION("AREAD_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If [L]ASTATUS<[V]CST_AERROR & POCTOSIGM.APARENT=null
 # Load Link
 [L]ASTATUS = max([L]ASTATUS,fmet POCTOSIGM._ALNKPRO)
 If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
Endif

$AREAD_END
If POCTOSIGM.APARENT=null
 POCTOSIGM.snapshotEnabled = 1
 POCTOSIGM.setAccessorEnabled = 1
Endif
LogicClose File [AWS]
End



