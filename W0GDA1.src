  IMPMOD = 0 : IMPZON = "COA" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]COA(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "COA" : GZNUM = 29357 : Endif
