 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("COA","",[M]COA(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "COA" : GZNUM = 29357 : Endif
