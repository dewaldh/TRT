  If !mkstat
   If WCHAR<>[M]COA(indice) or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_COA([M]COA(indice)) From SUBGDA
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "COA" : GZNUM = 29357 : Endif
