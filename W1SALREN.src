# Supervisor
# Mask management SALREN 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:11:12"
A_USER  = "ADMIN"
A_STAMP = "20180828001112"
Return

$TITRES
Chgtzn [M:SREN]DISCRGCOD1 With GCOLREM(0)
Chgtzn [M:SREN]DISCRGCOD2 With GCOLREM(1)
Chgtzn [M:SREN]DISCRGCOD3 With GCOLREM(2)
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask SALREN [SREN]
Gosub HORODAT
Local Inpbox "SALREN" From GFONCTION At A_STAMP With A_WINDPREV Mask [SREN] As [SIM_]
Grizo [SREN]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [SREN]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=37 : X2(0)=75 : Y1(0)=9 : Y2(0)=19
Return

$ON_GROPRICOD
Onevent CONTROL Call C_GROPRICOD(zc) From W1SALREN
Onevent AFTER_CHANGE Gosub AMM_GROPRICOD From W1SALREN
Onevent BEFORE_INPUT Call AS_GROPRICOD(zc) From W1SALREN
Return

Subprog AS_GROPRICOD(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_GROPRICOD(zc) From W1SALREN
Gosub _AFF
End

Subprog C_GROPRICOD(VALEUR)
Variable Decimal VALEUR
## Action CTLREN
Local Char    PARMSK(250)(1..20) , SAVACT(20)
SAVACT = GACTION : GACTION = "CTLREN"
PARMSK(1) = "5"
PARMSK(2) = num$(GROPRICOD)
PARMSK(3) = num$(GROPRI)
Call S_AFF_NUM(VALEUR,PARMSK,"CTLREN","TRTPRICE","") From GACTION
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "GROPRICOD" : GZNUM = 2541 : Endif
Gosub _AFF
End

$AMM_GROPRICOD
Call AM_GROPRICOD(zc)
If !mkstat & GOBJET
 zoncou = "GROPRICOD" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_GROPRICOD(VALEUR)
Variable Decimal VALEUR
## Action ALIREN
Local Char    PARMSK(250)(1..20) , SAVACT(20)
SAVACT = GACTION : GACTION = "ALIREN"
PARMSK(1) = "5"
PARMSK(2) = [M]GROPRIDES
Call S_AFF_NUM(VALEUR,PARMSK,"ALIREN","TRTPRICE","") From GACTION
[M]GROPRIDES = PARMSK(2)
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "GROPRICOD" : GZNUM = 2541 : Endif
Gosub _AFF
End

Subprog S_GROPRICOD(VALEUR)
Variable Decimal VALEUR
If !GWEBSERV
 ## Action SELREN
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELREN"
 PARMSK(1) = "5"
 Call S_TAB_NUM(VALEUR,PARMSK,"SELREN","TRTPRICE","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

