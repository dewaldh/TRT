Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "PRX0"         : ABMSK(0)  = "PRX0"     : AMA(0) = 1
NOMMSK(1) = "PRX1"         : ABMSK(1)  = "PRX1"     : AMA(1) = 1
NOMMSK(2) = "PRX2"         : ABMSK(2)  = "PRX2"     : AMA(2) = 1
NOMMSK(3) = "PRX3"         : ABMSK(3)  = "PRX3"     : AMA(3) = 1
