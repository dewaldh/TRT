# Supervisor
# Mask management FGR1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBRCPY
mkstat = 0
zoncou = "NBRCPY"
indice = 0
status = 1
zoncou = "NBRCPY"
If varmode([M]NBRCPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRCPY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRCPY
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRCPY = nolign
   status = 75
   Call SAI_NBRCPY(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBRFCY
mkstat = 0
zoncou = "NBRFCY"
indice = 0
status = 1
zoncou = "NBRFCY"
If varmode([M]NBRFCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRFCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRFCY
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRFCY = nolign
   status = 75
   Call SAI_NBRFCY(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBRCPY(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBRCPY
mkstat = 0
zoncou = "NBRCPY"

## Zone CPY
mkstat = 0
zoncou = "CPY"
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CPY(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTCPY"
  Call CONTSOC([M]CPY(indice)) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("CPY","A",[M]CPY(indice),"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_CPY([M]CPY(indice)) From SUBAGF
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone NBRCPY
mkstat = 0
zoncou = "NBRCPY"

End

####################################
## Bloc tableau 2
####################################
