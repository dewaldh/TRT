######################################################################################################
$ACTION
Case ACTION
  When "IMP_VERIF_CRE" : Gosub IMP_VERIF_CRE
  When Default
Endcase

Return

# Verification after Creation
$IMP_VERIF_CRE

  If [F:YBBH]YBATNBR = "" Then
    Local Char NEW_YBUSBAT(30)
    Local Integer STA, I
    Call NUMERO("YBBHS","",date$,"null",NEW_YBUSBAT,STA) From SUBANM
    [F:YBBH]YBATNBR = NEW_YBUSBAT
    Write [F:YBBH]
    Infbox "1"
  Endif
  For I = 1 To [F:YBBD]YDETLIG
  Infbox [F:YBBD]YFCY
    If [F:YBBD]YBATNBR = ""
      [F:YBBD]YBATNBR = NEW_YBUSBAT
      Write [F:YBBD]
    Endif
  Next
#

Return

