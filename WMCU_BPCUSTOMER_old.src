############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(BPCUSTOMER,ASTATUS)
Variable Instance BPCUSTOMER Using C_BPCUSTOMER
Variable Integer ASTATUS

If BPCUSTOMER = null : End : Endif

Gosub _AVARIABLES From WMC00BPCUSTOMER

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_BPCUSTOMER

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(BPCUSTOMER,ASTATUS)
Variable Instance BPCUSTOMER Using C_BPCUSTOMER
Variable Integer ASTATUS

If BPCUSTOMER = null : End : Endif

Gosub AVARIABLES From WMC00BPCUSTOMER
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
BPCUSTOMER.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_BPCUSTOMER
Else
 Gosub AUPDATE_BPCUSTOMER
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = BPCUSTOMER.freeSnapshot
BPCUSTOMER.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If BPCUSTOMER.ASTALIN = [V]CST_ANEWDEL : Return : Endif
BPCUSTOMER.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(BPCUSTOMER.ASTALIN)<>""
 [L]A_ASTALIN = BPCUSTOMER.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_DAYMON)
 If BPCUSTOMER.C_DAYMON([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_DAYMON([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_DAYMON([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_DIE)
 If BPCUSTOMER.C_DIE([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_DIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_DIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_INVDTAAMT)
 If BPCUSTOMER.C_INVDTAAMT([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_INVDTAAMT([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_INVDTAAMT([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_REP)
 If BPCUSTOMER.C_REP([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_REP([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_REP([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_TSCCOD)
 If BPCUSTOMER.C_TSCCOD([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_TSCCOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_TSCCOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If BPCUSTOMER.BPR(A_N2) <> null
& and BPCUSTOMER.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If BPCUSTOMER.BPR(A_N2).ASTALIN<>[V]CST_ADEL & BPCUSTOMER.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(BPCUSTOMER,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPARTNER:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BPCUSTOMER.BPD) : # BPDLVCUST -- 3
 If BPCUSTOMER.BPD(A_N2) <> null
& and BPCUSTOMER.BPD(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
A_N2=1
If BPCUSTOMER.BPR(A_N2) <> null
& and BPCUSTOMER.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If BPCUSTOMER.BPR(A_N2).ASTALIN<>[V]CST_ADEL & BPCUSTOMER.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(BPCUSTOMER,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPARTNER:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BPCUSTOMER.BPD) : # BPDLVCUST -- 3
 If BPCUSTOMER.BPD(A_N2) <> null
& and BPCUSTOMER.BPD(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(BPCUSTOMER.C_DAYMON)
 If BPCUSTOMER.C_DAYMON([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_DAYMON([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_DAYMON([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_DIE)
 If BPCUSTOMER.C_DIE([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_DIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_DIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_INVDTAAMT)
 If BPCUSTOMER.C_INVDTAAMT([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_INVDTAAMT([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_INVDTAAMT([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_REP)
 If BPCUSTOMER.C_REP([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_REP([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_REP([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_TSCCOD)
 If BPCUSTOMER.C_TSCCOD([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMER.C_TSCCOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.C_TSCCOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(BPCUSTOMER.ASTALIN)<>""
 [L]A_ASTALIN = BPCUSTOMER.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_BPCUSTOMER
Trbegin [F:BPC]
 Gosub AUPDATE_BPCUSTOMER
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_BPCUSTOMER
[L]A_ASTALIN = BPCUSTOMER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If find( BPCUSTOMER.ASTALIN,[V]CST_ANEW,[V]CST_ADEL) | 
& (find( BPCUSTOMER.ASTALIN,[V]CST_AUPD) & 
& fmet BPCUSTOMER.AGETATTRIBUTE("CLAORDTEX1","$isLoaded")="true")
[L]ASTATUS = func WMC00BPCUSTOMER._MAJSQL_BPCUSTOMER_CLAORDTEX1(BPCUSTOMER,BPCUSTOMER.CLAORDTEX1)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
If find( BPCUSTOMER.ASTALIN,[V]CST_ANEW,[V]CST_ADEL) | 
& (find( BPCUSTOMER.ASTALIN,[V]CST_AUPD) & 
& fmet BPCUSTOMER.AGETATTRIBUTE("CLAINVTEX1","$isLoaded")="true")
[L]ASTATUS = func WMC00BPCUSTOMER._MAJSQL_BPCUSTOMER_CLAINVTEX1(BPCUSTOMER,BPCUSTOMER.CLAINVTEX1)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
[L]ASTATUS = max([L]ASTATUS,func WMC00BPCUSTOMER._MAJSQL_BPCUSTOMER(BPCUSTOMER))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 A_N2=1
 If BPCUSTOMER.BPR(A_N2) <> null and 
& BPCUSTOMER.BPR(A_N2).ASTALIN=[V]CST_ALL
 BPCUSTOMER.BPR(A_N2).ASTALIN = [V]CST_AUPD
 Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(BPCUSTOMER.BPD)
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BPCUSTOMER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If BPCUSTOMER.snapshotEnabled=1
 [L]A_RETIK = BPCUSTOMER.revertUpdTick
Endif
Local Integer ASTATUS2
A_N2=1
If BPCUSTOMER.BPR(A_N2) <> null
 [L]ASTATUS2 = fmet BPCUSTOMER.BPR(A_N2)._AUPDATE_ABORT
 If BPCUSTOMER.BPR(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance BPCUSTOMER.BPR(A_N2)
  BPCUSTOMER.BPR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BPCUSTOMER.BPD)
 If BPCUSTOMER.BPD(A_N2) <> null
  [L]ASTATUS2 = fmet BPCUSTOMER.BPD(A_N2)._AUPDATE_ABORT
  If BPCUSTOMER.BPD(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPD(A_N2)
   BPCUSTOMER.BPD(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet BPCUSTOMER.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(BPCUSTOMER.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 BPCUSTOMER.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_DAYMON)
 If BPCUSTOMER.C_DAYMON([L]A_I)<>null
  If BPCUSTOMER.C_DAYMON([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.C_DAYMON([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.C_DAYMON([L]A_I)
   BPCUSTOMER.C_DAYMON([L]A_I) = null
  Elsif BPCUSTOMER.C_DAYMON([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.C_DAYMON([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_DIE)
 If BPCUSTOMER.C_DIE([L]A_I)<>null
  If BPCUSTOMER.C_DIE([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.C_DIE([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.C_DIE([L]A_I)
   BPCUSTOMER.C_DIE([L]A_I) = null
  Elsif BPCUSTOMER.C_DIE([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.C_DIE([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_INVDTAAMT)
 If BPCUSTOMER.C_INVDTAAMT([L]A_I)<>null
  If BPCUSTOMER.C_INVDTAAMT([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.C_INVDTAAMT([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.C_INVDTAAMT([L]A_I)
   BPCUSTOMER.C_INVDTAAMT([L]A_I) = null
  Elsif BPCUSTOMER.C_INVDTAAMT([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.C_INVDTAAMT([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_REP)
 If BPCUSTOMER.C_REP([L]A_I)<>null
  If BPCUSTOMER.C_REP([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.C_REP([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.C_REP([L]A_I)
   BPCUSTOMER.C_REP([L]A_I) = null
  Elsif BPCUSTOMER.C_REP([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.C_REP([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER.C_TSCCOD)
 If BPCUSTOMER.C_TSCCOD([L]A_I)<>null
  If BPCUSTOMER.C_TSCCOD([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER.C_TSCCOD([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.C_TSCCOD([L]A_I)
   BPCUSTOMER.C_TSCCOD([L]A_I) = null
  Elsif BPCUSTOMER.C_TSCCOD([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER.C_TSCCOD([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
A_N2=1
If BPCUSTOMER.BPR(A_N2) <> null
 [L]ASTATUS2 = fmet BPCUSTOMER.BPR(A_N2)._AUPDATE_AFTER
 If BPCUSTOMER.BPR(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance BPCUSTOMER.BPR(A_N2)
  BPCUSTOMER.BPR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BPCUSTOMER.BPD)
 If BPCUSTOMER.BPD(A_N2) <> null
  [L]ASTATUS2 = fmet BPCUSTOMER.BPD(A_N2)._AUPDATE_AFTER
  If BPCUSTOMER.BPD(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER.BPD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER.BPD(A_N2)
   BPCUSTOMER.BPD(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



