# Supervisor
# Mask management MGD1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:05:42"
A_USER  = "ADMIN"
A_STAMP = "20180828000542"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBQUR"
  If [M]NBQUR=0
  Endif
 When "NBDES"
  If [M]NBDES=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask MGD1 [MGD1]
Gosub HORODAT
Local Inpbox "MGD1" From GFONCTION At A_STAMP With A_WINDPREV Mask [MGD1] As [SIM_]
Grizo [MGD1]5
Grizo [MGD1]10
Grizo [MGD1]25
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [MGD1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=112 : Y1(0)=1 : Y2(0)=3
X1(1)=1 : X2(1)=112 : Y1(1)=4 : Y2(1)=16
X1(2)=1 : X2(2)=112 : Y1(2)=17 : Y2(2)=28
Return

$ON_TGRFLT
Onevent CONTROL Call C_TGRFLT(zc) From W1MGD1
Onevent AFTER_CHANGE Gosub AMM_TGRFLT From W1MGD1
Onevent BEFORE_INPUT Call AS_TGRFLT(zc) From W1MGD1
Return

Subprog AS_TGRFLT(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(18660,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_TGRFLT(zc) From W1MGD1
Onkey   2 Titled GBOUTS Call S_TGRFLT(zc) From W1MGD1
Onkey 144 Titled GBOUTP Call P_TGRFLT(zc) From W1MGD1
Onkey 146 Titled GBOUTA Call SA_TGRFLT(zc) From W1MGD1
Gosub _AFF
End

Subprog C_TGRFLT(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("TGL","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "TGRFLT" : GZNUM = 18315 : Endif
If !mkstat
 ## Action STD
 Call C_TGRFLT(VALEUR) From SUBMGD1
 If mkstat : GERR = 1 : GZONE = "TGRFLT" : GZNUM = 18315 : Endif
Endif
Gosub _AFF
End

$AMM_TGRFLT
Call AM_TGRFLT(zc)
If !mkstat & GOBJET
 zoncou = "TGRFLT" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_TGRFLT(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_TGRFLT(VALEUR) From SUBMGD1
If mkstat : GERR = 1 : GZONE = "TGRFLT" : GZNUM = 18315 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZTGRFLT)")>indice
 [M]TGRFLT(indice) = VALEUR
 Affzo ZTGRFLT(indice)
Endif
Gosub _AFF
End

Subprog S_TGRFLT(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELTGL3TGL
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELTGL3TGL"
 Call S_LIS_CHAR(VALEUR,PARMSK,"SELTGL3TGL","TRTCRMSEL","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_TGRFLT(VALEUR)
Variable Char    VALEUR()
Call OBJET("TGL",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_TGRFLT(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TGL"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_TGRFLT(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TGL",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

