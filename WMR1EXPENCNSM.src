####################################################################
# 1 : FLGPCT Picture ( Field C 4)
####################################################################
 
$SET_FLGPCT
If [L]_AOLDVAL<>this.FLGPCT
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_FLGPCT
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.FLGPCT
 Gosub PROPAGATE_FLGPCT
Endif
Return

$INIT_FLGPCT
Gosub DECLARE_VAR From WMR0EXPENCNSM
[L]CURPRO = "FLGPCT"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_FLGPCT
Gosub DECLARE_VAR From WMR0EXPENCNSM
[L]CURPRO = "FLGPCT"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_FLGPCT                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_FLGPCT                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_FLGPCT
[L]CURPRO = "FLGPCT"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.FLGPCT=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_FLGPCT
[L]CURPRO = "FLGPCT"
Return

$_AOTHERS_FLGPCT
[L]CURPRO = "FLGPCT"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_FLGPCT
[L]CURPRO = "FLGPCT"
Return

$PROPAGATE_FLGPCT
this.isReadonly = 0
[L]CURPRO = "FLGPCT"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_FLGPCT
Gosub DECLARE_VAR From WMR0EXPENCNSM
[L]CURPRO = "FLGPCT"
$_LNKPRO_FLGPCT
Return




