$TEST
Local Integer SILENCE
Call OUVRE_TRACE("Test program")From LECFIC

Gosub ATRT_TEST

Call LEC_TRACE From LECFIC
Call SUPP_TRACE From LECFIC


Return

#---------------------------------------------------------------#
$ATRT_TEST
Local Integer ASTATUS
If !SILENCE : Call TRACE("1 --> Open main table" ) From ASYRCOM : Endif
Local File TABSIVTYP [TSV_]

If !SILENCE : Call TRACE("2 --> Local Instance R_TABSIVTYP") From ASYRCOM : Endif
Local Instance TABSIVTYP Using R_TABSIVTYP

If !SILENCE : Call TRACE("3 --> New Instance R_TABSIVTYP") From ASYRCOM : Endif
TABSIVTYP = NewInstance R_TABSIVTYP AllocGroup null

If !SILENCE : Call TRACE("4 --> Reading main table, Read First") From ASYRCOM : Endif
Read [TSV_]TSV0 First
If !fstat
 If !SILENCE : Call TRACE("Call program read of TABSIVTYP") From ASYRCOM : Endif
 [L]ASTATUS = max([L]ASTATUS,fmet TABSIVTYP.AREAD([TSV_]SIVTYP,[TSV_]LEG))
Endif

If !SILENCE
 Call TRACE("5 --> Check :"-num$([L]ASTATUS)) From ASYRCOM
 Call TRACE("TABSIVTYP.TSV.SIVTYP ="-num$(TABSIVTYP.TSV.SIVTYP)) From ASYRCOM
 Call TRACE("TABSIVTYP.TSV.LEG ="-num$(TABSIVTYP.TSV.LEG)) From ASYRCOM
 Call TRACE("6 --> List of keys :") From ASYRCOM
 For I=1 To maxtab(TABSIVTYP.TSV.AKEYS)
  Call TRACE("-"-num$(I)-":"-TABSIVTYP.TSV.AKEYS(I)) From ASYRCOM
 Next
Endif
Close Local File [TSV_]
FreeGroup TABSIVTYP
Return

############################################################################################
### Funprog _AREADETAG
############################################################################################
Funprog _AREADETAG(TABSIVTYP,ACLE,ASTATUS,_ETAG)
Variable Instance TABSIVTYP Using R_TABSIVTYP
Value Char ACLE
Variable Integer ASTATUS
Variable Char _ETAG
[L]ASTATUS = [V]CST_AOK
Local Char _SIVTYP(5)
Local Char _LEG(20)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_SIVTYP = num$([L]CLE(1)) : Endif
If [L]CLE(2)<>"" : [L]_LEG = num$([L]CLE(2)) : Endif
Raz [L]_ETAG
Local File TABSIVTYP [TSV]
Default File [F:TSV]
Read [F:TSV]TSV0 = [L]_SIVTYP;[L]_LEG
If fstat
 [L]ASTATUS = [V]CST_AERROR
Endif
If [L]ASTATUS=[V]CST_AOK and dim([F:TSV]UPDTICK)>0
 [L]_ETAG = num$([F:TSV]UPDTICK)
Endif
End [L]ASTATUS

############################################################################################
### Subprog _AREADSUP
############################################################################################
Subprog _AREADSUP(TABSIVTYP,ACLE,ASTATUS)
Variable Instance TABSIVTYP Using R_TABSIVTYP
Value Char ACLE
Variable Integer ASTATUS
Local Char _SIVTYP(5)
Local Char _LEG(20)
Gosub SET_CLE From ASYRSUB
If [L]CLE(1)<>"" : [L]_SIVTYP = num$([L]CLE(1)) : Endif
If [L]CLE(2)<>"" : [L]_LEG = num$([L]CLE(2)) : Endif
[L]ASTATUS = max([L]ASTATUS,fmet TABSIVTYP.AREAD([L]_SIVTYP,[L]_LEG))
End

############################################################################################
### Subprog AREAD
############################################################################################
Subprog AREAD(TABSIVTYP,_SIVTYP,_LEG,ASTATUS)
Variable Instance TABSIVTYP Using R_TABSIVTYP
Value Char _SIVTYP
Value Char _LEG
Variable Integer ASTATUS

If TABSIVTYP = null : End : Endif

Gosub AVARIABLES From WMR00TABSIVTYP
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If TABSIVTYP.APARENT=null
 [L]A_I = TABSIVTYP.freeSnapshot
 TABSIVTYP.snapshotEnabled = 0
 TABSIVTYP.setAccessorEnabled = 0
Endif

If fmet TABSIVTYP.ACTX.AGETAFCRIGHT(TABSIVTYP,"GESTSV",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END : Endif
Default File [F:TSV]
Read [F:TSV]TSV0 = [L]_SIVTYP;[L]_LEG
If fstat
[L]A_ERROR = [L]_SIVTYP+"~"+[L]_LEG
& -":"-mess(23,100,1)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(TABSIVTYP,"",0,[L]ASTATUS,[V]CST_AHTTP_NOT_FOUND,[L]A_ERROR) : Goto AREAD_END
Endif
[L]A_STRSITE = ""
[L]A_STRACC = ""
If fmet TABSIVTYP.ACTX.AGETAFCRIGHTR(TABSIVTYP,"GESTSV",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END
Endif
If fmet TABSIVTYP.ACTX.ASTACRITROLE(TABSIVTYP,[L]_SIVTYP+"~"+[L]_LEG
& ,"TSV","TSV",[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AREAD_END 
Endif

[L]ASTATUS = max([L]ASTATUS,fmet TABSIVTYP.ACTION("AREAD_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

TABSIVTYP.TSV.setAccessorEnabled = 0
[L]ASTATUS = max([L]ASTATUS,fmet TABSIVTYP.TSV.AREAD([L]_SIVTYP,[L]_LEG))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

[L]ASTATUS = max([L]ASTATUS,fmet TABSIVTYP.ACTION("AREAD_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif

If [L]ASTATUS<[V]CST_AERROR & TABSIVTYP.APARENT=null
 # Load Link
 [L]ASTATUS = max([L]ASTATUS,fmet TABSIVTYP._ALNKPRO)
 If [L]ASTATUS>=[V]CST_AERROR : Goto AREAD_END : Endif
Endif

$AREAD_END
If TABSIVTYP.APARENT=null
 TABSIVTYP.snapshotEnabled = 1
 TABSIVTYP.setAccessorEnabled = 1
Endif
LogicClose File [TSV]
End



