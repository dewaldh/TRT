$LEC_DERLU
Read [AFF]AFF0 = [F:OPP]SALFCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Read [OPPPJM]OPPPJM0=[F:OPP]OPPNUM
If fstat : Raz [F:OPPPJM] : Endif
Read [PIM]PIM0=[F:OPP]OPPNUM
If fstat : Raz [F:PIM] : Endif
Read [AX3]AXX0="PIMPL";"PIMDESAXX";GLANGUE;[F:OPP]OPPNUM;""
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:OPP9])>0
 Read [OPP9] = [F:OPP9]A_TABI
 [L]CLE = [F:OPP9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:OPP9])>0
 Filter [OPP9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [PJM1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:OPP9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:OPP]OPPNUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:OPP9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:OPP9]
 Endif
Endif
Return

