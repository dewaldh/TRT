# Supervisor
# Mask management VARPAR 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBPAR
mkstat = 0
zoncou = "NBPAR"
indice = 0
status = 1
zoncou = "NBPAR"
If varmode([M]NBPAR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBPAR" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBPAR
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBPAR = nolign
   status = 75
   Call SAI_NBPAR(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBPAR(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBPAR
mkstat = 0
zoncou = "NBPAR"

## Zone NOPA
mkstat = 0
zoncou = "NOPA"
[M]NOPA(indice) = nolign

## Zone VALPAR
mkstat = 0
zoncou = "VALPAR"
If varmode([M]VALPAR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "VALPAR" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]VALPAR(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "VALPAR" : GZNUM = 122 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call AP_VALPAR([M]VALPAR(indice)) From TRTVARPAR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "VALPAR" : GZNUM = 122 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone NBPAR
mkstat = 0
zoncou = "NBPAR"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "VARPAR"
Return

