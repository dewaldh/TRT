Subprog D_USRFLDA1(VALEUR)
Variable Char    VALEUR()
If varinit([M]USRFLDA1,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,num$(GABLIADI1),"") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_USRFLDA1(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_USRFLDA1(zc) From W1LEAFZ9
Onkey 144 Titled GBOUTP Call P_USRFLDA1(zc) From W1LEAFZ9
Onkey 146 Titled GBOUTA Call SA_USRFLDA1(zc) From W1LEAFZ9
Gosub _AFF
End

Subprog C_USRFLDA1(VALEUR)
Variable Char    VALEUR()
## Action CONTADI
Call CONTADI(VALEUR,num$(GABLIADI1),"") From SUBADI
If mkstat : GERR = 1 : GZONE = "USRFLDA1" : GZNUM = 0 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("ADI","!",VALEUR,num$(GABLIADI1),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "USRFLDA1" : GZNUM = 0 : Endif
Endif
Gosub _AFF
End

$AMM_USRFLDA1
Call AM_USRFLDA1(zc)
If !mkstat & GOBJET
 zoncou = "USRFLDA1" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_USRFLDA1(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_USRFLDA1(VALEUR) From SUBFRZ
If mkstat : GERR = 1 : GZONE = "USRFLDA1" : GZNUM = 0 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZUSRFLDA1)")>indice
 [M]USRFLDA1(indice) = VALEUR
 Affzo ZUSRFLDA1(indice)
Endif
Gosub _AFF
End

Subprog S_USRFLDA1(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = num$(GABLIADI1)
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog SA_USRFLDA1(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(2) = num$(GABLIADI1)
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_USRFLDA1(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = num$(GABLIADI1)
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
