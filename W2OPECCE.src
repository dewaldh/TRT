Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(8255,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_LABCCE12(VALEUR) From SUBOPCE
If mkstat=2
 [M]LABCCE12(INDIC) = VALEUR
 Affzo LABCCE12(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_LABCCE12(zc) From W2OPECCE
Onkey   2 Titled GBOUTS Call S_LABCCE12(zc) From W2OPECCE
Onkey 144 Titled GBOUTP Call P_LABCCE12(zc) From W2OPECCE
Onkey 146 Titled GBOUTA Call SA_LABCCE12(zc) From W2OPECCE
Gosub _AFF
End

Subprog C_LABCCE12(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$(GDIE(12))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "LABCCE12" : GZNUM = 0 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$(GDIE(12)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "LABCCE12" : GZNUM = 0 : Endif
 If !mkstat
  ## Action STD
  Call C_LABCCE12(VALEUR) From SUBOPCE
  If mkstat : GERR = 1 : GZONE = "LABCCE12" : GZNUM = 0 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_LABCCE12
Call AM_LABCCE12(zc)
If !mkstat & GOBJET
 zoncou = "LABCCE12" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_LABCCE12(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZLABCCE12)")>indice
 [M]LABCCE12(indice) = VALEUR
 Affzo ZLABCCE12(indice)
Endif
Gosub _AFF
End

Subprog S_LABCCE12(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "CCE"
PARMSK(2) = "!"
PARMSK(3) = num$(GDIE(12))
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_LABCCE12(VALEUR)
Variable Char    VALEUR()
Call OBJET("CCE",VALEUR,num$(GDIE(12))) From GOBJET
Gosub _AFF
End

Subprog SA_LABCCE12(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "CCE"
PARMSK(2) = num$(GDIE(12))
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_LABCCE12(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = num$(GDIE(12))
Call VISU("CCE",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZLABCCE12
Onevent BEFORE_AREA Gosub AV_ZLABCCE12 From W2OPECCE
Return

$AV_ZLABCCE12
Call LECTEXTRA(zc,"CACCE","DESTRA",num$(GDIE(12)),num$([M]LABCCE12(indice))) From ATEXTRA
Gosub _AFF
Return

$TIT_COL
Gosub TIT_COL From W3OPECCE
Return


