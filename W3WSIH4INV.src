$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_EECFLOPHY
Onevent BEFORE_INPUT Call AS_EECFLOPHY(zc) From W3WSIH4INV
Onevent AFTER_CHANGE Gosub AMM_EECFLOPHY From W3WSIH4INV
Onevent INIT_AREA Call D_EECFLOPHY(zc) From W3WSIH4INV
Return

Subprog D_EECFLOPHY(VALEUR)
Variable Integer VALEUR
If varinit([M]EECFLOPHY,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_EECFLOPHY(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action SASLINCPN
If find([M]LINTYP(nolign-1), 11,12,13) | GMODIF=6 mkstat=2 : Endif
If mkstat=2
 [M]EECFLOPHY(INDIC) = VALEUR
 Affzo EECFLOPHY(INDIC) : mkstat = 2
Endif
If !mkstat
 ## Action STD
 Call AS_EECFLOPHY(VALEUR) From SUBSIH
 If mkstat=2
  [M]EECFLOPHY(INDIC) = VALEUR
  Affzo EECFLOPHY(INDIC) : mkstat = 2
 Endif
 If VALEUR<1
  VALEUR = 1
 Endif
Endif
Gosub _AFF
End

$AMM_EECFLOPHY
Call AM_EECFLOPHY(zc)
If !mkstat & GOBJET
 zoncou = "EECFLOPHY" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_EECFLOPHY(VALEUR)
Variable Integer VALEUR
## Action STD
Call AM_EECFLOPHY(VALEUR) From SUBSIH
If mkstat : GERR = 1 : GZONE = "EECFLOPHY" : GZNUM = 26651 : Endif
Gosub _AFF
End

