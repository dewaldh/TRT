Subprog B2_VCRNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ASELOBJA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ASELOBJA"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB2_VCRNUM" : PROGSUB = "SUBMCC" : PROGSPE = "SPEMCC0" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = ""
 PARMSK(2) = ""
 PARMSK(3) = num$([M]STAFLT)
 Call SAISIE_CHAR(VALEUR,PARMSK,"ASELOBJ","ASELOBJ","XWASELOBJ") From GSAISIE
 [M]STAFLT = val(PARMSK(3))
 ACTION = "BB2_VCRNUM" : PROGSUB = "SUBMCC" : PROGSPE = "SPEMCC0" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If !GWEBSERV
 ## Action STD
 Call B2_VCRNUM(VALEUR) From SUBMCC
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

