Subprog AS_TXSNAM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_TXSNAM(zc) From W1TXSCAL
Onkey 144 Titled GBOUTP Call P_TXSNAM(zc) From W1TXSCAL
Onkey 146 Titled GBOUTA Call SA_TXSNAM(zc) From W1TXSCAL
Gosub _AFF
End

Subprog C_TXSNAM(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "TXSNAM" : GZNUM = 5581 : Endif
If !mkstat
 ## Action CONTOBJW
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONTOBJW"
 PARMSK(1) = "TXS"
 Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJW","CONTOBJ","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "TXSNAM" : GZNUM = 5581 : Endif
 If !mkstat
  ## Action STD
  Call C_TXSNAM(VALEUR) From TXSCAL
  If mkstat : GERR = 1 : GZONE = "TXSNAM" : GZNUM = 5581 : Endif
 Endif
Endif
Gosub _AFF
End

