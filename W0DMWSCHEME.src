#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management DMWSCHEME 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone COD
mkstat = 0
zoncou = "COD"
indice = 0
If varmode([M]COD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]COD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 5296 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone DES
mkstat = 0
zoncou = "DES"
indice = 0
If varinit([M]DES,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DES,"DMWSCHEME","DES",[M:DMWSC]COD,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DES,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DES" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

## Zone CRY
mkstat = 0
zoncou = "CRY"
indice = 0
IMPENT = [M]CRY
If varinit([M]CRY,indice)=0
 IMPENT = 1
Endif
If varmode([M]CRY,indice)=0
 [M]CRY(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "CRY" : Gosub ACTION From =IMPTRT
  IMPENT = [M]CRY(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CRY" : GZNUM = 263 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]CRY = IMPENT

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CPY,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTCPY"
  Call CONTSOC([M]CPY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("CPY","",[M]CPY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone LICCOD
mkstat = 0
zoncou = "LICCOD"
indice = 0
If varmode([M]LICCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LICCOD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]LICCOD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "LICCOD" : GZNUM = 46300 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone BPR
mkstat = 0
zoncou = "BPR"
indice = 0
If varmode([M]BPR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BPR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("BPR","!",[M]BPR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "BPR" : GZNUM = 54366 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone STRDAT
mkstat = 0
zoncou = "STRDAT"
indice = 0
If varmode([M]STRDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "STRDAT" : Gosub ACTION From =IMPTRT
 Endif
 If [M]STRDAT=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 1536 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone ENDDAT
mkstat = 0
zoncou = "ENDDAT"
indice = 0
If varmode([M]ENDDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENDDAT" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "DATDEBFIN"
 Call DATDEBFIN([M]ENDDAT,[M]STRDAT) From EXEFNC
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ENDDAT" : GZNUM = 5463 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone NOTE
mkstat = 0
zoncou = "NOTE"
indice = 0
If varmode([M]NOTE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NOTE" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "DMWSCHEME"
Return

