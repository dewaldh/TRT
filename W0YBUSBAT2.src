Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone YLIG
mkstat = 0
zoncou = "YLIG"
indice = 0
status = 1
zoncou = "YLIG"
Gosub SETDEV From TRTDIV
GDEV1(1) = ""
If varmode([M]YLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]YLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]YLIG = nolign
   status = 75
   Call SAI_YLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

