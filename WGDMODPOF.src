$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [28/08/2018]
A_HEURE = "00:35:54"
A_USER  = "ADMIN"
Call ISWINVALID(nomap,GLANGUE,"DMODPOF",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
