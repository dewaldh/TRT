$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "2"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M]POHTYP)
  PARBOUT(2) = [M]VCRNUM
  PARBOUT(3) = [M]POHFCY
  FLGEXE = 1
  GACTION = "VISUPWS"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"DPWRKSUI","TRTACHFEN1","") From GSAISIE
 When "3"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$([M:PBW5]MRPMES)
  PARBOUT(2) = [M:PBW5]XMRPMES
  PARBOUT(3) = [M:PBW5]MRPDAT
  PARBOUT(4) = num$([M:PBW5]MRPQTY)
  PARBOUT(5) = [M:PBW5]STU
  PARBOUT(6) = num$([M:PBW5]EXTQTY1)
  PARBOUT(7) = [M:PBW5]STU2
  PARBOUT(8) = [M:PBW5]STRDAT1
  PARBOUT(9) = [M:PBW5]ENDDAT1
  PARBOUT(10) = [M:PBW5]STOFCY
  PARBOUT(11) = num$([M:PBW5]EXTQTY2)
  PARBOUT(12) = [M:PBW5]STU2
  PARBOUT(13) = [M:PBW5]ENDDAT2
  PARBOUT(14) = [M:PBW5]ENDDAT2
  PARBOUT(15) = [M:PBW5]ITMREF
  FLGEXE = 1
  GACTION = "SAIBENCHD1"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"DBENCHD1","BENCHLIB","XYZBENCHLIB") From GSAISIE
  [M:PBW5]EXTQTY2 = val(PARBOUT(11))
  [M:PBW5]STU2 = PARBOUT(12)
  [M:PBW5]ENDDAT2 = PARBOUT(13)
  [M:PBW5]ENDDAT2 = PARBOUT(14)
Endcase
GACTION = SAVACT
Return

