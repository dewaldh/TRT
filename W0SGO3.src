#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management SGO3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone PERCOD
mkstat = 0
zoncou = "PERCOD"
indice = 0
IMPENT = [M]PERCOD
WNUM = IMPENT
If varinit([M]PERCOD,indice)=0
 IMPENT = 1
Endif
If varmode([M]PERCOD,indice)=0
 Call INICOMBO(2231) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_PERCOD(IMPENT) From FUNGRHSOH
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2231,1))))
 Endif
 [M]PERCOD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "PERCOD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]PERCOD(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "PERCOD" : GZNUM = 1355 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
    mkstat = 1 : GMESSAGE = mess(99,123,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "PERCOD" : GZNUM = 1355 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WNUM<>IMPENT or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_PERCOD(IMPENT) From FUNGRHSOH
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "PERCOD" : GZNUM = 1355 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif
[M]PERCOD = IMPENT

## Zone ORDDATDEB
mkstat = 0
zoncou = "ORDDATDEB"
indice = 0
WDATE = [M]ORDDATDEB
If varmode([M]ORDDATDEB,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ORDDATDEB" : Gosub ACTION From =IMPTRT
 Endif
 If WDATE<>[M]ORDDATDEB or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_ORDDATDEB([M]ORDDATDEB) From FUNGRHSOH
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ORDDATDEB" : GZNUM = 1805 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone PERNBR
mkstat = 0
zoncou = "PERNBR"
indice = 0
WNUM = [M]PERNBR
If varmode([M]PERNBR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PERNBR" : Gosub ACTION From =IMPTRT
 Endif
 If [M]PERNBR=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "PERNBR" : GZNUM = 29537 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_PERNBR([M]PERNBR) From FUNGRHSOH
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "PERNBR" : GZNUM = 29537 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WNUM<>[M]PERNBR or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_PERNBR([M]PERNBR) From FUNGRHSOH
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "PERNBR" : GZNUM = 29537 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone ORDDATFIN
mkstat = 0
zoncou = "ORDDATFIN"
indice = 0
WDATE = [M]ORDDATFIN
If varmode([M]ORDDATFIN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ORDDATFIN" : Gosub ACTION From =IMPTRT
 Endif
 If [M]ORDDATFIN=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ORDDATFIN" : GZNUM = 1806 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WDATE<>[M]ORDDATFIN or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_ORDDATFIN([M]ORDDATFIN) From FUNGRHSOH
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "ORDDATFIN" : GZNUM = 1806 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIGG
mkstat = 0
zoncou = "NBLIGG"
indice = 0
status = 1
zoncou = "NBLIGG"
If varmode([M]NBLIGG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIGG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIGG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIGG = nolign
   status = 75
   Call SAI_NBLIGG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIGG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIGG
mkstat = 0
zoncou = "NBLIGG"

## Zone NBLIGG
mkstat = 0
zoncou = "NBLIGG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SGO3"
Return

