If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AFORMAT_ECCVALMAJ From WMR1CCMIMPFOH : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AFORMAT_ECCVALMIN From WMR1CCMIMPFOH : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AFORMAT_CRSTATUS From WMR1CCMIMPFOH : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AFORMAT_CCMUSRID From WMR1CCMIMPFOH : Gosub TESTALLSTAT :  Endif
If this.CCMIFOH<>null
 [L]ASTATUS = fmet this.CCMIFOH._AFORMAT : Gosub TESTALLSTAT
Endif
