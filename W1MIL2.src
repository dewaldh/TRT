If mkstat : GERR = 1 : GZONE = "TRSCODS" : GZNUM = 18946 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("ADI","!",VALEUR,"14",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "TRSCODS" : GZNUM = 18946 : Endif
