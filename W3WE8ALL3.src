Subprog AS_FCSCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(49922,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_FCSCOD(VALEUR) From SUBPIH
If mkstat=2
 [M]FCSCOD(INDIC) = VALEUR
 Affzo FCSCOD(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_FCSCOD(zc) From W3WE8ALL3
Onkey   2 Titled GBOUTS Call S_FCSCOD(zc) From W3WE8ALL3
Onkey 144 Titled GBOUTP Call P_FCSCOD(zc) From W3WE8ALL3
Onkey 146 Titled GBOUTA Call SA_FCSCOD(zc) From W3WE8ALL3
Gosub _AFF
End

Subprog C_FCSCOD(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("FCS","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "FCSCOD" : GZNUM = 49921 : Endif
If !mkstat
 ## Action STD
 Call C_FCSCOD(VALEUR) From SUBPIH
 If mkstat : GERR = 1 : GZONE = "FCSCOD" : GZNUM = 49921 : Endif
Endif
Gosub _AFF
End

Subprog S_FCSCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "FCS"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_FCSCOD(VALEUR)
Variable Char    VALEUR()
Call OBJET("FCS",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_FCSCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "FCS"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_FCSCOD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("FCS",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

