$ON_MATTOL
Onevent INIT_AREA Call D_MATTOL(zc) From W3WE8ALL3
Onevent CONTROL Call C_MATTOL(zc) From W3WE8ALL3
Onevent BEFORE_INPUT Call AS_MATTOL(zc) From W3WE8ALL3
Return

Subprog D_MATTOL(VALEUR)
Variable Char    VALEUR()
If varinit([M]MATTOL,indice)=0
 ## Action STD
 Call D_MATTOL(VALEUR) From SUBPIH
Endif
Gosub _AFF
End

Subprog AS_MATTOL(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(43884,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_MATTOL(zc) From W3WE8ALL3
Onkey   2 Titled GBOUTS Call S_MATTOL(zc) From W3WE8ALL3
Onkey 144 Titled GBOUTP Call P_MATTOL(zc) From W3WE8ALL3
Onkey 146 Titled GBOUTA Call SA_MATTOL(zc) From W3WE8ALL3
Gosub _AFF
End

Subprog C_MATTOL(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("MAT","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "MATTOL" : GZNUM = 43884 : Endif
Gosub _AFF
End

Subprog S_MATTOL(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "MAT"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_MATTOL(VALEUR)
Variable Char    VALEUR()
Call OBJET("MAT",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_MATTOL(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "MAT"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_MATTOL(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("MAT",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

