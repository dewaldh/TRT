   Local Char VALBOUT(250) , PARBOUT(250)(1..20)
   PARBOUT(1) = [L]VALC2
   PARBOUT(2) = [L]VALC1
   PARBOUT(3) = [L]VALC3
   FLGEXE = 1
   GACTION = "GOBJETC0"
   Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  Endif
 When "4"
  FLGEXE = 1
  GACTION = "ANUBIS"
  Call DEVALID([M:BIS0]NUM) From ANUBIS
 When "5"
  FLGEXE = 1
  GACTION = "FACTAVO"
  Call FACTAVO([M:BIS0]NUM) From FACTAVO
 When "8"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$(1)
  PARBOUT(2) = [M:BIS0]BPR
  PARBOUT(3) = num$(248)
  PARBOUT(4) = num$(195)
  PARBOUT(5) = "2"
  FLGEXE = 1
  GACTION = "SAIADR"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IAFR","SUBAFR","") From GSAISIE
 When "9"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$(2)
  PARBOUT(2) = [M:BIS1]BPRPAY
  PARBOUT(3) = num$(249)
  PARBOUT(4) = num$(195)
  PARBOUT(5) = "2"
  FLGEXE = 1
  GACTION = "SAIADR"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IAFR","SUBAFR","") From GSAISIE
 When "Q"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "BIS"
  PARBOUT(2) = [M:BIS0]NUM
  PARBOUT(3) = num$("")
  FLGEXE = 1
  GACTION = "CONSPIA"
  Call CONSULT_CHAR(VALBOUT,"PIA",PARBOUT) From GCONSULT
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "C"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:BIS0]BPR
  PARBOUT(2) = [M:BIS0]CPY
  PARBOUT(3) = num$([L]SAIPRO)
  FLGEXE = 1
  GACTION = "SAIPROFAC"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"DPROFAC","RTZPROFAC","XWDPROFAC") From GSAISIE
  [L]SAIPRO = val(PARBOUT(3))
Endcase
GACTION = SAVACT
Return


