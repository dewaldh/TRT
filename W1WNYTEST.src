$ON_TUNNEL
Return

$TIT_COL
Local Char TITCL(200)(1..2)
Raz TITCL
Case BASPAG
 When "NBLIG"
  TITCL(1) = func AFNC.TEXTRA("ALISTED","INTITLIG","YTEST","1")
  TITCL(2) = func AFNC.TEXTRA("ALISTED","INTITLIG","YTEST","2")
  If !GORITITCOL
   Titcol [M]NBLIG TITCL(1) For C1
   Titcol [M]NBLIG TITCL(2) For C2
  Else
   Titcol [M:LST]NBLIG TITCL(1) For C1
   Titcol [M:LST]NBLIG TITCL(2) For C2
  Endif
Endcase
Return

$CHG_STY
Return


