####################################################################
# 1 : ITMNOM Code ( Field INO 12)
####################################################################

$SET_ITMNOM
If [L]_AOLDVAL<>this.ITMNOM
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_ITMNOM
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.ITMNOM
 Gosub PROPAGATE_ITMNOM
Endif
Return

$INIT_ITMNOM
Gosub DECLARE_VAR From WMC0ITMCUSNOM
[L]CURPRO = "ITMNOM"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_ITMNOM
Gosub DECLARE_VAR From WMC0ITMCUSNOM
[L]CURPRO = "ITMNOM"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_ITMNOM                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_ITMNOM                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_ITMNOM
[L]CURPRO = "ITMNOM"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.ITMNOM,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_ITMNOM
[L]CURPRO = "ITMNOM"
If vireblc(this.ITMNOM,2)<>vireblc(format$("K:12#",this.ITMNOM),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_ITMNOM
[L]CURPRO = "ITMNOM"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_ITMNOM
[L]CURPRO = "ITMNOM"
Return

$PROPAGATE_ITMNOM
this.isReadonly = 0
[L]CURPRO = "ITMNOM"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_ITMNOM
Gosub DECLARE_VAR From WMC0ITMCUSNOM
[L]CURPRO = "ITMNOM"
$_LNKPRO_ITMNOM
Return

$_ALOADATTRIBUTE_ITMNOM
[L]ASTATUS = fmet this.ASETATTRIBUTE("ITMNOM","$isMandatory","true")
Return


