# Supervisor
## CRUD-CLASSE ## PHYBUI
# 2018-08-27 13:21:32 960


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PHYBUI,ASTATUS)
Variable Instance PHYBUI Using R_PHYBUI
Variable Integer ASTATUS

If PHYBUI = null : End : Endif

Gosub AVARIABLES From WMR00PHYBUI
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PHYBUI.ACTX.AGETAFCRIGHT(PHYBUI,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PHYBUI.ACTX.AGETAFCRIGHTU(PHYBUI,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PHYBUI
Else
 Gosub AUPDATE_PHYBUI
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PHYBUI.freeSnapshot
PHYBUI.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PHYBUI.ASTALIN = [V]CST_AUPD
PHYBUI.BUI.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PHYBUI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYBUI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PHYBUI._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PHYBUI._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PHYBUI._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PHYBUI.BUI.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PHYBUI.BUI.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PHYBUI.BUI.ASTALIN)<>""
 [L]A_ASTALIN = PHYBUI.BUI.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PHYBUI.BUI.BPA) : # BPADDRESS -- 2
 If PHYBUI.BUI.BPA(A_N2) <> null
& and PHYBUI.BUI.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PHYBUI.BUI.BPA(A_N2).ASTALIN<>[V]CST_ANEW & PHYBUI.BUI.ASTALIN=[V]CST_ADEL
   PHYBUI.BUI.BPA(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PHYBUI.BUI.BPA(A_N2).ASTALIN=[V]CST_ANEW & find(PHYBUI.BUI.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PHYBUI.BUI.BPA(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PHYBUI.BUI.BPA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYBUI.BUI.BPA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
  For [L]A_I=1 To maxtab(PHYBUI.BUI.BPA(A_N2).COLTEL)
   If PHYBUI.BUI.BPA(A_N2).COLTEL([L]A_I)<>null
    [L]A_ASTALIN = PHYBUI.BUI.BPA(A_N2).COLTEL([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2).COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
  For [L]A_I=1 To maxtab(PHYBUI.BUI.BPA(A_N2).COLWEB)
   If PHYBUI.BUI.BPA(A_N2).COLWEB([L]A_I)<>null
    [L]A_ASTALIN = PHYBUI.BUI.BPA(A_N2).COLWEB([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2).COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PHYBUI.BUI.BPA) : # BPADDRESS -- 2
 If PHYBUI.BUI.BPA(A_N2) <> null
& and PHYBUI.BUI.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PHYBUI.BUI.BPA(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYBUI.BUI.BPA(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
  For [L]A_I=1 To maxtab(PHYBUI.BUI.BPA(A_N2).COLTEL)
   If PHYBUI.BUI.BPA(A_N2).COLTEL([L]A_I)<>null
    [L]A_ASTALIN = PHYBUI.BUI.BPA(A_N2).COLTEL([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2).COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
  For [L]A_I=1 To maxtab(PHYBUI.BUI.BPA(A_N2).COLWEB)
   If PHYBUI.BUI.BPA(A_N2).COLWEB([L]A_I)<>null
    [L]A_ASTALIN = PHYBUI.BUI.BPA(A_N2).COLWEB([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.BPA(A_N2).COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PHYBUI.BUI.ASTALIN)<>""
 [L]A_ASTALIN = PHYBUI.BUI.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PHYBUI
Trbegin [F:BUI]
 Gosub AUPDATE_PHYBUI
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PHYBUI.BUI.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PHYBUI.BUI.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYBUI.BUI.BPA)
 If PHYBUI.BUI.BPA(A_N2) <> null
  [L]ASTATUS2 = fmet PHYBUI.BUI.BPA(A_N2)._AUPDATE_AFTER
  If PHYBUI.BUI.BPA(A_N2).ASTALIN=[V]CST_ADEL or PHYBUI.BUI.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYBUI.BUI.BPA(A_N2)
   PHYBUI.BUI.BPA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_PHYBUI
[L]A_ASTALIN = PHYBUI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.BUI._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYBUI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYBUI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PHYBUI.snapshotEnabled=1
 [L]A_RETIK = PHYBUI.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYBUI.BUI.BPA)
 If PHYBUI.BUI.BPA(A_N2) <> null
  [L]ASTATUS2 = fmet PHYBUI.BUI.BPA(A_N2)._AUPDATE_ABORT
  If PHYBUI.BUI.BPA(A_N2).ASTALIN=[V]CST_ADEL or PHYBUI.BUI.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYBUI.BUI.BPA(A_N2)
   PHYBUI.BUI.BPA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PHYBUI.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



