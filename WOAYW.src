$LEC_DERLU
Read [AX3]AXX0="AYTELTBLCW";"INTIT";GLANGUE;FCYCOD;BLCWHNCOD
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:AYW9])>0
 Read [AYW9] = [F:AYW9]A_TABI
 [L]CLE = [F:AYW9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:AYW9])>0
 Filter [AYW9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AYW1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AYW9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AYW]BLCWHNCOD+"~"
 A_CLEDERLU += [F:AYW]FCYCOD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AYW9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AYW9]
 Endif
Endif
Return

