Subprog AS_ITMREF(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(12837,GBOUT13) From OBJDIV
Call TEXTE(8255,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_ITMREF(VALEUR)
If !([M]TIMTYP(nolign-1) = 2 & [M]WINDIC(nolign-1) = 0 & GMODIF = 2)
 Affzo ITMREF : mkstat = 2
Endif
If !mkstat
 Onkey 170 Titled GBOUT13 Call B13_ITMREF(zc) From W2WT60VT1
 Onkey   2 Titled GBOUTS Call S_ITMREF(zc) From W2WT60VT1
 Onkey 144 Titled GBOUTP Call P_ITMREF(zc) From W2WT60VT1
 Onkey 146 Titled GBOUTA Call SA_ITMREF(zc) From W2WT60VT1
Endif
Gosub _AFF
End

Subprog C_ITMREF(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ITMCHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ITMCHOICE"
 Call S_TAB_CHAR(VALEUR,PARMSK,"ITMCHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3817 : Endif
Endif
If !mkstat
 ## Action VXADCUPC
 Call ADCCTRUPC(VALEUR) From VXADCUPC
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3817 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ITM","!",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3817 : Endif
  If !mkstat
   ## Action STD
   Call C_ITMREF(VALEUR) From FUNBENCHO
   If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3817 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

