Subprog C_BOMALT(VALEUR)
Variable Decimal VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 3964 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTNUM("TBO","",VALEUR,"2",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 3964 : Endif
 If !mkstat
  ## Action CONTALT
  Call CONTALT(VALEUR,2) From TRTBOM
  If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 3964 : Endif
  If !mkstat
   ## Action STD
   Call C_BOMALT(VALEUR) From FUNBENCHO
   If mkstat : GERR = 1 : GZONE = "BOMALT" : GZNUM = 3964 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

