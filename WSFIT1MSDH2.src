#<AdxTL>@(#)%I%
#Traitement généré pour SUBSFIT
#================================================================================
End

$DYNRIEN
Local Integer IERRSFIT
  IERRSFIT = 1
Resume

Subprog CONTROLE(SVERSION, IOK)
Value Char SVERSION
Variable Integer IOK

  IOK = 1
  If SVERSION <> 'V1.3' Then
    IOK = 0
    End
  Endif 
  If 29 <> dim([M:SDH2]INVDTAAMT)-1 Then
    IOK = 0
    End
  Endif 
End

$DYNAMIQUE_RAZ
  Onerrgo DYNRIEN
  Raz [M:SDH2]INVDTAAMT
  Onerrgo
Return

$DYNAMIQUE_DESTINATION
  Onerrgo DYNRIEN
  If [M:SDH2]AMTCOD(IELT-1)=1 Then # Taux
    [M:SDH2]INVDTAAMT(IELT-1) = arr([L]NEWDTAAMT,[F:TCU]CURRND/100)   # nb de decimale = nb de decimale +2 (pour arrondis) - CH
    If arr([L]NEWDTAAMT,[F:TCU]CURRND/100) <> arr([L]NEWDTAAMT,[F:TCU]CURRND) Then # Si plus de X décimales, on augmente le format
      [M:SDH2]FORMAT(IELT-1) = 'NZ:3.' + num$(len(num$([F:TCU]CURRND)) - instr(0,num$([F:TCU]CURRND),'.')+2)
    Endif
  Else
    [M:SDH2]INVDTAAMT(IELT-1) = val(ctrans(format$(([M:SDH2]FORMAT(IELT-1)), [L]NEWDTAAMT),",","."))
  Endif
  Onerrgo
Return

$DYNAMIQUE_OLDDTAAMT
  Onerrgo DYNRIEN
  [L]OLDDTAAMT = [M:SDH2]INVDTAAMT(IELT-1)
  Onerrgo
Return
