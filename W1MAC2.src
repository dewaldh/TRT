$ON_NBCON
Onevent CONTROL Call C_NBCON From W1MAC2
Onevent BEFORE_INPUT Call AS_NBCON From W1MAC2
Return

Subprog IB_NBCON
Local Decimal BASPAG
If !GWEBSERV
 ## Action STD
 Call IB_NBCON From SUBMAC
Endif
Gosub _AFF
End

Subprog AS_NBCON
Local Decimal BASPAG
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(735,GBOUT1) From OBJDIV
Call IB_NBCON
Onkey 129 Titled GBOUT1 Call B1_NBCON From W1MAC2
Gosub _AFF
End

Subprog C_NBCON
Local Decimal BASPAG
## Action STD
Call C_NBCON From SUBMAC
If mkstat : GERR = 1 : GZONE = "NBCON" : GZNUM = 0 : Endif
Gosub _AFF
End

Subprog B1_NBCON
Local Decimal BASPAG
If !GWEBSERV
 ## Action GOBJETC1
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GOBJETC1"
 Call OBJET_CHAR([M:MAC2]MACCONNUM(nolign - 1),"CON","") From GOBJET
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_MACCONNUM
Onevent CONTROL Call C_MACCONNUM(zc) From W1MAC2
Onevent BEFORE_INPUT Call AS_MACCONNUM(zc) From W1MAC2
Return

Subprog AS_MACCONNUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(22883,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_MACCONNUM(zc) From W1MAC2
Onkey   2 Titled GBOUTS Call S_MACCONNUM(zc) From W1MAC2
Onkey 144 Titled GBOUTP Call P_MACCONNUM(zc) From W1MAC2
Onkey 146 Titled GBOUTA Call SA_MACCONNUM(zc) From W1MAC2
Gosub _AFF
End

Subprog C_MACCONNUM(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("CON","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "MACCONNUM" : GZNUM = 16090 : Endif
If !mkstat
 ## Action STD
 Call C_MACCONNUM(VALEUR) From SUBMAC
 If mkstat : GERR = 1 : GZONE = "MACCONNUM" : GZNUM = 16090 : Endif
Endif
Gosub _AFF
End

Subprog S_MACCONNUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELMAC2CO1
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELMAC2CO1"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELMAC2CO1","TRTCRMSEL","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_MACCONNUM(VALEUR)
Variable Char    VALEUR()
Call OBJET("CON",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_MACCONNUM(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "CON"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_MACCONNUM(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("CON",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

