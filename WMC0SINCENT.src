$_ADDTRT
this.AINF.TRT(1) = "SINCENT_CSTD"
this.AINF.TYP(1) = 1
this.AINF.PTH(1) = ""
this.AINF.NBTRT = 1
Return

####################################################################
#Declaration of variables
####################################################################

$DECLARE_VAR
Local Char    ACTION(50) , CURPRO(250) , VALPRO(250) , CURQRY(50) , CURPTH(250)
Local Char    AMETHOD(50) ,AOPERATION(50),AEVENT(50),ARULE(50)
Local Integer ALLSTAT,AISMET,ASTATUS
Return


####################################################################
# Global methods & standards
####################################################################
$AINIT
Gosub DECLARE_VAR
[L]AEVENT = "AINIT" : [L]ACTION = "AINIT" : [L]CURPRO = "" : Gosub ACTION From ASYRSUB
Return

####################################################################
# AERRORS
####################################################################

$_AERROR
Gosub DECLARE_VAR From WMC0SINCENT
Local Integer ALLSTAT
Call ALLERRORS(this,_CTX,_SUPP) From ASYRSUB
For AINDCLA=1 To 10
 If this.C_TBL(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_TBL(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
For AINDCLA=1 To 20
 If this.C_VARCOD(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_VARCOD(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
Return

####################################################################
# Delete all errors
####################################################################

$_ADELERRORS
Gosub DECLARE_VAR From WMC0SINCENT
Local Integer ALLSTAT
[L]ASTATUS = fmet this.ADELETEERROR([L]_APARAM)
For AINDCLA=1 To 10
 If this.C_TBL(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_TBL(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
For AINDCLA=1 To 20
 If this.C_VARCOD(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_VARCOD(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
Return

####################################################################
# _ASEARCHINFOS
####################################################################

$_ASEARCHINFOS
Gosub DECLARE_VAR From WMC0SINCENT
ASTATUS= func ASYRSUB.ASEARCHINFOS(this,[L]AINS_ORI,[L]ERR_TYPE,[L]PROPERTY,[L]ASTAT,[L]SELSTAT,[L]PATH)
If [L]ERR_TYPE=[V]CST_ACURRENT : Return : Endif
For AINDCLA=1 To 10
 If this.C_TBL(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_TBL(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_TBL('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
For AINDCLA=1 To 20
 If this.C_VARCOD(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_VARCOD(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_VARCOD('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
Return

####################################################################
# AGETERRORSFROM
####################################################################

$_AGETERRORSFROM
Gosub DECLARE_VAR From WMC0SINCENT
If AINS_FROM<>null
 [L]ASTATUS = fmet AINS_FROM.ASEARCHINFOS( [V]CST_ALL, "", [V]CST_AFATAL, 0)
Endif
Call AGETERRORSFROM(AINS_FROM,this,ENVIRONNEMENT) From ASYRSUB
Return

####################################################################
$ACONTROL
####################################################################
Gosub DECLARE_VAR
[L]AEVENT = "ACONTROL" : [L]ACTION = "ACONTROL" : [L]CURPRO = "" : Gosub ACTION From ASYRSUB
Return

####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "C_TBL"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_TBL",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBTBL -= 1 : Endif
 When "C_VARCOD"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_VARCOD",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBVAR -= 1 : Endif
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# ADDLINE
####################################################################
$ADDLINE
Case _ACOLLECTION
 When "C_TBL"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_TBL","C_SINCENT_C_TBL",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBTBL += 1 : Endif
 When "C_VARCOD"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_VARCOD","C_SINCENT_C_VARCOD",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBVAR += 1 : Endif
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# AGETDIM
####################################################################
$AGETDIM
Case _ACOLLECTION
 When "C_TBL"
  [L]_ALINE = 10
 When "C_VARCOD"
  [L]_ALINE = 20
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# AGETMIN
####################################################################
$AGETMIN
Case _ACOLLECTION
 When "C_TBL"
  [L]_ALINE = 2
 When "C_VARCOD"
  [L]_ALINE = 2
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

