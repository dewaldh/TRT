############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(BPADDRESS,ASTATUS)
Variable Instance BPADDRESS Using R_BPADDRESS
Variable Integer ASTATUS

If BPADDRESS = null : End : Endif

Gosub AVARIABLES From WMR00BPADDRESS
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_BPADDRESS
Else
 Gosub AUPDATE_BPADDRESS
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = BPADDRESS.freeSnapshot
BPADDRESS.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
BPADDRESS.ASTALIN = [V]CST_AUPD
BPADDRESS.BPA.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = BPADDRESS.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BPADDRESS.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet BPADDRESS._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet BPADDRESS._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet BPADDRESS._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If BPADDRESS.BPA.ASTALIN = [V]CST_ANEWDEL : Return : Endif
BPADDRESS.BPA.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(BPADDRESS.BPA.ASTALIN)<>""
 [L]A_ASTALIN = BPADDRESS.BPA.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.BPA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(BPADDRESS.BPA.COLTEL)
 If BPADDRESS.BPA.COLTEL([L]A_I)<>null
  [L]A_ASTALIN = BPADDRESS.BPA.COLTEL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.BPA.COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPADDRESS.BPA.COLWEB)
 If BPADDRESS.BPA.COLWEB([L]A_I)<>null
  [L]A_ASTALIN = BPADDRESS.BPA.COLWEB([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.BPA.COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

For [L]A_I=1 To maxtab(BPADDRESS.BPA.COLTEL)
 If BPADDRESS.BPA.COLTEL([L]A_I)<>null
  [L]A_ASTALIN = BPADDRESS.BPA.COLTEL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.BPA.COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPADDRESS.BPA.COLWEB)
 If BPADDRESS.BPA.COLWEB([L]A_I)<>null
  [L]A_ASTALIN = BPADDRESS.BPA.COLWEB([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.BPA.COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(BPADDRESS.BPA.ASTALIN)<>""
 [L]A_ASTALIN = BPADDRESS.BPA.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.BPA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_BPADDRESS
Trbegin [F:BPA]
 Gosub AUPDATE_BPADDRESS
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(BPADDRESS.BPA.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 BPADDRESS.BPA.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(BPADDRESS.BPA.COLTEL)
 If BPADDRESS.BPA.COLTEL([L]A_I)<>null
  If BPADDRESS.BPA.COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or BPADDRESS.BPA.COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPADDRESS.BPA.COLTEL([L]A_I)
   BPADDRESS.BPA.COLTEL([L]A_I) = null
  Elsif BPADDRESS.BPA.COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
   BPADDRESS.BPA.COLTEL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPADDRESS.BPA.COLWEB)
 If BPADDRESS.BPA.COLWEB([L]A_I)<>null
  If BPADDRESS.BPA.COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or BPADDRESS.BPA.COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPADDRESS.BPA.COLWEB([L]A_I)
   BPADDRESS.BPA.COLWEB([L]A_I) = null
  Elsif BPADDRESS.BPA.COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
   BPADDRESS.BPA.COLWEB([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Return

#-----------------------------------------------------#
$AUPDATE_BPADDRESS
[L]A_ASTALIN = BPADDRESS.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.BPA._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BPADDRESS.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPADDRESS.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If BPADDRESS.snapshotEnabled=1
 [L]A_RETIK = BPADDRESS.revertUpdTick
Endif

[L]ASTATUS = max(fmet BPADDRESS.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



