REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Code
     REP.CLE(1).PAR = 'ITMNOM' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'ITMNOM' : REP.CLE(1).VPR= 'INO.ITMNOM' : REP.CLE(1).DES = 5296
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'ITMCUSNOM_RSTD' : REP.TRT(1).TYP = 1
