# Supervisor
# Mask management WL1STD3 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:22:11"
A_USER  = "ADMIN"
A_STAMP = "20180828002211"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIGTSK"
  If [M]NBLIGTSK=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask WL1STD3 [CRM3]
Gosub HORODAT
Local Inpbox "WL1STD3" From GFONCTION At A_STAMP With A_WINDPREV Mask [CRM3] As [SIM_]
Grizo [CRM3]5
Grizo [CRM3]20
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [CRM3]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=112 : Y1(0)=1 : Y2(0)=3
X1(1)=1 : X2(1)=112 : Y1(1)=4 : Y2(1)=25
Return

$ON_SEATSKTYP
Onevent INIT_AREA Call D_SEATSKTYP(zc) From W1WL1STD3
Onevent CONTROL Call C_SEATSKTYP(zc) From W1WL1STD3
Onevent BEFORE_INPUT Call AS_SEATSKTYP(zc) From W1WL1STD3
Onevent AFTER_CHANGE Gosub AMM_SEATSKTYP From W1WL1STD3
Return

Subprog D_SEATSKTYP(VALEUR)
Variable Char    VALEUR()
If varinit([M]SEATSKTYP,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"431","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_SEATSKTYP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"431") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_SEATSKTYP(zc) From W1WL1STD3
Onkey   2 Titled GBOUTS Call S_SEATSKTYP(zc) From W1WL1STD3
Onkey 144 Titled GBOUTP Call P_SEATSKTYP(zc) From W1WL1STD3
Onkey 146 Titled GBOUTA Call SA_SEATSKTYP(zc) From W1WL1STD3
Gosub _AFF
End

Subprog C_SEATSKTYP(VALEUR)
Variable Char    VALEUR()
## Action CONTADI
Call CONTADI(VALEUR,"431","") From SUBADI
If mkstat : GERR = 1 : GZONE = "SEATSKTYP" : GZNUM = 707 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("ADI","!",VALEUR,"431",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "SEATSKTYP" : GZNUM = 707 : Endif
Endif
Gosub _AFF
End

$AMM_SEATSKTYP
Call AM_SEATSKTYP(zc)
If !mkstat & GOBJET
 zoncou = "SEATSKTYP" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_SEATSKTYP(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZSEATSKTYP)")>indice
 [M]SEATSKTYP(indice) = VALEUR
 Affzo ZSEATSKTYP(indice)
Endif
Gosub _AFF
End

Subprog S_SEATSKTYP(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = "431"
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_SEATSKTYP(VALEUR)
