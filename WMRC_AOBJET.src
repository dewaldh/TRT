############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(AOBJET,ASTATUS)
Variable Instance AOBJET Using R_AOBJET
Variable Integer ASTATUS

If AOBJET = null : End : Endif

Gosub AVARIABLES From WMR00AOBJET
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If fmet AOBJET.ACTX.AGETAFCRIGHT(AOBJET,"GESAOB",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END : Endif
If fmet AOBJET.ACTX.AGETAFCRIGHTC(AOBJET,"GESAOB",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END
Endif

Gosub AREPCONTROL_AINSERT
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If !adxlog
 Gosub AINSERT_TR_AOBJET
Else
 Gosub AINSERT_AOBJET
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_END : Return : Endif

Gosub _AINSERT_AFTER
$AINSERT_END
[L]A_I = AOBJET.freeSnapshot
AOBJET.snapshotEnabled = 1
End

#-----------------------------------------------------#
$AREPCONTROL_AINSERT
AOBJET.ASTALIN = [V]CST_ANEW
AOBJET.AOB.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet AOBJET.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet AOBJET.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet AOBJET._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet AOBJET._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet AOBJET._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If AOBJET.AOB.ASTALIN = [V]CST_ANEWDEL : Return : Endif
AOBJET.AOB.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.IMP)
 If AOBJET.AOB.IMP([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.IMP([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.OPTCN)
 If AOBJET.AOB.OPTCN([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.OPTCN([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.SCR)
 If AOBJET.AOB.SCR([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.SCR([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.VUE)
 If AOBJET.AOB.VUE([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.VUE([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If AOBJET.AOB.AOL(A_N2) <> null
& and !find(AOBJET.AOB.AOL(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(AOBJET.AOB.AOT) : # AOBJTAB -- 3
 If AOBJET.AOB.AOT(A_N2) <> null
& and !find(AOBJET.AOB.AOT(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
A_N2=1
If AOBJET.AOB.AOL(A_N2) <> null
& and !find(AOBJET.AOB.AOL(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(AOBJET.AOB.AOT) : # AOBJTAB -- 3
 If AOBJET.AOB.AOT(A_N2) <> null
& and !find(AOBJET.AOB.AOT(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(AOBJET.AOB.IMP)
 If AOBJET.AOB.IMP([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.IMP([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.OPTCN)
 If AOBJET.AOB.OPTCN([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.OPTCN([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.SCR)
 If AOBJET.AOB.SCR([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.SCR([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.VUE)
 If AOBJET.AOB.VUE([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.VUE([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_AOBJET
Trbegin [F:AOB]
 Gosub AINSERT_AOBJET
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AINSERT_AFTER
If find(AOBJET.AOB.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 AOBJET.AOB.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.IMP)
 If AOBJET.AOB.IMP([L]A_I)<>null
  If AOBJET.AOB.IMP([L]A_I).ASTALIN=[V]CST_ADEL or AOBJET.AOB.IMP([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AOBJET.AOB.IMP([L]A_I)
   AOBJET.AOB.IMP([L]A_I) = null
  Elsif AOBJET.AOB.IMP([L]A_I).ASTALIN <> [V]CST_ALL
   AOBJET.AOB.IMP([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.OPTCN)
 If AOBJET.AOB.OPTCN([L]A_I)<>null
  If AOBJET.AOB.OPTCN([L]A_I).ASTALIN=[V]CST_ADEL or AOBJET.AOB.OPTCN([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AOBJET.AOB.OPTCN([L]A_I)
   AOBJET.AOB.OPTCN([L]A_I) = null
  Elsif AOBJET.AOB.OPTCN([L]A_I).ASTALIN <> [V]CST_ALL
   AOBJET.AOB.OPTCN([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.SCR)
 If AOBJET.AOB.SCR([L]A_I)<>null
  If AOBJET.AOB.SCR([L]A_I).ASTALIN=[V]CST_ADEL or AOBJET.AOB.SCR([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AOBJET.AOB.SCR([L]A_I)
   AOBJET.AOB.SCR([L]A_I) = null
  Elsif AOBJET.AOB.SCR([L]A_I).ASTALIN <> [V]CST_ALL
   AOBJET.AOB.SCR([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
For [L]A_I=1 To maxtab(AOBJET.AOB.VUE)
 If AOBJET.AOB.VUE([L]A_I)<>null
  If AOBJET.AOB.VUE([L]A_I).ASTALIN=[V]CST_ADEL or AOBJET.AOB.VUE([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AOBJET.AOB.VUE([L]A_I)
   AOBJET.AOB.VUE([L]A_I) = null
  Elsif AOBJET.AOB.VUE([L]A_I).ASTALIN <> [V]CST_ALL
   AOBJET.AOB.VUE([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
Local Integer ASTATUS2
A_N2=1
If AOBJET.AOB.AOL(A_N2) <> null
 If AOBJET.AOB.AOL(A_N2).ASTALIN=[V]CST_ADEL or AOBJET.AOB.AOL(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance AOBJET.AOB.AOL(A_N2)
  AOBJET.AOB.AOL(A_N2) = null
 Elsif AOBJET.AOB.AOL(A_N2).ASTALIN <> [V]CST_ALL
  AOBJET.AOB.AOL(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(AOBJET.AOB.AOL(A_N2).ASELCOL)
   If AOBJET.AOB.AOL(A_N2).ASELCOL([L]A_I)<>null
    If AOBJET.AOB.AOL(A_N2).ASELCOL([L]A_I).ASTALIN=[V]CST_ADEL or AOBJET.AOB.AOL(A_N2).ASELCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance AOBJET.AOB.AOL(A_N2).ASELCOL([L]A_I)
     AOBJET.AOB.AOL(A_N2).ASELCOL([L]A_I) = null
    Elsif AOBJET.AOB.AOL(A_N2).ASELCOL([L]A_I).ASTALIN <> [V]CST_ALL
     AOBJET.AOB.AOL(A_N2).ASELCOL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(AOBJET.AOB.AOT)
 If AOBJET.AOB.AOT(A_N2) <> null
  If AOBJET.AOB.AOT(A_N2).ASTALIN=[V]CST_ADEL or AOBJET.AOB.AOT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AOBJET.AOB.AOT(A_N2)
   AOBJET.AOB.AOT(A_N2) = null
  Elsif AOBJET.AOB.AOT(A_N2).ASTALIN <> [V]CST_ALL
   AOBJET.AOB.AOT(A_N2).ASTALIN = [V]CST_ALL
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AINSERT_AOBJET
[L]ASTATUS = max([L]ASTATUS,fmet AOBJET.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet AOBJET.AOB._AINSERT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet AOBJET.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If AOBJET.snapshotEnabled=1
 [L]A_RETIK = AOBJET.revertUpdTick
Endif
[L]ASTATUS = max(fmet AOBJET.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



