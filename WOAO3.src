$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 2
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(1)"
SELEXP(NBSEL) = "mess(23,107,1)-[F:AOU]NUMLOT-' : '-[F:AOW]EXT-'('-[F:AXX]TEXTE-')'"
Call TEXTE(29277,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = ""
SELMAJ(NBSEL) = 0
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(2)"
SELEXP(NBSEL) = "mess(3,40,1)-num$([F:AOU]NUMLIG)"
Call TEXTE(42,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = ""
SELMAJ(NBSEL) = 0
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(3)"
SELEXP(NBSEL) = "string$([AOU]FLDNUM<1000,mess(1,89,1)-num$([AOU]FLDNUM)-': ')+string$([AOU]FLDNUM>1000 & [AOU]FLDNUM < 2000,mess(253,133,1)-':')+string$([AOU]FLDNUM>2000,mess(1,89,1)-':')"
Call TEXTE(0,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = ""
SELMAJ(NBSEL) = 0
NBSEL += 1
SELZON(NBSEL) = "[F:AOU]ERR"
Call TEXTE(7243,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:250X"
SELFOR(NBSEL) = "K:250X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:AOU]ERR"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:AOU_])=0
 Gosub OUVFIC_LISTE
Endif
Return

