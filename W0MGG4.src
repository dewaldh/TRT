# Supervisor
# Mask management MGG4 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone QURNUM
mkstat = 0
zoncou = "QURNUM"
WCHAR = [M]QURNUM(indice)
If varmode([M]QURNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "QURNUM" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_QURNUM([M]QURNUM(indice)) From SUBMGG4
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "QURNUM" : GZNUM = 18941 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WCHAR<>[M]QURNUM(indice) or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_QURNUM([M]QURNUM(indice)) From SUBMGG4
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "QURNUM" : GZNUM = 18941 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone QURCAT
mkstat = 0
zoncou = "QURCAT"
If varinit([M]QURCAT,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]QURCAT(indice),"438","") From SUBADI
 GACTION = SAVACT
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "MGG4"
Return

