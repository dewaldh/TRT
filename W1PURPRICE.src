Subprog AS_GROPRI(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(7958,GBOUT1) From OBJDIV
Call TEXTE(16098,GBOUT2) From OBJDIV
Call TEXTE(454,GBOUT3) From OBJDIV
Call IB_GROPRI(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_GROPRI(zc) From W1PURPRICE
Onkey 130 Titled GBOUT2 Call B2_GROPRI(zc) From W1PURPRICE
Onkey 131 Titled GBOUT3 Call B3_GROPRI(zc) From W1PURPRICE
Gosub _AFF
End

Subprog AP_GROPRI(VALEUR)
Variable Decimal VALEUR
## Action STD
Call AP_GROPRI(VALEUR) From SUBPPRI
If mkstat : GERR = 1 : GZONE = "GROPRI" : GZNUM = 2541 : Endif
Gosub _AFF
End

$AMM_GROPRI
Call AM_GROPRI(zc)
If !mkstat & GOBJET
 zoncou = "GROPRI" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_GROPRI(VALEUR)
Variable Decimal VALEUR
## Action PAMDISCRG
Call PAMDISCRG(VALEUR,"PPRI",0,0,[M:PPRI]CUR) From TRTACHPRI1
If mkstat : GERR = 1 : GZONE = "GROPRI" : GZNUM = 2541 : Endif
If !mkstat
 ## Action STD
 Call AM_GROPRI(VALEUR) From SUBPPRI
 If mkstat : GERR = 1 : GZONE = "GROPRI" : GZNUM = 2541 : Endif
Endif
Gosub _AFF
End

Subprog B1_GROPRI(VALEUR)
Variable Decimal VALEUR
If !GWEBSERV
 ## Action VISUPPV
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "VISUPPV"
 PARMSK(1) = date$
 PARMSK(2) = [M:PPRI]ITMREF
 PARMSK(3) = [M:PPRI]BPSNUM
 Call SAISIE_NUM(VALEUR,PARMSK,"IPPV","FUNACHPPV","XWIPPV") From GSAISIE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

