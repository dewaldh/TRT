#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management GDS1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone COD
mkstat = 0
zoncou = "COD"
indice = 0
If varmode([M]COD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]COD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 46 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "GDS"
  Local Char    VALEUR(250)
  VALEUR = [M]COD
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]COD = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 46 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DESTRA
mkstat = 0
zoncou = "DESTRA"
indice = 0
If varinit([M]DESTRA,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DESTRA,"GDUDSCR","DESTRA",[M]COD,"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DESTRA,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DESTRA" : Gosub ACTION From =IMPTRT
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
If varinit([M]ENAFLG,indice)=0
 IMPENT = 2
Endif
If varmode([M]ENAFLG,indice)=0
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
Endif
[M]ENAFLG = IMPENT

## Zone NBRLIG
mkstat = 0
zoncou = "NBRLIG"
indice = 0
If varmode([M]NBRLIG,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBRLIG([M]NBRLIG) From SUBGDS
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBRLIG : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If [M]NBRLIG=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "NBRLIG" : GZNUM = 769 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If [M]NBRLIG<0
    mkstat = 1 : GMESSAGE = mess(180,141,1)
   Endif
   If mkstat : GERR = 1 : GZONE = "NBRLIG" : GZNUM = 769 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone TOTDUD
mkstat = 0
zoncou = "TOTDUD"
indice = 0
IMPENT = [M]TOTDUD
If varinit([M]TOTDUD,indice)=0
 IMPENT = 1
Endif
If varmode([M]TOTDUD,indice)=0
 [M]TOTDUD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TOTDUD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TOTDUD(indice)
 Endif
Endif
[M]TOTDUD = IMPENT

## Zone NBRCOL
mkstat = 0
zoncou = "NBRCOL"
indice = 0
If varmode([M]NBRCOL,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRCOL" : Gosub ACTION From =IMPTRT
 Endif
 If [M]NBRCOL<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "NBRCOL" : GZNUM = 14372 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBRFLD
mkstat = 0
zoncou = "NBRFLD"
indice = 0
status = 1
zoncou = "NBRFLD"
If varmode([M]NBRFLD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRFLD" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRFLD
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRFLD = nolign
   status = 75
   Call SAI_NBRFLD(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBRFLD(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBRFLD
mkstat = 0
zoncou = "NBRFLD"

## Zone DAC
mkstat = 0
zoncou = "DAC"
WNUM = [M]DAC(indice)
If varmode([M]DAC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DAC" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_DAC([M]DAC(indice)) From SUBGDS
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DAC" : GZNUM = 5626 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>[M]DAC(indice) or IMPMOD
   SAVACT = GACTION : GACTION = "SPE"
   Call AM_DAC([M]DAC(indice)) From SPEGDS
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DAC" : GZNUM = 5626 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone SAI
mkstat = 0
zoncou = "SAI"
IMPENT = [M]SAI(indice)
If varinit([M]SAI,indice)=0
 IMPENT = 2
Endif
If varmode([M]SAI,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_SAI(IMPENT) From SUBGDS
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]SAI(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "SAI" : Gosub ACTION From =IMPTRT
  IMPENT = [M]SAI(indice)
 Endif
Endif
[M]SAI(indice) = IMPENT

## Zone NBRFLD
mkstat = 0
zoncou = "NBRFLD"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "GDS1"
Return

