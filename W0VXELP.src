## Zone EMPNUM
mkstat = 0
zoncou = "EMPNUM"
indice = 0
WNUM = [M]EMPNUM
If varmode([M]EMPNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "EMPNUM" : Gosub ACTION From =IMPTRT
 Endif
 If [M]EMPNUM=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "EMPNUM" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If [M]EMPNUM<0
   mkstat = 1 : GMESSAGE = mess(180,141,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "EMPNUM" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_EMPNUM([M]EMPNUM) From VXELP
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "EMPNUM" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WNUM<>[M]EMPNUM or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_EMPNUM([M]EMPNUM) From VXELP
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "EMPNUM" : GZNUM = 0 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

