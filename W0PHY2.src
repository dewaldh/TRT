# Supervisor
# Mask management PHY2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone COMMENT
mkstat = 0
zoncou = "COMMENT"
indice = 0
If varmode([M]COMMENT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COMMENT" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone PHOTO
mkstat = 0
zoncou = "PHOTO"
indice = 0
If varmode([M]PHOTO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PHOTO" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "PHY2"
Return

