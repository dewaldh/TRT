# Supervisor
# Mask management YSTA1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YESTID
mkstat = 0
zoncou = "YESTID"
indice = 0
If varmode([M]YESTID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YESTID" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YESTID=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YESTID" : GZNUM = 100372 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTNUM("YEST","",[M]YESTID,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YESTID" : GZNUM = 100372 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone YPREID
mkstat = 0
zoncou = "YPREID"
indice = 0
If varmode([M]YPREID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YPREID" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YPREID,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YPREID" : GZNUM = 100373 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("YPRE","",[M]YPREID,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YPREID" : GZNUM = 100373 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone YDEVNBRFLG
mkstat = 0
zoncou = "YDEVNBRFLG"
indice = 0
IMPENT = [M]YDEVNBRFLG
If varinit([M]YDEVNBRFLG,indice)=0
 IMPENT = 1
Endif
[M]YDEVNBRFLG = IMPENT

## Zone YPRJMANFLG
mkstat = 0
zoncou = "YPRJMANFLG"
indice = 0
IMPENT = [M]YPRJMANFLG
If varinit([M]YPRJMANFLG,indice)=0
 IMPENT = 1
Endif
[M]YPRJMANFLG = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

indice = 0
status = 1
zoncou = "NBLIGL"
If !mkstat
 NBLIG = [M]NBLIGL
 For nolign=1 To NBLIG
  indice = nolign-1
  [M]NBLIGL = nolign
  status = 75
  Call SAI_NBLIGL(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

mkstat = 0

End

####################################
## Bloc tableau 4
####################################
Subprog SAI_NBLIGL(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1



End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YSTA1"
Return

