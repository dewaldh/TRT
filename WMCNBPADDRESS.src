####################################################################
# AINIT
####################################################################

$AINIT
Gosub DECLARE_VAR From WMC0BPADDRESS
Local Integer ALINE,TBLINE,ASTASNAPSHOT,ALLSTAT
If this.APARENT=null & this.snapshotEnabled=0
 this.snapshotEnabled = 1
Elsif this.APARENT<>null
 this.snapshotEnabled = this.APARENT.snapshotEnabled
Endif
If this.ASTALIN=[V]CST_ALL : this.ASTALIN = [V]CST_ANEW : Endif
For TBLINE=1 to 5
 [L]ALINE = func ASYRSUBTAB.ADDLINE([L]TBLINE,"COLTEL","C_BPADDRESS_COLTEL",this)
 If [L]ALINE=[V]CST_ANOTDEFINED : [L]ASTATUS = [V]CST_AERROR : Break : Endif
Next
If [L]ASTATUS = [V]CST_AERROR :  Gosub TESTALLSTAT : Endif
For TBLINE=1 to 5
 [L]ALINE = func ASYRSUBTAB.ADDLINE([L]TBLINE,"COLWEB","C_BPADDRESS_COLWEB",this)
 If [L]ALINE=[V]CST_ANOTDEFINED : [L]ASTATUS = [V]CST_AERROR : Break : Endif
Next
If [L]ASTATUS = [V]CST_AERROR :  Gosub TESTALLSTAT : Endif
Gosub INIT_BPATYP From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_BPANUM From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_BPAADD From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_BPADES From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_BPABID From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_BPAADDFLG From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_BPAADDLIG1 From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_BPAADDLIG2 From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_BPAADDLIG3 From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_POSCOD From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_CTY From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_SAT From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_CRY From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_CRYNAM From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_FAX From WMC1BPADDRESS : Gosub TESTALLSTAT
Gosub INIT_MOB From WMC1BPADDRESS : Gosub TESTALLSTAT
