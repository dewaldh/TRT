# Supervisor
# Mask management YPRE4 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [13/11/2018]
A_HEURE = "15:52:29"
A_USER  = "MAJID"
A_STAMP = "20181113155229"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIG"
  If [M]NBLIG=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask YPRE4 [YPRE4]
Gosub HORODAT
Local Inpbox "YPRE4" From GFONCTION At A_STAMP With A_WINDPREV Mask [YPRE4] As [SIM_]
Grizo [YPRE4]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [YPRE4]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=25
Return

$ON_YBP
Onevent CONTROL Call C_YBP(zc) From W1YPRE4
Onevent BEFORE_INPUT Call AS_YBP(zc) From W1YPRE4
Return

Subprog AS_YBP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_YBP(zc) From W1YPRE4
Onkey 144 Titled GBOUTP Call P_YBP(zc) From W1YPRE4
Onkey 146 Titled GBOUTA Call SA_YBP(zc) From W1YPRE4
Gosub _AFF
End

Subprog C_YBP(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YBP" : GZNUM = 19569 : Endif
If !GWEBSERV
 If !mkstat
  ## Action BPRCHOICE
  Local Char    PARMSK(250)(1..20) , SAVACT(20)
  SAVACT = GACTION : GACTION = "BPRCHOICE"
  Call S_TAB_CHAR(VALEUR,PARMSK,"BPRCHOICE","TRTSEL","") From GACTION
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "YBP" : GZNUM = 19569 : Endif
 Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("BPR","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YBP" : GZNUM = 19569 : Endif
Endif
Gosub _AFF
End

Subprog S_YBP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "BPR"
PARMSK(2) = ""
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_YBP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "BPR"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YBP(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("BPR",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_YBPCLS
Onevent INIT_AREA Call D_YBPCLS(zc) From W1YPRE4
Onevent BEFORE_INPUT Call AS_YBPCLS(zc) From W1YPRE4
Onevent CONTROL Call C_YBPCLS(zc) From W1YPRE4
Return

Subprog D_YBPCLS(VALEUR)
Variable Integer VALEUR
If varinit([M]YBPCLS,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YBPCLS(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog C_YBPCLS(VALEUR)
Variable Integer VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YBPCLS" : GZNUM = 5887 : Endif
Gosub _AFF
End

$ON_YVALFRM
Onevent CONTROL Call C_YVALFRM(zc) From W1YPRE4
Onevent BEFORE_AREA Gosub AV_YVALFRM From W1YPRE4
Return

$AV_YVALFRM
Chgfmt [M]YVALFRM(indice) With "D:"+GFMDAT
Gosub _AFF
Return

Subprog C_YVALFRM(VALEUR)
Variable Date    VALEUR
## Action %OBLIG
If VALEUR=[0/0/0]
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YVALFRM" : GZNUM = 1536 : Endif
Gosub _AFF
End

$ON_YVALTO
Onevent BEFORE_AREA Gosub AV_YVALTO From W1YPRE4
Return

$AV_YVALTO
Chgfmt [M]YVALTO(indice) With "DZ:"+GFMDAT
Gosub _AFF
Return

$CHG_STY
Return


