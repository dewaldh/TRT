############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(COMPANYM,ASTATUS)
Variable Instance COMPANYM Using R_COMPANYM
Variable Integer ASTATUS

If COMPANYM = null : End : Endif

Gosub AVARIABLES From WMR00COMPANYM
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If fmet COMPANYM.ACTX.AGETAFCRIGHT(COMPANYM,"GESCPY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END : Endif
If fmet COMPANYM.ACTX.AGETAFCRIGHTC(COMPANYM,"GESCPY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END
Endif

Gosub AREPCONTROL_AINSERT
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If !adxlog
 Gosub AINSERT_TR_COMPANYM
Else
 Gosub AINSERT_COMPANYM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_END : Return : Endif

Gosub _AINSERT_AFTER
$AINSERT_END
[L]A_I = COMPANYM.freeSnapshot
COMPANYM.snapshotEnabled = 1
End

#-----------------------------------------------------#
$AREPCONTROL_AINSERT
COMPANYM.ASTALIN = [V]CST_ANEW
COMPANYM.CPY.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet COMPANYM.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet COMPANYM.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet COMPANYM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet COMPANYM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet COMPANYM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If COMPANYM.CPY.ASTALIN = [V]CST_ANEWDEL : Return : Endif
COMPANYM.CPY.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet COMPANYM.CPY.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(COMPANYM.CPY.BPA) : # BPADDRESS -- 2
 If COMPANYM.CPY.BPA(A_N2) <> null
& and !find(COMPANYM.CPY.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(COMPANYM.CPY.BID) : # BID -- 3
 If COMPANYM.CPY.BID(A_N2) <> null
& and !find(COMPANYM.CPY.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
For A_N2=1 To maxtab(COMPANYM.CPY.BPA) : # BPADDRESS -- 2
 If COMPANYM.CPY.BPA(A_N2) <> null
& and !find(COMPANYM.CPY.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(COMPANYM.CPY.BID) : # BID -- 3
 If COMPANYM.CPY.BID(A_N2) <> null
& and !find(COMPANYM.CPY.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet COMPANYM.CPY.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_COMPANYM
Trbegin [F:CPY]
 Gosub AINSERT_COMPANYM
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AINSERT_AFTER
If find(COMPANYM.CPY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 COMPANYM.CPY.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(COMPANYM.CPY.BPA)
 If COMPANYM.CPY.BPA(A_N2) <> null
  [L]ASTATUS2 = fmet COMPANYM.CPY.BPA(A_N2)._AINSERT_AFTER
  If COMPANYM.CPY.BPA(A_N2).ASTALIN=[V]CST_ADEL or COMPANYM.CPY.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance COMPANYM.CPY.BPA(A_N2)
   COMPANYM.CPY.BPA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(COMPANYM.CPY.BID)
 If COMPANYM.CPY.BID(A_N2) <> null
  [L]ASTATUS2 = fmet COMPANYM.CPY.BID(A_N2)._AINSERT_AFTER
  If COMPANYM.CPY.BID(A_N2).ASTALIN=[V]CST_ADEL or COMPANYM.CPY.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance COMPANYM.CPY.BID(A_N2)
   COMPANYM.CPY.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AINSERT_COMPANYM
[L]ASTATUS = max([L]ASTATUS,fmet COMPANYM.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet COMPANYM.CPY._AINSERT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet COMPANYM.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If COMPANYM.snapshotEnabled=1
 [L]A_RETIK = COMPANYM.revertUpdTick
Endif
[L]ASTATUS = max(fmet COMPANYM.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



