Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBRIB
mkstat = 0
zoncou = "NBRIB"
indice = 0
status = 1
zoncou = "NBRIB"
If varmode([M]NBRIB,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBRIB From SUBBID
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBRIB : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRIB" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRIB
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRIB = nolign
   status = 75
   Call SAI_NBRIB(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

mkstat = 0

End

