           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "REP.REPNUM"
NBPRO += 1 : TBPRO(NBPRO) = "REPNAM" : TBFIL(NBPRO) = "[F:REP]REPNAM"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "REP.REPNAM"
