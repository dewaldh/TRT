Onevent AFTER_CHANGE Gosub AMM_TYPFIL From W1FICMAG
Return

Subprog D_TYPFIL(VALEUR)
Variable Integer VALEUR
If varinit([M]TYPFIL,indice)=0
 VALEUR = 1
 ## Action STD
 Call D_TYPFIL(VALEUR) From FICMAG
Endif
Gosub _AFF
End

Subprog AS_TYPFIL(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action STD
Call AS_TYPFIL(VALEUR) From FICMAG
If mkstat=2
 [M]TYPFIL(INDIC) = VALEUR
 Affzo TYPFIL(INDIC) : mkstat = 2
Endif
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$AMM_TYPFIL
