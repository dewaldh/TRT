 SAVACT = GACTION : GACTION = "ASELFILFOL"
 PARMSK(1) = "2"
 PARMSK(2) = "1"
 PARMSK(3) = ""
 Call TRAITE_CHAR(VALEUR,PARMSK,"","ASELHIERA","ASELHIERA",3,"XWASELHIERA") From GTRAITE
 GACTION = SAVACT
Endif
Gosub _AFF
End

