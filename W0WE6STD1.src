   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "DIVLINCONT"
    Call INSERE("PTDLIN") From TABLEAUX
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
