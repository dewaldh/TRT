#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management YELA0 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [03/01/2019]
A_HEURE = "10:42:38"
A_USER  = "DH"
A_STAMP = "20190103104238"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask YELA0 [YELA0]
Gosub HORODAT
Local Inpbox "YELA0" From GFONCTION At A_STAMP With A_WINDPREV Mask [YELA0] As [SIM_]
Grizo [YELA0]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [YELA0]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=5
Return

$ON_YLONAGR
Onevent BEFORE_INPUT Call AS_YLONAGR(zc) From W1YELA0
Onevent CONTROL Call C_YLONAGR(zc) From W1YELA0
Return

Subprog AS_YLONAGR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_YLONAGR(zc) From W1YELA0
Onkey 144 Titled GBOUTP Call P_YLONAGR(zc) From W1YELA0
Onkey 146 Titled GBOUTA Call SA_YLONAGR(zc) From W1YELA0
Gosub _AFF
End

Subprog C_YLONAGR(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("YELA","!$",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "YLONAGR" : GZNUM = 100118 : Endif
Gosub _AFF
End

Subprog S_YLONAGR(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action YELASEL
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "YELASEL"
 PARMSK(1) = ""
 Call S_LIS_CHAR(VALEUR,PARMSK,"YELASEL","","YSELELA") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog SA_YLONAGR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "YELA"
PARMSK(3) = "$"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YLONAGR(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("YELA",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_YENAFLG
Onevent INIT_AREA Call D_YENAFLG(zc) From W1YELA0
Onevent BEFORE_INPUT Call AS_YENAFLG(zc) From W1YELA0
Return

Subprog D_YENAFLG(VALEUR)
Variable Integer VALEUR
If varinit([M]YENAFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YENAFLG(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

