#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management LETTCRIT2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone FLD
mkstat = 0
zoncou = "FLD"
indice = 0
IMPENT = [M]FLD
If varinit([M]FLD,indice)=0
 IMPENT = 1
Endif
If varmode([M]FLD,indice)=0
 [M]FLD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLD(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_FLD(IMPENT) From LETTCRIT
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "FLD" : GZNUM = 10035 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]FLD = IMPENT

## Zone LEG
mkstat = 0
zoncou = "LEG"
indice = 0
If varinit([M]LEG,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]LEG,"909","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]LEG,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "LEG" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]LEG,"909","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!",[M]LEG,"909",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_LEG([M]LEG) From LETTCRIT
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "LEG" : GZNUM = 26213 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTCPY"
 Call CONTSOC([M]CPY) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("CPY","!A",[M]CPY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_CPY([M]CPY) From LETTCRIT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]FCY,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]FCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","C",[M]FCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "LETTCRIT2"
Return

