$ON_YTYP
Onevent INIT_AREA Call D_YTYP(zc) From W1YSLC
Onevent CONTROL Call C_YTYP(zc) From W1YSLC
Onevent BEFORE_INPUT Call AS_YTYP(zc) From W1YSLC
Return

Subprog D_YTYP(VALEUR)
Variable Char    VALEUR()
If varinit([M]YTYP,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"6006","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_YTYP(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"6006") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YTYP(zc) From W1YSLC
Onkey   2 Titled GBOUTS Call S_YTYP(zc) From W1YSLC
Onkey 144 Titled GBOUTP Call P_YTYP(zc) From W1YSLC
Onkey 146 Titled GBOUTA Call SA_YTYP(zc) From W1YSLC
If VALEUR="" : Call INIADI(VALEUR,"6006") From TRTDIV : Endif
Gosub _AFF
End

Subprog C_YTYP(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YTYP" : GZNUM = 680 : Endif
If !mkstat
 ## Action CONTADI
 Call CONTADI(VALEUR,"6006","") From SUBADI
 If mkstat : GERR = 1 : GZONE = "YTYP" : GZNUM = 680 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ADI","",VALEUR,"6006",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YTYP" : GZNUM = 680 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_YTYP(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = "6006"
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_YTYP(VALEUR)
Variable Char    VALEUR()
Call OBJET("ADI",VALEUR,"6006") From GOBJET
Gosub _AFF
End

Subprog SA_YTYP(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(2) = "6006"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YTYP(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "6006"
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

