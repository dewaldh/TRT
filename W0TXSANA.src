# Supervisor
# Mask management TXSANA 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone L1
mkstat = 0
zoncou = "L1"
indice = 0
If varmode([M]L1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "L1" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone L2
mkstat = 0
zoncou = "L2"
indice = 0
If varmode([M]L2,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "L2" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone L3
mkstat = 0
zoncou = "L3"
indice = 0
If varmode([M]L3,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "L3" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone L4
mkstat = 0
zoncou = "L4"
indice = 0
If varmode([M]L4,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "L4" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone C1
mkstat = 0
zoncou = "C1"
indice = 0
If varmode([M]C1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "C1" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone C2
mkstat = 0
zoncou = "C2"
indice = 0
If varmode([M]C2,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "C2" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone C3
mkstat = 0
zoncou = "C3"
indice = 0
If varmode([M]C3,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "C3" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone C4
mkstat = 0
zoncou = "C4"
indice = 0
If varmode([M]C4,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "C4" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone FRMX
mkstat = 0
zoncou = "FRMX"
indice = 0
If varmode([M]FRMX,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FRMX" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBLIG From TXSANA
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBLIG : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone T1
mkstat = 0
zoncou = "T1"
indice = 0
IMPENT = [M]T1
If varinit([M]T1,indice)=0
 IMPENT = 1
Endif
If varmode([M]T1,indice)=0
 Call INICOMBO(2634) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_T1(IMPENT) From TXSANA
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2634,1))))
 Endif
 [M]T1(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "T1" : Gosub ACTION From =IMPTRT
  IMPENT = [M]T1(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "T1" : GZNUM = 6521 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]T1 = IMPENT

## Zone T2
mkstat = 0
zoncou = "T2"
indice = 0
IMPENT = [M]T2
If varinit([M]T2,indice)=0
 IMPENT = 1
Endif
If varmode([M]T2,indice)=0
 Call INICOMBO(2634) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_T2(IMPENT) From TXSANA
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2634,1))))
 Endif
 [M]T2(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "T2" : Gosub ACTION From =IMPTRT
  IMPENT = [M]T2(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "T2" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]T2 = IMPENT

## Zone T3
mkstat = 0
zoncou = "T3"
indice = 0
IMPENT = [M]T3
If varinit([M]T3,indice)=0
 IMPENT = 1
Endif
If varmode([M]T3,indice)=0
 Call INICOMBO(2634) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_T3(IMPENT) From TXSANA
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2634,1))))
 Endif
 [M]T3(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "T3" : Gosub ACTION From =IMPTRT
  IMPENT = [M]T3(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "T3" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]T3 = IMPENT

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone FRM0
mkstat = 0
zoncou = "FRM0"
If varmode([M]FRM0,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FRM0([M]FRM0(indice)) From TXSANA
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FRM0(indice) : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "FRM0" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone AMTVAL
mkstat = 0
zoncou = "AMTVAL"
If varmode([M]AMTVAL,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_AMTVAL([M]AMTVAL(indice)) From TXSANA
 GACTION = SAVACT
 If mkstat=1 : Raz [M]AMTVAL(indice) : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "AMTVAL" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NAT
mkstat = 0
zoncou = "NAT"
SAVACT = GACTION : GACTION = "INIACCFMT"
Call INIACCFMT("") From SETGLBCOA
GACTION = SAVACT
If mkstat=1 : Raz [M]NAT(indice) : Endif

## Zone SEC
mkstat = 0
zoncou = "SEC"
SAVACT = GACTION : GACTION = "INICCEFMT"
Call INICCEFMT("") From SETGLBDIE
GACTION = SAVACT
If mkstat=1 : Raz [M]SEC(indice) : Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "TXSANA"
Return

