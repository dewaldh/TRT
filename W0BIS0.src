Endif

## Zone INVTYP
mkstat = 0
zoncou = "INVTYP"
indice = 0
IMPENT = [M]INVTYP
If varinit([M]INVTYP,indice)=0
 IMPENT = 1
Endif
[M]INVTYP = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone BPAINV
mkstat = 0
zoncou = "BPAINV"
indice = 0
WCHAR = [M]BPAINV
If varinit([M]BPAINV,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_BPAINV([M]BPAINV) From SUBBIS
 GACTION = SAVACT
Endif

## Zone BPAADDLIG
mkstat = 0
zoncou = "BPAADDLIG"
For indice=0 To 2
 SAVACT = GACTION : GACTION = "FMTADRPAY"
 Call FMTADRPAY(CRY) From TRTDIV
 GACTION = SAVACT
 If mkstat=1 : Raz [M]BPAADDLIG(indice) : Endif
 mkstat = 0
Next indice
If mkstat : End : Endif

## Zone POSCOD
mkstat = 0
zoncou = "POSCOD"
indice = 0
WCHAR = [M]POSCOD
