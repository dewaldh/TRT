   If mkstat : GERR = 1 : GZONE = "BPR" : GZNUM = 3680 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_BPR([M]BPR) From SUBBIS
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "BPR" : GZNUM = 3680 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTBPRCPY"
     Call CONTBPRCPY([M]BPR,[M]FCY,"",[M]BPRSAC,"F") From CPTSUB
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "BPR" : GZNUM = 3680 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
     If !mkstat
      SAVACT = GACTION : GACTION = "STD"
      Call AP_BPR([M]BPR) From SUBBIS
      GACTION = SAVACT
      If mkstat : GERR = 1 : GZONE = "BPR" : GZNUM = 3680 : Endif
      Gosub AFF From GSAISIE
      If mkstat : End : Endif
      If !mkstat
       If WCHAR<>[M]BPR or IMPMOD
        SAVACT = GACTION : GACTION = "STD"
        Call AM_BPR([M]BPR) From SUBBIS
        GACTION = SAVACT
        If mkstat : GERR = 1 : GZONE = "BPR" : GZNUM = 3680 : Endif
        Gosub AFF From GSAISIE
        If mkstat : End : Endif
        If !mkstat
         SAVACT = GACTION : GACTION = "SPE"
         Call AM_BPR([M]BPR) From SPEBIS
         GACTION = SAVACT
         If mkstat : GERR = 1 : GZONE = "BPR" : GZNUM = 3680 : Endif
         Gosub AFF From GSAISIE
         If mkstat : End : Endif
        Endif
       Endif
      Endif
     Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone PIVTYP
