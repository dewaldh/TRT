$AV_STRDAT
## Action ADATRELI
Call INIDAT(zc,[M]DATFRMD) From ADATREL
Chgfmt [M]STRDAT(indice) With "DzZ:"+GFMDAT
Gosub _AFF
Return

Subprog D_STRDAT(VALEUR)
Variable Date    VALEUR
If varinit([M]STRDAT,indice)=0
 ## Action STD
 Call D_STRDAT(VALEUR) From DCLESD
Endif
Gosub _AFF
End

Subprog AS_STRDAT(VALEUR)
Variable Date    VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(32514,GBOUT1) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_STRDAT(zc) From W1DCLESD
Gosub _AFF
End

Subprog C_STRDAT(VALEUR)
Variable Date    VALEUR
## Action %OBLIG
If VALEUR=[0/0/0]
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 9938 : Endif
If !mkstat
 ## Action STD
 Call C_STRDAT(VALEUR) From DCLESD
 If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 9938 : Endif
Endif
Gosub _AFF
End

$AMM_STRDAT
Call AM_STRDAT(zc)
If !mkstat & GOBJET
 zoncou = "STRDAT" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_STRDAT(VALEUR)
Variable Date    VALEUR
## Action ADATRELM
Call MODIFDAT(VALEUR,[M]DATFRMD) From ADATREL
If mkstat : GERR = 1 : GZONE = "STRDAT" : GZNUM = 9938 : Endif
Gosub _AFF
End

Subprog B1_STRDAT(VALEUR)
Variable Date    VALEUR
If !GWEBSERV
 ## Action ADATREL
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ADATREL"
 PARMSK(1) = [M]DATFRMD
 Call TRAITE_DAT(VALEUR,PARMSK,"","ADATREL","ADATREL",3,"") From GTRAITE
 [M]DATFRMD = PARMSK(1)
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

