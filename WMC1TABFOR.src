$LNKPRO_FORCOD
Gosub DECLARE_VAR From WMC0TABFOR
[L]CURPRO = "FORCOD"
$_LNKPRO_FORCOD
If vireblc(this.FORCOD,2)<>""
 # Link TABFOR
 Local File TABFOR [WTFO]
 Read [F:WTFO]TFO0 = this.FORTYP;this.FORCOD
 If !fstat
  Local File ATEXTRA [WAXX]
  Local Char ILAN(GLONLAN)
  For ILAN=this.ACTX.LAN, this.ACTX.AFOLD.ALANGDEF
   For [F:WAXX] Where CODFIC="TABFOR" & ZONE="DESSHO" & IDENT1=[F:WTFO]FORNUM & IDENT2=[F:WTFO]FORCOD & LANGUE = ILAN
    [L]ASTATUS = fmet this.ASETATTRIBUTE("FORCOD","$title",[WAXX]TEXTE)
    Break 2
   Next WAXX
  Next ILAN
  LogicClose File [WAXX]
  Local File ATEXTRA [WAXX]
  Local Char ILAN(GLONLAN)
  For ILAN=this.ACTX.LAN, this.ACTX.AFOLD.ALANGDEF
   For [F:WAXX] Where CODFIC="TABFOR" & ZONE="DES" & IDENT1=[F:WTFO]FORNUM & IDENT2=[F:WTFO]FORCOD & LANGUE = ILAN
    [L]ASTATUS = fmet this.ASETATTRIBUTE("FORCOD","$description",[WAXX]TEXTE)
    Break 2
   Next WAXX
  Next ILAN
  LogicClose File [WAXX]
 Endif
 LogicClose File [WTFO]
Endif
Return

$_ALOADATTRIBUTE_FORCOD
[L]ASTATUS = fmet this.ASETATTRIBUTE("FORCOD","$isMandatory","true")
Return


