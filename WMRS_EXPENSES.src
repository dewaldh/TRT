# Supervisor
## ACLA-CLASSE ## EXPENSES
# 2018-08-27 13:14:36 530

#-------------------------------------------------------#
$METHODS
$EVENTS
Case [L]AEVENT
 When "AREAD_AFTER"
  Case [L]CURPTH
   When "EXS" : Gosub ADDACS1
   When "EXS.CODEXPENS" : Gosub ADDACS2
   When "EXS.DIMCEN" : Gosub ADDACS3
   When "EXS.LNKRATLED" : Gosub ADDACS4
   When "EXS.RATEXPENS" : Gosub ADDACS5
   When "" : Gosub ADDACSREP
  Endcase
 When "AINIT"
  Case [L]CURPTH
   When "EXS" : Gosub ADDACS1
   When "EXS.CODEXPENS" : Gosub ADDACS2
   When "EXS.DIMCEN" : Gosub ADDACS3
   When "EXS.LNKRATLED" : Gosub ADDACS4
   When "EXS.RATEXPENS" : Gosub ADDACS5
   When "" : Gosub ADDACSREP
  Endcase
 When Default
Endcase
Return

#-------------------------------------------------------#
$PROPERTIES
Case [L]ARULE
 When "CONTROL"
  Case [L]CURPRO
   When "EXS.CPY"
    If this.CPY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTCPY(this,"GESEXS",this.CPY,[V]CST_ATRUE)
    Endif
   When "EXS.FCY"
    If this.FCY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTFCY(this,"GESEXS",this.FCY,[V]CST_ATRUE)
    Endif
   When Default
  Endcase
 When "PROPAGATE"
  Case [L]CURPRO
   When Default
  Endcase
 Endcase
 Return
 
 #-------------------------------------------------------#
 # EXS
 $ADDACS1
 Return
 
 # EXS.CODEXPENS
 $ADDACS2
 Return
 
 # EXS.DIMCEN
 $ADDACS3
 Return
 
 # EXS.LNKRATLED
 $ADDACS4
 Return
 
 # EXS.RATEXPENS
 $ADDACS5
 Return
 
 $ADDACSREP
 Return
 


