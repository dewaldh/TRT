# Supervisor
# Mask management CSTPOH 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBCST
mkstat = 0
zoncou = "NBCST"
indice = 0
status = 1
zoncou = "NBCST"
If varmode([M]NBCST,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBCST" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBCST
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBCST = nolign
   status = 75
   Call SAI_NBCST(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone STCD
mkstat = 0
zoncou = "STCD"
indice = 0
If varinit([M]STCD,indice)=0
 [M]STCD = "21"
Endif
If varmode([M]STCD,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_STCD([M]STCD) From TRTSTCDOC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]STCD : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "STCD" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone WVCRTYP
mkstat = 0
zoncou = "WVCRTYP"
indice = 0
IMPENT = [M]WVCRTYP
If varinit([M]WVCRTYP,indice)=0
 IMPENT = 1
Endif
[M]WVCRTYP = IMPENT

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBCST(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBCST
mkstat = 0
zoncou = "NBCST"

## Zone TOTDOC
mkstat = 0
zoncou = "TOTDOC"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find(CURDOC,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]TOTDOC(indice) : Endif

## Zone TOTSOC
mkstat = 0
zoncou = "TOTSOC"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find(CURSOC,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]TOTSOC(indice) : Endif

## Zone NBCST
mkstat = 0
zoncou = "NBCST"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CSTPOH"
Return

