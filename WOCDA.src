$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(5)(0..GNBDERNIER)
Local Char     A_BUF2(80)(0..GNBDERNIER)
Local Char     A_BUF3(5)(0..GNBDERNIER)
Local Char     A_BUF4(30)(0..GNBDERNIER)
If clalev([F:CDA9])>0 : Close Local File [CDA9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(5),
& Char     A_TAB2(80),
& Char     A_TAB3(5),
& Char     A_TAB4(30)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4
& As [CDA9]
Return

$LEC_DERLU
Read [ACC]CODACC = GUSER ; [F:CDA]ACS
If fstat : Raz [F:ACC] : Endif
Read [AXX]AXX0="GACCDENCOD";"DESTRA";GLANGUE;[F:CDA]COD;[F:CDA]LEG
If fstat : Raz [F:AXX] : Endif
Read [AX3]AXX0="GACCDENCOD";"SHOTRA";GLANGUE;COD;LEG
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:CDA9])>0
 Read [CDA9] = [F:CDA9]A_TABI
 [L]CLE = [F:CDA9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:CDA9])>0
 Filter [CDA9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [CDA1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:CDA9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:CDA]LEG+"~"
 A_CLEDERLU += [F:CDA]COD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:CDA9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:CDA9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 0
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:CDA]COD"
Call TEXTE(5296,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:5X"
SELFOR(NBSEL) = "K:5c"
SELMAJ(NBSEL) = 1
NBTRI += 1
TRIZON(NBTRI) = "[F:CDA]COD"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:AXX]TEXTE"
Call TEXTE(8,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:80X"
SELFOR(NBSEL) = "K:80X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:AXX]TEXTE"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(3)"
SELEXP(NBSEL) = "string$([F:CDA]LEG<>'',[F:CDA]LEG)+string$([F:CDA]LEG='',mess(91,187,1))"
Call TEXTE(26213,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:5X"
SELFOR(NBSEL) = "K:5X"
SELMAJ(NBSEL) = 0
NBSEL += 1
SELZON(NBSEL) = "[L]SAIRAP(4)"
SELEXP(NBSEL) = "string$([F:CDA]GFY<>'',[F:CDA]GFY)+string$([F:CDA]GFY='',mess(220,117,1))"
Call TEXTE(616,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = ""
SELMAJ(NBSEL) = 0
If clalev([F:CDA_])=0
 Gosub OUVFIC_LISTE
Endif
Return

