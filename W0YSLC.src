#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management YSLC 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YSLCID
mkstat = 0
zoncou = "YSLCID"
indice = 0
If varmode([M]YSLCID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSLCID" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YSLCID,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YSLCID" : GZNUM = 100472 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YLOT
mkstat = 0
zoncou = "YLOT"
indice = 0
If varmode([M]YLOT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLOT" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YLOT,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YLOT" : GZNUM = 2721 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YTYP
mkstat = 0
zoncou = "YTYP"
indice = 0
If varinit([M]YTYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]YTYP,"6006","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]YTYP,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "YTYP" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]YTYP,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "YTYP" : GZNUM = 680 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CONTADI"
   Call CONTADI([M]YTYP,"6006","") From SUBADI
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "YTYP" : GZNUM = 680 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("ADI","",[M]YTYP,"6006",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "YTYP" : GZNUM = 680 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone YSALSTA
mkstat = 0
zoncou = "YSALSTA"
indice = 0
IMPENT = [M]YSALSTA
If varinit([M]YSALSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YSALSTA,indice)=0
 [M]YSALSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSALSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YSALSTA(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YSALSTA" : GZNUM = 100199 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YSALSTA = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YSLC"
Return

