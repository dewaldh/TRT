#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management YGSC1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [23/09/2018]
A_HEURE = "03:39:01"
A_USER  = "MB"
A_STAMP = "20180923033901"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask YGSC1 [YGSC1]
Gosub HORODAT
Local Inpbox "YGSC1" From GFONCTION At A_STAMP With A_WINDPREV Mask [YGSC1] As [SIM_]
Grizo [YGSC1]5
Grizo [YGSC1]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [YGSC1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=3
X1(1)=1 : X2(1)=74 : Y1(1)=4 : Y2(1)=25
Return

$ON_YESTID
Onevent CONTROL Call C_YESTID(zc) From W1YGSC1
Return

Subprog C_YESTID(VALEUR)
Variable Decimal VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YESTID" : GZNUM = 100395 : Endif
Gosub _AFF
End

$ON_YPREID
Onevent CONTROL Call C_YPREID(zc) From W1YGSC1
Return

Subprog C_YPREID(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YPREID" : GZNUM = 100373 : Endif
Gosub _AFF
End

$ON_YDEVFLG
Onevent INIT_AREA Call D_YDEVFLG(zc) From W1YGSC1
Onevent BEFORE_INPUT Call AS_YDEVFLG(zc) From W1YGSC1
Return

Subprog D_YDEVFLG(VALEUR)
Variable Integer VALEUR
If varinit([M]YDEVFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YDEVFLG(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$ON_YPRJFLG
Onevent INIT_AREA Call D_YPRJFLG(zc) From W1YGSC1
Onevent BEFORE_INPUT Call AS_YPRJFLG(zc) From W1YGSC1
Return

Subprog D_YPRJFLG(VALEUR)
Variable Integer VALEUR
If varinit([M]YPRJFLG,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YPRJFLG(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

$CHG_STY
Return


