####################################################################
# Labels of the main class and methods 
####################################################################

####################################################################
# 1 : BPINAM Bill-to customer name ( Field NAM 35)
####################################################################

$SET_BPINAM
If [L]_AOLDVAL<>this.BPINAM
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_BPINAM
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.BPINAM
 Gosub PROPAGATE_BPINAM
Endif
Return

$INIT_BPINAM
Gosub DECLARE_VAR From WMC0VSORDER_L_BPINAM
[L]CURPRO = "BPINAM"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_BPINAM
Gosub DECLARE_VAR From WMC0VSORDER_L_BPINAM
[L]CURPRO = "BPINAM"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_BPINAM                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_BPINAM                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_BPINAM
[L]CURPRO = "BPINAM"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.BPINAM,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_BPINAM
[L]CURPRO = "BPINAM"
If vireblc(this.BPINAM,2)<>vireblc(format$("K:35X",this.BPINAM),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_BPINAM
[L]CURPRO = "BPINAM"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_BPINAM
[L]CURPRO = "BPINAM"
Return

$PROPAGATE_BPINAM
this.isReadonly = 0
[L]CURPRO = "BPINAM"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_BPINAM
Gosub DECLARE_VAR From WMC0VSORDER_L_BPINAM
[L]CURPRO = "BPINAM"
$_LNKPRO_BPINAM
Return




