# Supervisor
# Mask management SPC3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBREM
mkstat = 0
zoncou = "NBREM"
indice = 0
status = 1
zoncou = "NBREM"
If varmode([M]NBREM,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBREM From SUBSPC
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBREM : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBREM" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBREM
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBREM = nolign
   status = 75
   Call SAI_NBREM(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBREM(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBREM
mkstat = 0
zoncou = "NBREM"

## Zone COLDES
mkstat = 0
zoncou = "COLDES"
[M]COLDES(indice) = GLIBDES(nolign-1)

## Zone DISCRGPRO
mkstat = 0
zoncou = "DISCRGPRO"
IMPENT = [M]DISCRGPRO(indice)
If varinit([M]DISCRGPRO,indice)=0
 IMPENT = 1
Endif
If varmode([M]DISCRGPRO,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_DISCRGPRO(IMPENT) From SUBSPC
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]DISCRGPRO(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DISCRGPRO" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DISCRGPRO(indice)
 Endif
 If !mkstat
  If IMPENT=0
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "DISCRGPRO" : GZNUM = 4007 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]DISCRGPRO(indice) = IMPENT

## Zone NBREM
mkstat = 0
zoncou = "NBREM"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SPC3"
Return

