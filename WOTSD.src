Endif
Return

$REMP_DERLU
If clalev([F:TSD9])>0
 Filter [TSD9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [TSD1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:TSD9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:TSD]LEG+"~"
 A_CLEDERLU += [F:TSD]SDHTYP+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:TSD9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:TSD9]
 Endif
Endif
Return

