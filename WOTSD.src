$LEC_DERLU
Read [AXX]AXX0="TABSDHTYP";"DESAXX";GLANGUE;[F:TSD]SDHTYP;[F:TSD]LEG
If fstat : Raz [F:AXX] : Endif
Read [AX3]AXX0="TABSDHTYP";"SHOAXX";GLANGUE;[F:TSD]SDHTYP;[F:TSD]LEG
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:TSD9])>0
 Read [TSD9] = [F:TSD9]A_TABI
