$ON_MFCDET
Onevent BEFORE_INPUT Call AS_MFCDET(zc) From W1MFC2
Onevent INIT_AREA Call D_MFCDET(zc) From W1MFC2
Return

Subprog D_MFCDET(VALEUR)
Variable Char    VALEUR()
If varinit([M]MFCDET,indice)=0
 VALEUR = "80"
Endif
Gosub _AFF
End

Subprog AS_MFCDET(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(22249,GBOUTI) From OBJDIV
Onkey 178 Titled GBOUTI Call CL_MFCDET(zc) From W1MFC2
Gosub _AFF
End

Subprog CL_MFCDET(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SAIDECPRI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SAIDECPRI"
 PARMSK(1) = "MFC"
 PARMSK(2) = "0"
 PARMSK(3) = "0"
 PARMSK(4) = "0"
 PARMSK(5) = ""
 PARMSK(6) = "2"
 PARMSK(7) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"ISAIDECPRI","SUBSDP","SPESDP") From GSAISIE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

