#-------------------------------------------------------#
$METHODS
$EVENTS
Case [L]AEVENT
 When "ADDLINE_AFTER"
  Case [L]CURPTH
   When "YSRT"
    [L]ASTATUS = fmet this.ASETATTRIBUTE("YRULTYPCOD","$namProParent","YRULTYPCOD")
    this.YRULTYPCOD = this.APARENT.YRULTYPCOD
   When Default
  Endcase
 When Default
Endcase
Return

#-------------------------------------------------------#
$PROPERTIES
Local Integer ALINE
Case [L]CURPRO
 When "YRULTYPCOD" : Gosub PRO_YRULTYPCOD
Endcase
Return

#-------------------------------------------------------#
$PRO_YRULTYPCOD
Case [L]ARULE
 When "PROPAGATE"
  For ALINE=1 to maxtab(this.YSRT)
   If this.YSRT([L]ALINE)<>null
    this.YSRT([L]ALINE).YRULTYPCOD = this.YRULTYPCOD
   Endif
  Next
Endcase
Return



