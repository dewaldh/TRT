 Local Char    OBJCLE(30)
 OBJCLE = [M:SMV2]NUMVCR(nolign-1)
 Call OBJET_CHAR(OBJCLE,"GAS",[M:SMV2]GTE(nolign-1)) From GOBJET
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
