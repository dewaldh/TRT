# Supervisor
# Mask management YBUSBAT0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YBATFCY
mkstat = 0
zoncou = "YBATFCY"
indice = 0
If varmode([M]YBATFCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBATFCY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YBATFCY,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YBATFCY" : GZNUM = 1344 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]YBATFCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "YBATFCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","",[M]YBATFCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "YBATFCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "SPE"
    Call C_YBATFCY([M]YBATFCY) From SPEYBB
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "YBATFCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone YBATNBR
mkstat = 0
zoncou = "YBATNBR"
indice = 0
If varmode([M]YBATNBR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBATNBR" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YBATDES
mkstat = 0
zoncou = "YBATDES"
indice = 0
If varmode([M]YBATDES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBATDES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YBATDAT
mkstat = 0
zoncou = "YBATDAT"
indice = 0
If varmode([M]YBATDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBATDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YBATREF
mkstat = 0
zoncou = "YBATREF"
indice = 0
If varmode([M]YBATREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBATREF" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YBATSTA
mkstat = 0
zoncou = "YBATSTA"
indice = 0
IMPENT = [M]YBATSTA
If varinit([M]YBATSTA,indice)=0
 IMPENT = 1
Endif
[M]YBATSTA = IMPENT

## Zone YBATCAT
mkstat = 0
zoncou = "YBATCAT"
indice = 0
IMPENT = [M]YBATCAT
If varinit([M]YBATCAT,indice)=0
 IMPENT = 1
Endif
If varmode([M]YBATCAT,indice)=0
 [M]YBATCAT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBATCAT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YBATCAT(indice)
 Endif
Endif
[M]YBATCAT = IMPENT

## Zone YFINSTA
mkstat = 0
zoncou = "YFINSTA"
indice = 0
IMPENT = [M]YFINSTA
If varinit([M]YFINSTA,indice)=0
 IMPENT = 1
Endif
[M]YFINSTA = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YBUSBAT0"
Return

