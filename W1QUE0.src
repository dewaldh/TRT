#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management QUE0 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:10:22"
A_USER  = "ADMIN"
A_STAMP = "20180828001022"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask QUE0 [QUE0]
Gosub HORODAT
Local Inpbox "QUE0" From GFONCTION At A_STAMP With A_WINDPREV Mask [QUE0] As [SIM_]
Grizo [QUE0]5
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [QUE0]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=3
Return

$ON_NUM
Onevent CONTROL Call C_NUM(zc) From W1QUE0
Onevent BEFORE_INPUT Call AS_NUM(zc) From W1QUE0
Return

Subprog AS_NUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_NUM(zc) From W1QUE0
Gosub _AFF
End

Subprog C_NUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CONTVCR
 Call CONTVCR(VALEUR) From TRTDIV
 If mkstat : GERR = 1 : GZONE = "NUM" : GZNUM = 1263 : Endif
Endif
Gosub _AFF
End

Subprog S_NUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELOBJET
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELOBJET"
 PARMSK(1) = "QUE"
 PARMSK(2) = "NUM"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJET","SELOBJ","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

$ON_QUEDESAXX
Onevent INIT_AREA Call D_QUEDESAXX(zc) From W1QUE0
Onevent BEFORE_INPUT Call AS_QUEDESAXX(zc) From W1QUE0
Return

Subprog IB_QUEDESAXX(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action BOUTTEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "BOUTTEXTRA"
 Call S_AFF_CHAR(VALEUR,PARMSK,"","ATEXTRA","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog D_QUEDESAXX(VALEUR)
Variable Char    VALEUR()
If varinit([M]QUEDESAXX,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"QUEUE","QUEDESAXX",NUM,"") From ATEXTRA
Endif
Gosub _AFF
End

Subprog AS_QUEDESAXX(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(9637,GBOUT1) From OBJDIV
Call IB_QUEDESAXX(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_QUEDESAXX(zc) From W1QUE0
Gosub _AFF
End

Subprog B1_QUEDESAXX(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ATEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ATEXTRA"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_QUEDESAXX" : PROGSUB = "SUBQUE" : PROGSPE = "SPEQUE" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "QUEUE"
 PARMSK(2) = "QUEDESAXX"
 PARMSK(3) = NUM
 PARMSK(4) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"ATEXTRA","ATEXTRA","") From GSAISIE
 ACTION = "BB1_QUEDESAXX" : PROGSUB = "SUBQUE" : PROGSPE = "SPEQUE" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$CHG_STY
Return


