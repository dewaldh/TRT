####################################################################
# 61 : PHYSTO Physical stock ( Field QTY 0)
####################################################################

$SET_PHYSTO
If [L]_AOLDVAL<>this.PHYSTO
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_PHYSTO
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.PHYSTO
 Gosub PROPAGATE_PHYSTO
Endif
Return

$INIT_PHYSTO
Gosub DECLARE_VAR From WMC0ITMCOSTA
[L]CURPRO = "PHYSTO"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_PHYSTO
Gosub DECLARE_VAR From WMC0ITMCOSTA
[L]CURPRO = "PHYSTO"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_PHYSTO                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_PHYSTO                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_PHYSTO
[L]CURPRO = "PHYSTO"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.PHYSTO=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_PHYSTO
[L]CURPRO = "PHYSTO"
Return

$_AOTHERS_PHYSTO
[L]CURPRO = "PHYSTO"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_PHYSTO
[L]CURPRO = "PHYSTO"
Return

$PROPAGATE_PHYSTO
this.isReadonly = 0
[L]CURPRO = "PHYSTO"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_PHYSTO
Gosub DECLARE_VAR From WMC0ITMCOSTA
[L]CURPRO = "PHYSTO"
$_LNKPRO_PHYSTO
Return


