# Supervisor
# Mask management FASLISTDEP 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]CPY,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTCPY"
  Call CONTSOC([M]CPY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("CPY","",[M]CPY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone DPRPLN
mkstat = 0
zoncou = "DPRPLN"
indice = 0
IMPENT = [M]DPRPLN
If varinit([M]DPRPLN,indice)=0
 IMPENT = 1
Endif
If varmode([M]DPRPLN,indice)=0
 [M]DPRPLN(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DPRPLN" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DPRPLN(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DPRPLN" : GZNUM = 33972 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]DPRPLN = IMPENT

## Zone TYPPER
mkstat = 0
zoncou = "TYPPER"
indice = 0
IMPENT = [M]TYPPER
If varinit([M]TYPPER,indice)=0
 IMPENT = 1
Endif
If varmode([M]TYPPER,indice)=0
 [M]TYPPER(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYPPER" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TYPPER(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "TYPPER" : GZNUM = 39823 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]TYPPER = IMPENT

## Zone DATSTR
mkstat = 0
zoncou = "DATSTR"
indice = 0
If varmode([M]DATSTR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATSTR" : Gosub ACTION From =IMPTRT
 Endif
 If [M]DATSTR=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DATSTR" : GZNUM = 1805 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone DATEND
mkstat = 0
zoncou = "DATEND"
indice = 0
If varmode([M]DATEND,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATEND" : Gosub ACTION From =IMPTRT
 Endif
 If [M]DATEND=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DATEND" : GZNUM = 1806 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"
indice = 0
status = 1
zoncou = "BASPAG"
If varmode([M]BASPAG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BASPAG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]BASPAG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]BASPAG = nolign
   status = 75
   Call SAI_BASPAG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_BASPAG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"

## Zone GAC
mkstat = 0
zoncou = "GAC"
SAVACT = GACTION : GACTION = "INIACCFMT"
Call INIACCFMT("") From SETGLBCOA
GACTION = SAVACT
If mkstat=1 : Raz [M]GAC(indice) : Endif

## Zone DPRBASSTR
mkstat = 0
zoncou = "DPRBASSTR"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:FLIS]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]DPRBASSTR(indice) : Endif

## Zone NBV
mkstat = 0
zoncou = "NBV"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:FLIS]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]NBV(indice) : Endif

## Zone DPE
mkstat = 0
zoncou = "DPE"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:FLIS]CUR,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]DPE(indice) : Endif

## Zone BASPAG
mkstat = 0
zoncou = "BASPAG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "FASLISTDEP"
Return

