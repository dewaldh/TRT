$TIT_COL
Local Char TITCL(200)(1..20)
Raz TITCL
Case BASPAG
 When "NBLIG"
  TITCL(1) = func SUBPJBU1D.COL_GET_TITLE(5)
  TITCL(2) = func SUBPJBU1D.COL_GET_TITLE(6)
  TITCL(3) = func SUBPJBU1D.COL_GET_TITLE(7)
  TITCL(4) = func SUBPJBU1D.COL_GET_TITLE(8)
  TITCL(5) = func SUBPJBU1D.COL_GET_TITLE(9)
  TITCL(6) = func SUBPJBU1D.COL_GET_TITLE(10)
  TITCL(7) = func SUBPJBU1D.COL_GET_TITLE(11)
  TITCL(8) = func SUBPJBU1D.COL_GET_TITLE(12)
  TITCL(9) = func SUBPJBU1D.COL_GET_TITLE(13)
  TITCL(10) = func SUBPJBU1D.COL_GET_TITLE(14)
  TITCL(11) = func SUBPJBU1D.COL_GET_TITLE(15)
  TITCL(12) = func SUBPJBU1D.COL_GET_TITLE(16)
  TITCL(13) = func SUBPJBU1D.COL_GET_TITLE(17)
  TITCL(14) = func SUBPJBU1D.COL_GET_TITLE(18)
  TITCL(15) = func SUBPJBU1D.COL_GET_TITLE(19)
  TITCL(16) = func SUBPJBU1D.COL_GET_TITLE(20)
  TITCL(17) = func SUBPJBU1D.COL_GET_TITLE(21)
  TITCL(18) = func SUBPJBU1D.COL_GET_TITLE(23)
  TITCL(19) = func SUBPJBU1D.COL_GET_TITLE(22)
  TITCL(20) = func SUBPJBU1D.COL_GET_TITLE(24)
  If !GORITITCOL
   Titcol [M]NBLIG TITCL(1) For PSCQTY1
   Titcol [M]NBLIG TITCL(2) For PSCAMT1
   Titcol [M]NBLIG TITCL(3) For PSCQTY2
   Titcol [M]NBLIG TITCL(4) For PSCAMT2
   Titcol [M]NBLIG TITCL(5) For PSCQTY3
   Titcol [M]NBLIG TITCL(6) For PSCAMT3
   Titcol [M]NBLIG TITCL(7) For PSCQTY4
   Titcol [M]NBLIG TITCL(8) For PSCAMT4
   Titcol [M]NBLIG TITCL(9) For PSCQTY5
   Titcol [M]NBLIG TITCL(10) For PSCAMT5
   Titcol [M]NBLIG TITCL(11) For PSCREMQTY
   Titcol [M]NBLIG TITCL(12) For PSCREMAMT
   Titcol [M]NBLIG TITCL(13) For PSCWIPQTY
   Titcol [M]NBLIG TITCL(14) For PSCWIPAMT
   Titcol [M]NBLIG TITCL(15) For PSCTOTQTY
   Titcol [M]NBLIG TITCL(16) For PSCTOTAMT
   Titcol [M]NBLIG TITCL(17) For PSCMARGQTY
   Titcol [M]NBLIG TITCL(18) For PSCMARGAMT
   Titcol [M]NBLIG TITCL(19) For PSCPCTMQTY
   Titcol [M]NBLIG TITCL(20) For PSCPCTMAMT
  Else
   Titcol [M:PJBU1T]NBLIG TITCL(1) For PSCQTY1
   Titcol [M:PJBU1T]NBLIG TITCL(2) For PSCAMT1
