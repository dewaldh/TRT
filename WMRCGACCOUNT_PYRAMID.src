####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMR0GACCOUNT_PYRAMID
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_PYM From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_PYM From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LIBPYM From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LIBPYM From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_GRU From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_GRU From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LIBGRU From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LIBGRU From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LEV From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LEV From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_PRNROW From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_PRNROW From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CRI From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CRI From WMR1GACCOUNT_PYRAMID : Gosub TESTALLSTAT :  Endif
[L]ASTATUS = fmet this.ACONTROL
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



