Subprog AS_STDDET(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(454,GBOUTI) From OBJDIV
Onkey 178 Titled GBOUTI Call CL_STDDET(zc) From W1ITC2
Gosub _AFF
End

Subprog CL_STDDET(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action STD
 Call CL_STDDET(VALEUR) From SUBITC
Endif
If !GWEBSERV
 ## Action SAIDECPRI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SAIDECPRI"
 PARMSK(1) = "STDL"
 PARMSK(2) = ""
 PARMSK(3) = ""
 PARMSK(4) = ""
 PARMSK(5) = ""
 PARMSK(6) = "2"
 PARMSK(7) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"ISAIDECPRI","SUBSDP","SPESDP") From GSAISIE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

