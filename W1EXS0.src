Subprog B1_NBR
Local Decimal BASPAG
If !GWEBSERV
 ## Action GOBJETC
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GOBJETC"
 Call OBJET_CHAR([M]VCRNUM(nolign-1),"GAS",[M]VCRTYP(nolign-1)) From GOBJET
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog B3_NBR
Local Decimal BASPAG
If !GWEBSERV
 ## Action VISUEXSBLB
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "VISUEXSBLB"
 PARMSK(1) = [M]CLB
 PARMSK(2) = [M]DATEXS(nolign-1)
 PARMSK(3) = num$([M]ACCNUM(nolign-1))
 Call SAISIE_NUM(BASPAG,PARMSK,"FEXSBLB","SUBEXS","") From GSAISIE
 GACTION = SAVACT
Endif
If !GWEBSERV
 ## Action STD
 Call B3_NBR From SUBEXS
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

