Subprog AS_CLB(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(485,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_CLB(zc) From W1EXS0
Onkey   2 Titled GBOUTS Call S_CLB(zc) From W1EXS0
Onkey 144 Titled GBOUTP Call P_CLB(zc) From W1EXS0
Onkey 146 Titled GBOUTA Call SA_CLB(zc) From W1EXS0
Gosub _AFF
End

Subprog C_CLB(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "CLB" : GZNUM = 16058 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("AUS","D",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CLB" : GZNUM = 16058 : Endif
 If !mkstat
  ## Action STD
  Call C_CLB(VALEUR) From SUBEXS
  If mkstat : GERR = 1 : GZONE = "CLB" : GZNUM = 16058 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_CLB
Call AM_CLB(zc)
If !mkstat & GOBJET
 zoncou = "CLB" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_CLB(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_CLB(VALEUR) From SUBEXS
If mkstat : GERR = 1 : GZONE = "CLB" : GZNUM = 16058 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZCLB)")>indice
 [M]CLB(indice) = VALEUR
 Affzo ZCLB(indice)
Endif
Gosub _AFF
End

Subprog S_CLB(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "AUS"
PARMSK(2) = "D"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_CLB(VALEUR)
Variable Char    VALEUR()
Call OBJET("AUS",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_CLB(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AUS"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CLB(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AUS",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

