# Supervisor
# Mask management AYY1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:51:44"
A_USER  = "ADMIN"
A_STAMP = "20180827235144"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask AYY1 [AYY1]
Gosub HORODAT
Local Inpbox "AYY1" From GFONCTION At A_STAMP With A_WINDPREV Mask [AYY1] As [SIM_]
Grizo [AYY1]5
Grizo [AYY1]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [AYY1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=3
X1(1)=1 : X2(1)=74 : Y1(1)=4 : Y2(1)=6
X1(2)=1 : X2(2)=74 : Y1(2)=7 : Y2(2)=28
Return

$ON_DOCCOD
Onevent CONTROL Call C_DOCCOD(zc) From W1AYY1
Onevent BEFORE_INPUT Call AS_DOCCOD(zc) From W1AYY1
Return

Subprog AS_DOCCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_DOCCOD(zc) From W1AYY1
Onkey 144 Titled GBOUTP Call P_DOCCOD(zc) From W1AYY1
Onkey 146 Titled GBOUTA Call SA_DOCCOD(zc) From W1AYY1
Gosub _AFF
End

Subprog C_DOCCOD(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "DOCCOD" : GZNUM = 38613 : Endif
If !mkstat
 ## Action CONTOBJW
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONTOBJW"
 PARMSK(1) = "AYY"
 Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJW","CONTOBJ","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DOCCOD" : GZNUM = 38613 : Endif
Endif
Gosub _AFF
End

Subprog S_DOCCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "AYY"
PARMSK(2) = "$"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog SA_DOCCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "AYY"
PARMSK(3) = "$"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_DOCCOD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("AYY",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_LAN
Onevent CONTROL Call C_LAN(zc) From W1AYY1
Onevent BEFORE_INPUT Call AS_LAN(zc) From W1AYY1
Onevent AFTER_CHANGE Gosub AMM_LAN From W1AYY1
Return

Subprog AS_LAN(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3587,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_LAN(zc) From W1AYY1
Onkey   2 Titled GBOUTS Call S_LAN(zc) From W1AYY1
Onkey 144 Titled GBOUTP Call P_LAN(zc) From W1AYY1
Onkey 146 Titled GBOUTA Call SA_LAN(zc) From W1AYY1
Gosub _AFF
End

Subprog C_LAN(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("TLA","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "LAN" : GZNUM = 1505 : Endif
Gosub _AFF
End

$AMM_LAN
Call AM_LAN(zc)
If !mkstat & GOBJET
 zoncou = "LAN" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_LAN(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZLAN)")>indice
 [M]LAN(indice) = VALEUR
 Affzo ZLAN(indice)
Endif
Gosub _AFF
End

Subprog S_LAN(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "TLA"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_LAN(VALEUR)
Variable Char    VALEUR()
Call OBJET("TLA",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_LAN(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TLA"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_LAN(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TLA",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZLAN
