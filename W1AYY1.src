Subprog D_CAT(VALEUR)
Variable Char    VALEUR()
If varinit([M]CAT,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"920","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_CAT(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"920") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_CAT(zc) From W1AYY1
Onkey   2 Titled GBOUTS Call S_CAT(zc) From W1AYY1
Onkey 144 Titled GBOUTP Call P_CAT(zc) From W1AYY1
Onkey 146 Titled GBOUTA Call SA_CAT(zc) From W1AYY1
If VALEUR="" : Call INIADI(VALEUR,"920") From TRTDIV : Endif
Gosub _AFF
End

Subprog C_CAT(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "CAT" : GZNUM = 37711 : Endif
If !mkstat
 ## Action CONTADI
 Call CONTADI(VALEUR,"920","") From SUBADI
 If mkstat : GERR = 1 : GZONE = "CAT" : GZNUM = 37711 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ADI","",VALEUR,"920",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CAT" : GZNUM = 37711 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_CAT
Call AM_CAT(zc)
If !mkstat & GOBJET
