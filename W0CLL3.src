# Supervisor
# Mask management CLL3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone FULRPO
mkstat = 0
zoncou = "FULRPO"
indice = 0
WCLOB = [M]FULRPO
If varmode([M]FULRPO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FULRPO" : Gosub ACTION From =IMPTRT
 Endif
 If WCLOB<>[M]FULRPO or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_FULRPO([M]FULRPO) From SUBCLL
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FULRPO" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone RPOICO
mkstat = 0
zoncou = "RPOICO"
indice = 0
If varmode([M]RPOICO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "RPOICO" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone CLLSAO
mkstat = 0
zoncou = "CLLSAO"
indice = 0
If varinit([M]CLLSAO,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]CLLSAO,"436","") From SUBADI
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]CLLSAO,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "CLLSAO" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]CLLSAO,"436","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "CLLSAO" : GZNUM = 22154 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!",[M]CLLSAO,"436",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "CLLSAO" : GZNUM = 22154 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone CLLDUR
mkstat = 0
zoncou = "CLLDUR"
indice = 0
If varmode([M]CLLDUR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CLLDUR" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CLL3"
Return

