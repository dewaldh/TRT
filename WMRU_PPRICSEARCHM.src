############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PPRICSEARCHM,ASTATUS)
Variable Instance PPRICSEARCHM Using R_PPRICSEARCHM
Variable Integer ASTATUS

If PPRICSEARCHM = null : End : Endif

Gosub AVARIABLES From WMR00PPRICSEARCHM
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PPRICSEARCHM.ACTX.AGETAFCRIGHT(PPRICSEARCHM,"SYRACHPPSI",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PPRICSEARCHM.ACTX.AGETAFCRIGHTU(PPRICSEARCHM,"SYRACHPPSI",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PPRICSEARCHM
Else
 Gosub AUPDATE_PPRICSEARCHM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PPRICSEARCHM.freeSnapshot
PPRICSEARCHM.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PPRICSEARCHM.ASTALIN = [V]CST_AUPD
PPRICSEARCHM.PPRISEA.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PPRICSEARCHM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PPRICSEARCHM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PPRICSEARCHM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PPRICSEARCHM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PPRICSEARCHM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PPRICSEARCHM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PPRICSEARCHM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PPRICSEARCHM.PPRISEA.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PPRICSEARCHM.PPRISEA.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PPRICSEARCHM.PPRISEA.ASTALIN)<>""
 [L]A_ASTALIN = PPRICSEARCHM.PPRISEA.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PPRICSEARCHM.PPRISEA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER

If [V]CST_ASTALIN(PPRICSEARCHM.PPRISEA.ASTALIN)<>""
 [L]A_ASTALIN = PPRICSEARCHM.PPRISEA.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PPRICSEARCHM.PPRISEA.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PPRICSEARCHM
Gosub AUPDATE_PPRICSEARCHM
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PPRICSEARCHM.PPRISEA.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PPRICSEARCHM.PPRISEA.ASTALIN = [V]CST_ALL
Endif
Return

#-----------------------------------------------------#
$AUPDATE_PPRICSEARCHM
[L]A_ASTALIN = PPRICSEARCHM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PPRICSEARCHM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PPRICSEARCHM.PPRISEA._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PPRICSEARCHM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PPRICSEARCHM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
$_AUPDATE_ABORT
If PPRICSEARCHM.snapshotEnabled=1
 [L]A_RETIK = PPRICSEARCHM.revertUpdTick
Endif

[L]ASTATUS = max(fmet PPRICSEARCHM.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



