# Supervisor
# Mask management CONSSH31 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
indice = 0
If varmode([M]ITMREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREF" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]ITMREF,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "VXADCUPC"
  Call ADCCTRUPC([M]ITMREF) From VXADCUPC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ITM","I",[M]ITMREF,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone LOTSEL
mkstat = 0
zoncou = "LOTSEL"
indice = 0
If varmode([M]LOTSEL,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LOTSEL" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone DEMSEL
mkstat = 0
zoncou = "DEMSEL"
indice = 0
If varmode([M]DEMSEL,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DEMSEL" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_DEMSEL([M]DEMSEL) From CNSSH3STD
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DEMSEL" : GZNUM = 9806 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone VCRTYPSEL
mkstat = 0
zoncou = "VCRTYPSEL"
indice = 0
IMPENT = [M]VCRTYPSEL
If varinit([M]VCRTYPSEL,indice)=0
 IMPENT = 1
Endif
[M]VCRTYPSEL = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CONSSH31"
Return

