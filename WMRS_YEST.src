#-------------------------------------------------------#
$METHODS
$EVENTS
Case [L]AEVENT
 When "AREAD_AFTER"
  Case [L]CURPTH
   When "YESTR" : Gosub ADDACS1
   When "YESTR.YRMA" : Gosub ADDACS2
   When "" : Gosub ADDACSREP
  Endcase
 When "AINIT"
  Case [L]CURPTH
   When "YESTR" : Gosub ADDACS1
   When "YESTR.YRMA" : Gosub ADDACS2
   When "" : Gosub ADDACSREP
  Endcase
 When Default
Endcase
Return

#-------------------------------------------------------#
$PROPERTIES
Case [L]ARULE
 When "CONTROL"
  Case [L]CURPRO
   When "YESTR.YPJTFCY"
    If this.YPJTFCY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTFCY(this,"GESYEST",this.YPJTFCY,[V]CST_ATRUE)
    Endif
   When "YESTR.YDEVFCY"
    If this.YDEVFCY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTFCY(this,"GESYEST",this.YDEVFCY,[V]CST_ATRUE)
    Endif
   When Default
  Endcase
 When "PROPAGATE"
  Case [L]CURPRO
   When Default
  Endcase
 Endcase
 Return
 
 #-------------------------------------------------------#
 # YESTR
 $ADDACS1
 Return
 
 # YESTR.YRMA
 $ADDACS2
 Return
 
 $ADDACSREP
 Return
 


