# Supervisor
# Mask management ASSOPASWEB 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone USER
mkstat = 0
zoncou = "USER"
indice = 0
If varmode([M]USER,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_USER([M]USER) From ASSOPASWEB
 GACTION = SAVACT
 If mkstat=1 : Raz [M]USER : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "USER" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_USER([M]USER) From ASSOPASWEB
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "USER" : GZNUM = 190 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone PASSE
mkstat = 0
zoncou = "PASSE"
indice = 0
If varmode([M]PASSE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PASSE" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ASSOPASWEB"
Return

