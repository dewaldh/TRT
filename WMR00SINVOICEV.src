Gosub TEST From WMCR_SINVOICEV : End

############################################################################################
## R : Subprog AREAD(INSTANCE,NUM)
############################################################################################
# SINVOICEV : SINVOICEV
# SINVOICE : SINVOICEV SINVOICE
# SINVOICED : SINVOICEV SINVOICED
# CPTANALIN : SINVOICED CPTANALIN
# SVCRVAT : SINVOICEV SVCRVAT
# SVCRFOOT : SINVOICEV SVCRFOOT
# CPTANALIN : SVCRFOOT CPTANALIN
############################################################################################

############################################################################################
### AVARIABLES
############################################################################################
$AVARIABLES
[L]ASTATUS =max([L]ASTATUS,fmet SINVOICEV._ADELERRORS([V]CST_ALLERRORS))
$_AVARIABLES
Local    Integer     A_LEVEL,A_I,A_NEWCOL,A_N1,A_N2,A_S2,A_N3,A_S3,A_N4,A_S4,A_N5,A_S5,A_N6,A_S6,A_N7,A_S7
Local    Integer    ASTATUS,A_RETIK,A_TYPMAJ,A_ASTALIN
Local    Char        A_STRSITE(GLONFCY),A_STRACC(GLONACS),CURPRO(50),A_PARENT(50),A_PATH(200),A_ABR(GLONABR),A_ERROR(250)
Local    Uuident    A_UUID

[L]A_LEVEL    = SINVOICEV.ACTX.LEVWML
[L]ASTATUS  = 0

Onerrgo ERR_OPENTAB
If find(SINVOICEV.ACTX.AFOLDER,nomap,"")>0
 Local File SINVOICEV [SIV]
 Local File SINVOICE [SIH]
 Local File SINVOICED [SID]
 Local File CPTANALIN [CALSID]
 Local File SVCRVAT [SVV]
 Local File SVCRFOOT [SVF]
 Local File CPTANALIN [CALSVF]
 Local File ATEXTRA [AXX]
Else
 Local File =SINVOICEV.ACTX.AFOLDER+".SINVOICEV" [SIV]
 Local File =SINVOICEV.ACTX.AFOLDER+".SINVOICE" [SIH]
 Local File =SINVOICEV.ACTX.AFOLDER+".SINVOICED" [SID]
 Local File =SINVOICEV.ACTX.AFOLDER+".CPTANALIN" [CALSID]
 Local File =SINVOICEV.ACTX.AFOLDER+".SVCRVAT" [SVV]
 Local File =SINVOICEV.ACTX.AFOLDER+".SVCRFOOT" [SVF]
 Local File =SINVOICEV.ACTX.AFOLDER+".CPTANALIN" [CALSVF]
 Local File =SINVOICEV.ACTX.AFOLDER+".ATEXTRA" [AXX]
Endif
Onerrgo
Return

$ERR_OPENTAB
[L]ASTATUS = max([L]ASTATUS,fmet SINVOICEV.ASETERROR("",errp+"("+num$(errl)+")"-mess(6,123,1)-num$(errn)-errmes$(errn)-errm,[V]CST_AERROR))
Resume

############################################################################################
### Link
############################################################################################
$AOPEN_LINK
Gosub AVARIABLES
Link [SIV] With
& [SIH]SIH0 ~= [F:SIV]NUM,
& [SID]SID0 ~= [F:SIV]NUM,
& [CALSID]CAL0 ~= "SID";0;[F:SID]NUM;[F:SID]SIDLIN;0;"";1,
& [SVV]SVV0 = 4;[F:SIV]NUM,
& [SVF]SVF1 = 4;[F:SIV]NUM,
& [CALSVF]CAL0 ~= "SVF";[F:SVF]VCRTYP;[F:SVF]VCRNUM;0;[F:SVF]DTA;"";1
& As [LNK_]
Return



