#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CRITBAF 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTCPY"
 Call CONTSOC([M]CPY) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("CPY","!",[M]CPY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_CPY([M]CPY) From CNSBAFSTD
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FCY([M]FCY) From CNSBAFSTD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]FCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","!",[M]FCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_FCY([M]FCY) From CNSBAFSTD
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone LEDTYP
mkstat = 0
zoncou = "LEDTYP"
indice = 0
IMPENT = [M]LEDTYP
If varinit([M]LEDTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]LEDTYP,indice)=0
 Call INICOMBO(2644) From COMBOS
 SAVACT = GACTION : GACTION = "STD"
 Call AS_LEDTYP(IMPENT) From CNSBAFSTD
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 If IMPENT<1 | GMENLOC(IMPENT)=1
  IMPENT = find(0,GMENLOC(1..len(mess(0,2644,1))))
 Endif
 [M]LEDTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LEDTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LEDTYP(indice)
 Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_LEDTYP(IMPENT) From CNSBAFSTD
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]LEDTYP = IMPENT

## Zone DATDEB
mkstat = 0
zoncou = "DATDEB"
indice = 0
WDATE = [M]DATDEB
SAVACT = GACTION : GACTION = "ADATRELI"
Call INIDAT([M]DATDEB,[M]DATFRMD) From ADATREL
GACTION = SAVACT
If mkstat=1 : Raz [M]DATDEB : Endif
If !mkstat
 If varmode([M]DATDEB,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DATDEB" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "STD"
  Call C_DATDEB([M]DATDEB) From CNSBAFSTD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WDATE<>[M]DATDEB or IMPMOD
    SAVACT = GACTION : GACTION = "ADATRELM"
    Call MODIFDAT([M]DATDEB,[M]DATFRMD) From ADATREL
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone DATFIN
mkstat = 0
zoncou = "DATFIN"
indice = 0
WDATE = [M]DATFIN
SAVACT = GACTION : GACTION = "ADATRELI"
Call INIDAT([M]DATFIN,[M]DATFRMF) From ADATREL
GACTION = SAVACT
If mkstat=1 : Raz [M]DATFIN : Endif
If !mkstat
 If varmode([M]DATFIN,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DATFIN" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "DATDEBFIN"
  Call DATDEBFIN([M]DATFIN,[M]DATDEB) From EXEFNC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_DATFIN([M]DATFIN) From CNSBAFSTD
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WDATE<>[M]DATFIN or IMPMOD
     SAVACT = GACTION : GACTION = "ADATRELM"
     Call MODIFDAT([M]DATFIN,[M]DATFRMF) From ADATREL
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone FIY1
mkstat = 0
zoncou = "FIY1"
indice = 0
IMPENT = [M]FIY1
If varinit([M]FIY1,indice)=0
 IMPENT = 1
Endif
If varmode([M]FIY1,indice)=0
 [M]FIY1(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FIY1" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FIY1(indice)
 Endif
Endif
[M]FIY1 = IMPENT

## Zone CLO
mkstat = 0
zoncou = "CLO"
indice = 0
IMPENT = [M]CLO
If varinit([M]CLO,indice)=0
 IMPENT = 1
Endif
If varmode([M]CLO,indice)=0
 [M]CLO(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "CLO" : Gosub ACTION From =IMPTRT
  IMPENT = [M]CLO(indice)
 Endif
Endif
[M]CLO = IMPENT

## Zone EXC
mkstat = 0
zoncou = "EXC"
indice = 0
IMPENT = [M]EXC
If varinit([M]EXC,indice)=0
 IMPENT = 1
Endif
If varmode([M]EXC,indice)=0
 [M]EXC(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "EXC" : Gosub ACTION From =IMPTRT
  IMPENT = [M]EXC(indice)
 Endif
Endif
[M]EXC = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone CUR
mkstat = 0
zoncou = "CUR"
indice = 0
If varmode([M]CUR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CUR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("TCU","!",[M]CUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CUR" : GZNUM = 323 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone ECRAN
mkstat = 0
zoncou = "ECRAN"
indice = 0
If varmode([M]ECRAN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ECRAN" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("GTC","!",[M]ECRAN,"BAF",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "ECRAN" : GZNUM = 46 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CRITBAF"
Return

