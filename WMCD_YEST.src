############################################################################################
### Subprog _ADELETE
############################################################################################
Subprog _ADELETE(YEST,ASTATUS)
Variable Instance YEST Using C_YEST
Variable Integer ASTATUS

If YEST = null : End : Endif

Gosub _AVARIABLES From WMC00YEST

[L]A_STRSITE = ""
[L]A_STRACC = ""
Local Instance ALNKDELTAB Using C_ALNKDELTAB
Gosub ADELETE_YEST

End

############################################################################################
### Subprog ADELETE
############################################################################################
Subprog ADELETE(YEST,ASTATUS)
Variable Instance YEST Using C_YEST
Variable Integer ASTATUS

If YEST = null : End : Endif

Gosub AVARIABLES From WMC00YEST
If [L]ASTATUS>=[V]CST_AERROR : Goto ADELETE_END : Endif

Local Instance ALNKDELTAB Using C_ALNKDELTAB

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub ADELETE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto ADELETE_END : Endif

If !adxlog
 Gosub ADELETE_TR_YEST
Else
 Gosub ADELETE_YEST
Endif

$ADELETE_END
End


$ADELETE_CONTROL
Gosub _ADELETE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Verification delete
[L]ASTATUS = max([L]ASTATUS,func ASYRSUB.VERIFY_INSTANCE(YEST,ALNKDELTAB))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _ADELETE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_ADELETE_CONTROL_BEFORE
If YEST.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YEST.ASTALIN = [V]CST_ADEL
[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION("ADELETE_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YSTAGE) : # YSTA -- 2
 If YEST.YSTAGE(A_N2) <> null
& and YEST.YSTAGE(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YEST.YSTAGE(A_N2).ASTALIN<>[V]CST_ANEW
   YEST.YSTAGE(A_N2).ASTALIN = [V]CST_ADEL
   [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._ADELETE_CONTROL_BEFORE)
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YLDP) : # YLNDPUR -- 3
 If YEST.YLDP(A_N2) <> null
& and YEST.YLDP(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEPR) : # YPRE -- 4
 If YEST.YEPR(A_N2) <> null
& and YEST.YEPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YEST.YEPR(A_N2).ASTALIN<>[V]CST_ANEW
   YEST.YEPR(A_N2).ASTALIN = [V]CST_ADEL
   [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._ADELETE_CONTROL_BEFORE)
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YREL) : # YREL -- 5
 If YEST.YREL(A_N2) <> null
& and YEST.YREL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YEST.YREL(A_N2).ASTALIN<>[V]CST_ANEW
   YEST.YREL(A_N2).ASTALIN = [V]CST_ADEL
   [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._ADELETE_CONTROL_BEFORE)
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_ADELETE_CONTROL_AFTER
For A_N2=1 To maxtab(YEST.YSTAGE) : # YSTA -- 2
 If YEST.YSTAGE(A_N2) <> null
& and YEST.YSTAGE(A_N2).ASTALIN<>[V]CST_ANEWDEL
  [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._ADELETE_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YLDP) : # YLNDPUR -- 3
 If YEST.YLDP(A_N2) <> null
& and YEST.YLDP(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEPR) : # YPRE -- 4
 If YEST.YEPR(A_N2) <> null
& and YEST.YEPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
  [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._ADELETE_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YREL) : # YREL -- 5
 If YEST.YREL(A_N2) <> null
& and YEST.YREL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._ADELETE_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION("ADELETE_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$ADELETE_TR_YEST
Trbegin [F:YEST]
 Gosub ADELETE_YEST
 If [L]ASTATUS>=[V]CST_AERROR : Gosub ADELETE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$ADELETE_YEST
[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION("ADELETE_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

#-----------------------------------------------------#
For A_N2=1 To maxtab(YEST.YSTAGE)
 If YEST.YSTAGE(A_N2) <> null
  [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._ADELETE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

#-----------------------------------------------------#
For A_N2=1 To maxtab(YEST.YLDP)
 If YEST.YLDP(A_N2) <> null
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

#-----------------------------------------------------#
For A_N2=1 To maxtab(YEST.YEPR)
 If YEST.YEPR(A_N2) <> null
  [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._ADELETE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

#-----------------------------------------------------#
For A_N2=1 To maxtab(YEST.YREL)
 If YEST.YREL(A_N2) <> null
  [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._ADELETE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

[L]ASTATUS = func ASYRSUB.DELETE_LINKS(YEST,ALNKDELTAB)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YEST._MAJSQL_YESTA(YEST))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION("ADELETE_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$ADELETE_ABORT
Rollback
$_ADELETE_ABORT
If YEST.snapshotEnabled=1
 [L]A_RETIK = YEST.revertUpdTick
Endif
[L]ASTATUS = max(fmet YEST.ACTION("ADELETE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#

#-----------------------------------------------------#
$_ADELETE_AFTER
Return



