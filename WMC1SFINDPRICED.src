Endif
Gosub CONTROL_INDEXLINE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.INDEXLINE
 Gosub PROPAGATE_INDEXLINE
Endif
Return

$INIT_INDEXLINE
Gosub DECLARE_VAR From WMC0SFINDPRICED
[L]CURPRO = "INDEXLINE"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_INDEXLINE
Gosub DECLARE_VAR From WMC0SFINDPRICED
[L]CURPRO = "INDEXLINE"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_INDEXLINE             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_INDEXLINE             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_INDEXLINE
[L]CURPRO = "INDEXLINE"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.INDEXLINE=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_INDEXLINE
[L]CURPRO = "INDEXLINE"
Return

$_AOTHERS_INDEXLINE
[L]CURPRO = "INDEXLINE"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_INDEXLINE
[L]CURPRO = "INDEXLINE"
Return

$PROPAGATE_INDEXLINE
this.isReadonly = 0
[L]CURPRO = "INDEXLINE"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

