#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CREEVL 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone TYPALT
mkstat = 0
zoncou = "TYPALT"
indice = 0
IMPENT = [M]TYPALT
If varinit([M]TYPALT,indice)=0
 IMPENT = 1
Endif
[M]TYPALT = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone INV
mkstat = 0
zoncou = "INV"
indice = 0
If varinit([M]INV,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_INV([M]INV) From CREEVL
 GACTION = SAVACT
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone DATCRE
mkstat = 0
zoncou = "DATCRE"
indice = 0
If varmode([M]DATCRE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DATCRE" : Gosub ACTION From =IMPTRT
 Endif
 If [M]DATCRE=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DATCRE" : GZNUM = 1805 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_DATCRE([M]DATCRE) From CREEVL
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DATCRE" : GZNUM = 1805 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone TYPCRE
mkstat = 0
zoncou = "TYPCRE"
indice = 0
IMPENT = [M]TYPCRE
WNUM = IMPENT
If varinit([M]TYPCRE,indice)=0
 IMPENT = 1
Endif
If varmode([M]TYPCRE,indice)=0
 [M]TYPCRE(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYPCRE" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TYPCRE(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "TYPCRE" : GZNUM = 39285 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_TYPCRE(IMPENT) From CREEVL
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "TYPCRE" : GZNUM = 39285 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]TYPCRE = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CREEVL"
Return

