$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_CCE5
Onevent BEFORE_AREA Gosub AV_CCE5 From W3WMAC2STDON
Onevent INIT_AREA Call D_CCE5(zc) From W3WMAC2STDON
Onevent BEFORE_INPUT Call AS_CCE5(zc) From W3WMAC2STDON
Onevent CONTROL Call C_CCE5(zc) From W3WMAC2STDON
Onevent AFTER_AREA Call AP_CCE5(zc) From W3WMAC2STDON
Onevent AFTER_CHANGE Gosub AMM_CCE5 From W3WMAC2STDON
Return

$AV_CCE5
## Action STD
Call AV_CCE5(zc) From SUBGA0
If GCCEFMT<>""
 Chgfmt [M]CCE5(indice) With GCCEFMT
Else
 Chgfmt [M]CCE5(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog D_CCE5(VALEUR)
Variable Char    VALEUR()
If varinit([M]CCE5,indice)=0
 ## Action FLDDEFCAR
 Call FLDDEFCAR(VALEUR,"CCE5."+[M:HAEF]DIE(4)) From TRTGAS
 ## Action STD
 Call D_CCE5(VALEUR) From SUBGA0
Endif
Gosub _AFF
End

Subprog AS_CCE5(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
## Action FLDAVTCAR
Call FLDAVTCAR(VALEUR,"CCE5."+[M:HAEF]DIE(4)) From TRTGAS
If mkstat=2
 [M]CCE5(INDIC) = VALEUR
 Affzo CCE5(INDIC) : mkstat = 2
Endif
If !mkstat
 ## Action STD
 Call AS_CCE5(VALEUR) From SUBGA0
 If mkstat=2
  [M]CCE5(INDIC) = VALEUR
  Affzo CCE5(INDIC) : mkstat = 2
 Endif
 Onkey   2 Titled GBOUTS Call S_CCE5(zc) From W3WMAC2STDON
Endif
Gosub _AFF
End

Subprog C_CCE5(VALEUR)
Variable Char    VALEUR()
## Action FLDCTLCAR
Call FLDCTLCAR(VALEUR,"CCE5."+[M:HAEF]DIE(4)) From TRTGAS
If mkstat : GERR = 1 : GZONE = "CCE5" : GZNUM = 0 : Endif
If !mkstat
 ## Action STD
 Call C_CCE5(VALEUR) From SUBGA0
 If mkstat : GERR = 1 : GZONE = "CCE5" : GZNUM = 0 : Endif
Endif
Gosub _AFF
End

Subprog AP_CCE5(VALEUR)
