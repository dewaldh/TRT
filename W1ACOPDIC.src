$ON_DOSORG
Onevent CONTROL Call C_DOSORG(zc) From W1ACOPDIC
Onevent AFTER_CHANGE Gosub AMM_DOSORG From W1ACOPDIC
Onevent INIT_AREA Call D_DOSORG(zc) From W1ACOPDIC
Onevent BEFORE_INPUT Call AS_DOSORG(zc) From W1ACOPDIC
Return

Subprog D_DOSORG(VALEUR)
Variable Char    VALEUR()
If varinit([M]DOSORG,indice)=0
 VALEUR = nomap
 ## Action STD
 Call D_DOSORG(VALEUR) From ACOPDIC
Endif
Gosub _AFF
End

Subprog AS_DOSORG(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
## Action STD
Call AS_DOSORG(VALEUR) From ACOPDIC
If mkstat=2
 [M]DOSORG(INDIC) = VALEUR
 Affzo DOSORG(INDIC) : mkstat = 2
Endif
Onkey   2 Titled GBOUTS Call S_DOSORG(zc) From W1ACOPDIC
Gosub _AFF
End

Subprog C_DOSORG(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1402 : Endif
If !mkstat
 ## Action CONTDOS
 Call C_DOSSIER(VALEUR) From DOSCONT
 If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1402 : Endif
 If !mkstat
  ## Action ACCESDOS
  Call CTL_DROIT_DOSSIER(VALEUR,"") From DOSCONT
  If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1402 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_DOSORG
Call AM_DOSORG(zc)
If !mkstat & GOBJET
 zoncou = "DOSORG" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_DOSORG(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action APASSWD
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "APASSWD"
 PARMSK(1) = VALEUR
 PARMSK(2) = ""
 Call SAISIE_CHAR(VALEUR,PARMSK,"APASSWD","APASSWD","SPEPASSWD") From GSAISIE
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "DOSORG" : GZNUM = 1402 : Endif
Endif
Gosub _AFF
End

Subprog S_DOSORG(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADS
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADS"
 Call S_LIS_CHAR(VALEUR,PARMSK,"SELADS","SUBADS","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

