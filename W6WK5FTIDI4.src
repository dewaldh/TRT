  TITCL(2) = GCOLREM(1)
  TITCL(3) = GCOLREM(2)
  TITCL(4) = GCOLDIE(1)
  TITCL(5) = GCOLDIE(2)
  TITCL(6) = GCOLDIE(3)
  TITCL(7) = GCOLDIE(4)
  TITCL(8) = GCOLDIE(5)
  TITCL(9) = GCOLDIE(6)
  TITCL(10) = GCOLDIE(7)
  TITCL(11) = GCOLDIE(8)
  TITCL(12) = GCOLDIE(9)
  TITCL(13) = GCOLDIE(10)
  TITCL(14) = GCOLDIE(11)
  TITCL(15) = GCOLDIE(12)
  TITCL(16) = GCOLDIE(13)
  TITCL(17) = GCOLDIE(14)
  TITCL(18) = GCOLDIE(15)
  TITCL(19) = GCOLDIE(16)
  TITCL(20) = GCOLDIE(17)
