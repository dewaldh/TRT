############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PHYELTLCTNI,ASTATUS)
Variable Instance PHYELTLCTNI Using R_PHYELTLCTNI
Variable Integer ASTATUS

If PHYELTLCTNI = null : End : Endif

Gosub AVARIABLES From WMR00PHYELTLCTNI
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PHYELTLCTNI.ACTX.AGETAFCRIGHT(PHYELTLCTNI,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PHYELTLCTNI.ACTX.AGETAFCRIGHTU(PHYELTLCTNI,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PHYELTLCTNI
Else
 Gosub AUPDATE_PHYELTLCTNI
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PHYELTLCTNI.freeSnapshot
PHYELTLCTNI.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PHYELTLCTNI.ASTALIN = [V]CST_AUPD
PHYELTLCTNI.PHY.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PHYELTLCTNI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTLCTNI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTLCTNI._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTLCTNI._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTLCTNI._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PHYELTLCTNI.PHY.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PHYELTLCTNI.PHY.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PHYELTLCTNI.PHY.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTLCTNI.PHY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PHYELTLCTNI.PHY.PHYMVT) : # PHYMVT -- 2
 If PHYELTLCTNI.PHY.PHYMVT(A_N2) <> null
& and PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEW & PHYELTLCTNI.PHY.ASTALIN=[V]CST_ADEL
   PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEW & find(PHYELTLCTNI.PHY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY.PHYMVT(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY.PHYMVT(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY.PHYMVT(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PHYELTLCTNI.PHY.PHYMVT) : # PHYMVT -- 2
 If PHYELTLCTNI.PHY.PHYMVT(A_N2) <> null
& and PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY.PHYMVT(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY.PHYMVT(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY.PHYMVT(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PHYELTLCTNI.PHY.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTLCTNI.PHY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PHYELTLCTNI
Trbegin [F:PHY]
 Gosub AUPDATE_PHYELTLCTNI
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PHYELTLCTNI.PHY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PHYELTLCTNI.PHY.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTLCTNI.PHY.PHYMVT)
 If PHYELTLCTNI.PHY.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTLCTNI.PHY.PHYMVT(A_N2)._AUPDATE_AFTER
  If PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTLCTNI.PHY.PHYMVT(A_N2)
   PHYELTLCTNI.PHY.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_PHYELTLCTNI
[L]A_ASTALIN = PHYELTLCTNI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.PHY._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTLCTNI.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNI.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PHYELTLCTNI.snapshotEnabled=1
 [L]A_RETIK = PHYELTLCTNI.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTLCTNI.PHY.PHYMVT)
 If PHYELTLCTNI.PHY.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTLCTNI.PHY.PHYMVT(A_N2)._AUPDATE_ABORT
  If PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTLCTNI.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTLCTNI.PHY.PHYMVT(A_N2)
   PHYELTLCTNI.PHY.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PHYELTLCTNI.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



