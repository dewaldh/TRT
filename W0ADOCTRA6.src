# Supervisor
# Mask management ADOCTRA6 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ALLTYP
mkstat = 0
zoncou = "ALLTYP"
indice = 0
IMPENT = [M]ALLTYP
If varinit([M]ALLTYP,indice)=0
 IMPENT = 1
Endif
[M]ALLTYP = IMPENT

## Zone TYP
mkstat = 0
zoncou = "TYP"
indice = 0
If varinit([M]TYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TYP,"910","") From SUBADI
 GACTION = SAVACT
Endif

## Zone ALLMOD
mkstat = 0
zoncou = "ALLMOD"
indice = 0
IMPENT = [M]ALLMOD
If varinit([M]ALLMOD,indice)=0
 IMPENT = 1
Endif
[M]ALLMOD = IMPENT

## Zone MODULE
mkstat = 0
zoncou = "MODULE"
indice = 0
IMPENT = [M]MODULE
If varinit([M]MODULE,indice)=0
 IMPENT = 1
Endif
[M]MODULE = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBDOC
mkstat = 0
zoncou = "NBDOC"
indice = 0
status = 1
zoncou = "NBDOC"
If varmode([M]NBDOC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBDOC" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBDOC
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBDOC = nolign
   status = 75
   Call SAI_NBDOC(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBDOC(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBDOC
mkstat = 0
zoncou = "NBDOC"

## Zone DOCTYP
mkstat = 0
zoncou = "DOCTYP"
If varinit([M]DOCTYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]DOCTYP(indice),"910","") From SUBADI
 GACTION = SAVACT
Endif

## Zone PRIO
mkstat = 0
zoncou = "PRIO"
IMPENT = [M]PRIO(indice)
If varinit([M]PRIO,indice)=0
 IMPENT = 1
Endif
[M]PRIO(indice) = IMPENT

## Zone NBDOC
mkstat = 0
zoncou = "NBDOC"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ADOCTRA6"
Return

