&   [L]SAIRAP(2) = [F:SOH9]A_TAB2 Using "K:15c" Titled TITSEL9(2)  ,
&   [L]SAIRAP(3) = [F:SOH9]A_TAB3 Using "" Titled TITSEL9(3)  ,
&   [L]SAIRAP(4) = [F:SOH9]A_TAB4 Using "K:20X" Titled TITSEL9(4)  ,
