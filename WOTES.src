$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(10)(0..GNBDERNIER)
Local Char     A_BUF2(80)(0..GNBDERNIER)
If clalev([F:TES9])>0 : Close Local File [TES9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(10),
& Char     A_TAB2(80)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2
& As [TES9]
Return

$LEC_DERLU
Read [ACC]CODACC = GUSER ; [F:TES]ACS
If fstat : Raz [F:ACC] : Endif
Read [AXX]AXX0="TABEXPENS";"DESTRA";GLANGUE;[F:TES]CODEXP;""
If fstat : Raz [F:AXX] : Endif
Read [AX3]AXX0="TABEXPENS";"SHOTRA";GLANGUE;CODEXP;""
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:TES9])>0
 Read [TES9] = [F:TES9]A_TABI
 [L]CLE = [F:TES9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:TES9])>0
 Filter [TES9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [TES0] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:TES9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:TES]CODEXP+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:TES9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:TES9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 2
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:TES]CODEXP"
Call TEXTE(41193,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:10X"
SELFOR(NBSEL) = "K:10c"
SELMAJ(NBSEL) = 1
NBTRI += 1
TRIZON(NBTRI) = "[F:TES]CODEXP"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:AXX]TEXTE"
Call TEXTE(8,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:80X"
SELFOR(NBSEL) = "K:80X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:AXX]TEXTE"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:TES_])=0
 Gosub OUVFIC_LISTE
Endif
Return

