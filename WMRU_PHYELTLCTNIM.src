############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(PHYELTLCTNIM,ASTATUS)
Variable Instance PHYELTLCTNIM Using R_PHYELTLCTNIM
Variable Integer ASTATUS

If PHYELTLCTNIM = null : End : Endif

Gosub AVARIABLES From WMR00PHYELTLCTNIM
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet PHYELTLCTNIM.ACTX.AGETAFCRIGHT(PHYELTLCTNIM,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet PHYELTLCTNIM.ACTX.AGETAFCRIGHTU(PHYELTLCTNIM,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_PHYELTLCTNIM
Else
 Gosub AUPDATE_PHYELTLCTNIM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = PHYELTLCTNIM.freeSnapshot
PHYELTLCTNIM.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
PHYELTLCTNIM.ASTALIN = [V]CST_AUPD
PHYELTLCTNIM.PHY.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = PHYELTLCTNIM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTLCTNIM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTLCTNIM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTLCTNIM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTLCTNIM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If PHYELTLCTNIM.PHY.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PHYELTLCTNIM.PHY.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(PHYELTLCTNIM.PHY.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTLCTNIM.PHY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(PHYELTLCTNIM.PHY.PHYMVT) : # PHYMVT -- 2
 If PHYELTLCTNIM.PHY.PHYMVT(A_N2) <> null
& and PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEW & PHYELTLCTNIM.PHY.ASTALIN=[V]CST_ADEL
   PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEW & find(PHYELTLCTNIM.PHY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY.PHYMVT(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY.PHYMVT(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY.PHYMVT(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(PHYELTLCTNIM.PHY.PHYMVT) : # PHYMVT -- 2
 If PHYELTLCTNIM.PHY.PHYMVT(A_N2) <> null
& and PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY.PHYMVT(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY.PHYMVT(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY.PHYMVT(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(PHYELTLCTNIM.PHY.ASTALIN)<>""
 [L]A_ASTALIN = PHYELTLCTNIM.PHY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_PHYELTLCTNIM
Trbegin [F:PHY]
 Gosub AUPDATE_PHYELTLCTNIM
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(PHYELTLCTNIM.PHY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PHYELTLCTNIM.PHY.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTLCTNIM.PHY.PHYMVT)
 If PHYELTLCTNIM.PHY.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTLCTNIM.PHY.PHYMVT(A_N2)._AUPDATE_AFTER
  If PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTLCTNIM.PHY.PHYMVT(A_N2)
   PHYELTLCTNIM.PHY.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_PHYELTLCTNIM
[L]A_ASTALIN = PHYELTLCTNIM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.PHY._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = PHYELTLCTNIM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTLCTNIM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If PHYELTLCTNIM.snapshotEnabled=1
 [L]A_RETIK = PHYELTLCTNIM.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTLCTNIM.PHY.PHYMVT)
 If PHYELTLCTNIM.PHY.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTLCTNIM.PHY.PHYMVT(A_N2)._AUPDATE_ABORT
  If PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTLCTNIM.PHY.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTLCTNIM.PHY.PHYMVT(A_N2)
   PHYELTLCTNIM.PHY.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet PHYELTLCTNIM.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



