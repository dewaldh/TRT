#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management BAN0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone BAN
mkstat = 0
zoncou = "BAN"
indice = 0
WCHAR = [M]BAN
If varmode([M]BAN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BAN" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]BAN,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "BAN" : GZNUM = 5296 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "CONTOBJW"
  PARAM(1) = "BAN"
  Local Char    VALEUR(250)
  VALEUR = [M]BAN
  Call S_AFF_CHAR(VALEUR,PARAM,"CONTOBJW","CONTOBJ","") From GACTION
  [M]BAN = VALEUR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "BAN" : GZNUM = 5296 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WCHAR<>[M]BAN or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_BAN([M]BAN) From SUBBAN
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "BAN" : GZNUM = 5296 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone DES
mkstat = 0
zoncou = "DES"
indice = 0
If varmode([M]DES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YBANPRNSTA
mkstat = 0
zoncou = "YBANPRNSTA"
indice = 0
IMPENT = [M]YBANPRNSTA
If varinit([M]YBANPRNSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YBANPRNSTA,indice)=0
 [M]YBANPRNSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBANPRNSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YBANPRNSTA(indice)
 Endif
 SAVACT = GACTION : GACTION = "SPE"
 Call C_YBANPRNSTA(IMPENT) From SPEBAN
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "YBANPRNSTA" : GZNUM = 100108 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YBANPRNSTA = IMPENT

## Zone YSTA
mkstat = 0
zoncou = "YSTA"
indice = 0
IMPENT = [M]YSTA
If varinit([M]YSTA,indice)=0
 IMPENT = 2
Endif
If varmode([M]YSTA,indice)=0
 [M]YSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YSTA(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YSTA" : GZNUM = 188 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YSTA = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "BAN0"
Return

