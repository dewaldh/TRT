####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "CRICOL"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"CRICOL",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBRCRI -= 1 : Endif
 When "TBLCOL"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"TBLCOL",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBRTBL -= 1 : Endif
 When "VARCOL"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"VARCOL",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBRVAR -= 1 : Endif
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# ADDLINE
####################################################################
$ADDLINE
Case _ACOLLECTION
 When "CRICOL"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"CRICOL","C_PARSTA1_CRICOL",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBRCRI += 1 : Endif
 When "TBLCOL"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"TBLCOL","C_PARSTA1_TBLCOL",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBRTBL += 1 : Endif
 When "VARCOL"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"VARCOL","C_PARSTA1_VARCOL",this)
  If [L]_ALINE<>[V]CST_ANOTDEFINED : this.NBRVAR += 1 : Endif
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# AGETDIM
####################################################################
$AGETDIM
Case _ACOLLECTION
 When "CRICOL"
  [L]_ALINE = 5
 When "TBLCOL"
  [L]_ALINE = 10
 When "VARCOL"
  [L]_ALINE = 10
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# AGETMIN
####################################################################
$AGETMIN
Case _ACOLLECTION
 When "CRICOL"
  [L]_ALINE = 2
 When "TBLCOL"
  [L]_ALINE = 2
 When "VARCOL"
  [L]_ALINE = 2
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

