####################################################################
# ADELLINE
####################################################################
$ADELLINE
Case _ACOLLECTION
 When "C_ATB"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_ATB",this)
 When "C_ATX"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_ATX",this)
 When "C_DISCRGPRO"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_DISCRGPRO",this)
 When "C_FIL"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"C_FIL",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# ADDLINE
####################################################################
$ADDLINE
Case _ACOLLECTION
 When "C_ATB"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_ATB","C_ATABLE",this)
 When "C_ATX"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_ATX","C_ATEXTE",this)
 When "C_DISCRGPRO"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_DISCRGPRO","C_SPRICCONF_C_DISCRGPRO",this)
 When "C_FIL"
  [L]_ALINE = func ASYRSUBTAB.ADDLINE([L]ALINE,"C_FIL","C_SPRICCONF_C_FIL",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# AGETDIM
####################################################################
$AGETDIM
Case _ACOLLECTION
 When "C_ATB"
  [L]_ALINE = 5
 When "C_ATX"
  [L]_ALINE = 5
 When "C_DISCRGPRO"
  [L]_ALINE = 9
 When "C_FIL"
  [L]_ALINE = 5
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

####################################################################
# AGETMIN
####################################################################
$AGETMIN
Case _ACOLLECTION
 When "C_ATB"
  [L]_ALINE = 1
 When "C_ATX"
  [L]_ALINE = 1
 When "C_DISCRGPRO"
  [L]_ALINE = 2
 When "C_FIL"
  [L]_ALINE = 1
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

