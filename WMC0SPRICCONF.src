####################################################################
# Global methods & standards
####################################################################
$AINIT
Gosub DECLARE_VAR
[L]AEVENT = "AINIT" : [L]ACTION = "AINIT" : [L]CURPRO = "" : Gosub ACTION From ASYRSUB
Return

####################################################################
# AERRORS
####################################################################

$_AERROR
Gosub DECLARE_VAR From WMC0SPRICCONF
Local Integer ALLSTAT
Call ALLERRORS(this,_CTX,_SUPP) From ASYRSUB
For AINDCLA=1 To 9
 If this.C_DISCRGPRO(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_DISCRGPRO(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_FIL(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_FIL(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_ATB(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_ATB(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_ATX(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_ATX(AINDCLA)._AERROR(_CTX,_SUPP)
 Endif
Next AINDCLA
Return

####################################################################
# Delete all errors
####################################################################

$_ADELERRORS
Gosub DECLARE_VAR From WMC0SPRICCONF
Local Integer ALLSTAT
[L]ASTATUS = fmet this.ADELETEERROR([L]_APARAM)
For AINDCLA=1 To 9
 If this.C_DISCRGPRO(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_DISCRGPRO(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_FIL(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_FIL(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_ATB(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_ATB(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_ATX(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_ATX(AINDCLA)._ADELERRORS(_APARAM)
 Endif
Next AINDCLA
Return

####################################################################
# _ASEARCHINFOS
####################################################################

$_ASEARCHINFOS
Gosub DECLARE_VAR From WMC0SPRICCONF
ASTATUS= func ASYRSUB.ASEARCHINFOS(this,[L]AINS_ORI,[L]ERR_TYPE,[L]PROPERTY,[L]ASTAT,[L]SELSTAT,[L]PATH)
If [L]ERR_TYPE=[V]CST_ACURRENT : Return : Endif
For AINDCLA=1 To 9
 If this.C_DISCRGPRO(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_DISCRGPRO(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_DISCRGPRO('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_FIL(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_FIL(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_FIL('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_ATB(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_ATB(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_ATB('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
For AINDCLA=1 To 5
 If this.C_ATX(AINDCLA)<>null
  [L]ASTATUS = fmet this.C_ATX(AINDCLA)._ASEARCHINFOS([L]AINS_ORI,ERR_TYPE,PROPERTY,ASTAT,SELSTAT,string$(PATH<>'',PATH+'.')+'C_ATX('+num$(AINDCLA)+')')
 Endif
Next AINDCLA
Return

####################################################################
# AGETERRORSFROM
####################################################################

$_AGETERRORSFROM
Gosub DECLARE_VAR From WMC0SPRICCONF
If AINS_FROM<>null
 [L]ASTATUS = fmet AINS_FROM.ASEARCHINFOS( [V]CST_ALL, "", [V]CST_AFATAL, 0)
Endif
Call AGETERRORSFROM(AINS_FROM,this,ENVIRONNEMENT) From ASYRSUB
Return

