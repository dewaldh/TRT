NBPRO += 1 : TBPRO(NBPRO) = "YFFXDVAL" : TBFIL(NBPRO) = "[F:YSTR]YFFXDVAL"
           : TBTYP(NBPRO) = 4 : TBPTH(NBPRO) = "YSTR.YFFXDVAL"
NBPRO += 1 : TBPRO(NBPRO) = "YSTR_YSETRULTYP_YENTTYP" : TBFIL(NBPRO) = "[F:YSRT]YENTTYP"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "YSTR.YSETRULTYP.YENTTYP"
NBPRO += 1 : TBPRO(NBPRO) = "YSTR_YSETRULTYP_YJOU" : TBFIL(NBPRO) = "[F:YSRT]YJOU"
           : TBTYP(NBPRO) = 7 : TBPTH(NBPRO) = "YSTR.YSETRULTYP.YJOU"
ACTION = "AQUERY_DECODE_CRITERIA_AFTER" : Gosub AQUERY_ACTION
