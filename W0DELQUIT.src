# Supervisor
# Mask management DELQUIT 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone BPRFCT
mkstat = 0
zoncou = "BPRFCT"
indice = 0
WCHAR = [M]BPRFCT
If varmode([M]BPRFCT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BPRFCT" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]BPRFCT,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "BPRFCT" : GZNUM = 3669 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("FCT","",[M]BPRFCT,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "BPRFCT" : GZNUM = 3669 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WCHAR<>[M]BPRFCT or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_BPRFCT([M]BPRFCT) From DELQUIT
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "BPRFCT" : GZNUM = 3669 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone FCTVCR
mkstat = 0
zoncou = "FCTVCR"
indice = 0
If varmode([M]FCTVCR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCTVCR" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]FCTVCR,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "FCTVCR" : GZNUM = 2553 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_FCTVCR([M]FCTVCR) From DELQUIT
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FCTVCR" : GZNUM = 2553 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone TRACE
mkstat = 0
zoncou = "TRACE"
indice = 0
IMPENT = [M]TRACE
If varinit([M]TRACE,indice)=0
 IMPENT = 2
Endif
If varmode([M]TRACE,indice)=0
 [M]TRACE(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TRACE" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TRACE(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "TRACE" : GZNUM = 6058 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]TRACE = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "DELQUIT"
Return

