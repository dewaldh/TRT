# Supervisor
# Mask management YSETRUL 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_YFPJTLIN
Onevent CONTROL Call C_YFPJTLIN(zc) From W2YSETRUL
Onevent BEFORE_INPUT Call AS_YFPJTLIN(zc) From W2YSETRUL
Return

Subprog IB_YFPJTLIN(VALEUR)
Variable Char    VALEUR()
## Action IBUPJMPIM
Call IBUPIM(VALEUR) From PJMSELECT
Gosub _AFF
End

Subprog AS_YFPJTLIN(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(14664,GBOUT1) From OBJDIV
Call TEXTE(14664,GBOUT2) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_YFPJTLIN(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_YFPJTLIN(zc) From W2YSETRUL
Onkey 130 Titled GBOUT2 Call B2_YFPJTLIN(zc) From W2YSETRUL
Onkey   2 Titled GBOUTS Call S_YFPJTLIN(zc) From W2YSETRUL
Onkey 144 Titled GBOUTP Call P_YFPJTLIN(zc) From W2YSETRUL
Onkey 146 Titled GBOUTA Call SA_YFPJTLIN(zc) From W2YSETRUL
Gosub _AFF
End

Subprog C_YFPJTLIN(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YFPJTLIN" : GZNUM = 54491 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("PIM","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YFPJTLIN" : GZNUM = 54491 : Endif
 If !mkstat
  ## Action SPE
  Call C_YFPJTLIN(VALEUR) From SPEYSETRUL
  If mkstat : GERR = 1 : GZONE = "YFPJTLIN" : GZNUM = 54491 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_YFPJTLIN(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action YSELPJM
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "YSELPJM"
 PARMSK(1) = [M:YSTR]YFFCY
 Call S_LIS_CHAR(VALEUR,PARMSK,"YSELPJM","","SPEYSELPJM") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_YFPJTLIN(VALEUR)
Variable Char    VALEUR()
Call OBJET("PIM",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog B2_YFPJTLIN(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action TUNPJMOPP
 Call TUNCRM(VALEUR) From PJMSELECT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog SA_YFPJTLIN(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "PIM"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YFPJTLIN(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("PIM",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

