# Supervisor
# Mask management YSETRUL 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_YTAMTAMD
Onevent INIT_AREA Call D_YTAMTAMD(zc) From W2YSETRUL
Onevent BEFORE_INPUT Call AS_YTAMTAMD(zc) From W2YSETRUL
Onevent CONTROL Call C_YTAMTAMD(zc) From W2YSETRUL
Return

Subprog D_YTAMTAMD(VALEUR)
Variable Integer VALEUR
If varinit([M]YTAMTAMD,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YTAMTAMD(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog C_YTAMTAMD(VALEUR)
Variable Integer VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YTAMTAMD" : GZNUM = 101132 : Endif
Gosub _AFF
End

$ON_YTLONLGR
Onevent BEFORE_INPUT Call AS_YTLONLGR(zc) From W2YSETRUL
Onevent CONTROL Call C_YTLONLGR(zc) From W2YSETRUL
Return

Subprog AS_YTLONLGR(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(100685,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YTLONLGR(zc) From W2YSETRUL
Onkey   2 Titled GBOUTS Call S_YTLONLGR(zc) From W2YSETRUL
Onkey 144 Titled GBOUTP Call P_YTLONLGR(zc) From W2YSETRUL
Onkey 146 Titled GBOUTA Call SA_YTLONLGR(zc) From W2YSETRUL
Gosub _AFF
End

Subprog C_YTLONLGR(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("YELA","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "YTLONLGR" : GZNUM = 100685 : Endif
Gosub _AFF
End

Subprog S_YTLONLGR(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action YELASEL
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "YELASEL"
 PARMSK(1) = ""
 Call S_LIS_CHAR(VALEUR,PARMSK,"YELASEL","","YSELELA") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_YTLONLGR(VALEUR)
Variable Char    VALEUR()
Call OBJET("YELA",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_YTLONLGR(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "YELA"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YTLONLGR(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("YELA",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_YTPJTLIN
