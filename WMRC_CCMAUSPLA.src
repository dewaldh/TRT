# Supervisor
## CRUD-CLASSE ## CCMAUSPLA
# 2018-08-27 13:09:43 179


############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(CCMAUSPLA,ASTATUS)
Variable Instance CCMAUSPLA Using R_CCMAUSPLA
Variable Integer ASTATUS

If CCMAUSPLA = null : End : Endif

Gosub AVARIABLES From WMR00CCMAUSPLA
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If fmet CCMAUSPLA.ACTX.AGETAFCRIGHT(CCMAUSPLA,"GESAUS",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END : Endif
If fmet CCMAUSPLA.ACTX.AGETAFCRIGHTC(CCMAUSPLA,"GESAUS",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END
Endif
If fmet CCMAUSPLA.ACTX.AGETACSRIGHTM(CCMAUSPLA,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AINSERT_END
  Endif
  
  Gosub AREPCONTROL_AINSERT
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
  
If !adxlog
 Gosub AINSERT_TR_CCMAUSPLA
Else
 Gosub AINSERT_CCMAUSPLA
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_END : Return : Endif

Gosub _AINSERT_AFTER
$AINSERT_END
[L]A_I = CCMAUSPLA.freeSnapshot
CCMAUSPLA.snapshotEnabled = 1
End

#-----------------------------------------------------#
$AREPCONTROL_AINSERT
CCMAUSPLA.ASTALIN = [V]CST_ANEW
CCMAUSPLA.AUS.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSPLA._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSPLA._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet CCMAUSPLA._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If CCMAUSPLA.AUS.ASTALIN = [V]CST_ANEWDEL : Return : Endif
CCMAUSPLA.AUS.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.FNCCOL)
 If CCMAUSPLA.AUS.FNCCOL([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS.FNCCOL([L]A_I).ACTION("AINSERT_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If CCMAUSPLA.AUS.BPA(A_N2) <> null
& and !find(CCMAUSPLA.AUS.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUSPLA.AUS.BID) : # BID -- 3
 If CCMAUSPLA.AUS.BID(A_N2) <> null
& and !find(CCMAUSPLA.AUS.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
A_N2=1
If CCMAUSPLA.AUS.BPA(A_N2) <> null
& and !find(CCMAUSPLA.AUS.BPA(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(CCMAUSPLA.AUS.BID) : # BID -- 3
 If CCMAUSPLA.AUS.BID(A_N2) <> null
& and !find(CCMAUSPLA.AUS.BID(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.FNCCOL)
 If CCMAUSPLA.AUS.FNCCOL([L]A_I)<>null
  [L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS.FNCCOL([L]A_I).ACTION("AINSERT_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_CCMAUSPLA
Trbegin [F:AUS]
 Gosub AINSERT_CCMAUSPLA
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AINSERT_AFTER
If find(CCMAUSPLA.AUS.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 CCMAUSPLA.AUS.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.FNCCOL)
 If CCMAUSPLA.AUS.FNCCOL([L]A_I)<>null
  If CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSPLA.AUS.FNCCOL([L]A_I)
   CCMAUSPLA.AUS.FNCCOL([L]A_I) = null
  Elsif CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN <> [V]CST_ALL
   CCMAUSPLA.AUS.FNCCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
Local Integer ASTATUS2
A_N2=1
If CCMAUSPLA.AUS.BPA(A_N2) <> null
 If CCMAUSPLA.AUS.BPA(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance CCMAUSPLA.AUS.BPA(A_N2)
  CCMAUSPLA.AUS.BPA(A_N2) = null
 Elsif CCMAUSPLA.AUS.BPA(A_N2).ASTALIN <> [V]CST_ALL
  CCMAUSPLA.AUS.BPA(A_N2).ASTALIN = [V]CST_ALL
  For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.BPA(A_N2).COLTEL)
   If CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I)<>null
    If CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I)
     CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I) = null
    Elsif CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUSPLA.AUS.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
  For [L]A_I=1 To maxtab(CCMAUSPLA.AUS.BPA(A_N2).COLWEB)
   If CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I)<>null
    If CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
     FreeInstance CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I)
     CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I) = null
    Elsif CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
     CCMAUSPLA.AUS.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
    Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(CCMAUSPLA.AUS.BID)
 If CCMAUSPLA.AUS.BID(A_N2) <> null
  [L]ASTATUS2 = fmet CCMAUSPLA.AUS.BID(A_N2)._AINSERT_AFTER
  If CCMAUSPLA.AUS.BID(A_N2).ASTALIN=[V]CST_ADEL or CCMAUSPLA.AUS.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance CCMAUSPLA.AUS.BID(A_N2)
   CCMAUSPLA.AUS.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AINSERT_CCMAUSPLA
[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.AUS._AINSERT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet CCMAUSPLA.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If CCMAUSPLA.snapshotEnabled=1
 [L]A_RETIK = CCMAUSPLA.revertUpdTick
Endif
[L]ASTATUS = max(fmet CCMAUSPLA.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



