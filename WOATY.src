$LEC_DERLU
Read [ATX]NUMERO = GLANGUE ; [F:ATY]INTITTYP
If fstat : Raz [F:ATX] : Endif
Read [ATYP]ATYP0=[F:ATY]TYPPROSYR
If fstat : Raz [F:ATYP] : Endif
Return

$CHARGE_DERLU
If clalev([F:ATY9])>0
 Read [ATY9] = [F:ATY9]A_TABI
 [L]CLE = [F:ATY9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:ATY9])>0
 Filter [ATY9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [ATY11] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:ATY9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:ATY]CODTYP+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:ATY9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:ATY9]
 Endif
Endif
Return

