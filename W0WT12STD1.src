mkstat = 0
zoncou = "ORITYP"
IMPENT = [M]ORITYP(indice)
SAVACT = GACTION : GACTION = "STD"
Call AV_ORITYP(IMPENT) From SUBPRH
GACTION = SAVACT
If mkstat=1 : Raz IMPENT : Endif
If !mkstat
 If varinit([M]ORITYP,indice)=0
  IMPENT = 1
 Endif
Endif
[M]ORITYP(indice) = IMPENT

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
