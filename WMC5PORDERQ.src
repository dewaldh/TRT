####################################################################
# 121 : SOQSEQ Sequence number ( Field L 8)
####################################################################

$SET_SOQSEQ
If [L]_AOLDVAL<>this.SOQSEQ
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_SOQSEQ
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.SOQSEQ
 Gosub PROPAGATE_SOQSEQ
Endif
Return

$INIT_SOQSEQ
Gosub DECLARE_VAR From WMC0PORDERQ
[L]CURPRO = "SOQSEQ"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_SOQSEQ
Gosub DECLARE_VAR From WMC0PORDERQ
[L]CURPRO = "SOQSEQ"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_SOQSEQ                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_SOQSEQ                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_SOQSEQ
[L]CURPRO = "SOQSEQ"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.SOQSEQ=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_SOQSEQ
[L]CURPRO = "SOQSEQ"
Return

$_AOTHERS_SOQSEQ
[L]CURPRO = "SOQSEQ"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_SOQSEQ
[L]CURPRO = "SOQSEQ"
Return

$PROPAGATE_SOQSEQ
this.isReadonly = 0
[L]CURPRO = "SOQSEQ"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_SOQSEQ
Gosub DECLARE_VAR From WMC0PORDERQ
[L]CURPRO = "SOQSEQ"
$_LNKPRO_SOQSEQ
Return


