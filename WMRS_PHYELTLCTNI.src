# Supervisor
## ACLA-CLASSE ## PHYELTLCTNI
# 2018-08-27 13:22:19 386

#-------------------------------------------------------#
$METHODS
$EVENTS
Case [L]AEVENT
 When "AREAD_AFTER"
  Case [L]CURPTH
   When "PHY" : Gosub ADDACS1
   When "PHY.PHYMVT" : Gosub ADDACS2
   When "" : Gosub ADDACSREP
  Endcase
 When "AINIT"
  Case [L]CURPTH
   When "PHY" : Gosub ADDACS1
   When "PHY.PHYMVT" : Gosub ADDACS2
   When "" : Gosub ADDACSREP
  Endcase
 When Default
Endcase
Return

#-------------------------------------------------------#
$PROPERTIES
Case [L]ARULE
 When "CONTROL"
  Case [L]CURPRO
   When "PHY.CPY"
    If this.CPY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTCPY(this,"GESPHY",this.CPY,[V]CST_ATRUE)
    Endif
   When "PHY.FCY"
    If this.FCY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTFCY(this,"GESPHY",this.FCY,[V]CST_ATRUE)
    Endif
   When Default
  Endcase
 When "PROPAGATE"
  Case [L]CURPRO
   When Default
  Endcase
 Endcase
 Return
 
 #-------------------------------------------------------#
 # PHY
 $ADDACS1
 If fmet this.ACTX.AGETAFCRIGHTN(this,"GESPHY","",[V]CST_AFALSE)>=[V]CST_AERROR
  [L]ASTATUS = fmet this.ASETATTRIBUTE("ENAFLG","$isReadOnly","true")
 Endif
 Return
 
 # PHY.PHYMVT
 $ADDACS2
 Return
 
 $ADDACSREP
 Return
 


