# Supervisor
# Mask management YSLC3 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [26/10/2018]
A_HEURE = "16:49:11"
A_USER  = "MAJID"
A_STAMP = "20181026164911"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIT"
  If [M]NBLIT=0
  Endif
 When "NBLIA"
  If [M]NBLIA=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask YSLC3 [YSLC3]
Gosub HORODAT
Local Inpbox "YSLC3" From GFONCTION At A_STAMP With A_WINDPREV Mask [YSLC3] As [SIM_]
Grizo [YSLC3]5
Grizo [YSLC3]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [YSLC3]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=13
X1(1)=1 : X2(1)=74 : Y1(1)=14 : Y2(1)=25
Return

$ON_YSALROL
Onevent INIT_AREA Call D_YSALROL(zc) From W1YSLC3
Onevent CONTROL Call C_YSALROL(zc) From W1YSLC3
Onevent BEFORE_INPUT Call AS_YSALROL(zc) From W1YSLC3
Return

Subprog D_YSALROL(VALEUR)
Variable Char    VALEUR()
If varinit([M]YSALROL,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"6007","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_YSALROL(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"6007") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YSALROL(zc) From W1YSLC3
Onkey   2 Titled GBOUTS Call S_YSALROL(zc) From W1YSLC3
Onkey 144 Titled GBOUTP Call P_YSALROL(zc) From W1YSLC3
Onkey 146 Titled GBOUTA Call SA_YSALROL(zc) From W1YSLC3
If VALEUR="" : Call INIADI(VALEUR,"6007") From TRTDIV : Endif
Gosub _AFF
End

Subprog C_YSALROL(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YSALROL" : GZNUM = 100536 : Endif
If !mkstat
 ## Action CONTADI
 Call CONTADI(VALEUR,"6007","") From SUBADI
 If mkstat : GERR = 1 : GZONE = "YSALROL" : GZNUM = 100536 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("ADI","",VALEUR,"6007",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YSALROL" : GZNUM = 100536 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_YSALROL(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = "6007"
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_YSALROL(VALEUR)
Variable Char    VALEUR()
Call OBJET("ADI",VALEUR,"6007") From GOBJET
Gosub _AFF
End

Subprog SA_YSALROL(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(2) = "6007"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YSALROL(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "6007"
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

