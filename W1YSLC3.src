Endif
Gosub _AFF
End

Subprog C_YSALROLALL(VALEUR)
Variable Integer VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YSALROLALL" : GZNUM = 100536 : Endif
Gosub _AFF
End

