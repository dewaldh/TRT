#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management YCSWEB3 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [08/05/2019]
A_HEURE = "13:10:27"
A_USER  = "JR"
A_STAMP = "20190508131027"
Return

$TITRES
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask YCSWEB3 [YCS3]
Gosub HORODAT
Local Inpbox "YCSWEB3" From GFONCTION At A_STAMP With A_WINDPREV Mask [YCS3] As [SIM_]
Grizo [YCS3]10
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [YCS3]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=14 : Y1(0)=1 : Y2(0)=25
X1(1)=57 : X2(1)=74 : Y1(1)=1 : Y2(1)=25
Return

$ON_YOBJID
Onevent BEFORE_INPUT Call IB_YOBJID(zc) From W1YCSWEB3
Onevent INIT_AREA Call D_YOBJID(zc) From W1YCSWEB3
Return

Subprog IB_YOBJID(VALEUR)
Variable Clbfile VALEUR
## Action SPE
Call IB_YOBJID From SPEYCS
Gosub _AFF
End

Subprog D_YOBJID(VALEUR)
Variable Clbfile VALEUR
If varinit([M]YOBJID)=0
 Call INITURL(VALEUR,"YCS","","") From INTRUTILA
Endif
Gosub _AFF
End

$ON_YCREFLD
Onevent BEFORE_INPUT Call AS_YCREFLD(zc) From W1YCSWEB3
Onevent INIT_AREA Call D_YCREFLD(zc) From W1YCSWEB3
Return

Subprog D_YCREFLD(VALEUR)
Variable Char    VALEUR()
If varinit([M]YCREFLD,indice)=0
 VALEUR = 61
Endif
Gosub _AFF
End

Subprog AS_YCREFLD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(455,GBOUTI) From OBJDIV
Onkey 178 Titled GBOUTI Call CL_YCREFLD(zc) From W1YCSWEB3
Gosub _AFF
End

Subprog CL_YCREFLD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SPE
 Call CL_YCREFLD(VALEUR) From SPEYCS
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_YLINK
Onevent BEFORE_INPUT Call AS_YLINK(zc) From W1YCSWEB3
Onevent INIT_AREA Call D_YLINK(zc) From W1YCSWEB3
Return

Subprog D_YLINK(VALEUR)
Variable Char    VALEUR()
If varinit([M]YLINK,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YLINK(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(455,GBOUTI) From OBJDIV
Onkey 178 Titled GBOUTI Call CL_YLINK(zc) From W1YCSWEB3
Gosub _AFF
End

Subprog CL_YLINK(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action YCSL
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "YCSL"
 PARMSK(1) = GABREV
 PARMSK(2) = num$(evalue([F:YCSFL]YOBJIDFLD))
 Call SAISIE_CHAR(VALEUR,PARMSK,"YOCSL","SUBYCSL","SPEYCSL") From GSAISIE
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$CHG_STY
Return


