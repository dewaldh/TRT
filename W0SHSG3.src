# Supervisor
# Mask management SHSG3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ENDDAT
mkstat = 0
zoncou = "ENDDAT"
indice = 0
WDATE = [M]ENDDAT
SAVACT = GACTION : GACTION = "STD"
Call AV_ENDDAT([M]ENDDAT) From TRTSTKGRH
GACTION = SAVACT
If mkstat=1 : Raz [M]ENDDAT : Endif
If !mkstat
 If varmode([M]ENDDAT,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "ENDDAT" : Gosub ACTION From =IMPTRT
  Endif
  If WDATE<>[M]ENDDAT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_ENDDAT([M]ENDDAT) From TRTSTKGRH
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "ENDDAT" : GZNUM = 1806 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone MONNBR
mkstat = 0
zoncou = "MONNBR"
indice = 0
WNUM = [M]MONNBR
If varmode([M]MONNBR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "MONNBR" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_MONNBR([M]MONNBR) From TRTSTKGRH
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "MONNBR" : GZNUM = 29537 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>[M]MONNBR or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_MONNBR([M]MONNBR) From TRTSTKGRH
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "MONNBR" : GZNUM = 29537 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIGC
mkstat = 0
zoncou = "NBLIGC"
indice = 0
status = 1
zoncou = "NBLIGC"
If varmode([M]NBLIGC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIGC" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIGC
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIGC = nolign
   status = 75
   Call SAI_NBLIGC(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIGC(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIGC
mkstat = 0
zoncou = "NBLIGC"

## Zone NBLIGC
mkstat = 0
zoncou = "NBLIGC"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SHSG3"
Return

