Onkey 140 Titled GBOUT12 Call B12_NBLIG From W1WMSFCALL
Onkey 170 Titled GBOUT13 Call B13_NBLIG From W1WMSFCALL
Gosub _AFF
End

Subprog B1_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action CONSSLO
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONSSLO"
 PARMSK(1) = "1"
 PARMSK(2) = [M]STOFCY(nolign-1)
 PARMSK(3) = [M]ITMREF
 PARMSK(4) = ""
 PARMSK(5) = "0"
 PARMSK(6) = ""
 PARMSK(7) = [M]LOTSEL
 Call CONSULT_NUM(BASPAG,"SLO",PARMSK) From GCONSULT
 [M]LOTSEL = PARMSK(7)
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

