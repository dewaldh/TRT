# Sage X3 supervisor
# Mask management WE8FTIST3 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_VAT1
Onevent CONTROL Call C_VAT1(zc) From W4WE8FTIST3
Onevent BEFORE_INPUT Call AS_VAT1(zc) From W4WE8FTIST3
Onevent AFTER_CHANGE Gosub AMM_VAT1 From W4WE8FTIST3
Return

Subprog IB_VAT1(VALEUR)
Variable Char    VALEUR()
## Action CONTOBJN
Local Char    PARMSK(250)(1..20) , SAVACT(20)
SAVACT = GACTION : GACTION = "CONTOBJN"
Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJN","CONTOBJ","") From GACTION
GACTION = SAVACT
If !GWEBSERV
 ## Action IBTUNLEG
 Call IBTUNLEG("") From TRTVENCTL
Endif
Gosub _AFF
End

Subprog AS_VAT1(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(5342,GBOUT1) From OBJDIV
Call TEXTE(8255,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Call IB_VAT1(VALEUR)
## Action PASLINPIH
Call PASLINPIH("VAT1") From SUBPIH
If mkstat=2
 [M]VAT1(INDIC) = VALEUR
 Affzo VAT1(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_VAT1(zc) From W4WE8FTIST3
Onkey   2 Titled GBOUTS Call S_VAT1(zc) From W4WE8FTIST3
Onkey 144 Titled GBOUTP Call P_VAT1(zc) From W4WE8FTIST3
Onkey 146 Titled GBOUTA Call SA_VAT1(zc) From W4WE8FTIST3
Gosub _AFF
End

Subprog C_VAT1(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "VAT1" : GZNUM = 6743 : Endif
If !mkstat
 ## Action CONTTVT
 Call CONTTVT(VALEUR,GCURLEG,"PIH3","VAT1",[M:PIH0]FCY,"",GSOCIETE) From SUBTVT
 If mkstat : GERR = 1 : GZONE = "VAT1" : GZNUM = 6743 : Endif
 If !mkstat
  ## Action PCOVAT
  Call PCOVAT(VALEUR,[M:PIH1]VAC) From TRTACHCTL1
  If mkstat : GERR = 1 : GZONE = "VAT1" : GZNUM = 6743 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_VAT1
Call AM_VAT1(zc)
If !mkstat & GOBJET
 zoncou = "VAT1" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_VAT1(VALEUR)
Variable Char    VALEUR()
## Action PAMVATFLG
Call PAMVATFLG("PIH3",nolign-1,"VAT1") From TRTACHFAC1
If mkstat : GERR = 1 : GZONE = "VAT1" : GZNUM = 6743 : Endif
Gosub _AFF
End

Subprog S_VAT1(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELTVTLEG
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELTVTLEG"
 PARMSK(1) = GCURLEG
 PARMSK(2) = [M:PIH0]FCY
 PARMSK(3) = ""
 PARMSK(4) = GSOCIETE
 PARMSK(5) = "[F:TVT]VATTYP=1"
 PARMSK(6) = "PIH3"
 PARMSK(7) = "VAT1"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELTVT","SUBTVT","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_VAT1(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action TUNTVT
 Call TUNTVT(VALEUR,GCURLEG) From SUBTVT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog SA_VAT1(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TVT"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_VAT1(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TVT",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

