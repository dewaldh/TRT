# Supervisor
# Mask management YSETADJ 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !([M:YSLC2]YSETACT<>[0/0/0])
  mkstat = 2
 Endif
 If mkstat=1 : Raz [M]NBLIG : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone YVALTOT
mkstat = 0
zoncou = "YVALTOT"
indice = 0
If varmode([M]YVALTOT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YVALTOT" : Gosub ACTION From =IMPTRT
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone YSRID
mkstat = 0
zoncou = "YSRID"
If varmode([M]YSRID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSRID" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YSRID(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YSRID" : GZNUM = 101130 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("YSETRUL","",[M]YSRID(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "YSRID" : GZNUM = 101130 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "SPE"
   Call C_YSRID([M]YSRID(indice)) From SPEYSLC
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "YSRID" : GZNUM = 101130 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "SPE"
    Call AP_YSRID([M]YSRID(indice)) From SPEYSLC
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "YSRID" : GZNUM = 101130 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone YAMT
mkstat = 0
zoncou = "YAMT"
If varmode([M]YAMT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YAMT" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YAMT(indice)=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YAMT" : GZNUM = 3603 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YSETADJ"
Return

