# Supervisor
# Mask management YGSC 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YGSCID
mkstat = 0
zoncou = "YGSCID"
indice = 0
If varmode([M]YGSCID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YGSCID" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YGSCID,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YGSCID" : GZNUM = 100713 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YNUM
mkstat = 0
zoncou = "YNUM"
indice = 0
If varmode([M]YNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YNUM" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YNUM=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YNUM" : GZNUM = 1120 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YDES
mkstat = 0
zoncou = "YDES"
indice = 0
If varmode([M]YDES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YDES" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YDES,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YDES" : GZNUM = 1470 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YSTAATV
mkstat = 0
zoncou = "YSTAATV"
indice = 0
IMPENT = [M]YSTAATV
If varinit([M]YSTAATV,indice)=0
 IMPENT = 1
Endif
If varmode([M]YSTAATV,indice)=0
 [M]YSTAATV(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSTAATV" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YSTAATV(indice)
 Endif
Endif
[M]YSTAATV = IMPENT

## Zone YJOBACTFLG
mkstat = 0
zoncou = "YJOBACTFLG"
indice = 0
IMPENT = [M]YJOBACTFLG
If varinit([M]YJOBACTFLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]YJOBACTFLG,indice)=0
 [M]YJOBACTFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YJOBACTFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YJOBACTFLG(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YJOBACTFLG" : GZNUM = 100380 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YJOBACTFLG = IMPENT

## Zone YJOBCOD
mkstat = 0
zoncou = "YJOBCOD"
indice = 0
If varmode([M]YJOBCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YJOBCOD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YJOBCOD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YJOBCOD" : GZNUM = 21780 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YGSC"
Return

