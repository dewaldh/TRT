#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1SFTSHIFT
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "SFTSHIFT"
REP.CLA = "SFTSHIFT"
REP.ICL = "SFTS"
REP.AFCRIGHT = "GESSHFT"
REP.NCL = 54320 : # Shift code
REP.DES = 54320 : # Shift code
REP.TIT = "SHFDES"
REP.ANX = "SHF0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Shift code
     REP.CLE(1).PAR = 'SHFCOD' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'SHFCOD' : REP.CLE(1).VPR= 'SFTS.SHFCOD' : REP.CLE(1).DES = 54320
REP.TRT(1) = NewInstance C_AREPTRT AllocGroup REP
     REP.TRT(1).TRT = 'SFTSHIFT_RSTD' : REP.TRT(1).TYP = 1
# Facets
REP.AENAFAC(1) = 2 : # Detail
REP.AENAFAC(2) = 2 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 2 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "SHIFT" : REP.SEC(1).NSC = 54320 : # Shift code
     REP.SEC(1).BLC(1)="SHIFT" : REP.SEC(1).NBC(1)=54320 : # Shift code
Return

#-------------------------------------------------------#
# Liens de la représentation
