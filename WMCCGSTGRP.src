If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AUUID From WMC1GSTGRP : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AUUID From WMC1GSTGRP : Gosub TESTALLSTAT :  Endif
