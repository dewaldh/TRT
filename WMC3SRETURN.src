####################################################################
# 61 : LICPLATER Registration ( Field REGLIC 10)
####################################################################

$SET_LICPLATER
If [L]_AOLDVAL<>this.LICPLATER
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_LICPLATER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.LICPLATER
 Gosub PROPAGATE_LICPLATER
Endif
Return

$INIT_LICPLATER
Gosub DECLARE_VAR From WMC0SRETURN
[L]CURPRO = "LICPLATER"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_LICPLATER
Gosub DECLARE_VAR From WMC0SRETURN
[L]CURPRO = "LICPLATER"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_LICPLATER             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_LICPLATER             : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_LICPLATER
[L]CURPRO = "LICPLATER"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.LICPLATER,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_LICPLATER
[L]CURPRO = "LICPLATER"
If vireblc(this.LICPLATER,2)<>vireblc(format$("K:10X",this.LICPLATER),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_LICPLATER
[L]CURPRO = "LICPLATER"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_LICPLATER
[L]CURPRO = "LICPLATER"
Return

$PROPAGATE_LICPLATER
this.isReadonly = 0
[L]CURPRO = "LICPLATER"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_LICPLATER
Gosub DECLARE_VAR From WMC0SRETURN
[L]CURPRO = "LICPLATER"
$_LNKPRO_LICPLATER
Return


