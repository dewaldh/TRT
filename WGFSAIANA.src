$BOUTON
STDBOUT = "EA09<>"
NBTEX  = 0
NBTEX += 1
CBOUT(NBTEX) = "O" : LBOUT(NBTEX) = 1801 : Call TEXTE(6425,TEXTE(NBTEX)) From OBJDIV
NBTEX += 1
CBOUT(NBTEX) = "B" : LBOUT(NBTEX) = 1802 : Call TEXTE(3170,TEXTE(NBTEX)) From OBJDIV
Raz NBMEN, CMEN, TMEN, NMEN, LMEN
Return

$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "O"
  FLGEXE = 1
  GACTION = "STD"
 When "B"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [F:HAE]NUM
  PARBOUT(2) = "GAS"
  PARBOUT(3) = [F:HAE]TYP
  FLGEXE = 1
  GACTION = "GOBJETC"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  [F:HAE]NUM = PARBOUT(1)
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


