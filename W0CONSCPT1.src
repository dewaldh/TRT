#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management CONSCPT1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTCPY"
 Call CONTSOC([M]CPY) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("CPY","!",[M]CPY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_CPY([M]CPY) From CNSCPTSTD
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FCY([M]FCY) From CNSCPTSTD
 GACTION = SAVACT
 If mkstat=1 : Raz [M]FCY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]FCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","!",[M]FCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_FCY([M]FCY) From CNSCPTSTD
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone LEDTYP
mkstat = 0
zoncou = "LEDTYP"
indice = 0
IMPENT = [M]LEDTYP
If varinit([M]LEDTYP,indice)=0
 IMPENT = 1
Endif
If varmode([M]LEDTYP,indice)=0
 Call INICOMBO(2644) From COMBOS
 [M]LEDTYP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LEDTYP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LEDTYP(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If IMPENT>0 & IMPENT<dim(GMENLOC) & GMENLOC(IMPENT)<>0
   mkstat = 1 : GMESSAGE = mess(99,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_LEDTYP(IMPENT) From CNSCPTSTD
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "LEDTYP" : GZNUM = 27525 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]LEDTYP = IMPENT

## Zone CURSEL
mkstat = 0
zoncou = "CURSEL"
indice = 0
If varmode([M]CURSEL,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CURSEL" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("TCU","!",[M]CURSEL,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CURSEL" : GZNUM = 21541 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone DATDEB
mkstat = 0
zoncou = "DATDEB"
indice = 0
WDATE = [M]DATDEB
SAVACT = GACTION : GACTION = "ADATRELI"
Call INIDAT([M]DATDEB,[M:CCPT]DATFRMD) From ADATREL
GACTION = SAVACT
If mkstat=1 : Raz [M]DATDEB : Endif
If !mkstat
 If varmode([M]DATDEB,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DATDEB" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "STD"
  Call C_DATDEB([M]DATDEB) From CNSCPTSTD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WDATE<>[M]DATDEB or IMPMOD
    SAVACT = GACTION : GACTION = "ADATRELM"
    Call MODIFDAT([M]DATDEB,[M:CCPT]DATFRMD) From ADATREL
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "STD"
     Call AM_DATDEB([M]DATDEB) From CNSCPTSTD
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "DATDEB" : GZNUM = 1805 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone DATFIN
mkstat = 0
zoncou = "DATFIN"
indice = 0
WDATE = [M]DATFIN
SAVACT = GACTION : GACTION = "ADATRELI"
Call INIDAT([M]DATFIN,[M:CCPT]DATFRMF) From ADATREL
GACTION = SAVACT
If mkstat=1 : Raz [M]DATFIN : Endif
If !mkstat
 If varmode([M]DATFIN,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DATFIN" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "STD"
  Call C_DATFIN([M]DATFIN) From CNSCPTSTD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "DATDEBFIN"
   Call DATDEBFIN([M]DATFIN,[M]DATDEB) From EXEFNC
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WDATE<>[M]DATFIN or IMPMOD
     SAVACT = GACTION : GACTION = "ADATRELM"
     Call MODIFDAT([M]DATFIN,[M:CCPT]DATFRMF) From ADATREL
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "DATFIN" : GZNUM = 1806 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone SOLDE
mkstat = 0
zoncou = "SOLDE"
indice = 0
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]DEVISE,GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]SOLDE : Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone COMPTE
mkstat = 0
zoncou = "COMPTE"
indice = 0
SAVACT = GACTION : GACTION = "INIACCFMT"
Call INIACCFMT(num$([M]COA)) From SETGLBCOA
GACTION = SAVACT
If mkstat=1 : Raz [M]COMPTE : Endif
If !mkstat
 If varmode([M]COMPTE,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "COMPTE" : Gosub ACTION From =IMPTRT
  Endif
  Call CONTROLE("GAC","!G",[M]COMPTE,num$([M]COA),mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "COMPTE" : GZNUM = 3956 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_COMPTE([M]COMPTE) From CNSCPTSTD
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "COMPTE" : GZNUM = 3956 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone COLLEC
mkstat = 0
zoncou = "COLLEC"
indice = 0
If varmode([M]COLLEC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "COLLEC" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_COLLEC([M]COLLEC) From CNSCPTSTD
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "COLLEC" : GZNUM = 321 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone GROUPE
mkstat = 0
zoncou = "GROUPE"
indice = 0
If varmode([M]GROUPE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "GROUPE" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("GSC","!",[M]GROUPE,num$([M:CPT1]COA),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "GROUPE" : GZNUM = 6088 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_GROUPE([M]GROUPE) From CNSCPTSTD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "GROUPE" : GZNUM = 6088 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone TIERS
mkstat = 0
zoncou = "TIERS"
indice = 0
If varmode([M]TIERS,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "TIERS" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("BPR","!",[M]TIERS,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "TIERS" : GZNUM = 19569 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_TIERS([M]TIERS) From CNSCPTSTD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "TIERS" : GZNUM = 19569 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone FMTDIE
mkstat = 0
zoncou = "FMTDIE"
For indice=0 To 8
 If varinit([M]FMTDIE,indice)=0
  [M]FMTDIE(indice) = "KP:15X"
 Endif
 mkstat = 0
Next indice
If mkstat : End : Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CONSCPT1"
Return

