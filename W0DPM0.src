#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management DPM0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
WCHAR = [M]CPY
If varinit([M]CPY,indice)=0
 [M]CPY = GCOUSOC
Endif
If varmode([M]CPY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "ACONTCPY"
 Call CONTSOC([M]CPY) From SUBAGF
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("CPY","!A",[M]CPY,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_CPY([M]CPY) From SUBDPM
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WCHAR<>[M]CPY or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_CPY([M]CPY) From SUBDPM
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone DPM
mkstat = 0
zoncou = "DPM"
indice = 0
If varmode([M]DPM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DPM" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]DPM,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DPM" : GZNUM = 34033 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call AP_DPM([M]DPM) From SUBDPM
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "DPM" : GZNUM = 34033 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DES
mkstat = 0
zoncou = "DES"
indice = 0
If varmode([M]DES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DES" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]DES,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "DES" : GZNUM = 282 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone MAJDES
mkstat = 0
zoncou = "MAJDES"
indice = 0
If varinit([M]MAJDES,indice)=0
 [M]MAJDES = "153"
Endif
If varmode([M]MAJDES,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "MAJDES" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone ENAFLG
mkstat = 0
zoncou = "ENAFLG"
indice = 0
IMPENT = [M]ENAFLG
If varinit([M]ENAFLG,indice)=0
 IMPENT = 2
Endif
If varmode([M]ENAFLG,indice)=0
 [M]ENAFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ENAFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ENAFLG(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "ENAFLG" : GZNUM = 188 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]ENAFLG = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone UTILISE
mkstat = 0
zoncou = "UTILISE"
indice = 0
IMPENT = [M]UTILISE
If varinit([M]UTILISE,indice)=0
 IMPENT = 1
Endif
[M]UTILISE = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "DPM0"
Return

