$LEC_DERLU
Read [AFF]AFF0 = [F:FXM]FCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Return

$CHARGE_DERLU
If clalev([F:FXM9])>0
 Read [FXM9] = [F:FXM9]A_TABI
 [L]CLE = [F:FXM9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:FXM9])>0
 Filter [FXM9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [FXM1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:FXM9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = num$([F:FXM]IDMVT)+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:FXM9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:FXM9]
 Endif
Endif
Return

