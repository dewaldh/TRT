Subprog AS_PQHFCY(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_PQHFCY(zc) From W1WE1ALL1
Onkey 144 Titled GBOUTP Call P_PQHFCY(zc) From W1WE1ALL1
Onkey 146 Titled GBOUTA Call SA_PQHFCY(zc) From W1WE1ALL1
Gosub _AFF
End

Subprog C_PQHFCY(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "PQHFCY" : GZNUM = 4532 : Endif
If !mkstat
 ## Action ACONTFCY
 Call CONTSITE(VALEUR) From SUBAGF
 If mkstat : GERR = 1 : GZONE = "PQHFCY" : GZNUM = 4532 : Endif
 If !mkstat
  ## Action %CNTOBJ
  Call CONTSEL("FCY","",VALEUR,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "PQHFCY" : GZNUM = 4532 : Endif
  If !mkstat
   ## Action CONTFCY
   Call CONTFCY(VALEUR) From EXEFNC
   If mkstat : GERR = 1 : GZONE = "PQHFCY" : GZNUM = 4532 : Endif
  Endif
 Endif
Endif
Gosub _AFF
End

