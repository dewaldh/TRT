# Supervisor
# Mask management WB2T101 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone PLITYP
mkstat = 0
zoncou = "PLITYP"
indice = 0
IMPENT = [M]PLITYP
If varinit([M]PLITYP,indice)=0
 IMPENT = 1
Endif
[M]PLITYP = IMPENT

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
indice = 0
If varmode([M]ITMREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "ITMREF" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "LECITM"
 Call LECITM([M]ITMREF) From TRTX3
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("ITS","!",[M]ITMREF,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "ITMREF" : GZNUM = 3651 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone BPCNUM
mkstat = 0
zoncou = "BPCNUM"
indice = 0
If varmode([M]BPCNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BPCNUM" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("BPC","!A",[M]BPCNUM,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "BPCNUM" : GZNUM = 3662 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone PLIDAT
mkstat = 0
zoncou = "PLIDAT"
indice = 0
If varmode([M]PLIDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PLIDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
SAVACT = GACTION : GACTION = "STD"
Call AV_NBLIG From FUNVENSPV
GACTION = SAVACT
If mkstat=1 : Raz [M]NBLIG : Endif
If !mkstat
 If varmode([M]NBLIG,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
  Endif
  If !mkstat
   NBLIG = [M]NBLIG
   For nolign=1 To NBLIG
    indice = nolign-1
    [M]NBLIG = nolign
    status = 75
    Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
    If mkstat : Break : Endif
   Next nolign
   If mkstat : End : Endif
   indice = 0
   SAVACT = GACTION : GACTION = "DIVLINCONT"
   Call INSERE("PLILIN") From TABLEAUX
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc liste 4
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
If !mkstat
 SAVACT = GACTION : GACTION = "DIVLINCONT"
 Call INSERE("PLILIN") From TABLEAUX
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone NUMLIG
mkstat = 0
zoncou = "NUMLIG"
[M]NUMLIG(indice) = nolign

## Zone MINAMT
mkstat = 0
zoncou = "MINAMT"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M]CUR(nolign-1),GDEVISE(1..GNBDEV)),1))
