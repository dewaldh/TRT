Local Char YBATNUM(30) : [L]YBATNUM = "BB20181000027"

  If clalev([F:YBBD]) = 0 : Local File YBBDETAIL [YBBD] : Endif
  If clalev([F:YBBD1]) = 0 : Local File YBBDETAIL [YBBD1] : Endif

  Local Integer C : C = 0
  Local Integer ZDETLIG(0..500)
  For [F:YBBD]YBBD1 Where [F:YBBD]YBATNBR = [L]YBATNUM
    For [F:YBBD1]YBBD0 Where [F:YBBD1]YBATNBR = [L]YBATNUM and [F:YBBD1]YDETLIG = [L]C
      If [F:YBBD1]YDETLIG <> [L]C
        Infbox"DELETE WHERE BATNO + DETLIG"
      Endif
    Next
    C += 1
  Next
  #Infbox "record array:"-num$(ZDETLIG(3))


