Local Char YBATNBR(50) : YBATNBR = "BB181200020"
Local Char PREVFCYIE(5)
Local Char PREVGST(5)

  If clalev([F:YBDT])   = 0 : Local File YBBDETAILTEM [YBDT]    : Endif
  If clalev([F:YBDT2])  = 0 : Local File YBBDETAILTEM [YBDT2]   : Endif

  For [F:YBDT] Where YBATNBR = YBATNBR
    If [F:YBDT]YBATTYP = 1 and [F:YBDT2]YTAX <> ""
     Read [F:YBDT2]YBDT1 = YBATNBR;[F:YBDT]YDETLIG
      If PREVFCYIE = "" : PREVFCYIE = [F:YBDT2]YFCY : Endif
      If PREVGST = "" : PREVGST = [F:YBDT2]YTAX : Endif
        Infbox [F:YBDT2]YTAX, PREVGST
        If [F:YBDT2]YFCY <> PREVFCYIE
          If [F:YBDT2]YTAX = PREVGST
            Infbox "Same GST Group"
          Else
            Infbox "Not the same GST"
          Endif
        Endif
        Infbox "PREFCY"-PREVFCYIE, "TABFCY"-[F:YBDT2]YFCY
        PREVFCYIE = [F:YBDT2]YFCY
    Endif
  Next

