Subprog D_PFICCE12(VALEUR)
Variable Char    VALEUR()
If varinit([M]PFICCE12,indice)=0
 ## Action STD
 Call D_PFICCE12(VALEUR) From SUBPOH
Endif
Gosub _AFF
End

Subprog AS_PFICCE12(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_PFICCE12(VALEUR) From SUBPOH
If mkstat=2
 [M]PFICCE12(INDIC) = VALEUR
 Affzo PFICCE12(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_PFICCE12(zc) From W2WE3FTIST3
Onkey   2 Titled GBOUTS Call S_PFICCE12(zc) From W2WE3FTIST3
Onkey 144 Titled GBOUTP Call P_PFICCE12(zc) From W2WE3FTIST3
Onkey 146 Titled GBOUTA Call SA_PFICCE12(zc) From W2WE3FTIST3
Gosub _AFF
End

Subprog C_PFICCE12(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$(GDIE(12))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "PFICCE12" : GZNUM = 0 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$(GDIE(12)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "PFICCE12" : GZNUM = 0 : Endif
 If !mkstat
  ## Action STD
  Call C_PFICCE12(VALEUR) From SUBPOH
  If mkstat : GERR = 1 : GZONE = "PFICCE12" : GZNUM = 0 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_PFICCE12(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELCCE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELCCE"
 PARMSK(1) = GDIE(12)
 PARMSK(2) = GFINRSP
 PARMSK(3) = [M:POH0]ORDDAT
 PARMSK(4) = num$(nolign-1)
 PARMSK(5) = "GDIE(1"
 PARMSK(6) = "[M:POH3]PFICCE#"
 PARMSK(7) = "GCOA(1"
