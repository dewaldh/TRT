# Supervisor
# Mask management CIT0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CIGCOD
mkstat = 0
zoncou = "CIGCOD"
indice = 0
WCHAR = [M]CIGCOD
If varmode([M]CIGCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "CIGCOD" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("CIG","!",[M]CIGCOD,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CIGCOD" : GZNUM = 1371 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WCHAR<>[M]CIGCOD or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_CIGCOD([M]CIGCOD) From SUBCIT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CIGCOD" : GZNUM = 1371 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone CIGTYP
mkstat = 0
zoncou = "CIGTYP"
indice = 0
IMPENT = [M]CIGTYP
If varinit([M]CIGTYP,indice)=0
 IMPENT = 1
Endif
[M]CIGTYP = IMPENT

## Zone CIGSTA
mkstat = 0
zoncou = "CIGSTA"
indice = 0
IMPENT = [M]CIGSTA
If varinit([M]CIGSTA,indice)=0
 IMPENT = 1
Endif
[M]CIGSTA = IMPENT

## Zone EDIFAS
mkstat = 0
zoncou = "EDIFAS"
indice = 0
IMPENT = [M]EDIFAS
If varinit([M]EDIFAS,indice)=0
 IMPENT = 2
Endif
If varmode([M]EDIFAS,indice)=0
 [M]EDIFAS(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "EDIFAS" : Gosub ACTION From =IMPTRT
  IMPENT = [M]EDIFAS(indice)
 Endif
Endif
[M]EDIFAS = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CIT0"
Return

