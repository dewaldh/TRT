# Supervisor
# Mask management WH1STD2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone STDCOV
mkstat = 0
zoncou = "STDCOV"
indice = 0
IMPENT = [M]STDCOV
If varinit([M]STDCOV,indice)=0
 IMPENT = 1
Endif
[M]STDCOV = IMPENT

## Zone CRSCOV
mkstat = 0
zoncou = "CRSCOV"
indice = 0
IMPENT = [M]CRSCOV
If varinit([M]CRSCOV,indice)=0
 IMPENT = 1
Endif
[M]CRSCOV = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBCON
mkstat = 0
zoncou = "NBCON"
indice = 0
status = 1
zoncou = "NBCON"
If varmode([M]NBCON,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBCON" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBCON
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBCON = nolign
   status = 75
   Call SAI_NBCON(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  SAVACT = GACTION : GACTION = "STD"
  Call C_NBCON From SUBMAC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NBCON" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBWRE
mkstat = 0
zoncou = "NBWRE"
indice = 0
status = 1
zoncou = "NBWRE"
If varmode([M]NBWRE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBWRE" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBWRE
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBWRE = nolign
   status = 75
   Call SAI_NBWRE(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBCON(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBCON
mkstat = 0
zoncou = "NBCON"
SAVACT = GACTION : GACTION = "STD"
Call C_NBCON From SUBMAC
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "NBCON" : GZNUM = 0 : Endif
Gosub AFF From GSAISIE
If mkstat : End : Endif

## Zone MACCONNUM
mkstat = 0
zoncou = "MACCONNUM"
If varmode([M]MACCONNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "MACCONNUM" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("CON","!",[M]MACCONNUM(indice),"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "MACCONNUM" : GZNUM = 16090 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_MACCONNUM([M]MACCONNUM(indice)) From SUBMAC
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "MACCONNUM" : GZNUM = 16090 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone MACCONTYP
mkstat = 0
zoncou = "MACCONTYP"
IMPENT = [M]MACCONTYP(indice)
If varinit([M]MACCONTYP,indice)=0
 IMPENT = 1
Endif
[M]MACCONTYP(indice) = IMPENT

## Zone NBCON
mkstat = 0
zoncou = "NBCON"

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBWRE(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBWRE
mkstat = 0
zoncou = "NBWRE"

## Zone NBWRE
mkstat = 0
zoncou = "NBWRE"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "WH1STD2"
Return

