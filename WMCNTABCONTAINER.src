####################################################################
# AINIT
####################################################################

$AINIT
Gosub DECLARE_VAR From WMC0TABCONTAINER
Local Integer ALINE,TBLINE,ASTASNAPSHOT,ALLSTAT
If this.APARENT=null & this.snapshotEnabled=0
 this.snapshotEnabled = 1
Elsif this.APARENT<>null
 this.snapshotEnabled = this.APARENT.snapshotEnabled
Endif
If this.ASTALIN=[V]CST_ALL : this.ASTALIN = [V]CST_ANEW : Endif
Gosub INIT_TCTRTYP From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_TCTRNUM From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_DESAXX From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_SHOAXX From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_RGT From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_TEU From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_ENAFLG From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_DIU From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_TCTRHEI From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_TCTRLEN From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_TCTRWID From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_WEU From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_TARWEI From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_MAXWEI From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_VOU From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_TCTRVOL From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_USFVOL From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_IMG From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_TEX From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_CREUSR From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_UPDUSR From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_CREDATTIM From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_UPDDATTIM From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub INIT_AUUID From WMC1TABCONTAINER : Gosub TESTALLSTAT
Gosub AINIT From WMC0TABCONTAINER : Gosub TESTALLSTAT
If this.snapshotEnabled=1 & this.APARENT=null
 [L]ASTASNAPSHOT = this.freeSnapshot
Endif
Return

