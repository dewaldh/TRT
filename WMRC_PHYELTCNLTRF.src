############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(PHYELTCNLTRF,ASTATUS)
Variable Instance PHYELTCNLTRF Using R_PHYELTCNLTRF
Variable Integer ASTATUS

If PHYELTCNLTRF = null : End : Endif

Gosub AVARIABLES From WMR00PHYELTCNLTRF
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If fmet PHYELTCNLTRF.ACTX.AGETAFCRIGHT(PHYELTCNLTRF,"GESPHY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END : Endif
If fmet PHYELTCNLTRF.ACTX.AGETAFCRIGHTC(PHYELTCNLTRF,"GESPHY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AINSERT_END
Endif

Gosub AREPCONTROL_AINSERT
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If !adxlog
 Gosub AINSERT_TR_PHYELTCNLTRF
Else
 Gosub AINSERT_PHYELTCNLTRF
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_END : Return : Endif

Gosub _AINSERT_AFTER
$AINSERT_END
[L]A_I = PHYELTCNLTRF.freeSnapshot
PHYELTCNLTRF.snapshotEnabled = 1
End

#-----------------------------------------------------#
$AREPCONTROL_AINSERT
PHYELTCNLTRF.ASTALIN = [V]CST_ANEW
PHYELTCNLTRF.CNLTRF.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTCNLTRF._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTCNLTRF._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet PHYELTCNLTRF._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If PHYELTCNLTRF.CNLTRF.ASTALIN = [V]CST_ANEWDEL : Return : Endif
PHYELTCNLTRF.CNLTRF.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(PHYELTCNLTRF.CNLTRF.PHYMVT) : # PHYMVT -- 2
 If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) <> null
& and !find(PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AINSERT_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
For A_N2=1 To maxtab(PHYELTCNLTRF.CNLTRF.PHYMVT) : # PHYMVT -- 2
 If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) <> null
& and !find(PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AINSERT_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_PHYELTCNLTRF
Trbegin [F:PHY]
 Gosub AINSERT_PHYELTCNLTRF
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AINSERT_AFTER
If find(PHYELTCNLTRF.CNLTRF.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 PHYELTCNLTRF.CNLTRF.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(PHYELTCNLTRF.CNLTRF.PHYMVT)
 If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) <> null
  [L]ASTATUS2 = fmet PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)._AINSERT_AFTER
  If PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ADEL or PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2)
   PHYELTCNLTRF.CNLTRF.PHYMVT(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AINSERT_PHYELTCNLTRF
[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.CNLTRF._AINSERT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet PHYELTCNLTRF.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If PHYELTCNLTRF.snapshotEnabled=1
 [L]A_RETIK = PHYELTCNLTRF.revertUpdTick
Endif
[L]ASTATUS = max(fmet PHYELTCNLTRF.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



