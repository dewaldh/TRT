[L]A_NEWCOL = 0
For [L]A_I=1 To 15
 If ([F:ITF]LOCNUM([L]A_I-1)<>0 or [F:ITF]DEFLOCTYP([L]A_I-1)<>"" or [F:ITF]DEFLOC([L]A_I-1)<>"")
  [L]A_NEWCOL += 1
  ITMFACILIT.CLOCNUM(A_NEWCOL) = NewInstance  C_ITMFACILIT_CLOCNUM(ITMFACILIT.ACTX,NullUuid,[L]A_I,ITMFACILIT,"CLOCNUM") AllocGroup ITMFACILIT
  ITMFACILIT.CLOCNUM(A_NEWCOL).LOCNUM = [F:ITF]LOCNUM([L]A_I-1)
  ITMFACILIT.CLOCNUM(A_NEWCOL).DEFLOCTYP = [F:ITF]DEFLOCTYP([L]A_I-1)
  ITMFACILIT.CLOCNUM(A_NEWCOL).DEFLOC = [F:ITF]DEFLOC([L]A_I-1)
  [L]ASTATUS = max([L]ASTATUS,fmet ITMFACILIT.CLOCNUM(A_NEWCOL).ACTION("AREAD_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next
[L]A_NEWCOL = 0
For [L]A_I=1 To 5
 If ([F:ITF]OTRSTYP([L]A_I-1)<>0 or [F:ITF]OVECOD([L]A_I-1)<>"" or [F:ITF]OVECPNFLG([L]A_I-1)<>0)
  [L]A_NEWCOL += 1
  ITMFACILIT.COTRSTYP(A_NEWCOL) = NewInstance  C_ITMFACILIT_COTRSTYP(ITMFACILIT.ACTX,NullUuid,[L]A_I,ITMFACILIT,"COTRSTYP") AllocGroup ITMFACILIT
  ITMFACILIT.COTRSTYP(A_NEWCOL).OTRSTYP = [F:ITF]OTRSTYP([L]A_I-1)
  ITMFACILIT.COTRSTYP(A_NEWCOL).OVECOD = [F:ITF]OVECOD([L]A_I-1)
  ITMFACILIT.COTRSTYP(A_NEWCOL).OVECPNFLG = [F:ITF]OVECPNFLG([L]A_I-1)
  [L]ASTATUS = max([L]ASTATUS,fmet ITMFACILIT.COTRSTYP(A_NEWCOL).ACTION("AREAD_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next
[L]ASTATUS = max([L]ASTATUS,fmet ITMFACILIT.ACTION("AREAD_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif


Return



