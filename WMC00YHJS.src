[L]A_LEVEL    = YHJS.ACTX.LEVWML
[L]ASTATUS  = 0

Onerrgo ERR_OPENTAB
If find(YHJS.ACTX.AFOLDER,nomap,"")>0
 Local File YHOUJSTA [YHJS]
 Local File YHOUJSTA [AAA_]
Else
 Local File =YHJS.ACTX.AFOLDER+".YHOUJSTA" [YHJS]
 Local File =YHJS.ACTX.AFOLDER+".YHOUJSTA" [AAA_]
