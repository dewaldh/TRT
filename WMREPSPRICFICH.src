#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1SPRICFICH
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "SPRICFICH"
REP.CLA = "SPRICFICH"
REP.ICL = "SPF"
REP.AFCRIGHT = "GESSPL"
REP.NCL = 12517 : # Price list entry
REP.DES = 12517 : # Price list entry
REP.TIT = ""
REP.ANX = "SPF0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Price list code
     REP.CLE(1).PAR = 'PLI' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'PLI' : REP.CLE(1).VPR= 'SPF.PLI' : REP.CLE(1).DES = 2652
REP.CLE(2) = NewInstance C_AREPPAR AllocGroup REP : # Price list record
     REP.CLE(2).PAR = 'PLICRD' : REP.CLE(2).TYP = 7 : REP.CLE(2).PRO= 'PLICRD' : REP.CLE(2).VPR= 'SPF.PLICRD' : REP.CLE(2).DES = 2679
# Facets
REP.AENAFAC(1) = 1 : # Detail
REP.AENAFAC(2) = 1 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 1 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "HEADER" : REP.SEC(1).NSC = 5042 : # Header
     REP.SEC(1).BLC(1)="HEAD" : REP.SEC(1).NBC(1)=5042 : # Header
Return

