#-------------------------------------------------------#
$METHODS
$EVENTS
Case [L]AEVENT
 When "AREAD_AFTER"
  Case [L]CURPTH
   When "DUD" : Gosub ADDACS1
   When "C_BPCNAM" : Gosub ADDACS2
   When "C_REP" : Gosub ADDACS3
   When "" : Gosub ADDACSREP
  Endcase
 When "AINIT"
  Case [L]CURPTH
   When "DUD" : Gosub ADDACS1
   When "C_BPCNAM" : Gosub ADDACS2
   When "C_REP" : Gosub ADDACS3
   When "" : Gosub ADDACSREP
  Endcase
 When Default
Endcase
Return

#-------------------------------------------------------#
$PROPERTIES
Case [L]ARULE
 When "CONTROL"
  Case [L]CURPRO
   When "DUD.FCY"
    If this.FCY<>""
     [L]ASTATUS = fmet this.ACTX.AGETAFCRIGHTFCY(this,"GESSOH",this.FCY,[V]CST_ATRUE)
    Endif
   When Default
  Endcase
 When "PROPAGATE"
  Case [L]CURPRO
   When Default
  Endcase
 Endcase
 Return
 
 #-------------------------------------------------------#
 # DUD
 $ADDACS1
 Return
 
 # C_BPCNAM
 $ADDACS2
 Return
 
 # C_REP
 $ADDACS3
 Return
 
 $ADDACSREP
 Return
 


