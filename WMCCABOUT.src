####################################################################

$_AMANDATORY
Gosub DECLARE_VAR From WMC0ABOUT
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_SOLUTION From WMC1ABOUT : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AMANDATORY_HTTPADR From WMC1ABOUT : Gosub TESTALLSTAT :  Endif
