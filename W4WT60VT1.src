# Sage X3 supervisor
# Mask management WT60VT1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_LABCCE19
Onevent BEFORE_AREA Gosub AV_LABCCE19 From W4WT60VT1
Onevent CONTROL Call C_LABCCE19(zc) From W4WT60VT1
Onevent INIT_AREA Call D_LABCCE19(zc) From W4WT60VT1
Onevent BEFORE_INPUT Call AS_LABCCE19(zc) From W4WT60VT1
Return

$AV_LABCCE19
## Action INICCEFMT
Call INICCEFMT(num$(GDIE(19))) From SETGLBDIE
If GFMTDIE(19)<>""
 Chgfmt [M]LABCCE19(indice) With GFMTDIE(19)
Else
 Chgfmt [M]LABCCE19(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog D_LABCCE19(VALEUR)
Variable Char    VALEUR()
If varinit([M]LABCCE19,indice)=0
 ## Action STD
 Call D_LABCCE19(VALEUR) From FUNBENCHO
Endif
Gosub _AFF
End

Subprog AS_LABCCE19(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(8255,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
## Action STD
Call AS_LABCCE19(VALEUR) From FUNBENCHO
If mkstat=2
 [M]LABCCE19(INDIC) = VALEUR
 Affzo LABCCE19(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_LABCCE19(zc) From W4WT60VT1
Onkey   2 Titled GBOUTS Call S_LABCCE19(zc) From W4WT60VT1
Onkey 144 Titled GBOUTP Call P_LABCCE19(zc) From W4WT60VT1
Onkey 146 Titled GBOUTA Call SA_LABCCE19(zc) From W4WT60VT1
Gosub _AFF
End

Subprog C_LABCCE19(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = num$(GDIE(19))
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "LABCCE19" : GZNUM = 0 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,num$(GDIE(19)),mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "LABCCE19" : GZNUM = 0 : Endif
 If !mkstat
  ## Action STD
  Call C_LABCCE19(VALEUR) From FUNBENCHO
  If mkstat : GERR = 1 : GZONE = "LABCCE19" : GZNUM = 0 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_LABCCE19(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "CCE"
PARMSK(2) = "!"
PARMSK(3) = num$(GDIE(19))
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_LABCCE19(VALEUR)
Variable Char    VALEUR()
Call OBJET("CCE",VALEUR,num$(GDIE(19))) From GOBJET
Gosub _AFF
End

Subprog SA_LABCCE19(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "CCE"
PARMSK(2) = num$(GDIE(19))
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_LABCCE19(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = num$(GDIE(19))
Call VISU("CCE",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

