  Read [F:YVAT]TVT1 = VALEUR
  If [M:BIS1]PURPRITYP = 2
    [M:BIS3]AMTVAT(nolign-1) = (([M:BIS3]AMTNOTLIN(nolign-1) / (1 +([F:YVAT]VATRAT / 100))) - [M:BIS3]AMTNOTLIN(nolign-1)) *-1
