######################################################################################################
Gosub YJNLBBENT
End

$ACTION
  Case ACTION
    When "EXEC"   : Gosub YJNLBBENT
    When Default
  Endcase

Return

#-------------------------------------------------------------------------------#
# Tunnel to Business Batch from Business Batch
#-------------------------------------------------------------------------------#
$YJNLBBENT

  Local Char PARBOUT(250)(1..3)
  PARBOUT(1)  = ""
  PARBOUT(2)  = "YBB"
  PARBOUT(3)  = ""
  GACTION = "GOBJETC1"
  Global Char PARAM(250)(0..)
  [V]PARAM(0) = [M]BPRVCR
  [V]PARAM(1) = "YBATNBR"
  Call OBJET_CHAR(PARBOUT(1),PARBOUT(2),PARBOUT(3)) From GOBJET
  Kill [V]PARAM

Return

