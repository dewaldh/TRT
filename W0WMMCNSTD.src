# Supervisor
# Mask management WMMCNSTD 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone MACITNTYP
mkstat = 0
zoncou = "MACITNTYP"
IMPENT = [M]MACITNTYP(indice)
If varinit([M]MACITNTYP,indice)=0
 IMPENT = 1
Endif
[M]MACITNTYP(indice) = IMPENT

## Zone MACITNLND
mkstat = 0
zoncou = "MACITNLND"
IMPENT = [M]MACITNLND(indice)
If varinit([M]MACITNLND,indice)=0
 IMPENT = 1
Endif
[M]MACITNLND(indice) = IMPENT

## Zone MACORI
mkstat = 0
zoncou = "MACORI"
IMPENT = [M]MACORI(indice)
If varinit([M]MACORI,indice)=0
 IMPENT = 1
Endif
[M]MACORI(indice) = IMPENT

## Zone MACSALPRI
mkstat = 0
zoncou = "MACSALPRI"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:MCN2]CUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]MACSALPRI(indice) : Endif

## Zone MACBPCPRI
mkstat = 0
zoncou = "MACBPCPRI"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find([M:MCN2]MACBPCCUR(nolign-1),GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]MACBPCPRI(indice) : Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "WMMCNSTD"
Return

