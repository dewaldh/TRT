SAVACT = GACTION
Case BOUT
 When "1"
  FLGEXE = 1
  GACTION = "STD"
 When "2"
  FLGEXE = 1
  GACTION = "STD"
 When "4"
  FLGEXE = 1
  GACTION = "STD"
 When "3"
  FLGEXE = 1
  GACTION = "STD"
 When "B"
  FLGEXE = 1
  GACTION = "STD"
 When "C"
  FLGEXE = 1
  GACTION = "STD"
Endcase
GACTION = SAVACT
Return

