# Supervisor
## CRUD-CLASSE ## BSIIMP
# 2018-08-27 13:07:49 084


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(BSIIMP,ASTATUS)
Variable Instance BSIIMP Using R_BSIIMP
Variable Integer ASTATUS

If BSIIMP = null : End : Endif

Gosub AVARIABLES From WMR00BSIIMP
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet BSIIMP.ACTX.AGETAFCRIGHT(BSIIMP,"BSIIMPORT",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet BSIIMP.ACTX.AGETAFCRIGHTU(BSIIMP,"BSIIMPORT",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_BSIIMP
Else
 Gosub AUPDATE_BSIIMP
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = BSIIMP.freeSnapshot
BSIIMP.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
BSIIMP.ASTALIN = [V]CST_AUPD
BSIIMP.BSIIMP.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = BSIIMP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BSIIMP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet BSIIMP._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet BSIIMP._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet BSIIMP._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If BSIIMP.BSIIMP.ASTALIN = [V]CST_ANEWDEL : Return : Endif
BSIIMP.BSIIMP.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(BSIIMP.BSIIMP.ASTALIN)<>""
 [L]A_ASTALIN = BSIIMP.BSIIMP.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BSIIMP.BSIIMP.LINES) : # BSIIMPD -- 2
 If BSIIMP.BSIIMP.LINES(A_N2) <> null
& and BSIIMP.BSIIMP.LINES(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If BSIIMP.BSIIMP.LINES(A_N2).ASTALIN<>[V]CST_ANEW & BSIIMP.BSIIMP.ASTALIN=[V]CST_ADEL
   BSIIMP.BSIIMP.LINES(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If BSIIMP.BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ANEW & find(BSIIMP.BSIIMP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   BSIIMP.BSIIMP.LINES(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(BSIIMP.BSIIMP.LINES(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = BSIIMP.BSIIMP.LINES(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.BSIIMP.LINES(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
 For A_N3=1 To maxtab(BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS) : # BSIIMPDS -- 3
  If BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null
& and BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN<>[V]CST_ANEWDEL
   If BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN<>[V]CST_ANEW & BSIIMP.BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ADEL
    BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN = [V]CST_ADEL
   Endif
   If BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ANEW & find(BSIIMP.BSIIMP.LINES(A_N2).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
    BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN = [V]CST_ANEWDEL
   Endif
   If [V]CST_ASTALIN(BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN)<>""
    [L]A_ASTALIN = BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
 Next A_N3
 If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(BSIIMP.BSIIMP.LINES) : # BSIIMPD -- 2
 If BSIIMP.BSIIMP.LINES(A_N2) <> null
& and BSIIMP.BSIIMP.LINES(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(BSIIMP.BSIIMP.LINES(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = BSIIMP.BSIIMP.LINES(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.BSIIMP.LINES(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
 For A_N3=1 To maxtab(BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS) : # BSIIMPDS -- 3
  If BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null
& and BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN<>[V]CST_ANEWDEL
   If [V]CST_ASTALIN(BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN)<>""
    [L]A_ASTALIN = BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
 Next A_N3
 If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(BSIIMP.BSIIMP.ASTALIN)<>""
 [L]A_ASTALIN = BSIIMP.BSIIMP.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_BSIIMP
Trbegin [F:BSIIMP]
 Gosub AUPDATE_BSIIMP
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(BSIIMP.BSIIMP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 BSIIMP.BSIIMP.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BSIIMP.BSIIMP.LINES)
 If BSIIMP.BSIIMP.LINES(A_N2) <> null
  Local Integer ASTATUS2
  For A_N3=1 To maxtab(BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS)
   If BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null
    If BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ADEL or BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ANEWDEL
     FreeInstance BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3)
     BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) = null
    Elsif BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN <> [V]CST_ALL
     BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN = [V]CST_ALL
    Endif
    [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
   Endif
  Next A_N3
  If BSIIMP.BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ADEL or BSIIMP.BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BSIIMP.BSIIMP.LINES(A_N2)
   BSIIMP.BSIIMP.LINES(A_N2) = null
  Elsif BSIIMP.BSIIMP.LINES(A_N2).ASTALIN <> [V]CST_ALL
   BSIIMP.BSIIMP.LINES(A_N2).ASTALIN = [V]CST_ALL
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_BSIIMP
[L]A_ASTALIN = BSIIMP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.BSIIMP._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BSIIMP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If BSIIMP.snapshotEnabled=1
 [L]A_RETIK = BSIIMP.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BSIIMP.BSIIMP.LINES)
 If BSIIMP.BSIIMP.LINES(A_N2) <> null
  Local Integer ASTATUS2
  For A_N3=1 To maxtab(BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS)
   If BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null
    If BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ANEWDEL
     BSIIMP.BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN = [V]CST_ANEW
    Endif
    [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
   Endif
  Next A_N3
  If BSIIMP.BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ANEWDEL
   BSIIMP.BSIIMP.LINES(A_N2).ASTALIN = [V]CST_ANEW
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet BSIIMP.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



