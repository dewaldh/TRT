# Supervisor
# Mask management CONSSPR2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

####################################
## Bloc liste 2
####################################
IMPTBL = 0

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBLIG0
mkstat = 0
zoncou = "NBLIG0"
indice = 0
status = 1
zoncou = "NBLIG0"
If varmode([M]NBLIG0,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG0" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG0
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG0 = nolign
   status = 75
   Call SAI_NBLIG0(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 3
####################################
Subprog SAI_NBLIG0(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG0
mkstat = 0
zoncou = "NBLIG0"

## Zone INTFLD
mkstat = 0
zoncou = "INTFLD"
SAVACT = GACTION : GACTION = "STD"
Call AV_INTFLD([M]INTFLD(indice)) From CNSSPRSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]INTFLD(indice) : Endif

## Zone NBLIG0
mkstat = 0
zoncou = "NBLIG0"

End

####################################
## Bloc tableau 4
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone WIPTYP
mkstat = 0
zoncou = "WIPTYP"
IMPENT = [M]WIPTYP(indice)
SAVACT = GACTION : GACTION = "STD"
Call AV_WIPTYP(IMPENT) From CNSSPRSTD
GACTION = SAVACT
If mkstat=1 : Raz IMPENT : Endif
If !mkstat
 If varinit([M]WIPTYP,indice)=0
  IMPENT = 1
 Endif
Endif
[M]WIPTYP(indice) = IMPENT

## Zone WIPTYPSTA
mkstat = 0
zoncou = "WIPTYPSTA"
SAVACT = GACTION : GACTION = "STD"
Call AV_WIPTYPSTA([M]WIPTYPSTA(indice)) From CNSSPRSTD
GACTION = SAVACT
If mkstat=1 : Raz [M]WIPTYPSTA(indice) : Endif

## Zone WIPSTA
mkstat = 0
zoncou = "WIPSTA"
IMPENT = [M]WIPSTA(indice)
If varinit([M]WIPSTA,indice)=0
 IMPENT = 1
Endif
[M]WIPSTA(indice) = IMPENT

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
IMPENT = [M]VCRTYP(indice)
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP(indice) = IMPENT

## Zone MRPMES
mkstat = 0
zoncou = "MRPMES"
IMPENT = [M]MRPMES(indice)
If varinit([M]MRPMES,indice)=0
 IMPENT = 1
Endif
[M]MRPMES(indice) = IMPENT

## Zone ORI
mkstat = 0
zoncou = "ORI"
IMPENT = [M]ORI(indice)
If varinit([M]ORI,indice)=0
 IMPENT = 1
Endif
[M]ORI(indice) = IMPENT

## Zone VCRTYPORI
mkstat = 0
zoncou = "VCRTYPORI"
IMPENT = [M]VCRTYPORI(indice)
If varinit([M]VCRTYPORI,indice)=0
 IMPENT = 1
Endif
[M]VCRTYPORI(indice) = IMPENT

## Zone POHTYP
mkstat = 0
zoncou = "POHTYP"
IMPENT = [M]POHTYP(indice)
If varinit([M]POHTYP,indice)=0
 IMPENT = 1
Endif
[M]POHTYP(indice) = IMPENT

## Zone SOHCAT
mkstat = 0
zoncou = "SOHCAT"
IMPENT = [M]SOHCAT(indice)
If varinit([M]SOHCAT,indice)=0
 IMPENT = 1
Endif
[M]SOHCAT(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CONSSPR2"
Return

