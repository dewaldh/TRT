####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0PFINDPRICE_C_LED
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_INDEXLEX From WMC1PFINDPRICE_C_LED : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_INDEXLEX From WMC1PFINDPRICE_C_LED : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LED From WMC1PFINDPRICE_C_LED : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LED From WMC1PFINDPRICE_C_LED : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CURLED From WMC1PFINDPRICE_C_LED : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CURLED From WMC1PFINDPRICE_C_LED : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_RATDIV From WMC1PFINDPRICE_C_LED : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_RATDIV From WMC1PFINDPRICE_C_LED : Gosub TESTALLSTAT :  Endif
