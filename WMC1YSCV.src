####################################################################
# 1 : YSLCID Sales Contract ID ( Field YSLC 7)
####################################################################

$SET_YSLCID
If [L]_AOLDVAL<>this.YSLCID
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_YSLCID
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.YSLCID
 Gosub PROPAGATE_YSLCID
Endif
Return

$INIT_YSLCID
Gosub DECLARE_VAR From WMC0YSCV
[L]CURPRO = "YSLCID"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_YSLCID
Gosub DECLARE_VAR From WMC0YSCV
[L]CURPRO = "YSLCID"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_YSLCID                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_YSLCID                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_YSLCID
[L]CURPRO = "YSLCID"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.YSLCID,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_YSLCID
[L]CURPRO = "YSLCID"
If vireblc(this.YSLCID,2)<>vireblc(format$("K:7X",this.YSLCID),2)
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(149,100,1)) :  Return : # Incorrect Format
Endif
Return

$_AOTHERS_YSLCID
[L]CURPRO = "YSLCID"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_YSLCID
[L]CURPRO = "YSLCID"
Return

$PROPAGATE_YSLCID
this.isReadonly = 0
[L]CURPRO = "YSLCID"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_YSLCID
Gosub DECLARE_VAR From WMC0YSCV
[L]CURPRO = "YSLCID"
$_LNKPRO_YSLCID
Return

$_ALOADATTRIBUTE_YSLCID
[L]ASTATUS = fmet this.ASETATTRIBUTE("YSLCID","$isMandatory","true")
Return


