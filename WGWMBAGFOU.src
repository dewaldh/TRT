NOMMSK(1) = "WMBAGFOU"     : ABMSK(1)  = "BAG2"     : AMA(1) = 1
NOMMSK(2) = "WMBAG3FOU"    : ABMSK(2)  = "BAG3"     : AMA(2) = 1
NOMMSK(3) = "CONSBAG4"     : ABMSK(3)  = "BAG4"     : AMA(3) = 1
