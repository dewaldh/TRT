$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "1"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "PAY"
  PARBOUT(2) = ""
  FLGEXE = 1
  GACTION = "GOBJET"
  Call OBJET_CHAR(VALBOUT,PARBOUT(1),PARBOUT(2)) From GOBJET
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


