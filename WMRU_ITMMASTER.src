  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.ITM.CPCU([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(ITMMASTER.ITM.CTSICOD)
 If ITMMASTER.ITM.CTSICOD([L]A_I)<>null
  [L]A_ASTALIN = ITMMASTER.ITM.CTSICOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.ITM.CTSICOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
