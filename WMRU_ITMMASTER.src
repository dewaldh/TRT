 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(ITMMASTER.ITM.ITA) : # ITMACGGRP -- 4
 If ITMMASTER.ITM.ITA(A_N2) <> null
& and ITMMASTER.ITM.ITA(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If ITMMASTER.ITM.ITS(A_N2) <> null
& and ITMMASTER.ITM.ITS(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If ITMMASTER.ITM.ITS(A_N2).ASTALIN<>[V]CST_ADEL & ITMMASTER.ITM.ITS(A_N2).ASTALIN<>[V]CST_ANEWDEL
