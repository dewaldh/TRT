  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(ITMMASTER.ITM.ITU) : # ITMBPC -- 2
 If ITMMASTER.ITM.ITU(A_N2) <> null
& and ITMMASTER.ITM.ITU(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(ITMMASTER.ITM.ITP) : # ITMBPS -- 3
 If ITMMASTER.ITM.ITP(A_N2) <> null
