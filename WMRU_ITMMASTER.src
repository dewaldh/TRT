############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(ITMMASTER,ASTATUS)
Variable Instance ITMMASTER Using R_ITMMASTER
Variable Integer ASTATUS

If ITMMASTER = null : End : Endif

Gosub AVARIABLES From WMR00ITMMASTER
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet ITMMASTER.ACTX.AGETAFCRIGHT(ITMMASTER,"GESITM",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet ITMMASTER.ACTX.AGETAFCRIGHTU(ITMMASTER,"GESITM",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_ITMMASTER
Else
 Gosub AUPDATE_ITMMASTER
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = ITMMASTER.freeSnapshot
ITMMASTER.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
ITMMASTER.ASTALIN = [V]CST_AUPD
ITMMASTER.ITM.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = ITMMASTER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = ITMMASTER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet ITMMASTER._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet ITMMASTER._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet ITMMASTER._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
