####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMR0PHYELTTRF
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_P_TRFDAT From WMR1PHYELTTRF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_P_TRFDAT From WMR1PHYELTTRF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_P_TRFTYP From WMR1PHYELTTRF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_P_TRFTYP From WMR1PHYELTTRF : Gosub TESTALLSTAT :  Endif
