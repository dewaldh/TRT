If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_P_BUICOD_ORI From WMR1PHYELTTRF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_P_BUICOD_ORI From WMR1PHYELTTRF : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_P_CPY_ORI From WMR1PHYELTTRF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_P_CPY_ORI From WMR1PHYELTTRF : Gosub TESTALLSTAT :  Endif
If this.PHYTRF<>null
