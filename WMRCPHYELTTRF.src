If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_P_LCTCOD_ORI From WMR1PHYELTTRF : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_P_LCTCOD_ORI From WMR1PHYELTTRF : Gosub TESTALLSTAT :  Endif
