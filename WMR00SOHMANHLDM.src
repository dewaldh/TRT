Gosub TEST From WMCR_SORDER : End

############################################################################################
## R : Subprog AREAD(INSTANCE,SOHNUM)
## U : Subprog AUPDATE(INSTANCE)
############################################################################################
# SORDER : SORDER
# SORDERP : SORDER SORDERP
# SORDERQ : SORDERP SORDERQ
# CPTANALIN : SORDERP CPTANALIN
# SVCRVAT : SORDER SVCRVAT
# SVCRFOOT : SORDER SVCRFOOT
# CPTANALIN : SVCRFOOT CPTANALIN
# GACCDUDATE : SORDER GACCDUDATE
############################################################################################

############################################################################################
### AVARIABLES
############################################################################################
$AVARIABLES
[L]ASTATUS =max([L]ASTATUS,fmet SOHMANHLDM._ADELERRORS([V]CST_ALLERRORS))
$_AVARIABLES
Local    Integer     A_LEVEL,A_I,A_NEWCOL,A_N1,A_N2,A_S2,A_N3,A_S3,A_N4,A_S4,A_N5,A_S5,A_N6,A_S6,A_N7,A_S7,A_N8,A_S8
Local    Integer    ASTATUS,A_RETIK,A_TYPMAJ,A_ASTALIN
Local    Char        A_STRSITE(GLONFCY),A_STRACC(GLONACS),CURPRO(50),A_PARENT(50),A_PATH(200),A_ABR(GLONABR),A_ERROR(250)
Local    Uuident    A_UUID

[L]A_LEVEL    = SOHMANHLDM.ACTX.LEVWML
[L]ASTATUS  = 0

Onerrgo ERR_OPENTAB
If find(SOHMANHLDM.ACTX.AFOLDER,nomap,"")>0
 Local File SORDER [SOH]
 Local File SORDERP [SOP]
 Local File SORDERQ [SOQ]
 Local File CPTANALIN [CALSOP]
 Local File SVCRVAT [SVV]
 Local File SVCRFOOT [SVF]
 Local File CPTANALIN [CALSVF]
 Local File GACCDUDATE [DUD]
 Local File ATEXTRA [AXX]
Else
 Local File =SOHMANHLDM.ACTX.AFOLDER+".SORDER" [SOH]
 Local File =SOHMANHLDM.ACTX.AFOLDER+".SORDERP" [SOP]
 Local File =SOHMANHLDM.ACTX.AFOLDER+".SORDERQ" [SOQ]
 Local File =SOHMANHLDM.ACTX.AFOLDER+".CPTANALIN" [CALSOP]
 Local File =SOHMANHLDM.ACTX.AFOLDER+".SVCRVAT" [SVV]
 Local File =SOHMANHLDM.ACTX.AFOLDER+".SVCRFOOT" [SVF]
 Local File =SOHMANHLDM.ACTX.AFOLDER+".CPTANALIN" [CALSVF]
 Local File =SOHMANHLDM.ACTX.AFOLDER+".GACCDUDATE" [DUD]
 Local File =SOHMANHLDM.ACTX.AFOLDER+".ATEXTRA" [AXX]
Endif
Onerrgo
Return

$ERR_OPENTAB
[L]ASTATUS = max([L]ASTATUS,fmet SOHMANHLDM.ASETERROR("",errp+"("+num$(errl)+")"-mess(6,123,1)-num$(errn)-errmes$(errn)-errm,[V]CST_AERROR))
Resume

############################################################################################
### Link
############################################################################################
$AOPEN_LINK
Gosub AVARIABLES
Link [SOH] With
& [SOP]SOP0 ~= [F:SOH]SOHNUM,
& [SOQ]SOQ0 = [F:SOP]SOHNUM;[F:SOP]SOPLIN;[F:SOP]SOPSEQ,
& [CALSOP]CAL0 ~= "SOP";0;[F:SOP]SOHNUM;[F:SOP]SOPLIN;[F:SOP]SOPSEQ;"";1,
& [SVV]SVV0 = 20;[F:SOH]SOHNUM,
& [SVF]SVF1 = 20;[F:SOH]SOHNUM,
& [CALSVF]CAL0 ~= "SVF";[F:SVF]VCRTYP;[F:SVF]VCRNUM;0;[F:SVF]DTA;"";1,
& [DUD]DUD0 = "*SO";[F:SOH]SOHNUM
& As [LNK_]
Return



