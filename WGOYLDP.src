$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "1"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = num$("YLDP")
  PARBOUT(2) = num$("LO")
  FLGEXE = 1
  GACTION = "YPJMCRE"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","","",1,"SPEYPJMCRE") From GTRAITE
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
Endcase
GACTION = SAVACT
Return


