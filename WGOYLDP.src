# Supervisor
# Window management OYLDP

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(14)
Local Integer A_RESULT
A_DATE  = [17/10/2018]
A_HEURE = "11:54:59"
A_USER  = "MAJID"
Call ISWINVALID(nomap,GLANGUE,"OYLDP",0,A_STAMP,A_RESULT,1) From INTRUTILB
Return

$NAVIGMSK
Local Char NAVMSK(200)
NAVMSK="YLDP;YLDP1;YLDP2;YLDP3;"
Return

$OUVRE_BOITE
Gosub INIT_BOITE
Gosub DEF_BOITE
Gosub AFF_BOITE
Return

$INIT_BOITE
Local Char    OBJLIS(GLONAOB)(0..10), DESLIS(80)(0..10), BOX1(10)
Local Integer CHGLIS(0..10), BROLIS(0..10), TRELIS(0..10)
Local Char    SAIRAP(50)(1..16)
Local Integer DERLU , AFOLDEF
Local Char    ABRENA(GLONABR)
DERLU = 1
AFOLDEF = 1
ABRENA = ""
BOX1 = "GAU_CHE"
OBJLIS(0) = "YLDP"
CHGLIS(0) = 2
BROLIS(0) = 2
TRELIS(0) = 1
DESLIS(0) = "YLNDPURID"
If clalev([M:YLDP])=0 : Local Mask YLDP [YLDP] : Endif
If clalev([M:YLDP1])=0 : Local Mask YLDP1 [YLDP1] : Endif
If clalev([M:YLDP2])=0 : Local Mask YLDP2 [YLDP2] : Endif
If clalev([M:YLDP3])=0 : Local Mask YLDP3 [YLDP3] : Endif
Gosub TIT
nolign = 1
Return

$DEF_BOITE
Gosub HORODAT From WGOYLDP
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "OYLDP" From GFONCTION At A_STAMP With A_WINDPREV Mask [YLDP]
&   Folder Mask [YLDP1]
& , Folder Mask [YLDP2]
& , Folder Mask [YLDP3]
&  Listbox [YLDP] GAU_CHE
&   [L]SAIRAP(1) = [F:YLDP]YLNDPURID Using "K:15X" Titled TITSEL(1)
&  Listbox [YLDP9] GAU_CHE9
&   [L]SAIRAP(1) = [F:YLDP9]A_TAB1 Using "K:15X" Titled TITSEL9(1) 
& As [YLDP1]
Titled [YLDP1] TIT
Return

$VISU_BOITE
Gosub INIT_BOITE
Gosub HORODAT From WGOYLDP
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Inpbox "OYLDP" From GFONCTION At A_STAMP With A_WINDPREV Mask [YLDP]
&   Folder Mask [YLDP1]
& , Folder Mask [YLDP2]
& , Folder Mask [YLDP3]
& As [YLDP1]
Titled [YLDP1] TIT
Gosub AFF_BOITE
Return

$AFF_BOITE
Boxact [YLDP1] Folder AFOLDEF
Return

$SAISIE_BOITE
Boxinp [YLDP1] Using REPONSE
Return

$FERME_BOITE
Onerrgo RIEN
Close Local Inpbox [YLDP1]
Onerrgo
Return

$RIEN
Resume

$TIT
Local Char TITLIS(30)
Call TEXTE(100879,TITLIS) From OBJDIV
Local Char TITLIS9(30)
TITLIS9 = mess(196,123,1)
Local Char TITSEL(30)(1..1)
Call TEXTE(100623,TITSEL(1)) From OBJDIV
Local Char TITSEL9(30)(1..1)
Call TEXTE(100623,TITSEL9(1)) From OBJDIV
Local Char TIT(60) , TITTRN(60)
Call TEXTE(100650,TIT) From OBJDIV
If GPROGRAMME<>""
 If dim([L]ACTION)<0 : Local Char ACTION(20) : Endif
 [L]ACTION = "TITRE" : Gosub ACTION From =GPROGRAMME
Endif
Return

$REMPLIT_
Gosub SETCLE_
If dim(FILTREG)<=0 or FILTREG
 If CRITERE=""  : CRITERE  = "1=1" : Endif
 If FILTSUP=""  : FILTSUP  = "1=1" : Endif
 If FILROLE=""  : FILROLE  = "1=1" : Endif
 If FILSUITE="" : FILSUITE = "1=1" : Endif
 Filter [YLDP] Where evalue(CRITERE) & evalue(FILTSUP) & evalue(FILROLE) & evalue(FILSUITE)
&       Order With CLELISTE
Endif
GPOINT = "AV_FILLBOX" : Gosub ENTREE From EXEFNC
ACTION = "HINT" : Gosub ACTION From =GPROGRAMME
If HINTLIS(0)<>'' or NOHLIS(0)<>0
 Fillbox [YLDP1] GAU_CHE Hint Key =HINTLIS(0)
Else
 Gosub CALNOHOBJ From GOBJET
 If A_NOH
  Fillbox [YLDP1] GAU_CHE
 Else
  Fillbox [YLDP1] GAU_CHE Hint Key =""
 Endif
Endif
GPOINT = "AP_FILLBOX" : Gosub ENTREE From EXEFNC
Filter [YLDP]
Return

$SETCLE_
Local Char    CLELISTE(250)
Local Integer INDLIS , SNSLIS , LENLIS, GAUTRELIS
INDLIS = 1
SNSLIS = 1
LENLIS = 1
GAUTRELIS = 1
CLELISTE = "YLNDPURID"
ACTION = "CLE_GAUCHE" : Gosub ACTION From =GPROGRAMME
Return

$DEFAUT_BRO
Case ILIS
 When 0 : Default file [YLDP]
Endcase
Return

$SETBOX
Case currbox
 When "GAU_CHE"
  Gosub SETCLE_
  Filter [YLDP] Order With CLELISTE
  Setlbox [YLDP1] GAU_CHE
  Filter [YLDP]
 When "GAU_CHE9"
  Gosub CLE_DERLU From =PROGOBJ
  Setlbox [YLDP1] GAU_CHE9
Endcase
Return

$SETORDER
Case currbox
 When "GAU_CHE"
  Gosub SETCLE_
  Filter [YLDP] Order With CLELISTE
Endcase
Return

$ENDORDER
Case currbox
 When "GAU_CHE"
  Gosub SETCLE_
  Filter [YLDP]
Endcase
Return

$PICKING
Return

$DEPICK
Return

$REPICK
Return

$SETCUR
Return

$DEFVAR
Local Char    ABMSK(GLONABR)(0..15) , NOMMSK(GLONAMK)(0..15)
Local Integer NBMASK , AMA(0..15) , NBMSKCMP
NOMMSK(0) = "YLDP"         : ABMSK(0)  = "YLDP"     : AMA(0) = 1
NOMMSK(1) = "YLDP1"        : ABMSK(1)  = "YLDP1"    : AMA(1) = 1
NOMMSK(2) = "YLDP2"        : ABMSK(2)  = "YLDP2"    : AMA(2) = 1
NOMMSK(3) = "YLDP3"        : ABMSK(3)  = "YLDP3"    : AMA(3) = 1
NBMASK = 3
NBMSKCMP = 0
Return

$SETMASK
Case MA
 When 0 : If clalev([M:YLDP])>0 : Default Mask [YLDP] : Endif
 When 1 : If clalev([M:YLDP1])>0 : Default Mask [YLDP1] : Endif
 When 2 : If clalev([M:YLDP2])>0 : Default Mask [YLDP2] : Endif
 When 3 : If clalev([M:YLDP3])>0 : Default Mask [YLDP3] : Endif
Endcase
Return

$TITMASK
