# Supervisor
# Mask management YSETRUL 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_YTCCE3
Onevent BEFORE_AREA Gosub AV_YTCCE3 From W3YSETRUL
Onevent CONTROL Call C_YTCCE3(zc) From W3YSETRUL
Onevent BEFORE_INPUT Call AS_YTCCE3(zc) From W3YSETRUL
Return

$AV_YTCCE3
## Action INICCEFMT
Call INICCEFMT("") From SETGLBDIE
If GCCEFMT<>""
 Chgfmt [M]YTCCE3(indice) With "K:"+GCCEFMT
Else
 Chgfmt [M]YTCCE3(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog AS_YTCCE3(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3531,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_YTCCE3(zc) From W3YSETRUL
Onkey   2 Titled GBOUTS Call S_YTCCE3(zc) From W3YSETRUL
Onkey 144 Titled GBOUTP Call P_YTCCE3(zc) From W3YSETRUL
Onkey 146 Titled GBOUTA Call SA_YTCCE3(zc) From W3YSETRUL
Gosub _AFF
End

Subprog C_YTCCE3(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action CCECHOICE
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CCECHOICE"
 PARMSK(1) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"CCECHOICE","TRTSEL","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "YTCCE3" : GZNUM = 45810 : Endif
Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("CCE","!",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "YTCCE3" : GZNUM = 45810 : Endif
Endif
Gosub _AFF
End

Subprog S_YTCCE3(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "CCE"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_YTCCE3(VALEUR)
Variable Char    VALEUR()
Call OBJET("CCE",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_YTCCE3(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "CCE"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_YTCCE3(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("CCE",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

