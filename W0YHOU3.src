Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBSTA
mkstat = 0
zoncou = "NBSTA"
indice = 0
status = 1
zoncou = "NBSTA"
If varmode([M]NBSTA,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBSTA" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBSTA
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBSTA = nolign
   status = 75
   Call SAI_NBSTA(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  SAVACT = GACTION : GACTION = "SPE"
  Call C_NBSTA From SPEYHOU
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NBSTA" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBVAR
mkstat = 0
zoncou = "NBVAR"
