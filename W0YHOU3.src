   indice = nolign-1
   [M]NBREC = nolign
   status = 75
   Call SAI_NBREC(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 4
####################################
IMPTBL = 0

mkstat = 0

End

