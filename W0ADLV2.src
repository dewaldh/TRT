# Supervisor
# Mask management ADLV2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBRACT
mkstat = 0
zoncou = "NBRACT"
indice = 0
status = 1
zoncou = "NBRACT"
If varmode([M]NBRACT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBRACT From SUBADLV
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBRACT : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBRACT" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBRACT
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBRACT = nolign
   status = 75
   Call SAI_NBRACT(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBRACT(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBRACT
mkstat = 0
zoncou = "NBRACT"

## Zone MODACV
mkstat = 0
zoncou = "MODACV"
IMPENT = [M]MODACV(indice)
If varinit([M]MODACV,indice)=0
 IMPENT = 1
Endif
[M]MODACV(indice) = IMPENT

## Zone LICACT
mkstat = 0
zoncou = "LICACT"
IMPENT = [M]LICACT(indice)
WNUM = IMPENT
If varinit([M]LICACT,indice)=0
 IMPENT = 1
Endif
If varmode([M]LICACT,indice)=0
 [M]LICACT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "LICACT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]LICACT(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_LICACT(IMPENT) From SUBADLV
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "LICACT" : GZNUM = 50132 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]LICACT(indice) = IMPENT

## Zone FLGACT
mkstat = 0
zoncou = "FLGACT"
IMPENT = [M]FLGACT(indice)
WNUM = IMPENT
If varinit([M]FLGACT,indice)=0
 IMPENT = 1
Endif
If varmode([M]FLGACT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_FLGACT(IMPENT) From SUBADLV
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]FLGACT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLGACT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLGACT(indice)
 Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_FLGACT(IMPENT) From SUBADLV
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "FLGACT" : GZNUM = 5083 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]FLGACT(indice) = IMPENT

## Zone NOTAVAACT
mkstat = 0
zoncou = "NOTAVAACT"
IMPENT = [M]NOTAVAACT(indice)
WNUM = IMPENT
If varinit([M]NOTAVAACT,indice)=0
 IMPENT = 1
Endif
If varmode([M]NOTAVAACT,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NOTAVAACT(IMPENT) From SUBADLV
 GACTION = SAVACT
 If mkstat=1 : Raz IMPENT : Endif
 [M]NOTAVAACT(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "NOTAVAACT" : Gosub ACTION From =IMPTRT
  IMPENT = [M]NOTAVAACT(indice)
 Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_NOTAVAACT(IMPENT) From SUBADLV
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NOTAVAACT" : GZNUM = 51380 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]NOTAVAACT(indice) = IMPENT

## Zone TYPACV
mkstat = 0
zoncou = "TYPACV"
IMPENT = [M]TYPACV(indice)
If varinit([M]TYPACV,indice)=0
 IMPENT = 1
Endif
[M]TYPACV(indice) = IMPENT

## Zone NBRACT
mkstat = 0
zoncou = "NBRACT"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "ADLV2"
Return

