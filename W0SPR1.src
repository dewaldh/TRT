# Supervisor
# Mask management SPR1 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBREN
mkstat = 0
zoncou = "NBREN"
indice = 0
status = 1
zoncou = "NBREN"
If varmode([M]NBREN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBREN" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBREN
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBREN = nolign
   status = 75
   Call SAI_NBREN(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  SAVACT = GACTION : GACTION = "STD"
  Call C_NBREN From SUBSPR
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NBREN" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBREN(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBREN
mkstat = 0
zoncou = "NBREN"
If !mkstat
 SAVACT = GACTION : GACTION = "STD"
 Call C_NBREN From SUBSPR
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "NBREN" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone DESAXX
mkstat = 0
zoncou = "DESAXX"
If varinit([M]DESAXX,indice)=0
 SAVACT = GACTION : GACTION = "LECTEXTRA"
 Call LECTEXTRA([M]DESAXX(indice),"SPREASON","DESAXX",num$(nolign),"") From ATEXTRA
 GACTION = SAVACT
Endif
If !mkstat
 If varmode([M]DESAXX,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "DESAXX" : Gosub ACTION From =IMPTRT
  Endif
  If vireblc([M]DESAXX(indice),2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "DESAXX" : GZNUM = 8 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone DACMAN
mkstat = 0
zoncou = "DACMAN"
IMPENT = [M]DACMAN(indice)
If varinit([M]DACMAN,indice)=0
 IMPENT = 1
Endif
If varmode([M]DACMAN,indice)=0
 [M]DACMAN(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "DACMAN" : Gosub ACTION From =IMPTRT
  IMPENT = [M]DACMAN(indice)
 Endif
Endif
[M]DACMAN(indice) = IMPENT

## Zone UPDPRI
mkstat = 0
zoncou = "UPDPRI"
IMPENT = [M]UPDPRI(indice)
If varinit([M]UPDPRI,indice)=0
 IMPENT = 1
Endif
If varmode([M]UPDPRI,indice)=0
 [M]UPDPRI(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "UPDPRI" : Gosub ACTION From =IMPTRT
  IMPENT = [M]UPDPRI(indice)
 Endif
Endif
[M]UPDPRI(indice) = IMPENT

## Zone NBREN
mkstat = 0
zoncou = "NBREN"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SPR1"
Return

