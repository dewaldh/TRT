############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(BSIIMP,ASTATUS)
Variable Instance BSIIMP Using C_BSIIMP
Variable Integer ASTATUS

If BSIIMP = null : End : Endif

Gosub _AVARIABLES From WMC00BSIIMP

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_BSIIMP

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(BSIIMP,ASTATUS)
Variable Instance BSIIMP Using C_BSIIMP
Variable Integer ASTATUS

If BSIIMP = null : End : Endif

Gosub AVARIABLES From WMC00BSIIMP
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
BSIIMP.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_BSIIMP
Else
 Gosub AUPDATE_BSIIMP
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = BSIIMP.freeSnapshot
BSIIMP.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet BSIIMP._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet BSIIMP._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet BSIIMP._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If BSIIMP.ASTALIN = [V]CST_ANEWDEL : Return : Endif
BSIIMP.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(BSIIMP.ASTALIN)<>""
 [L]A_ASTALIN = BSIIMP.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BSIIMP.LINES) : # BSIIMPD -- 2
 If BSIIMP.LINES(A_N2) <> null
& and BSIIMP.LINES(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If BSIIMP.LINES(A_N2).ASTALIN<>[V]CST_ANEW & BSIIMP.ASTALIN=[V]CST_ADEL
   BSIIMP.LINES(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ANEW & find(BSIIMP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   BSIIMP.LINES(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(BSIIMP.LINES(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = BSIIMP.LINES(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.LINES(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
 For A_N3=1 To maxtab(BSIIMP.LINES(A_N2).SUBDETAILS) : # BSIIMPDS -- 3
  If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null
& and BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN<>[V]CST_ANEWDEL
   If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN<>[V]CST_ANEW & BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ADEL
    BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN = [V]CST_ADEL
   Endif
   If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ANEW & find(BSIIMP.LINES(A_N2).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
    BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN = [V]CST_ANEWDEL
   Endif
   If [V]CST_ASTALIN(BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN)<>""
    [L]A_ASTALIN = BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
 Next A_N3
 If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(BSIIMP.LINES) : # BSIIMPD -- 2
 If BSIIMP.LINES(A_N2) <> null
& and BSIIMP.LINES(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(BSIIMP.LINES(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = BSIIMP.LINES(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.LINES(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
 For A_N3=1 To maxtab(BSIIMP.LINES(A_N2).SUBDETAILS) : # BSIIMPDS -- 3
  If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null
& and BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN<>[V]CST_ANEWDEL
   If [V]CST_ASTALIN(BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN)<>""
    [L]A_ASTALIN = BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
 Next A_N3
 If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(BSIIMP.ASTALIN)<>""
 [L]A_ASTALIN = BSIIMP.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_BSIIMP
Trbegin [F:BSIIMP]
 Gosub AUPDATE_BSIIMP
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_BSIIMP
[L]A_ASTALIN = BSIIMP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00BSIIMP._MAJSQL_BSIIMP(BSIIMP))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 For A_N2=1 To maxtab(BSIIMP.LINES)
  If BSIIMP.LINES(A_N2) <> null and
& (BSIIMP.LINES(A_N2).ASTALIN=[L]A_TYPMAJ or
& BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ALL)
   If BSIIMP.LINES(A_N2).ASTALIN=[L]A_TYPMAJ
    If [V]CST_ASTALIN(BSIIMP.LINES(A_N2).ASTALIN)<>""
     [L]A_ASTALIN = BSIIMP.LINES(A_N2).ASTALIN
     [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.LINES(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
     If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
    Endif
    [L]ASTATUS = func WMC00BSIIMP._MAJSQL_BSIIMPD(BSIIMP.LINES(A_N2))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
    If [V]CST_ASTALIN(BSIIMP.LINES(A_N2).ASTALIN)<>""
     [L]A_ASTALIN = BSIIMP.LINES(A_N2).ASTALIN
     [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.LINES(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
     If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
    Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
  #-----------------------#
  For A_N3=1 To maxtab(BSIIMP.LINES(A_N2).SUBDETAILS)
   If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null and
& (BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[L]A_TYPMAJ or
& BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ALL)
    If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[L]A_TYPMAJ
     If [V]CST_ASTALIN(BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN)<>""
      [L]A_ASTALIN = BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN
      [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
      If [L]ASTATUS>=[V]CST_AERROR : Break 3 : Endif
     Endif
     [L]ASTATUS = func WMC00BSIIMP._MAJSQL_BSIIMPDS(BSIIMP.LINES(A_N2).SUBDETAILS(A_N3))
     If [L]ASTATUS>=[V]CST_AERROR : Break 3 : Endif
     If [V]CST_ASTALIN(BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN)<>""
      [L]A_ASTALIN = BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN
      [L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
      If [L]ASTATUS>=[V]CST_AERROR : Break 3 : Endif
     Endif
    Endif
   Endif
   If [L]ASTATUS>=[V]CST_AERROR : Break 3 : Endif
  Next A_N3
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
  #-----------------------#
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BSIIMP.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BSIIMP.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If BSIIMP.snapshotEnabled=1
 [L]A_RETIK = BSIIMP.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BSIIMP.LINES)
 If BSIIMP.LINES(A_N2) <> null
  Local Integer ASTATUS2
  For A_N3=1 To maxtab(BSIIMP.LINES(A_N2).SUBDETAILS)
   If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null
    If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ANEWDEL
     BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN = [V]CST_ANEW
    Endif
    [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
   Endif
  Next A_N3
  If BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ANEWDEL
   BSIIMP.LINES(A_N2).ASTALIN = [V]CST_ANEW
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet BSIIMP.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(BSIIMP.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 BSIIMP.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BSIIMP.LINES)
 If BSIIMP.LINES(A_N2) <> null
  Local Integer ASTATUS2
  For A_N3=1 To maxtab(BSIIMP.LINES(A_N2).SUBDETAILS)
   If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) <> null
    If BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ADEL or BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN=[V]CST_ANEWDEL
     FreeInstance BSIIMP.LINES(A_N2).SUBDETAILS(A_N3)
     BSIIMP.LINES(A_N2).SUBDETAILS(A_N3) = null
    Elsif BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN <> [V]CST_ALL
     BSIIMP.LINES(A_N2).SUBDETAILS(A_N3).ASTALIN = [V]CST_ALL
    Endif
    [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
   Endif
  Next A_N3
  If BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ADEL or BSIIMP.LINES(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BSIIMP.LINES(A_N2)
   BSIIMP.LINES(A_N2) = null
  Elsif BSIIMP.LINES(A_N2).ASTALIN <> [V]CST_ALL
   BSIIMP.LINES(A_N2).ASTALIN = [V]CST_ALL
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



