Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CRSCOVSAM
mkstat = 0
zoncou = "CRSCOVSAM"
indice = 0
IMPENT = [M]CRSCOVSAM
If varinit([M]CRSCOVSAM,indice)=0
 IMPENT = 1
Endif
[M]CRSCOVSAM = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBITM
mkstat = 0
zoncou = "NBITM"
indice = 0
status = 1
zoncou = "NBITM"
If varmode([M]NBITM,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBITM From SUBCONH
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBITM : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBITM" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBITM
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBITM = nolign
   status = 75
   Call SAI_NBITM(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_NBITM From SUBCONH
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NBITM" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone ICDFLGX
mkstat = 0
zoncou = "ICDFLGX"
indice = 0
IMPENT = [M]ICDFLGX
WNUM = IMPENT
If varinit([M]ICDFLGX,indice)=0
 IMPENT = 1
Endif
If varmode([M]ICDFLGX,indice)=0
 [M]ICDFLGX(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "ICDFLGX" : Gosub ACTION From =IMPTRT
  IMPENT = [M]ICDFLGX(indice)
 Endif
 If WNUM<>IMPENT or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_ICDFLGX(IMPENT) From SUBCONH
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "ICDFLGX" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif
[M]ICDFLGX = IMPENT

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

## Zone NBCPN
mkstat = 0
zoncou = "NBCPN"
indice = 0
status = 1
zoncou = "NBCPN"
If varmode([M]NBCPN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBCPN" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBCPN
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBCPN = nolign
   status = 75
   Call SAI_NBCPN(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

