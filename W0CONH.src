  Call CONTROLE("ITM","!",[M]CNNCPN(indice),"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CNNCPN" : GZNUM = 4600 : Endif
  Gosub AFF From GSAISIE
