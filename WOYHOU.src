Endif
Return

$REMP_DERLU
If clalev([F:YHJD9])>0
 Filter [YHJD9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [YHOU1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:YHJD9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:YHJD]YJBNR+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:YHJD9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:YHJD9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 4
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:YHJD]YJBNR"
Call TEXTE(100095,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:15X"
SELFOR(NBSEL) = "K:15X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:YHJD]YJBNR"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:YHJD]YJBCOD"
Call TEXTE(100195,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:40X"
SELFOR(NBSEL) = "K:40X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:YHJD]YJBCOD"
TRILIB(NBTRI) = SELLIB(NBSEL)
If clalev([F:YHJD_])=0
 Gosub OUVFIC_LISTE
Endif
Return

