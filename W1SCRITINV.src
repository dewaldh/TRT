Subprog AS_CRISRENUM(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey   2 Titled GBOUTS Call S_CRISRENUM(zc) From W1SCRITINV
Onkey 144 Titled GBOUTP Call P_CRISRENUM(zc) From W1SCRITINV
Onkey 146 Titled GBOUTA Call SA_CRISRENUM(zc) From W1SCRITINV
Gosub _AFF
End

Subprog C_CRISRENUM(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("SRE","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "CRISRENUM" : GZNUM = 26094 : Endif
If !mkstat
 ## Action STD
 Call C_CRISRENUM(VALEUR) From TRTCRITSIH
 If mkstat : GERR = 1 : GZONE = "CRISRENUM" : GZNUM = 26094 : Endif
Endif
Gosub _AFF
End

$AMM_CRISRENUM
Call AM_CRISRENUM(zc)
If !mkstat & GOBJET
 zoncou = "CRISRENUM" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_CRISRENUM(VALEUR)
Variable Char    VALEUR()
## Action STD
Call AM_CRISRENUM(VALEUR) From TRTCRITSIH
If mkstat : GERR = 1 : GZONE = "CRISRENUM" : GZNUM = 26094 : Endif
If !mkstat & evalue("dim("+clanam([M])+"ZCRISRENUM)")>indice
 [M]CRISRENUM(indice) = VALEUR
 Affzo ZCRISRENUM(indice)
Endif
Gosub _AFF
End

Subprog S_CRISRENUM(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action GSELECT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
