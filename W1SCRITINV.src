 PARMSK(1) = "CRISRENUM"
 PARMSK(2) = "TRTCRITSIH"
 Call S_TAB_CHAR(VALEUR,PARMSK,"GSELECT","ASELGEN","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog SA_CRISRENUM(VALEUR)
