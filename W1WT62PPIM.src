Subprog C_STA(VALEUR)
Variable Char    VALEUR()
## Action STD
Call C_STA(VALEUR) From TRTSTOENM
If mkstat : GERR = 1 : GZONE = "STA" : GZNUM = 1287 : Endif
If !mkstat
 ## Action STKCTRSTA
 Call STKCTRSTA(VALEUR,[F:SRU]AUZSST,[M:ENM]QUAFLG,1,[M:ENM]QUAFRE,mkstat) From STKACT
 If mkstat : GERR = 1 : GZONE = "STA" : GZNUM = 1287 : Endif
Endif
