####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0EXPENSES_CODEXPENS
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_ACC From WMC1EXPENSES_CODEXPENS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_ACC From WMC1EXPENSES_CODEXPENS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_COA From WMC1EXPENSES_CODEXPENS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_COA From WMC1EXPENSES_CODEXPENS : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_LED From WMC1EXPENSES_CODEXPENS : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_LED From WMC1EXPENSES_CODEXPENS : Gosub TESTALLSTAT :  Endif
