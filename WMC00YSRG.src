$ERR_OPENTAB
[L]ASTATUS = max([L]ASTATUS,fmet YSRG.ASETERROR("",errp+"("+num$(errl)+")"-mess(6,123,1)-num$(errn)-errmes$(errn)-errm,[V]CST_AERROR))
