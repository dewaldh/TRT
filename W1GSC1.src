 SAVACT = GACTION : GACTION = "SELSAC"
 PARMSK(1) = [M]COA
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELSAC","CPTSEL","") From GACTION
 GACTION = SAVACT
Endif
