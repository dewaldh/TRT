Subprog AS_SAC(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_SAC(zc) From W1GSC1
Gosub _AFF
End

Subprog C_SAC(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "SAC" : GZNUM = 321 : Endif
If !GWEBSERV
 If !mkstat
  ## Action CONTSAC
  Call CONTSAC(VALEUR,[M]COA) From CPTCNT
  If mkstat : GERR = 1 : GZONE = "SAC" : GZNUM = 321 : Endif
 Endif
Endif
If !mkstat
 ## Action STD
 Call C_SAC(VALEUR) From SUBGSC
 If mkstat : GERR = 1 : GZONE = "SAC" : GZNUM = 321 : Endif
Endif
Gosub _AFF
End

Subprog S_SAC(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELSAC
