# Supervisor
# Mask management GSC1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [28/08/2018]
A_HEURE = "00:02:42"
A_USER  = "ADMIN"
A_STAMP = "20180828000242"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBRSAC"
  If [M]NBRSAC=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask GSC1 [GSC1]
Gosub HORODAT
Local Inpbox "GSC1" From GFONCTION At A_STAMP With A_WINDPREV Mask [GSC1] As [SIM_]
Grizo [GSC1]5
Grizo [GSC1]10
Grizo [GSC1]15
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [GSC1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=3
X1(1)=1 : X2(1)=74 : Y1(1)=4 : Y2(1)=5
X1(2)=1 : X2(2)=74 : Y1(2)=6 : Y2(2)=28
Return

$ON_COA
Return

$ON_ZCOA
Onevent BEFORE_AREA Gosub AV_ZCOA From W1GSC1
Return

$AV_ZCOA
Call LECTEXTRA(zc,"GCOA","DESTRA",num$([M]COA(indice)),"") From ATEXTRA
Gosub _AFF
Return

$ON_GRU
Onevent CONTROL Call C_GRU(zc) From W1GSC1
Onevent BEFORE_INPUT Call AS_GRU(zc) From W1GSC1
Return

Subprog AS_GRU(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
Onkey   2 Titled GBOUTS Call S_GRU(zc) From W1GSC1
Gosub _AFF
End

Subprog C_GRU(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "GRU" : GZNUM = 29593 : Endif
If !mkstat
 ## Action CONTOBJW
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONTOBJW"
 PARMSK(1) = "GSC"
 Call S_AFF_CHAR(VALEUR,PARMSK,"CONTOBJW","CONTOBJ","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "GRU" : GZNUM = 29593 : Endif
Endif
Gosub _AFF
End

Subprog S_GRU(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELOBJET
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELOBJET"
 PARMSK(1) = "GSC"
 PARMSK(2) = "GRU"
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJET","SELOBJ","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

$ON_DESTRA
Onevent INIT_AREA Call D_DESTRA(zc) From W1GSC1
Onevent BEFORE_INPUT Call AS_DESTRA(zc) From W1GSC1
Return

Subprog IB_DESTRA(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action BOUTTEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "BOUTTEXTRA"
 Call S_AFF_CHAR(VALEUR,PARMSK,"","ATEXTRA","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog D_DESTRA(VALEUR)
Variable Char    VALEUR()
If varinit([M]DESTRA,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"GRPSAC","DESTRA",[M:GSC1]COA,[M:GSC1]GRU) From ATEXTRA
Endif
Gosub _AFF
End

Subprog AS_DESTRA(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(9637,GBOUT1) From OBJDIV
Call IB_DESTRA(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_DESTRA(zc) From W1GSC1
Gosub _AFF
End

Subprog B1_DESTRA(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ATEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ATEXTRA"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_DESTRA" : PROGSUB = "SUBGSC" : PROGSPE = "SPEGSC" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "GRPSAC"
 PARMSK(2) = "DESTRA"
 PARMSK(3) = [M:GSC1]COA
 PARMSK(4) = [M:GSC1]GRU
 Call SAISIE_CHAR(VALEUR,PARMSK,"ATEXTRA","ATEXTRA","") From GSAISIE
 ACTION = "BB1_DESTRA" : PROGSUB = "SUBGSC" : PROGSPE = "SPEGSC" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

