$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "2"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "CRITBENCH5"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"CRITBENCH5","MFGCRITLIB","XYZMFGCRITL") From GSAISIE
 When "5"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:MGP]FCY
  PARBOUT(2) = [M:MGP]ITMREFS
  PARBOUT(3) = num$([M:MGP]TOTPECQTY)
  PARBOUT(4) = [M:MGP]DATBES
  PARBOUT(5) = [M:MGP]HVCRNUM
  PARBOUT(6) = num$([M:MGP]HWIPSTA)
  PARBOUT(7) = num$([M:MGP]HWIPTYP)
  PARBOUT(8) = num$([M:MGP]INDPEC)
  PARBOUT(9) = num$([M:MGP]SCOHAB)
  PARBOUT(10) = [M:MGP]PLANNER
  PARBOUT(11) = [M:MGP]HPJT
  PARBOUT(12) = num$([M:MGP]REACT)
  FLGEXE = 1
  GACTION = "SAICREEOX"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"DCREEOX","BENCHLIBS","ZBENCHLIBS") From GSAISIE
  [M:MGP]HVCRNUM = PARBOUT(5)
  [M:MGP]HWIPSTA = val(PARBOUT(6))
  [M:MGP]HWIPTYP = val(PARBOUT(7))
  [M:MGP]REACT = val(PARBOUT(12))
 When "T"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GMRLNUM
  FLGEXE = 1
  GACTION = "VISUMRL"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IMRL","SELTRS","XWIMRL") From GSAISIE
Endcase
GACTION = SAVACT
Return

