Subprog AS_BPAADD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(15597,GBOUTS) From OBJDIV
## Action STD
Call AS_BPAADD(VALEUR) From SUBSADR
If mkstat=2
 [M]BPAADD(INDIC) = VALEUR
 Affzo BPAADD(INDIC) : mkstat = 2
Endif
Onkey   2 Titled GBOUTS Call S_BPAADD(zc) From W1SAIADR
Gosub _AFF
End

Subprog C_BPAADD(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "BPAADD" : GZNUM = 1499 : Endif
If !mkstat
 ## Action STD
 Call C_BPAADD(VALEUR) From SUBSADR
 If mkstat : GERR = 1 : GZONE = "BPAADD" : GZNUM = 1499 : Endif
Endif
Gosub _AFF
End

$AMM_BPAADD
Call AM_BPAADD(zc)
If !mkstat & GOBJET
 zoncou = "BPAADD" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_BPAADD(VALEUR)
Variable Char    VALEUR()
## Action STD
