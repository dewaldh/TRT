$LEC_DERLU
Read [AFF]AFF0 = [F:SLH]STOFCY;GPROFIL;GUSRFCT
If fstat : Raz [F:AFF] : Endif
Return

$CHARGE_DERLU
If clalev([F:SLH9])>0
 Read [SLH9] = [F:SLH9]A_TABI
 [L]CLE = [F:SLH9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:SLH9])>0
 Filter [SLH9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [SLM1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:SLH9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:SLH]VCRNUM+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:SLH9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:SLH9]
 Endif
Endif
Return

