Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone PCK
mkstat = 0
zoncou = "PCK"
indice = 0
WCHAR = [M]PCK
If varmode([M]PCK,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PCK" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]PCK,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "PCK" : GZNUM = 1846 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("TPA","",[M]PCK,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "PCK" : GZNUM = 1846 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   If WCHAR<>[M]PCK or IMPMOD
    SAVACT = GACTION : GACTION = "STD"
    Call AM_PCK([M]PCK) From FUNPKD
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "PCK" : GZNUM = 1846 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone PACNUM
mkstat = 0
zoncou = "PACNUM"
indice = 0
WCHAR = [M]PACNUM
If varmode([M]PACNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PACNUM" : Gosub ACTION From =IMPTRT
 Endif
 If WCHAR<>[M]PACNUM or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_PACNUM([M]PACNUM) From FUNPKD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "PACNUM" : GZNUM = 3126 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone LBLFMT
mkstat = 0
zoncou = "LBLFMT"
indice = 0
If varmode([M]LBLFMT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LBLFMT" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("ARP","!",[M]LBLFMT,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "LBLFMT" : GZNUM = 12057 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
  SAVACT = GACTION : GACTION = "STD"
  Call C_NBLIG From FUNPKD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone PREUSR
mkstat = 0
zoncou = "PREUSR"
indice = 0
If varmode([M]PREUSR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "PREUSR" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("AUS","!",[M]PREUSR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "PREUSR" : GZNUM = 37302 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone GROWEI
mkstat = 0
zoncou = "GROWEI"
indice = 0
If varmode([M]GROWEI,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "GROWEI" : Gosub ACTION From =IMPTRT
 Endif
 If [M]GROWEI<0
  mkstat = 1 : GMESSAGE = mess(180,141,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "GROWEI" : GZNUM = 29743 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_GROWEI([M]GROWEI) From FUNPKD
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "GROWEI" : GZNUM = 29743 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

mkstat = 0

End

