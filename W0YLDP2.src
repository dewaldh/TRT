# Supervisor
# Mask management YLDP2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG1
mkstat = 0
zoncou = "NBLIG1"
indice = 0
status = 1
zoncou = "NBLIG1"
If varmode([M]NBLIG1,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG1" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG1
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG1 = nolign
   status = 75
   Call SAI_NBLIG1(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone YALLOC
mkstat = 0
zoncou = "YALLOC"
If varmode([M]YALLOC,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YALLOC" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YALLOC(indice)=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YALLOC" : GZNUM = 4286 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YVALFRM
mkstat = 0
zoncou = "YVALFRM"
If varmode([M]YVALFRM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YVALFRM" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YVALFRM(indice)=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YVALFRM" : GZNUM = 100870 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YVALTO
mkstat = 0
zoncou = "YVALTO"
If varmode([M]YVALTO,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YVALTO" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

