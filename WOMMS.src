$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(10)(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
If clalev([F:MPA9])>0 : Close Local File [MPA9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(10),
& Char     A_TAB2(30)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2
& As [MPA9]
Return

$LEC_DERLU
Return

$CHARGE_DERLU
If clalev([F:MPA9])>0
 Read [MPA9] = [F:MPA9]A_TABI
 [L]CLE = [F:MPA9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:MPA9])>0
 Filter [MPA9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [MMS1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:MPA9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:MPA]ID+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:MPA9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:MPA9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
Local Char    SELZON(30)(0..16)
Local Char    SELEXP(200)(0..16)
Local Char    SELLIB(30)(0..16)
Local Char    SELFMT(30)(0..16)
Local Schar   SELFOR(30)(0..16)
Local Integer SELTYP(0..16)
Local Integer SELMAJ(0..16)
Local Char    TRIZON(20)(0..16)
Local Char    TRILIB(30)(0..16)
SELCAR = 1
NBSEL = 0 : NBTRI = 0
NBSEL += 1
SELZON(NBSEL) = "[F:MPA]ID"
Call TEXTE(1377,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:10X"
SELFOR(NBSEL) = "K:10X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:MPA]ID"
TRILIB(NBTRI) = SELLIB(NBSEL)
NBSEL += 1
SELZON(NBSEL) = "[F:MPA]INTIT"
Call TEXTE(8,SELLIB(NBSEL)) From OBJDIV
SELTYP(NBSEL) = 7
SELFMT(NBSEL) = "K:30X"
SELFOR(NBSEL) = "K:30X"
SELMAJ(NBSEL) = 0
NBTRI += 1
TRIZON(NBTRI) = "[F:MPA]INTIT"
TRILIB(NBTRI) = SELLIB(NBSEL)
Return

