# Supervisor
# Mask management PWI0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone VCRNUM
mkstat = 0
zoncou = "VCRNUM"
indice = 0
If varmode([M]VCRNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "VCRNUM" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]VCRNUM,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 4420 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "STD"
  Call C_VCRNUM([M]VCRNUM) From SUBPWI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "VCRNUM" : GZNUM = 4420 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone MFGTRKFLG
mkstat = 0
zoncou = "MFGTRKFLG"
indice = 0
IMPENT = [M]MFGTRKFLG
If varinit([M]MFGTRKFLG,indice)=0
 IMPENT = 1
Endif
[M]MFGTRKFLG = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone ISSVLTCOD
mkstat = 0
zoncou = "ISSVLTCOD"
indice = 0
IMPENT = [M]ISSVLTCOD
If varinit([M]ISSVLTCOD,indice)=0
 IMPENT = 1
Endif
[M]ISSVLTCOD = IMPENT

## Zone RCPVLTCOD
mkstat = 0
zoncou = "RCPVLTCOD"
indice = 0
IMPENT = [M]RCPVLTCOD
If varinit([M]RCPVLTCOD,indice)=0
 IMPENT = 1
Endif
[M]RCPVLTCOD = IMPENT

## Zone PRIREG
mkstat = 0
zoncou = "PRIREG"
indice = 0
IMPENT = [M]PRIREG
If varinit([M]PRIREG,indice)=0
 IMPENT = 1
Endif
[M]PRIREG = IMPENT

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
indice = 0
IMPENT = [M]VCRTYP
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc liste 4
####################################
IMPTBL = 0

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
