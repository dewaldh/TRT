# Supervisor
# Mask management CONSSET2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 1
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone PAYPRC
mkstat = 0
zoncou = "PAYPRC"
IMPENT = [M]PAYPRC(indice)
If varinit([M]PAYPRC,indice)=0
 IMPENT = 1
Endif
[M]PAYPRC(indice) = IMPENT

## Zone TXNTYP
mkstat = 0
zoncou = "TXNTYP"
If varinit([M]TXNTYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TXNTYP(indice),"399","") From SUBADI
 GACTION = SAVACT
Endif

## Zone REQRSP
mkstat = 0
zoncou = "REQRSP"
IMPENT = [M]REQRSP(indice)
If varinit([M]REQRSP,indice)=0
 IMPENT = 1
Endif
[M]REQRSP(indice) = IMPENT

## Zone AMT
mkstat = 0
zoncou = "AMT"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find("",GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]AMT(indice) : Endif

## Zone TAXAMT
mkstat = 0
zoncou = "TAXAMT"
SAVACT = GACTION : GACTION = "INIDEV"
GACTION = SAVACT
GDEVFMT="11.2" : GDEVFMT = GDEVFMT1(max(find("",GDEVISE(1..GNBDEV)),1))
If mkstat=1 : Raz [M]TAXAMT(indice) : Endif

## Zone CRDTYP
mkstat = 0
zoncou = "CRDTYP"
If varinit([M]CRDTYP,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]CRDTYP(indice),"398","") From SUBADI
 GACTION = SAVACT
Endif

## Zone ADDLIG
mkstat = 0
zoncou = "ADDLIG"
SAVACT = GACTION : GACTION = "FMTADRPAY"
Call FMTADRPAY("") From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]ADDLIG(indice) : Endif

## Zone CTY
mkstat = 0
zoncou = "CTY"
WCHAR = [M]CTY(indice)
SAVACT = GACTION : GACTION = "FMTCTYPAY"
Call FMTCTYPAY([M]CRY) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]CTY(indice) : Endif

## Zone POSCOD
mkstat = 0
zoncou = "POSCOD"
WCHAR = [M]POSCOD(indice)
SAVACT = GACTION : GACTION = "FMTPOSPAY"
Call FMTPOSPAY([M]CRY) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]POSCOD(indice) : Endif

## Zone CRY
mkstat = 0
zoncou = "CRY"
If varinit([M]CRY,indice)=0
 [M]CRY(indice) = GPAYS
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "CONSSET2"
Return

