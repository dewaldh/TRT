############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(TABINVCND,ASTATUS)
Variable Instance TABINVCND Using C_TABINVCND
Variable Integer ASTATUS

If TABINVCND = null : End : Endif

Gosub _AVARIABLES From WMC00TABINVCND

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_TABINVCND

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(TABINVCND,ASTATUS)
Variable Instance TABINVCND Using C_TABINVCND
Variable Integer ASTATUS

If TABINVCND = null : End : Endif

Gosub AVARIABLES From WMC00TABINVCND
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
TABINVCND.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_TABINVCND
Else
 Gosub AUPDATE_TABINVCND
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = TABINVCND.freeSnapshot
TABINVCND.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet TABINVCND._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet TABINVCND._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet TABINVCND._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If TABINVCND.ASTALIN = [V]CST_ANEWDEL : Return : Endif
TABINVCND.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(TABINVCND.ASTALIN)<>""
 [L]A_ASTALIN = TABINVCND.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(TABINVCND.INVCNDLIN) : # TABINVCNDLIN -- 2
 If TABINVCND.INVCNDLIN(A_N2) <> null
& and TABINVCND.INVCNDLIN(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If TABINVCND.INVCNDLIN(A_N2).ASTALIN<>[V]CST_ANEW & TABINVCND.ASTALIN=[V]CST_ADEL
   TABINVCND.INVCNDLIN(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If TABINVCND.INVCNDLIN(A_N2).ASTALIN=[V]CST_ANEW & find(TABINVCND.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   TABINVCND.INVCNDLIN(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(TABINVCND.INVCNDLIN(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(TABINVCND.INVCNDLIN(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(TABINVCND.INVCNDLIN) : # TABINVCNDLIN -- 2
 If TABINVCND.INVCNDLIN(A_N2) <> null
& and TABINVCND.INVCNDLIN(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(TABINVCND.INVCNDLIN(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(TABINVCND.INVCNDLIN(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(TABINVCND.ASTALIN)<>""
 [L]A_ASTALIN = TABINVCND.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_TABINVCND
Trbegin [F:INVCND]
 Gosub AUPDATE_TABINVCND
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_TABINVCND
[L]A_ASTALIN = TABINVCND.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00TABINVCND._MAJSQL_TABINVCND(TABINVCND))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 For A_N2=1 To maxtab(TABINVCND.INVCNDLIN)
  If TABINVCND.INVCNDLIN(A_N2) <> null and
& (TABINVCND.INVCNDLIN(A_N2).ASTALIN=[L]A_TYPMAJ or
& TABINVCND.INVCNDLIN(A_N2).ASTALIN=[V]CST_ALL)
   If TABINVCND.INVCNDLIN(A_N2).ASTALIN=[L]A_TYPMAJ
    Case TABINVCND.INVCNDLIN(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.INVCNDLIN(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = TABINVCND.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet TABINVCND.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If TABINVCND.snapshotEnabled=1
 [L]A_RETIK = TABINVCND.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(TABINVCND.INVCNDLIN)
 If TABINVCND.INVCNDLIN(A_N2) <> null
  [L]ASTATUS2 = fmet TABINVCND.INVCNDLIN(A_N2)._AUPDATE_ABORT
  If TABINVCND.INVCNDLIN(A_N2).ASTALIN=[V]CST_ADEL or TABINVCND.INVCNDLIN(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance TABINVCND.INVCNDLIN(A_N2)
   TABINVCND.INVCNDLIN(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet TABINVCND.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(TABINVCND.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 TABINVCND.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(TABINVCND.INVCNDLIN)
 If TABINVCND.INVCNDLIN(A_N2) <> null
  [L]ASTATUS2 = fmet TABINVCND.INVCNDLIN(A_N2)._AUPDATE_AFTER
  If TABINVCND.INVCNDLIN(A_N2).ASTALIN=[V]CST_ADEL or TABINVCND.INVCNDLIN(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance TABINVCND.INVCNDLIN(A_N2)
   TABINVCND.INVCNDLIN(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



