Funprog GET_X3FILTER(CODE)
Value Char    CODE
Local Clbfile    X3FILTER
 Case  CODE
  When  "YFIL01" : 
  X3FILTER='[F:YSWF]YCTRISS = [00/00/0000]'
  When  "YFIL02" : 
  X3FILTER='[F:YSWF]YCTRISS <> [00/00/0000] & [F:YSWF]YCTRSIG = [00/00/0000]'
  When  "YFIL03" : 
  X3FILTER='[F:YSWF]YCTRCAN = [00/00/0000] & [F:YSLC]YSALSTA  <= 5  & YTOTRCP < YDEPDUE'
  When  "YFIL04" : 
  X3FILTER='[F:YSWF]YFINAPP <= date$ & [F:YSWF]YDATSTA = 1 & [F:YSLC]YSALSTA  <= 5 & [F:YSWF]YFINAPP <> [00/00/0000]'
  When  "YFIL05" : 
  X3FILTER='[F:YSWF]YCANREQ = 2'
  When  "YFIL06" : 
  X3FILTER='[F:YSLC]YSALSTA = 7'
  When  "YFIL07" : 
  X3FILTER='[F:YSWF]YCALSETFOR <> [00/00/0000] & [F:YSWF]YCALSETFOR <= date$ + 14  & [F:YSWF]YOVRSETFOR<> [00/00/0000] & [F:YSWF]YOVRSETFOR<= date$ + 14 & [F:YSLC]YSALSTA  <= 5'
  When  "YFIL08" : 
  X3FILTER='[F:YSWF]YCALSETFOR <> [00/00/0000] & [F:YSWF]YCALSETFOR <= date$ & [F:YSWF]YOVRSETFOR <> [00/00/0000] & [F:YSWF]YOVRSETFOR <= date$ & [F:YSLC]YSALSTA  <= 5'
  When  "YFIL09" : 
  X3FILTER='[F:YSLC]YSALSTA = 6'
  When  Default: X3FILTER="1=1"
 Endcase
 End X3FILTER
#-------------------------------------------------------------------------------
Funprog GET_SDATAFILTER(CODE)
Value Char    CODE
Local Char T0(30)(1..)
Local Char T1(30)(1..)
Local Integer NBPRO : NBPRO=0
NBPRO += 1 : T0(NBPRO) = "[V]CST_ANO" : T1(NBPRO) = "false" 
NBPRO += 1 : T0(NBPRO) = "[V]CST_AYES" : T1(NBPRO) = "true" 
End TOSDATA(T0,T1,func GET_X3FILTER(CODE))
