####################################################################
# 1 : PIHLIN Terms ( Field A 40)
####################################################################
 
$SET_PIHLIN
If [L]_AOLDVAL<>this.PIHLIN
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_PIHLIN
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.PIHLIN
 Gosub PROPAGATE_PIHLIN
Endif
Return

$INIT_PIHLIN
Gosub DECLARE_VAR From WMR0PYDINQM
[L]CURPRO = "PIHLIN"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_PIHLIN
Gosub DECLARE_VAR From WMR0PYDINQM
[L]CURPRO = "PIHLIN"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_PIHLIN                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_PIHLIN                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_PIHLIN
[L]CURPRO = "PIHLIN"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & vireblc(this.PIHLIN,2)=""
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_PIHLIN
[L]CURPRO = "PIHLIN"
Return

$_AOTHERS_PIHLIN
[L]CURPRO = "PIHLIN"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_PIHLIN
[L]CURPRO = "PIHLIN"
Return

$PROPAGATE_PIHLIN
this.isReadonly = 0
[L]CURPRO = "PIHLIN"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_PIHLIN
Gosub DECLARE_VAR From WMR0PYDINQM
[L]CURPRO = "PIHLIN"
$_LNKPRO_PIHLIN
Return




