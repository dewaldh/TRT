$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "1"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "SIH"
  PARBOUT(2) = ""
  FLGEXE = 1
  GACTION = "GOBJET"
  Call OBJET_CHAR(VALBOUT,PARBOUT(1),PARBOUT(2)) From GOBJET
 When "2"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "SOH"
  PARBOUT(2) = ""
  FLGEXE = 1
  GACTION = "GOBJET"
  Call OBJET_CHAR(VALBOUT,PARBOUT(1),PARBOUT(2)) From GOBJET
 When "E"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:SQH0]SQHTEX1
  PARBOUT(2) = "SQH"
  PARBOUT(3) = "TRTX3TEX"
  PARBOUT(4) = ""
  PARBOUT(5) = num$(0)
  PARBOUT(6) = num$(1)
  FLGEXE = 1
  GACTION = "ACLOB"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ACLOB","ACLOB",3,"XYACLOB") From GTRAITE
  [M:SQH0]SQHTEX1 = PARBOUT(1)
 When "F"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:SQH0]SQHTEX2
  PARBOUT(2) = "SQH"
  PARBOUT(3) = "TRTX3TEX"
  PARBOUT(4) = ""
  PARBOUT(5) = num$(0)
  PARBOUT(6) = num$(1)
  FLGEXE = 1
  GACTION = "ACLOB"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ACLOB","ACLOB",3,"XYACLOB") From GTRAITE
  [M:SQH0]SQHTEX2 = PARBOUT(1)
 When "H"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "ADB1"
  PARBOUT(2) = [M:SQH0]BPCORD
  PARBOUT(3) = "SQH0"
  PARBOUT(4) = ""
  FLGEXE = 1
  GACTION = "SAIADRBPC"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IADRBPC","TRTVENFEN","XWIADRBPC") From GSAISIE
  OK = 1
  ACTION = "AVANT_SUITE" : Gosub ACTION From =GPROGRAMME
  If OK
   Local Char VALBOUT(250) , PARBOUT(250)(1..20)
   PARBOUT(1) = "SQH0"
   PARBOUT(2) = ""
   FLGEXE = 1
   GACTION = "TRTRECTAR"
   Call TRAITE_CHAR(VALBOUT,PARBOUT,"","TRTVENTAR","",2,"XYTRTVENTAR") From GTRAITE
  Endif
 When "J"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "ADB2"
  PARBOUT(2) = [M:SQH0]BPCORD
  PARBOUT(3) = "SQH0"
  PARBOUT(4) = "BPAADD"
  FLGEXE = 1
  GACTION = "SAIADRBPC"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IADRBPC","TRTVENFEN","XWIADRBPC") From GSAISIE
  OK = 1
  ACTION = "AVANT_SUITE" : Gosub ACTION From =GPROGRAMME
  If OK
   Local Char VALBOUT(250) , PARBOUT(250)(1..20)
   PARBOUT(1) = "SQH0"
   PARBOUT(2) = "BPAADD"
   FLGEXE = 1
   GACTION = "TRTRECTAR"
   Call TRAITE_CHAR(VALBOUT,PARBOUT,"","TRTVENTAR","",2,"XYTRTVENTAR") From GTRAITE
  Endif
 When "M"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "SQH0"
  FLGEXE = 1
  GACTION = "SAICEE"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ICEE","TRTVENFEN","XWICEE") From GSAISIE
 When "K"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "VISUSITSQH"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"ISQHS","TRTVENFEN","XWISQHS") From GSAISIE
 When "L"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
