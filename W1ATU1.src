# Supervisor
# Mask management ATU1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [27/08/2018]
A_HEURE = "23:50:32"
A_USER  = "ADMIN"
A_STAMP = "20180827235032"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIG"
  If [M]NBLIG=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask ATU1 [ATU1]
Gosub HORODAT
Local Inpbox "ATU1" From GFONCTION At A_STAMP With A_WINDPREV Mask [ATU1] As [SIM_]
Grizo [ATU1]10
Grizo [ATU1]20
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [ATU1]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=74 : Y1(0)=1 : Y2(0)=3
X1(1)=1 : X2(1)=66 : Y1(1)=4 : Y2(1)=28
Return

$ON_CRY
Onevent CONTROL Call C_CRY(zc) From W1ATU1
Onevent BEFORE_INPUT Call AS_CRY(zc) From W1ATU1
Onevent AFTER_CHANGE Gosub AMM_CRY From W1ATU1
Onevent INIT_AREA Call D_CRY(zc) From W1ATU1
Return

Subprog D_CRY(VALEUR)
Variable Char    VALEUR()
If varinit([M]CRY,indice)=0
 VALEUR = GPAYS
Endif
Gosub _AFF
End

Subprog AS_CRY(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(53316,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_CRY(zc) From W1ATU1
Onkey   2 Titled GBOUTS Call S_CRY(zc) From W1ATU1
Onkey 144 Titled GBOUTP Call P_CRY(zc) From W1ATU1
Onkey 146 Titled GBOUTA Call SA_CRY(zc) From W1ATU1
Gosub _AFF
End

Subprog C_CRY(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "CRY" : GZNUM = 263 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("TCY","",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "CRY" : GZNUM = 263 : Endif
 If !mkstat
  ## Action STD
  Call C_CRY(VALEUR) From SUBATU
  If mkstat : GERR = 1 : GZONE = "CRY" : GZNUM = 263 : Endif
 Endif
Endif
Gosub _AFF
End

$AMM_CRY
Call AM_CRY(zc)
If !mkstat & GOBJET
 zoncou = "CRY" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_CRY(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZCRY)")>indice
 [M]CRY(indice) = VALEUR
 Affzo ZCRY(indice)
Endif
Gosub _AFF
End

Subprog S_CRY(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "TCY"
PARMSK(2) = ""
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_CRY(VALEUR)
Variable Char    VALEUR()
Call OBJET("TCY",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_CRY(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TCY"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_CRY(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TCY",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZCRY
Onevent BEFORE_AREA Gosub AV_ZCRY From W1ATU1
Return

$AV_ZCRY
Call LECTEXTRA(zc,"TABCOUNTRY","CRYDES",num$([M]CRY(indice)),"") From ATEXTRA
Gosub _AFF
Return

$ON_TYP
Onevent BEFORE_INPUT Call AS_TYP(zc) From W1ATU1
Onevent CONTROL Call C_TYP(zc) From W1ATU1
Onevent INIT_AREA Call D_TYP(zc) From W1ATU1
Return

Subprog D_TYP(VALEUR)
Variable Integer VALEUR
If varinit([M]TYP,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_TYP(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(7831) From COMBOS
## Action STD
Call AS_TYP(VALEUR) From SUBATU
If mkstat=2
 [M]TYP(INDIC) = VALEUR
 Affzo TYP(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,7831,1))))
Endif
If !mkstat
 Discombo 0 From TYP(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From TYP(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_TYP(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
## Action STD
Call C_TYP(VALEUR) From SUBATU
If mkstat : GERR = 1 : GZONE = "TYP" : GZNUM = 31744 : Endif
Gosub _AFF
End

$ON_NBLIG
Onevent CONTROL Call C_NBLIG From W1ATU1
Onevent BEFORE_AREA Call AV_NBLIG From W1ATU1
Return

Subprog AV_NBLIG
Local Decimal BASPAG
If adxgtb=1 & !GORITITCOL & nolign=1
 Local Char BASPAG(10)
 BASPAG = "NBLIG" : Gosub TIT_COL From W1ATU1
Endif
Gosub _AFF
End

Subprog C_NBLIG
Local Decimal BASPAG
## Action STD
Call C_NBLIG From SUBATU
If mkstat : GERR = 1 : GZONE = "NBLIG" : GZNUM = 0 : Endif
Gosub _AFF
End

$ON_COD
Onevent BEFORE_AREA Gosub AV_COD From W1ATU1
Onevent BEFORE_INPUT Call AS_COD(zc) From W1ATU1
Onevent CONTROL Call C_COD(zc) From W1ATU1
Return

Subprog IB_COD(VALEUR)
Variable Char    VALEUR()
## Action INISAT
Local Char    PARMSK(250)(1..20) , SAVACT(20)
SAVACT = GACTION : GACTION = "INISAT"
PARMSK(1) = [M]CRY
PARMSK(2) = "1~" + num$([M]TYP)
Call S_AFF_CHAR(VALEUR,PARMSK,"INISAT","TRTDIV","") From GACTION
GACTION = SAVACT
Gosub _AFF
End

$AV_COD
If !GWEBSERV
 ## Action FMTSAT
 Call FMTSATPAY([M]CRY,"1~" + num$([M]TYP)) From TRTDIV
Endif
If GFMT<>""
 Chgfmt [M]COD(indice) With "K:"+GFMT
Else
 Chgfmt [M]COD(indice) With "-K:1X"
Endif
Gosub _AFF
Return

Subprog AS_COD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(31745,GBOUT2) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call IB_COD(VALEUR)
If  GWEBSERV
 ## Action SAISAT
 Call SAISAT([M]CRY,"1~" + num$([M]TYP)) From TRTDIV
 If mkstat=2
  [M]COD(INDIC) = VALEUR
  Affzo COD(INDIC) : mkstat = 2
 Endif
Endif
Onkey 130 Titled GBOUT2 Call B2_COD(zc) From W1ATU1
Onkey   2 Titled GBOUTS Call S_COD(zc) From W1ATU1
Gosub _AFF
End

Subprog C_COD(VALEUR)
Variable Char    VALEUR()
## Action %OBLIG
If vireblc(VALEUR,2)=""
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 0 : Endif
If !mkstat
 ## Action CTLSAT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CTLSAT"
 PARMSK(1) = [M]CRY
 PARMSK(2) = "1~" + num$([M]TYP)
 Call S_AFF_CHAR(VALEUR,PARMSK,"CTLSAT","TRTDIV","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 0 : Endif
 If !mkstat
  ## Action CONTUNI
  Call UNICITE2(VALEUR,[M]NBLIG,[M]COD) From OBJDIV
  If mkstat : GERR = 1 : GZONE = "COD" : GZNUM = 0 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_COD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELSAT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELSAT"
 PARMSK(1) = [M]CRY
 PARMSK(2) = "1~" + num$([M]TYP)
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELSAT","TRTDIV","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B2_COD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action TUNSAT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "TUNSAT"
 PARMSK(1) = [M]CRY
 PARMSK(2) = "1~" + num$([M]TYP)
 Call S_AFF_CHAR(VALEUR,PARMSK,"TUNSAT","TRTDIV","") From GACTION
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$ON_INTIT
Onevent INIT_AREA Call D_INTIT(zc) From W1ATU1
Onevent BEFORE_INPUT Call AS_INTIT(zc) From W1ATU1
Return

Subprog IB_INTIT(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action BOUTTEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "BOUTTEXTRA"
 Call S_AFF_CHAR(VALEUR,PARMSK,"","ATEXTRA","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog D_INTIT(VALEUR)
Variable Char    VALEUR()
If varinit([M]INTIT,indice)=0
 ## Action LECTEXTRA
 Call LECTEXTRA(VALEUR,"TABSUBDIV","INTIT",[M]CRYTYP,[M]COD(nolign-1)) From ATEXTRA
Endif
Gosub _AFF
End

Subprog AS_INTIT(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(9637,GBOUT1) From OBJDIV
Call IB_INTIT(VALEUR)
Onkey 129 Titled GBOUT1 Call B1_INTIT(zc) From W1ATU1
Gosub _AFF
End

Subprog B1_INTIT(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action ATEXTRA
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "ATEXTRA"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB1_INTIT" : PROGSUB = "SUBATU" : PROGSPE = "SPEATU" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "TABSUBDIV"
 PARMSK(2) = "INTIT"
 PARMSK(3) = [M]CRYTYP
 PARMSK(4) = [M]COD(nolign-1)
 Call SAISIE_CHAR(VALEUR,PARMSK,"ATEXTRA","ATEXTRA","") From GSAISIE
 ACTION = "BB1_INTIT" : PROGSUB = "SUBATU" : PROGSPE = "SPEATU" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

$TIT_COL
Local Char TITCL(200)(1..2)
Raz TITCL
Case BASPAG
 When "NBLIG"
  TITCL(1) = [M]INTCOD
  TITCL(2) = [M]INTINT
  If !GORITITCOL
   Titcol [M]NBLIG TITCL(1) For COD
   Titcol [M]NBLIG TITCL(2) For INTIT
  Else
   Titcol [M:ATU1]NBLIG TITCL(1) For COD
   Titcol [M:ATU1]NBLIG TITCL(2) For INTIT
  Endif
Endcase
Return

$CHG_STY
Return


