#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management RELLCR 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone BAN
mkstat = 0
zoncou = "BAN"
indice = 0
WCHAR = [M]BAN
If varmode([M]BAN,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "BAN" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("BAN","!",[M]BAN,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "BAN" : GZNUM = 7475 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WCHAR<>[M]BAN or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_BAN([M]BAN) From RELLCR
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "BAN" : GZNUM = 7475 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone INI
mkstat = 0
zoncou = "INI"
indice = 0
If varinit([M]INI,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call D_INI([M]INI) From RELLCR
 GACTION = SAVACT
Endif

## Zone CPY
mkstat = 0
zoncou = "CPY"
indice = 0
If varmode([M]CPY,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_CPY([M]CPY) From RELLCR
 GACTION = SAVACT
 If mkstat=1 : Raz [M]CPY : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "CPY" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  If vireblc([M]CPY,2)=""
   mkstat = 1 : GMESSAGE = mess(10,123,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "ACONTCPY"
   Call CONTSOC([M]CPY) From SUBAGF
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    Call CONTROLE("CPY","",[M]CPY,"",mkstat) From CONTOBJ
    If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTCPY"
     Call CONTCPY([M]CPY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "CPY" : GZNUM = 1343 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]FCY,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]FCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","",[M]FCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "CONTFCY"
    Call CONTFCY([M]FCY) From EXEFNC
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
    If !mkstat
     SAVACT = GACTION : GACTION = "CONTFCYCPY"
     Call CONTFCYCPY([M]FCY,[M]CPY) From EXEFNC
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 1344 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone TYPEXP
mkstat = 0
zoncou = "TYPEXP"
indice = 0
IMPENT = [M]TYPEXP
WNUM = IMPENT
If varinit([M]TYPEXP,indice)=0
 IMPENT = 2
Endif
If varmode([M]TYPEXP,indice)=0
 [M]TYPEXP(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "TYPEXP" : Gosub ACTION From =IMPTRT
  IMPENT = [M]TYPEXP(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_TYPEXP(IMPENT) From RELLCR
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "TYPEXP" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If WNUM<>IMPENT or IMPMOD
   SAVACT = GACTION : GACTION = "STD"
   Call AM_TYPEXP(IMPENT) From RELLCR
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "TYPEXP" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif
[M]TYPEXP = IMPENT

## Zone VOLFIL
mkstat = 0
zoncou = "VOLFIL"
indice = 0
If varmode([M]VOLFIL,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "VOLFIL" : Gosub ACTION From =IMPTRT
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_VOLFIL([M]VOLFIL) From RELLCR
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "VOLFIL" : GZNUM = 799 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

## Zone RAPLCRFLG
mkstat = 0
zoncou = "RAPLCRFLG"
indice = 0
IMPENT = [M]RAPLCRFLG
If varinit([M]RAPLCRFLG,indice)=0
 IMPENT = 2
Endif
If varmode([M]RAPLCRFLG,indice)=0
 [M]RAPLCRFLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "RAPLCRFLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]RAPLCRFLG(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call AP_RAPLCRFLG(IMPENT) From RELLCR
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "RAPLCRFLG" : GZNUM = 9911 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]RAPLCRFLG = IMPENT

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone FLG
mkstat = 0
zoncou = "FLG"
For indice=0 To 9
 IMPENT = [M]FLG(indice)
 If varinit([M]FLG,indice)=0
  IMPENT = 1
 Endif
 [M]FLG(indice) = IMPENT
 mkstat = 0
Next indice
If mkstat : End : Endif

## Zone TYPTYP
mkstat = 0
zoncou = "TYPTYP"
For indice=0 To 9
 IMPENT = [M]TYPTYP(indice)
 If varinit([M]TYPTYP,indice)=0
  IMPENT = 1
 Endif
 [M]TYPTYP(indice) = IMPENT
 mkstat = 0
Next indice
If mkstat : End : Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "RELLCR"
Return

