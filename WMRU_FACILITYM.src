# Supervisor
## CRUD-CLASSE ## FACILITYM
# 2018-08-27 13:14:56 442


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(FACILITYM,ASTATUS)
Variable Instance FACILITYM Using R_FACILITYM
Variable Integer ASTATUS

If FACILITYM = null : End : Endif

Gosub AVARIABLES From WMR00FACILITYM
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet FACILITYM.ACTX.AGETAFCRIGHT(FACILITYM,"GESFCY",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet FACILITYM.ACTX.AGETAFCRIGHTU(FACILITYM,"GESFCY",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_FACILITYM
Else
 Gosub AUPDATE_FACILITYM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = FACILITYM.freeSnapshot
FACILITYM.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
FACILITYM.ASTALIN = [V]CST_AUPD
FACILITYM.FCY.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = FACILITYM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = FACILITYM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet FACILITYM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet FACILITYM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet FACILITYM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If FACILITYM.FCY.ASTALIN = [V]CST_ANEWDEL : Return : Endif
FACILITYM.FCY.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(FACILITYM.FCY.ASTALIN)<>""
 [L]A_ASTALIN = FACILITYM.FCY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(FACILITYM.FCY.BPA) : # BPADDRESS -- 2
 If FACILITYM.FCY.BPA(A_N2) <> null
& and FACILITYM.FCY.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If FACILITYM.FCY.BPA(A_N2).ASTALIN<>[V]CST_ANEW & FACILITYM.FCY.ASTALIN=[V]CST_ADEL
   FACILITYM.FCY.BPA(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If FACILITYM.FCY.BPA(A_N2).ASTALIN=[V]CST_ANEW & find(FACILITYM.FCY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   FACILITYM.FCY.BPA(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(FACILITYM.FCY.BPA(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = FACILITYM.FCY.BPA(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BPA(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
  For [L]A_I=1 To maxtab(FACILITYM.FCY.BPA(A_N2).COLTEL)
   If FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I)<>null
    [L]A_ASTALIN = FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
  For [L]A_I=1 To maxtab(FACILITYM.FCY.BPA(A_N2).COLWEB)
   If FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I)<>null
    [L]A_ASTALIN = FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(FACILITYM.FCY.BID) : # BID -- 3
 If FACILITYM.FCY.BID(A_N2) <> null
& and FACILITYM.FCY.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If FACILITYM.FCY.BID(A_N2).ASTALIN<>[V]CST_ANEW & FACILITYM.FCY.ASTALIN=[V]CST_ADEL
   FACILITYM.FCY.BID(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If FACILITYM.FCY.BID(A_N2).ASTALIN=[V]CST_ANEW & find(FACILITYM.FCY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   FACILITYM.FCY.BID(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(FACILITYM.FCY.BID(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(FACILITYM.FCY.BID(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BID(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BID(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BID(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(FACILITYM.FCY.BPA) : # BPADDRESS -- 2
 If FACILITYM.FCY.BPA(A_N2) <> null
& and FACILITYM.FCY.BPA(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(FACILITYM.FCY.BPA(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = FACILITYM.FCY.BPA(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BPA(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
  For [L]A_I=1 To maxtab(FACILITYM.FCY.BPA(A_N2).COLTEL)
   If FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I)<>null
    [L]A_ASTALIN = FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
  For [L]A_I=1 To maxtab(FACILITYM.FCY.BPA(A_N2).COLWEB)
   If FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I)<>null
    [L]A_ASTALIN = FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN
    [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Next
  If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(FACILITYM.FCY.BID) : # BID -- 3
 If FACILITYM.FCY.BID(A_N2) <> null
& and FACILITYM.FCY.BID(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(FACILITYM.FCY.BID(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(FACILITYM.FCY.BID(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BID(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BID(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.BID(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(FACILITYM.FCY.ASTALIN)<>""
 [L]A_ASTALIN = FACILITYM.FCY.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_FACILITYM
Trbegin [F:FCY]
 Gosub AUPDATE_FACILITYM
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(FACILITYM.FCY.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 FACILITYM.FCY.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(FACILITYM.FCY.BPA)
 If FACILITYM.FCY.BPA(A_N2) <> null
  If FACILITYM.FCY.BPA(A_N2).ASTALIN=[V]CST_ADEL or FACILITYM.FCY.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance FACILITYM.FCY.BPA(A_N2)
   FACILITYM.FCY.BPA(A_N2) = null
  Elsif FACILITYM.FCY.BPA(A_N2).ASTALIN <> [V]CST_ALL
   FACILITYM.FCY.BPA(A_N2).ASTALIN = [V]CST_ALL
   For [L]A_I=1 To maxtab(FACILITYM.FCY.BPA(A_N2).COLTEL)
    If FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I)<>null
     If FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ADEL or FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
      FreeInstance FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I)
      FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I) = null
     Elsif FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN <> [V]CST_ALL
      FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ALL
     Endif
    Endif
   Next
   If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
   For [L]A_I=1 To maxtab(FACILITYM.FCY.BPA(A_N2).COLWEB)
    If FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I)<>null
     If FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ADEL or FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
      FreeInstance FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I)
      FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I) = null
     Elsif FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN <> [V]CST_ALL
      FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ALL
     Endif
    Endif
   Next
   If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(FACILITYM.FCY.BID)
 If FACILITYM.FCY.BID(A_N2) <> null
  [L]ASTATUS2 = fmet FACILITYM.FCY.BID(A_N2)._AUPDATE_AFTER
  If FACILITYM.FCY.BID(A_N2).ASTALIN=[V]CST_ADEL or FACILITYM.FCY.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance FACILITYM.FCY.BID(A_N2)
   FACILITYM.FCY.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_FACILITYM
[L]A_ASTALIN = FACILITYM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.FCY._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = FACILITYM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet FACILITYM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If FACILITYM.snapshotEnabled=1
 [L]A_RETIK = FACILITYM.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(FACILITYM.FCY.BPA)
 If FACILITYM.FCY.BPA(A_N2) <> null
  If FACILITYM.FCY.BPA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FACILITYM.FCY.BPA(A_N2).ASTALIN = [V]CST_ANEW
   For [L]A_I=1 To maxtab(FACILITYM.FCY.BPA(A_N2).COLTEL)
    If FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I)<>null
     If FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN=[V]CST_ANEWDEL
      FACILITYM.FCY.BPA(A_N2).COLTEL([L]A_I).ASTALIN = [V]CST_ANEW
     Endif
    Endif
   Next
   If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
   For [L]A_I=1 To maxtab(FACILITYM.FCY.BPA(A_N2).COLWEB)
    If FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I)<>null
     If FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN=[V]CST_ANEWDEL
      FACILITYM.FCY.BPA(A_N2).COLWEB([L]A_I).ASTALIN = [V]CST_ANEW
     Endif
    Endif
   Next
   If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(FACILITYM.FCY.BID)
 If FACILITYM.FCY.BID(A_N2) <> null
  [L]ASTATUS2 = fmet FACILITYM.FCY.BID(A_N2)._AUPDATE_ABORT
  If FACILITYM.FCY.BID(A_N2).ASTALIN=[V]CST_ADEL or FACILITYM.FCY.BID(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance FACILITYM.FCY.BID(A_N2)
   FACILITYM.FCY.BID(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet FACILITYM.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



