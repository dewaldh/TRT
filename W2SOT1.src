#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management SOT1 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$ON_TRSCOD
Onevent INIT_AREA Call D_TRSCOD(zc) From W2SOT1
Onevent CONTROL Call C_TRSCOD(zc) From W2SOT1
Onevent BEFORE_INPUT Call AS_TRSCOD(zc) From W2SOT1
Onevent AFTER_CHANGE Gosub AMM_TRSCOD From W2SOT1
Return

Subprog D_TRSCOD(VALEUR)
Variable Char    VALEUR()
If varinit([M]TRSCOD,indice)=0
 ## Action INILEG
 Call INILEG(VALEUR,"14","") From SUBADI
Endif
Gosub _AFF
End

Subprog AS_TRSCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TITADI(GBOUT1,"14") From TRTDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_TRSCOD(zc) From W2SOT1
Onkey   2 Titled GBOUTS Call S_TRSCOD(zc) From W2SOT1
Onkey 144 Titled GBOUTP Call P_TRSCOD(zc) From W2SOT1
Onkey 146 Titled GBOUTA Call SA_TRSCOD(zc) From W2SOT1
Gosub _AFF
End

Subprog C_TRSCOD(VALEUR)
Variable Char    VALEUR()
## Action CONTADI
Call CONTADI(VALEUR,"14","") From SUBADI
If mkstat : GERR = 1 : GZONE = "TRSCOD" : GZNUM = 18946 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("ADI","!",VALEUR,"14",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "TRSCOD" : GZNUM = 18946 : Endif
Endif
Gosub _AFF
End

$AMM_TRSCOD
Call AM_TRSCOD(zc)
If !mkstat & GOBJET
 zoncou = "TRSCOD" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_TRSCOD(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZTRSCOD)")>indice
 [M]TRSCOD(indice) = VALEUR
 Affzo ZTRSCOD(indice)
Endif
Gosub _AFF
End

Subprog S_TRSCOD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action SELADI
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "SELADI"
 PARMSK(1) = "14"
 PARMSK(2) = ""
 Call S_TAB_CHAR(VALEUR,PARMSK,"SELADI","SUBADI","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_TRSCOD(VALEUR)
Variable Char    VALEUR()
Call OBJET("ADI",VALEUR,"14") From GOBJET
Gosub _AFF
End

Subprog SA_TRSCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "ADI"
PARMSK(2) = "14"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_TRSCOD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = "14"
Call VISU("ADI",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZTRSCOD
Onevent BEFORE_AREA Gosub AV_ZTRSCOD From W2SOT1
Return

$AV_ZTRSCOD
Call LECTEXTRA(zc,"ATABDIV","SHODES","14",num$([M]TRSCOD(indice))) From ATEXTRA
Gosub _AFF
Return

$ON_ENTCOD
Onevent CONTROL Call C_ENTCOD(zc) From W2SOT1
Onevent BEFORE_INPUT Call AS_ENTCOD(zc) From W2SOT1
Onevent AFTER_CHANGE Gosub AMM_ENTCOD From W2SOT1
Return

Subprog AS_ENTCOD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(5514,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_ENTCOD(zc) From W2SOT1
Onkey   2 Titled GBOUTS Call S_ENTCOD(zc) From W2SOT1
Onkey 144 Titled GBOUTP Call P_ENTCOD(zc) From W2SOT1
Onkey 146 Titled GBOUTA Call SA_ENTCOD(zc) From W2SOT1
Gosub _AFF
End

Subprog C_ENTCOD(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("GAU","!A",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "ENTCOD" : GZNUM = 7500 : Endif
If !mkstat
 ## Action STKCTRGAU
 Call STKCTRGAU(VALEUR) From STKACT
 If mkstat : GERR = 1 : GZONE = "ENTCOD" : GZNUM = 7500 : Endif
Endif
Gosub _AFF
End

$AMM_ENTCOD
Call AM_ENTCOD(zc)
If !mkstat & GOBJET
 zoncou = "ENTCOD" : Gosub APRES_MODIF From =GPROGRAMME
Endif
Return

Subprog AM_ENTCOD(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZENTCOD)")>indice
 [M]ENTCOD(indice) = VALEUR
 Affzo ZENTCOD(indice)
Endif
Gosub _AFF
End

Subprog S_ENTCOD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action GSELECT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "GSELECT"
 PARMSK(1) = "ENTCOD"
 PARMSK(2) = "SUBSOT"
 Call S_TAB_CHAR(VALEUR,PARMSK,"GSELECT","ASELGEN","") From GACTION
 GACTION = SAVACT
Endif
Gosub _AFF
End

Subprog B1_ENTCOD(VALEUR)
Variable Char    VALEUR()
Call OBJET("GAU",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_ENTCOD(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "GAU"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_ENTCOD(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("GAU",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

$ON_ZENTCOD
Onevent BEFORE_AREA Gosub AV_ZENTCOD From W2SOT1
Return

$AV_ZENTCOD
Call LECTEXTRA(zc,"GAUTACE","DESTRA",num$([M]ENTCOD(indice)),"") From ATEXTRA
Gosub _AFF
Return

$CHG_STY
Return


