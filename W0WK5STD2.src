#<AdxTL>@(#)0.0.0.0 $Revision$
# Supervisor
# Mask management WK5STD2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone CURTYP
mkstat = 0
zoncou = "CURTYP"
indice = 0
IMPENT = [M]CURTYP
WNUM = IMPENT
If varinit([M]CURTYP,indice)=0
 IMPENT = 1
Endif
[M]CURTYP = IMPENT

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone SDDFLG
mkstat = 0
zoncou = "SDDFLG"
indice = 0
IMPENT = [M]SDDFLG
If varinit([M]SDDFLG,indice)=0
 IMPENT = 1
Endif
[M]SDDFLG = IMPENT

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

## Zone NBAXE
mkstat = 0
zoncou = "NBAXE"
indice = 0
status = 1
zoncou = "NBAXE"
If varinit([M]NBAXE,indice)=0
 SAVACT = GACTION : GACTION = "INICCEH"
 Call INIT_CCEH("SIH2","NBAXE","SIH") From TRTX3CPT
 GACTION = SAVACT
Endif
If !mkstat
 NBLIG = [M]NBAXE
 For nolign=1 To NBLIG
  indice = nolign-1
  [M]NBAXE = nolign
  status = 75
  Call SAI_NBAXE(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

####################################
## Bloc tableau 5
####################################
IMPTBL = 1

indice = 0
status = 1
zoncou = "NBFAC"
If !mkstat
 NBLIG = [M]NBFAC
 For nolign=1 To NBLIG
  indice = nolign-1
  [M]NBFAC = nolign
  status = 75
  Call SAI_NBFAC(IMPFIC,ACTION,IMPTRT)
  If mkstat : Break : Endif
 Next nolign
 If mkstat : End : Endif
 indice = 0
Endif

mkstat = 0

End

####################################
## Bloc tableau 4
####################################
Subprog SAI_NBAXE(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBAXE
mkstat = 0
zoncou = "NBAXE"
SAVACT = GACTION : GACTION = "STD"
Call C_NBAXE From SUBSIH
GACTION = SAVACT
If mkstat : GERR = 1 : GZONE = "NBAXE" : GZNUM = 0 : Endif
Gosub AFF From GSAISIE
If mkstat : End : Endif

## Zone NAMDIE
mkstat = 0
zoncou = "NAMDIE"
[M]NAMDIE(indice) = GCOLDIE(nolign)

## Zone CCE
mkstat = 0
zoncou = "CCE"
SAVACT = GACTION : GACTION = "INICCEFMT"
Call INICCEFMT(num$(GDIE(nolign))) From SETGLBDIE
GACTION = SAVACT
If mkstat=1 : Raz [M]CCE(indice) : Endif
If !mkstat
 If varmode([M]CCE,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "CCE" : Gosub ACTION From =IMPTRT
  Endif
  Call CONTROLE("CCE","!",[M]CCE(indice),num$(GDIE(nolign)),mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CCE" : GZNUM = 3997 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "CTLCCEH"
   Call CTRL_CCEH([M]CCE(indice),nolign,GFINRSP,[M:SIH0]INVDAT) From TRTX3CPT
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "CCE" : GZNUM = 3997 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone NBAXE
mkstat = 0
zoncou = "NBAXE"

End

####################################
## Bloc tableau 5
####################################
Subprog SAI_NBFAC(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1


## Zone INVDTAAMT
mkstat = 0
zoncou = "INVDTAAMT"
WNUM = [M]INVDTAAMT(indice)
If varmode([M]INVDTAAMT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "INVDTAAMT" : Gosub ACTION From =IMPTRT
 Endif
 If WNUM<>[M]INVDTAAMT(indice) or IMPMOD
  SAVACT = GACTION : GACTION = "STD"
  Call AM_INVDTAAMT([M]INVDTAAMT(indice)) From SUBSIH
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "INVDTAAMT" : GZNUM = 5476 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone INVDTATYP
mkstat = 0
zoncou = "INVDTATYP"
IMPENT = [M]INVDTATYP(indice)
If varinit([M]INVDTATYP,indice)=0
 IMPENT = 1
Endif
[M]INVDTATYP(indice) = IMPENT

## Zone SFISSTCOD
mkstat = 0
zoncou = "SFISSTCOD"
SAVACT = GACTION : GACTION = "STD"
Call AV_SFISSTCOD([M]SFISSTCOD(indice)) From SUBSIH
GACTION = SAVACT
If mkstat=1 : Raz [M]SFISSTCOD(indice) : Endif
If !mkstat
 If varinit([M]SFISSTCOD,indice)=0
  SAVACT = GACTION : GACTION = "INILEG"
  Call INILEG([M]SFISSTCOD(indice),"203","") From SUBADI
  GACTION = SAVACT
 Endif
Endif
If !mkstat
 If varmode([M]SFISSTCOD,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "SFISSTCOD" : Gosub ACTION From =IMPTRT
  Endif
  SAVACT = GACTION : GACTION = "CONTADI"
  Call CONTADI([M]SFISSTCOD(indice),"203","") From SUBADI
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "SFISSTCOD" : GZNUM = 48666 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("ADI","!",[M]SFISSTCOD(indice),"203",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "SFISSTCOD" : GZNUM = 48666 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

## Zone SFISSTOVRFLG
mkstat = 0
zoncou = "SFISSTOVRFLG"
IMPENT = [M]SFISSTOVRFLG(indice)
If varinit([M]SFISSTOVRFLG,indice)=0
 IMPENT = 1
Endif
[M]SFISSTOVRFLG(indice) = IMPENT

## Zone AMTCOD
mkstat = 0
zoncou = "AMTCOD"
IMPENT = [M]AMTCOD(indice)
If varinit([M]AMTCOD,indice)=0
 IMPENT = 1
Endif
[M]AMTCOD(indice) = IMPENT

status = 71
SAVACT = GACTION : GACTION = "STD"
Call APRES_NBFAC From SUBSIH
GACTION = SAVACT

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "WK5STD2"
Return

