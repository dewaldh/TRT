# Supervisor
# Mask management YSLC2 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone YALLREP
mkstat = 0
zoncou = "YALLREP"
indice = 0
If varmode([M]YALLREP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YALLREP" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YCTRISS
mkstat = 0
zoncou = "YCTRISS"
indice = 0
If varmode([M]YCTRISS,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCTRISS" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YFINAPP
mkstat = 0
zoncou = "YFINAPP"
indice = 0
If varmode([M]YFINAPP,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YFINAPP" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YFIRBDAT
mkstat = 0
zoncou = "YFIRBDAT"
indice = 0
If varmode([M]YFIRBDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YFIRBDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YFIRBADAT
mkstat = 0
zoncou = "YFIRBADAT"
indice = 0
If varmode([M]YFIRBADAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YFIRBADAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YDATSTA
mkstat = 0
zoncou = "YDATSTA"
indice = 0
IMPENT = [M]YDATSTA
If varinit([M]YDATSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YDATSTA,indice)=0
 [M]YDATSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YDATSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YDATSTA(indice)
 Endif
 If IMPENT=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YDATSTA" : GZNUM = 100509 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]YDATSTA = IMPENT

## Zone YOVRSETFOR
mkstat = 0
zoncou = "YOVRSETFOR"
indice = 0
If varmode([M]YOVRSETFOR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YOVRSETFOR" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YSETDAT
mkstat = 0
zoncou = "YSETDAT"
indice = 0
If varmode([M]YSETDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSETDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YSETBKD
mkstat = 0
zoncou = "YSETBKD"
indice = 0
IMPENT = [M]YSETBKD
If varinit([M]YSETBKD,indice)=0
 IMPENT = 1
Endif
If varmode([M]YSETBKD,indice)=0
 [M]YSETBKD(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSETBKD" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YSETBKD(indice)
 Endif
Endif
[M]YSETBKD = IMPENT

## Zone YCANREQ
mkstat = 0
zoncou = "YCANREQ"
indice = 0
IMPENT = [M]YCANREQ
If varinit([M]YCANREQ,indice)=0
 IMPENT = 1
Endif
If varmode([M]YCANREQ,indice)=0
 [M]YCANREQ(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCANREQ" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YCANREQ(indice)
 Endif
Endif
[M]YCANREQ = IMPENT

## Zone YOVRCSHSET
mkstat = 0
zoncou = "YOVRCSHSET"
indice = 0
If varmode([M]YOVRCSHSET,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YOVRCSHSET" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YCSHREV
mkstat = 0
zoncou = "YCSHREV"
indice = 0
If varmode([M]YCSHREV,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCSHREV" : Gosub ACTION From =IMPTRT
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "YSLC2"
Return

