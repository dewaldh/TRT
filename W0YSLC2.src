  IMPMOD = 0 : IMPZON = "YSTAGSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YSTAGSTA(indice)
 Endif
Endif
[M]YSTAGSTA = IMPENT

## Zone YFIRBDAT
mkstat = 0
zoncou = "YFIRBDAT"
indice = 0
If varmode([M]YFIRBDAT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YFIRBDAT" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YFIRBADAT
