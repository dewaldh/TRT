 If !mkstat
  IMPMOD = 0 : IMPZON = "YFINAPP" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone YDATSTA
mkstat = 0
zoncou = "YDATSTA"
indice = 0
IMPENT = [M]YDATSTA
If varinit([M]YDATSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YDATSTA,indice)=0
 [M]YDATSTA(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YDATSTA" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YDATSTA(indice)
 Endif
Endif
[M]YDATSTA = IMPENT

## Zone YSTAGSTA
mkstat = 0
zoncou = "YSTAGSTA"
indice = 0
IMPENT = [M]YSTAGSTA
If varinit([M]YSTAGSTA,indice)=0
 IMPENT = 1
Endif
If varmode([M]YSTAGSTA,indice)=0
 [M]YSTAGSTA(indice) = IMPENT
