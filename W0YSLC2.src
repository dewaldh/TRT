zoncou = "YCTRISS"
indice = 0
If varmode([M]YCTRISS,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCTRISS" : Gosub ACTION From =IMPTRT
 Endif
Endif

