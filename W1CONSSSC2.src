Subprog AS_MDL(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(3588,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
Call TEXTE(16134,GBOUTA) From OBJDIV
Onkey 129 Titled GBOUT1 Call B1_MDL(zc) From W1CONSSSC2
Onkey   2 Titled GBOUTS Call S_MDL(zc) From W1CONSSSC2
Onkey 144 Titled GBOUTP Call P_MDL(zc) From W1CONSSSC2
Onkey 146 Titled GBOUTA Call SA_MDL(zc) From W1CONSSSC2
Gosub _AFF
End

Subprog C_MDL(VALEUR)
Variable Char    VALEUR()
## Action %CNTOBJ
Call CONTSEL("TMD","!",VALEUR,"",mkstat) From CONTOBJ
If mkstat : GERR = 1 : GZONE = "MDL" : GZNUM = 1722 : Endif
Gosub _AFF
End

Subprog S_MDL(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16)
PARMSK(1) = "TMD"
PARMSK(2) = "!"
Call S_TAB_CHAR(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_MDL(VALEUR)
Variable Char    VALEUR()
Call OBJET("TMD",VALEUR,"") From GOBJET
Gosub _AFF
End

Subprog SA_MDL(VALEUR)
Variable Char    VALEUR()
Local Char PARMSK(250)(1..16) , SAVACT(20)
SAVACT = GACTION : GACTION = "SELOBJAW"
PARMSK(1) = "TMD"
Call SAISIE_CHAR(VALEUR,PARMSK,"DSELOBJAW","SELOBJAW","XWDSELOBJAW") From GSAISIE
GACTION = SAVACT
Gosub _AFF
End

Subprog P_MDL(VALEUR)
Variable Char    VALEUR()
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("TMD",VALEUR,PAROBJ) From SUBAOP
Gosub _AFF
End

