  [L]A_ASTALIN = ITMMASTER.CDIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CDIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
