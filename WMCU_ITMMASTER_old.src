#-----------------------------------------------------#
$AUPDATE_TR_ITMMASTER
Trbegin [F:ITM]
 Gosub AUPDATE_ITMMASTER
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_ITMMASTER
[L]A_ASTALIN = ITMMASTER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If find( ITMMASTER.ASTALIN,[V]CST_ANEW,[V]CST_ADEL) | 
& (find( ITMMASTER.ASTALIN,[V]CST_AUPD) & 
& fmet ITMMASTER.AGETATTRIBUTE("CLAPURTEX1","$isLoaded")="true")
[L]ASTATUS = func WMC00ITMMASTER._MAJSQL_ITMMASTER_CLAPURTEX1(ITMMASTER,ITMMASTER.CLAPURTEX1)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
If find( ITMMASTER.ASTALIN,[V]CST_ANEW,[V]CST_ADEL) | 
& (find( ITMMASTER.ASTALIN,[V]CST_AUPD) & 
& fmet ITMMASTER.AGETATTRIBUTE("CLAMFGTEX1","$isLoaded")="true")
[L]ASTATUS = func WMC00ITMMASTER._MAJSQL_ITMMASTER_CLAMFGTEX1(ITMMASTER,ITMMASTER.CLAMFGTEX1)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
If find( ITMMASTER.ASTALIN,[V]CST_ANEW,[V]CST_ADEL) | 
& (find( ITMMASTER.ASTALIN,[V]CST_AUPD) & 
& fmet ITMMASTER.AGETATTRIBUTE("IMG","$isLoaded")="true")
[L]ASTATUS = func WMC00ITMMASTER._MAJSQL_ITMMASTER_IMG(ITMMASTER,ITMMASTER.IMG)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
[L]ASTATUS = max([L]ASTATUS,func WMC00ITMMASTER._MAJSQL_ITMMASTER(ITMMASTER))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 For A_N2=1 To maxtab(ITMMASTER.ITU)
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(ITMMASTER.ITP)
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(ITMMASTER.ITA)
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 A_N2=1
 If ITMMASTER.ITS(A_N2) <> null and 
& ITMMASTER.ITS(A_N2).ASTALIN=[V]CST_ALL
 ITMMASTER.ITS(A_N2).ASTALIN = [V]CST_AUPD
 Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = ITMMASTER.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If ITMMASTER.snapshotEnabled=1
 [L]A_RETIK = ITMMASTER.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(ITMMASTER.ITU)
 If ITMMASTER.ITU(A_N2) <> null
  If ITMMASTER.ITU(A_N2).ASTALIN=[V]CST_ANEWDEL
   ITMMASTER.ITU(A_N2).ASTALIN = [V]CST_ANEW
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(ITMMASTER.ITP)
 If ITMMASTER.ITP(A_N2) <> null
  If ITMMASTER.ITP(A_N2).ASTALIN=[V]CST_ANEWDEL
   ITMMASTER.ITP(A_N2).ASTALIN = [V]CST_ANEW
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(ITMMASTER.ITA)
 If ITMMASTER.ITA(A_N2) <> null
  [L]ASTATUS2 = fmet ITMMASTER.ITA(A_N2)._AUPDATE_ABORT
  If ITMMASTER.ITA(A_N2).ASTALIN=[V]CST_ADEL or ITMMASTER.ITA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.ITA(A_N2)
   ITMMASTER.ITA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
A_N2=1
If ITMMASTER.ITS(A_N2) <> null
 If ITMMASTER.ITS(A_N2).ASTALIN=[V]CST_ANEWDEL
  ITMMASTER.ITS(A_N2).ASTALIN = [V]CST_ANEW
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif

[L]ASTATUS = max(fmet ITMMASTER.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(ITMMASTER.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 ITMMASTER.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(ITMMASTER.CDIE)
 If ITMMASTER.CDIE([L]A_I)<>null
  If ITMMASTER.CDIE([L]A_I).ASTALIN=[V]CST_ADEL or ITMMASTER.CDIE([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.CDIE([L]A_I)
   ITMMASTER.CDIE([L]A_I) = null
  Elsif ITMMASTER.CDIE([L]A_I).ASTALIN <> [V]CST_ALL
   ITMMASTER.CDIE([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CECCBOMALT)
 If ITMMASTER.CECCBOMALT([L]A_I)<>null
  If ITMMASTER.CECCBOMALT([L]A_I).ASTALIN=[V]CST_ADEL or ITMMASTER.CECCBOMALT([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.CECCBOMALT([L]A_I)
   ITMMASTER.CECCBOMALT([L]A_I) = null
  Elsif ITMMASTER.CECCBOMALT([L]A_I).ASTALIN <> [V]CST_ALL
   ITMMASTER.CECCBOMALT([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CPCU)
 If ITMMASTER.CPCU([L]A_I)<>null
  If ITMMASTER.CPCU([L]A_I).ASTALIN=[V]CST_ADEL or ITMMASTER.CPCU([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.CPCU([L]A_I)
   ITMMASTER.CPCU([L]A_I) = null
  Elsif ITMMASTER.CPCU([L]A_I).ASTALIN <> [V]CST_ALL
   ITMMASTER.CPCU([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CTSICOD)
 If ITMMASTER.CTSICOD([L]A_I)<>null
  If ITMMASTER.CTSICOD([L]A_I).ASTALIN=[V]CST_ADEL or ITMMASTER.CTSICOD([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.CTSICOD([L]A_I)
   ITMMASTER.CTSICOD([L]A_I) = null
  Elsif ITMMASTER.CTSICOD([L]A_I).ASTALIN <> [V]CST_ALL
   ITMMASTER.CTSICOD([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CVACITM)
 If ITMMASTER.CVACITM([L]A_I)<>null
  If ITMMASTER.CVACITM([L]A_I).ASTALIN=[V]CST_ADEL or ITMMASTER.CVACITM([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.CVACITM([L]A_I)
   ITMMASTER.CVACITM([L]A_I) = null
  Elsif ITMMASTER.CVACITM([L]A_I).ASTALIN <> [V]CST_ALL
   ITMMASTER.CVACITM([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(ITMMASTER.ITU)
 If ITMMASTER.ITU(A_N2) <> null
  If ITMMASTER.ITU(A_N2).ASTALIN=[V]CST_ADEL or ITMMASTER.ITU(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.ITU(A_N2)
   ITMMASTER.ITU(A_N2) = null
  Elsif ITMMASTER.ITU(A_N2).ASTALIN <> [V]CST_ALL
   ITMMASTER.ITU(A_N2).ASTALIN = [V]CST_ALL
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(ITMMASTER.ITP)
 If ITMMASTER.ITP(A_N2) <> null
  If ITMMASTER.ITP(A_N2).ASTALIN=[V]CST_ADEL or ITMMASTER.ITP(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.ITP(A_N2)
   ITMMASTER.ITP(A_N2) = null
  Elsif ITMMASTER.ITP(A_N2).ASTALIN <> [V]CST_ALL
   ITMMASTER.ITP(A_N2).ASTALIN = [V]CST_ALL
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(ITMMASTER.ITA)
 If ITMMASTER.ITA(A_N2) <> null
  [L]ASTATUS2 = fmet ITMMASTER.ITA(A_N2)._AUPDATE_AFTER
  If ITMMASTER.ITA(A_N2).ASTALIN=[V]CST_ADEL or ITMMASTER.ITA(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance ITMMASTER.ITA(A_N2)
   ITMMASTER.ITA(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
A_N2=1
If ITMMASTER.ITS(A_N2) <> null
 If ITMMASTER.ITS(A_N2).ASTALIN=[V]CST_ADEL or ITMMASTER.ITS(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance ITMMASTER.ITS(A_N2)
  ITMMASTER.ITS(A_N2) = null
 Elsif ITMMASTER.ITS(A_N2).ASTALIN <> [V]CST_ALL
  ITMMASTER.ITS(A_N2).ASTALIN = [V]CST_ALL
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Return



