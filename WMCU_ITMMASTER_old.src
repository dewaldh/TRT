If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CECCBOMALT)
 If ITMMASTER.CECCBOMALT([L]A_I)<>null
  [L]A_ASTALIN = ITMMASTER.CECCBOMALT([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CECCBOMALT([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CPCU)
 If ITMMASTER.CPCU([L]A_I)<>null
  [L]A_ASTALIN = ITMMASTER.CPCU([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CPCU([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CTSICOD)
 If ITMMASTER.CTSICOD([L]A_I)<>null
  [L]A_ASTALIN = ITMMASTER.CTSICOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CTSICOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CVACITM)
 If ITMMASTER.CVACITM([L]A_I)<>null
  [L]A_ASTALIN = ITMMASTER.CVACITM([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CVACITM([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
