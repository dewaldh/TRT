  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CECCBOMALT([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CPCU)
 If ITMMASTER.CPCU([L]A_I)<>null
  [L]A_ASTALIN = ITMMASTER.CPCU([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CPCU([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CTSICOD)
 If ITMMASTER.CTSICOD([L]A_I)<>null
  [L]A_ASTALIN = ITMMASTER.CTSICOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CTSICOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(ITMMASTER.CVACITM)
 If ITMMASTER.CVACITM([L]A_I)<>null
  [L]A_ASTALIN = ITMMASTER.CVACITM([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet ITMMASTER.CVACITM([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(ITMMASTER.ITU) : # ITMBPC -- 2
 If ITMMASTER.ITU(A_N2) <> null
& and ITMMASTER.ITU(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(ITMMASTER.ITP) : # ITMBPS -- 3
 If ITMMASTER.ITP(A_N2) <> null
& and ITMMASTER.ITP(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(ITMMASTER.ITA) : # ITMACGGRP -- 4
 If ITMMASTER.ITA(A_N2) <> null
& and ITMMASTER.ITA(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If ITMMASTER.ITS(A_N2) <> null
& and ITMMASTER.ITS(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If ITMMASTER.ITS(A_N2).ASTALIN<>[V]CST_ADEL & ITMMASTER.ITS(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(ITMMASTER,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"ITMSALES:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
