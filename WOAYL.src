$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(20)(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
Local Char     A_BUF3(20)(0..GNBDERNIER)
Local Char     A_BUF4(20)(0..GNBDERNIER)
Local Char     A_BUF5(20)(0..GNBDERNIER)
If clalev([F:AYL9])>0 : Close Local File [AYL9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(20),
& Char     A_TAB2(30),
& Char     A_TAB3(20),
& Char     A_TAB4(20),
& Char     A_TAB5(20)
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3,A_BUF4,A_BUF5
& As [AYL9]
Return

$LEC_DERLU
Read [AX3]AXX0="AYTELTDLK";"INTIT";GLANGUE;FCYCOD;DLKCOD
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:AYL9])>0
 Read [AYL9] = [F:AYL9]A_TABI
 [L]CLE = [F:AYL9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:AYL9])>0
 Filter [AYL9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AYL1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AYL9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AYL]DLKCOD+"~"
 A_CLEDERLU += [F:AYL]FCYCOD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AYL9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AYL9]
 Endif
Endif
Return

