$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "R"
  FLGEXE = 1
  GACTION = "STD"
 When "I"
  FLGEXE = 1
  GACTION = "STD"
 When "J"
  FLGEXE = 1
  GACTION = "STD"
 When "T"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  FLGEXE = 1
  GACTION = "SAIPREC"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IPREC","TRTPREC","XWTRTPREC") From GSAISIE
 When "P"
  FLGEXE = 1
  GACTION = "STD"
 When "Q"
  FLGEXE = 1
  GACTION = "STD"
 When "V"
  FLGEXE = 1
  GACTION = "STD"
 When "H"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = GSRTNUM
  FLGEXE = 1
  GACTION = "VISUPRE"
  Call SAISIE_CHAR(VALBOUT,PARBOUT,"IPRE","SELTRS","XWSELTRS") From GSAISIE
 When "N"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = "PRL"
  PARBOUT(2) = [M:PREP]PRLNUM
  PARBOUT(3) = num$("")
  FLGEXE = 1
  GACTION = "CONSPIV"
  Call CONSULT_CHAR(VALBOUT,"PIV",PARBOUT) From GCONSULT
Endcase
GACTION = SAVACT
Return

$EXEACT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "E"
  FLGEXE = 1
  GACTION = "STD"
 When "A"
  FLGEXE = 1
  GACTION = "STD"
 When "<"
  FLGEXE = 1
  GACTION = "STD"
 When ">"
  FLGEXE = 1
  GACTION = "STD"
Endcase
GACTION = SAVACT
Return


