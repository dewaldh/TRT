# Supervisor
## CRUD-CLASSE ## AOBJEXT
# 2018-08-27 13:02:05 535


############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(AOBJEXT,ASTATUS)
Variable Instance AOBJEXT Using R_AOBJEXT
Variable Integer ASTATUS

If AOBJEXT = null : End : Endif

Gosub AVARIABLES From WMR00AOBJEXT
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet AOBJEXT.ACTX.AGETAFCRIGHT(AOBJEXT,"GESAOE",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet AOBJEXT.ACTX.AGETAFCRIGHTU(AOBJEXT,"GESAOE",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif
If fmet AOBJEXT.ACTX.AGETACSRIGHTM(AOBJEXT,[L]A_STRACC,[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR
 Goto AUPDATE_END
  Endif
  
  Gosub AREPCONTROL_AUPDATE
  If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
  
If !adxlog
 Gosub AUPDATE_TR_AOBJEXT
Else
 Gosub AUPDATE_AOBJEXT
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = AOBJEXT.freeSnapshot
AOBJEXT.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
AOBJEXT.ASTALIN = [V]CST_AUPD
AOBJEXT.AOE.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = AOBJEXT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = AOBJEXT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet AOBJEXT._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet AOBJEXT._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet AOBJEXT._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If AOBJEXT.AOE.ASTALIN = [V]CST_ANEWDEL : Return : Endif
AOBJEXT.AOE.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(AOBJEXT.AOE.ASTALIN)<>""
 [L]A_ASTALIN = AOBJEXT.AOE.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(AOBJEXT.AOE.FILCOL)
 If AOBJEXT.AOE.FILCOL([L]A_I)<>null
  [L]A_ASTALIN = AOBJEXT.AOE.FILCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE.FILCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJEXT.AOE.FLGCOL)
 If AOBJEXT.AOE.FLGCOL([L]A_I)<>null
  [L]A_ASTALIN = AOBJEXT.AOE.FLGCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE.FLGCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(AOBJEXT.AOE.AOD) : # AOBJEXTD -- 2
 If AOBJEXT.AOE.AOD(A_N2) <> null
& and AOBJEXT.AOE.AOD(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If AOBJEXT.AOE.AOD(A_N2).ASTALIN<>[V]CST_ANEW & AOBJEXT.AOE.ASTALIN=[V]CST_ADEL
   AOBJEXT.AOE.AOD(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If AOBJEXT.AOE.AOD(A_N2).ASTALIN=[V]CST_ANEW & find(AOBJEXT.AOE.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   AOBJEXT.AOE.AOD(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(AOBJEXT.AOE.AOD(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = AOBJEXT.AOE.AOD(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE.AOD(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
For A_N2=1 to maxtab(AOBJEXT.AOE.AOD)
 If AOBJEXT.AOE.AOD(A_N2)<>null
  If AOBJEXT.AOE.AOD(A_N2).ASTALIN<>[V]CST_ADEL & AOBJEXT.AOE.AOD(A_N2).ASTALIN<>[V]CST_ANEWDEL
   [L]A_OK1 = 1 : Break
  Endif
 Endif
Next
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(AOBJEXT.AOE,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"AOBJEXTD:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(AOBJEXT.AOE.AOD) : # AOBJEXTD -- 2
 If AOBJEXT.AOE.AOD(A_N2) <> null
& and AOBJEXT.AOE.AOD(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(AOBJEXT.AOE.AOD(A_N2).ASTALIN)<>""
   [L]A_ASTALIN = AOBJEXT.AOE.AOD(A_N2).ASTALIN
   [L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE.AOD(A_N2).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
For A_N2=1 to maxtab(AOBJEXT.AOE.AOD)
 If AOBJEXT.AOE.AOD(A_N2)<>null
  If AOBJEXT.AOE.AOD(A_N2).ASTALIN<>[V]CST_ADEL & AOBJEXT.AOE.AOD(A_N2).ASTALIN<>[V]CST_ANEWDEL
   [L]A_OK1 = 1 : Break
  Endif
 Endif
Next
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(AOBJEXT.AOE,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"AOBJEXTD:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif

For [L]A_I=1 To maxtab(AOBJEXT.AOE.FILCOL)
 If AOBJEXT.AOE.FILCOL([L]A_I)<>null
  [L]A_ASTALIN = AOBJEXT.AOE.FILCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE.FILCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(AOBJEXT.AOE.FLGCOL)
 If AOBJEXT.AOE.FLGCOL([L]A_I)<>null
  [L]A_ASTALIN = AOBJEXT.AOE.FLGCOL([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE.FLGCOL([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(AOBJEXT.AOE.ASTALIN)<>""
 [L]A_ASTALIN = AOBJEXT.AOE.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_AOBJEXT
Trbegin [F:AOE]
 Gosub AUPDATE_AOBJEXT
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(AOBJEXT.AOE.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 AOBJEXT.AOE.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(AOBJEXT.AOE.FILCOL)
 If AOBJEXT.AOE.FILCOL([L]A_I)<>null
  If AOBJEXT.AOE.FILCOL([L]A_I).ASTALIN=[V]CST_ADEL or AOBJEXT.AOE.FILCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AOBJEXT.AOE.FILCOL([L]A_I)
   AOBJEXT.AOE.FILCOL([L]A_I) = null
  Elsif AOBJEXT.AOE.FILCOL([L]A_I).ASTALIN <> [V]CST_ALL
   AOBJEXT.AOE.FILCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(AOBJEXT.AOE.FLGCOL)
 If AOBJEXT.AOE.FLGCOL([L]A_I)<>null
  If AOBJEXT.AOE.FLGCOL([L]A_I).ASTALIN=[V]CST_ADEL or AOBJEXT.AOE.FLGCOL([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AOBJEXT.AOE.FLGCOL([L]A_I)
   AOBJEXT.AOE.FLGCOL([L]A_I) = null
  Elsif AOBJEXT.AOE.FLGCOL([L]A_I).ASTALIN <> [V]CST_ALL
   AOBJEXT.AOE.FLGCOL([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(AOBJEXT.AOE.AOD)
 If AOBJEXT.AOE.AOD(A_N2) <> null
  If AOBJEXT.AOE.AOD(A_N2).ASTALIN=[V]CST_ADEL or AOBJEXT.AOE.AOD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance AOBJEXT.AOE.AOD(A_N2)
   AOBJEXT.AOE.AOD(A_N2) = null
  Elsif AOBJEXT.AOE.AOD(A_N2).ASTALIN <> [V]CST_ALL
   AOBJEXT.AOE.AOD(A_N2).ASTALIN = [V]CST_ALL
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_AOBJEXT
[L]A_ASTALIN = AOBJEXT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.AOE._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = AOBJEXT.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet AOBJEXT.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If AOBJEXT.snapshotEnabled=1
 [L]A_RETIK = AOBJEXT.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(AOBJEXT.AOE.AOD)
 If AOBJEXT.AOE.AOD(A_N2) <> null
  If AOBJEXT.AOE.AOD(A_N2).ASTALIN=[V]CST_ANEWDEL
   AOBJEXT.AOE.AOD(A_N2).ASTALIN = [V]CST_ANEW
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet AOBJEXT.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



