#-------------------------------------------------------#
Subprog DESCRIPTION_REPRESENTATION(REP,GRPINS)
Variable Instance REP Using C_AREPENT
Gosub DESCRIPTION
End

#-------------------------------------------------------#
$DESCRIPTION
Gosub HEAD_REP
Gosub SECTION_REP
Gosub LINK_REP
Gosub COLLECTIONS_REP
Gosub PROPERTY_REP From WMREP1SHIPMENT
Return

#-------------------------------------------------------#
# Entete
#-------------------------------------------------------#
$HEAD_REP
REP = NewInstance =[L]GRPINS , C_AREPENT
REP.REP = "SHIPMENT"
REP.CLA = "SHIPMENT"
REP.ICL = "SHH"
REP.AFCRIGHT = ""
REP.NCL = 8571 : # Shipment
REP.DES = 8571 : # Shipment
REP.TIT = ""
REP.ANX = "SHH0"
REP.CLE(1) = NewInstance C_AREPPAR AllocGroup REP : # Shipment number
     REP.CLE(1).PAR = 'SHIPNUM' : REP.CLE(1).TYP = 7 : REP.CLE(1).PRO= 'SHIPNUM' : REP.CLE(1).VPR= 'SHH.SHIPNUM' : REP.CLE(1).DES = 53635
# Facets
REP.AENAFAC(1) = 2 : # Detail
REP.AENAFAC(2) = 2 : # Edit
REP.AENAFAC(3) = 2 : # Query
REP.AENAFAC(4) = 1 : # Lookup
REP.AENAFAC(5) = 1 : # Summary
Return

#-------------------------------------------------------#
# Section de la représentation
#-------------------------------------------------------#
$SECTION_REP
REP.SEC(1) = NewInstance C_AREPSEC AllocGroup REP
     REP.SEC(1).SEC = "1" : REP.SEC(1).NSC = 8571 : # Shipment
     REP.SEC(1).BLC(1)="1" : REP.SEC(1).NBC(1)=8571 : # Shipment
Return

