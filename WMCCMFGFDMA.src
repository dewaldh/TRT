If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AFORMAT_UPDDATTIM From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AFORMAT_AUUID From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AFORMAT_CREUSR From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AFORMAT_UPDUSR From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
Return

####################################################################
# OTHERS
####################################################################

$_AOTHERS
Gosub DECLARE_VAR From WMC0MFGFDMA
Local Integer ALLSTAT
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_MFGNUM From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_MFGNUM From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_MFGLIN From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_MFGLIN From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_MFGFCY From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_MFGFCY From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_FDMATIM From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_FDMATIM From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
For AINDCLA=1 To maxtab(this.COMFDMAS)
 If this.COMFDMAS(AINDCLA)<>null& find(this.COMFDMAS(AINDCLA).ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)=0
  [L]ASTATUS = fmet this.COMFDMAS(AINDCLA)._AOTHERS : Gosub TESTALLSTAT
 Endif
Next AINDCLA
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_TWD From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_TWD From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREDATTIM From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREDATTIM From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDDATTIM From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDDATTIM From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_AUUID From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_AUUID From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_CREUSR From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_CREUSR From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
If !find(this.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL) : Gosub _AOTHERS_UPDUSR From WMC1MFGFDMA : Gosub TESTALLSTAT : Gosub _AOTHERS_REF_UPDUSR From WMC1MFGFDMA : Gosub TESTALLSTAT :  Endif
[L]ASTATUS = fmet this.ACONTROL
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return

$TESTALLSTAT
[L]ALLSTAT = max([L]ALLSTAT,[L]ASTATUS)
Return



