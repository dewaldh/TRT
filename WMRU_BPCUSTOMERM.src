############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(BPCUSTOMERM,ASTATUS)
Variable Instance BPCUSTOMERM Using R_BPCUSTOMERM
Variable Integer ASTATUS

If BPCUSTOMERM = null : End : Endif

Gosub AVARIABLES From WMR00BPCUSTOMERM
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet BPCUSTOMERM.ACTX.AGETAFCRIGHT(BPCUSTOMERM,"GESBPC",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet BPCUSTOMERM.ACTX.AGETAFCRIGHTU(BPCUSTOMERM,"GESBPC",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_BPCUSTOMERM
Else
 Gosub AUPDATE_BPCUSTOMERM
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = BPCUSTOMERM.freeSnapshot
BPCUSTOMERM.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
BPCUSTOMERM.ASTALIN = [V]CST_AUPD
BPCUSTOMERM.BPC.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = BPCUSTOMERM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BPCUSTOMERM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMERM._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMERM._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMERM._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If BPCUSTOMERM.BPC.ASTALIN = [V]CST_ANEWDEL : Return : Endif
BPCUSTOMERM.BPC.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(BPCUSTOMERM.BPC.ASTALIN)<>""
 [L]A_ASTALIN = BPCUSTOMERM.BPC.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_DAYMON)
 If BPCUSTOMERM.BPC.C_DAYMON([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_DAYMON([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_DAYMON([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_DIE)
 If BPCUSTOMERM.BPC.C_DIE([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_DIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_DIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
