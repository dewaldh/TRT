If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_INVDTAAMT)
 If BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_REP)
 If BPCUSTOMERM.BPC.C_REP([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_REP([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_REP([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_TSCCOD)
 If BPCUSTOMERM.BPC.C_TSCCOD([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_TSCCOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_TSCCOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If BPCUSTOMERM.BPC.BPR(A_N2) <> null
& and BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN<>[V]CST_ADEL & BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(BPCUSTOMERM.BPC,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPARTNER:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BPCUSTOMERM.BPC.BPD) : # BPDLVCUST -- 3
 If BPCUSTOMERM.BPC.BPD(A_N2) <> null
& and BPCUSTOMERM.BPC.BPD(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
A_N2=1
If BPCUSTOMERM.BPC.BPR(A_N2) <> null
& and BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN<>[V]CST_ADEL & BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(BPCUSTOMERM.BPC,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPARTNER:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BPCUSTOMERM.BPC.BPD) : # BPDLVCUST -- 3
 If BPCUSTOMERM.BPC.BPD(A_N2) <> null
& and BPCUSTOMERM.BPC.BPD(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_DAYMON)
 If BPCUSTOMERM.BPC.C_DAYMON([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_DAYMON([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_DAYMON([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_DIE)
 If BPCUSTOMERM.BPC.C_DIE([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_DIE([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_DIE([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_INVDTAAMT)
 If BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_REP)
 If BPCUSTOMERM.BPC.C_REP([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_REP([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_REP([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_TSCCOD)
 If BPCUSTOMERM.BPC.C_TSCCOD([L]A_I)<>null
  [L]A_ASTALIN = BPCUSTOMERM.BPC.C_TSCCOD([L]A_I).ASTALIN
  [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.C_TSCCOD([L]A_I).ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [V]CST_ASTALIN(BPCUSTOMERM.BPC.ASTALIN)<>""
 [L]A_ASTALIN = BPCUSTOMERM.BPC.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_BPCUSTOMERM
Trbegin [F:BPC]
 Gosub AUPDATE_BPCUSTOMERM
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(BPCUSTOMERM.BPC.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 BPCUSTOMERM.BPC.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_DAYMON)
 If BPCUSTOMERM.BPC.C_DAYMON([L]A_I)<>null
  If BPCUSTOMERM.BPC.C_DAYMON([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.C_DAYMON([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMERM.BPC.C_DAYMON([L]A_I)
   BPCUSTOMERM.BPC.C_DAYMON([L]A_I) = null
  Elsif BPCUSTOMERM.BPC.C_DAYMON([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMERM.BPC.C_DAYMON([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_DIE)
 If BPCUSTOMERM.BPC.C_DIE([L]A_I)<>null
  If BPCUSTOMERM.BPC.C_DIE([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.C_DIE([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMERM.BPC.C_DIE([L]A_I)
   BPCUSTOMERM.BPC.C_DIE([L]A_I) = null
  Elsif BPCUSTOMERM.BPC.C_DIE([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMERM.BPC.C_DIE([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_INVDTAAMT)
 If BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I)<>null
  If BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I)
   BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I) = null
  Elsif BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMERM.BPC.C_INVDTAAMT([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_REP)
 If BPCUSTOMERM.BPC.C_REP([L]A_I)<>null
  If BPCUSTOMERM.BPC.C_REP([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.C_REP([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMERM.BPC.C_REP([L]A_I)
   BPCUSTOMERM.BPC.C_REP([L]A_I) = null
  Elsif BPCUSTOMERM.BPC.C_REP([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMERM.BPC.C_REP([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMERM.BPC.C_TSCCOD)
 If BPCUSTOMERM.BPC.C_TSCCOD([L]A_I)<>null
  If BPCUSTOMERM.BPC.C_TSCCOD([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.C_TSCCOD([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMERM.BPC.C_TSCCOD([L]A_I)
   BPCUSTOMERM.BPC.C_TSCCOD([L]A_I) = null
  Elsif BPCUSTOMERM.BPC.C_TSCCOD([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMERM.BPC.C_TSCCOD([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
Local Integer ASTATUS2
A_N2=1
If BPCUSTOMERM.BPC.BPR(A_N2) <> null
 [L]ASTATUS2 = fmet BPCUSTOMERM.BPC.BPR(A_N2)._AUPDATE_AFTER
 If BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance BPCUSTOMERM.BPC.BPR(A_N2)
  BPCUSTOMERM.BPC.BPR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BPCUSTOMERM.BPC.BPD)
 If BPCUSTOMERM.BPC.BPD(A_N2) <> null
  [L]ASTATUS2 = fmet BPCUSTOMERM.BPC.BPD(A_N2)._AUPDATE_AFTER
  If BPCUSTOMERM.BPC.BPD(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.BPD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMERM.BPC.BPD(A_N2)
   BPCUSTOMERM.BPC.BPD(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return

#-----------------------------------------------------#
$AUPDATE_BPCUSTOMERM
[L]A_ASTALIN = BPCUSTOMERM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.BPC._AUPDATE)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BPCUSTOMERM.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMERM.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If BPCUSTOMERM.snapshotEnabled=1
 [L]A_RETIK = BPCUSTOMERM.revertUpdTick
Endif
Local Integer ASTATUS2
A_N2=1
If BPCUSTOMERM.BPC.BPR(A_N2) <> null
 [L]ASTATUS2 = fmet BPCUSTOMERM.BPC.BPR(A_N2)._AUPDATE_ABORT
 If BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.BPR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance BPCUSTOMERM.BPC.BPR(A_N2)
  BPCUSTOMERM.BPC.BPR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BPCUSTOMERM.BPC.BPD)
 If BPCUSTOMERM.BPC.BPD(A_N2) <> null
  [L]ASTATUS2 = fmet BPCUSTOMERM.BPC.BPD(A_N2)._AUPDATE_ABORT
  If BPCUSTOMERM.BPC.BPD(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMERM.BPC.BPD(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMERM.BPC.BPD(A_N2)
   BPCUSTOMERM.BPC.BPD(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet BPCUSTOMERM.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#



