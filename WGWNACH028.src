$EXEBOUT
Local Char    SAVACT(GLONACT)
Local Integer FLGEXE
SAVACT = GACTION
Case BOUT
 When "C"
  FLGEXE = 1
  GACTION = "STD"
 When "H"
  FLGEXE = 1
  GACTION = "STD"
 When "R"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:LST]COD
  PARBOUT(2) = num$(0)
  PARBOUT(3) = num$(1)
  PARBOUT(4) = [L]SAV_PARAM(4)
  PARBOUT(5) = [L]SAV_PARAM(5)
  PARBOUT(6) = [L]SAV_PARAM(6)
  PARBOUT(7) = num$(0)
  FLGEXE = 1
  GACTION = "CALALH"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","SUBALH9","ALH9",4,"SPEALH") From GTRAITE
 When "P"
  FLGEXE = 1
  GACTION = "STD"
 When "M"
  FLGEXE = 1
  GACTION = "STD"
 When "X"
  Local Char VALBOUT(250) , PARBOUT(250)(1..20)
  PARBOUT(1) = [M:LST]COD
  PARBOUT(2) = [M:LST]USR
  PARBOUT(3) = num$([M:LST]NIV)
  FLGEXE = 1
  GACTION = "ALISTEEXP"
  Call TRAITE_CHAR(VALBOUT,PARBOUT,"","ALHE","ALHE",3,"XYALHE") From GTRAITE
Endcase
GACTION = SAVACT
Return

