# Supervisor
# Mask management FASTRFHIS 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varinit([M]FCY,indice)=0
 [M]FCY = GCOUSIT
Endif

## Zone LCTCOD
mkstat = 0
zoncou = "LCTCOD"
indice = 0
If varmode([M]LCTCOD,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "LCTCOD" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]LCTCOD,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "LCTCOD" : GZNUM = 7710 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("LCT","",[M]LCTCOD,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "LCTCOD" : GZNUM = 7710 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

####################################
## Bloc liste 2
####################################
IMPTBL = 0

## Zone UGTIAS
mkstat = 0
zoncou = "UGTIAS"
indice = 0
If varinit([M]UGTIAS,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]UGTIAS,"613","") From SUBADI
 GACTION = SAVACT
Endif

####################################
## Bloc liste 3
####################################
IMPTBL = 0

####################################
## Bloc tableau 4
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 5
####################################
IMPTBL = 0

mkstat = 0

End

####################################
## Bloc tableau 4
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone DIE
mkstat = 0
zoncou = "DIE"
If varmode([M]DIE,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "DIE" : Gosub ACTION From =IMPTRT
 Endif
 Call CONTROLE("DIE","!",[M]DIE(indice),"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "DIE" : GZNUM = 29327 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone CCE
mkstat = 0
zoncou = "CCE"
SAVACT = GACTION : GACTION = "INICCEFMT"
Call INICCEFMT(num$([M:FASH]DIE(nolign-1))) From SETGLBDIE
GACTION = SAVACT
If mkstat=1 : Raz [M]CCE(indice) : Endif
If !mkstat
 If varmode([M]CCE,indice)=0
  If !mkstat
   IMPMOD = 0 : IMPZON = "CCE" : Gosub ACTION From =IMPTRT
  Endif
  Call CONTROLE("CCE","!",[M]CCE(indice),num$([M:FASH]DIE(nolign-1)),mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "CCE" : GZNUM = 1688 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
 Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "FASTRFHIS"
Return

