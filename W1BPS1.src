Subprog AM_TPMCOD(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZTPMCOD)")>indice
 [M]TPMCOD(indice) = VALEUR
 Affzo ZTPMCOD(indice)
Endif
Gosub _AFF
End

