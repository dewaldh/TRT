Subprog AS_INVDTA(VALEUR)
Variable Decimal VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(6015,GBOUT1) From OBJDIV
Call TEXTE(15597,GBOUTS) From OBJDIV
Call TEXTE(12276,GBOUTP) From OBJDIV
## Action STD
Call AS_INVDTA(VALEUR) From SUBBPS
If mkstat=2
 [M]INVDTA(INDIC) = VALEUR
 Affzo INVDTA(INDIC) : mkstat = 2
Endif
Onkey 129 Titled GBOUT1 Call B1_INVDTA(zc) From W1BPS1
Onkey   2 Titled GBOUTS Call S_INVDTA(zc) From W1BPS1
Onkey 144 Titled GBOUTP Call P_INVDTA(zc) From W1BPS1
Gosub _AFF
End

Subprog C_INVDTA(VALEUR)
Variable Decimal VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "INVDTA" : GZNUM = 13563 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTNUM("PFI","AB",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "INVDTA" : GZNUM = 13563 : Endif
 If !mkstat
  ## Action STD
  Call C_INVDTA(VALEUR) From SUBBPS
  If mkstat : GERR = 1 : GZONE = "INVDTA" : GZNUM = 13563 : Endif
 Endif
Endif
Gosub _AFF
End

Subprog S_INVDTA(VALEUR)
Variable Decimal VALEUR
Local Char PARMSK(250)(1..16)
PARMSK(1) = "PFI"
PARMSK(2) = "AB"
Call S_TAB_NUM(VALEUR,PARMSK,"SELOBJ","SELOBJ","") From GACTION
Gosub _AFF
End

Subprog B1_INVDTA(VALEUR)
Variable Decimal VALEUR
Local Char VALCHAR(20)
VALCHAR = num$(VALEUR)
Call OBJET("PFI",VALCHAR,"") From GOBJET
VALEUR = val(VALCHAR)
Gosub _AFF
End

Subprog P_INVDTA(VALEUR)
Variable Decimal VALEUR
Local Char PAROBJ(250)
PAROBJ = ""
Call VISU("PFI",num$(VALEUR),PAROBJ) From SUBAOP
Gosub _AFF
End

