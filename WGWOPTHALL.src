&   +" "+num$(max(([F:SHD]SHIQTYPUU-[F:SHD]RCPQTYPUU)*([F:SHD]PRCPQTYPUU=0),([F:SHD]PRCPQTYPUU-[F:SHD]RCPQTYPUU)*([F:SHD]PRCPQTYPUU<>0)))
