############################################################################################
### Subprog _AINSERT
############################################################################################
Subprog _AINSERT(YLOT,ASTATUS)
Variable Instance YLOT Using C_YLOT
Variable Integer ASTATUS

If YLOT = null : End : Endif

Gosub _AVARIABLES From WMC00YLOT

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AINSERT_YLOT

End

#-----------------------------------------------------#

############################################################################################
### Subprog AINSERT
############################################################################################
Subprog AINSERT(YLOT,ASTATUS)
Variable Instance YLOT Using C_YLOT
Variable Integer ASTATUS

If YLOT = null : End : Endif

Gosub AVARIABLES From WMC00YLOT
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AINSERT_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AINSERT_END : Endif

If !adxlog
 Gosub AINSERT_TR_YLOT
Else
 Gosub AINSERT_YLOT
Endif

Gosub _AINSERT_AFTER
[L]A_I = YLOT.freeSnapshot
YLOT.snapshotEnabled = 1
$AINSERT_END
End

#-----------------------------------------------------#

$AINSERT_CONTROL
Gosub _AINSERT_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YLOT._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YLOT._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YLOT._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AINSERT_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_BEFORE
If YLOT.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YLOT.ASTALIN = [V]CST_ANEW
[L]ASTATUS = max([L]ASTATUS,fmet YLOT.ACTION("AINSERT_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLOT.YLPR) : # YLOTPRC -- 2
 If YLOT.YLPR(A_N2) <> null
& and !find(YLOT.YLPR(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLPR(A_N2)._AINSERT_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLOT.YREL) : # YREL -- 3
 If YLOT.YREL(A_N2) <> null
& and !find(YLOT.YREL(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YREL(A_N2)._AINSERT_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLOT.YLNL) : # YLOTANL -- 4
 If YLOT.YLNL(A_N2) <> null
& and !find(YLOT.YLNL(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLNL(A_N2)._AINSERT_CONTROL_BEFORE)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AINSERT_CONTROL_AFTER
For A_N2=1 To maxtab(YLOT.YLPR) : # YLOTPRC -- 2
 If YLOT.YLPR(A_N2) <> null
& and !find(YLOT.YLPR(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLPR(A_N2)._AINSERT_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLOT.YREL) : # YREL -- 3
 If YLOT.YREL(A_N2) <> null
& and !find(YLOT.YREL(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YREL(A_N2)._AINSERT_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YLOT.YLNL) : # YLOTANL -- 4
 If YLOT.YLNL(A_N2) <> null
& and !find(YLOT.YLNL(A_N2).ASTALIN,[V]CST_ANEWDEL,[V]CST_ADEL)
  [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLNL(A_N2)._AINSERT_CONTROL_AFTER)
  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,fmet YLOT.ACTION("AINSERT_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_TR_YLOT
Trbegin [F:YLOT]
 Gosub AINSERT_YLOT
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AINSERT_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AINSERT_YLOT
Raz [F:YLOT]
[L]ASTATUS = max([L]ASTATUS,fmet YLOT.ACTION("AINSERT_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YLOT._MAJSQL_YLOT(YLOT))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
[L]A_TYPMAJ=[V]CST_ANEW

#-----------------------------------------------------#
For A_N2=1 To maxtab(YLOT.YLPR)
 If YLOT.YLPR(A_N2) <> null and
& (YLOT.YLPR(A_N2).ASTALIN=[L]A_TYPMAJ or
& YLOT.YLPR(A_N2).ASTALIN=[V]CST_ALL)
  If YLOT.YLPR(A_N2).ASTALIN=[L]A_TYPMAJ
   Case YLOT.YLPR(A_N2).ASTALIN
    When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLPR(A_N2)._AUPDATE)
    When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLPR(A_N2)._AINSERT)
    When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLPR(A_N2)._ADELETE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
 If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

#-----------------------------------------------------#
For A_N2=1 To maxtab(YLOT.YREL)
 If YLOT.YREL(A_N2) <> null and
& (YLOT.YREL(A_N2).ASTALIN=[L]A_TYPMAJ or
& YLOT.YREL(A_N2).ASTALIN=[V]CST_ALL)
  If YLOT.YREL(A_N2).ASTALIN=[L]A_TYPMAJ
   Case YLOT.YREL(A_N2).ASTALIN
    When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YREL(A_N2)._AUPDATE)
    When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YREL(A_N2)._AINSERT)
    When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YREL(A_N2)._ADELETE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
 If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

#-----------------------------------------------------#
For A_N2=1 To maxtab(YLOT.YLNL)
 If YLOT.YLNL(A_N2) <> null and
& (YLOT.YLNL(A_N2).ASTALIN=[L]A_TYPMAJ or
& YLOT.YLNL(A_N2).ASTALIN=[V]CST_ALL)
  If YLOT.YLNL(A_N2).ASTALIN=[L]A_TYPMAJ
   Case YLOT.YLNL(A_N2).ASTALIN
    When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLNL(A_N2)._AUPDATE)
    When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLNL(A_N2)._AINSERT)
    When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YLOT.YLNL(A_N2)._ADELETE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
 If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
#-----------------------------------------------------#

[L]ASTATUS = max([L]ASTATUS,fmet YLOT.ACTION("AINSERT_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AINSERT_ABORT
Rollback
$_AINSERT_ABORT
If YLOT.snapshotEnabled=1
 [L]A_RETIK = YLOT.revertUpdTick
Endif
[L]ASTATUS = max(fmet YLOT.ACTION("AINSERT_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AINSERT_AFTER
If find(YLOT.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YLOT.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLOT.YLPR)
 If YLOT.YLPR(A_N2) <> null
  [L]ASTATUS2 = fmet YLOT.YLPR(A_N2)._AINSERT_AFTER
  If YLOT.YLPR(A_N2).ASTALIN=[V]CST_ADEL or YLOT.YLPR(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLOT.YLPR(A_N2)
   YLOT.YLPR(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLOT.YREL)
 If YLOT.YREL(A_N2) <> null
  [L]ASTATUS2 = fmet YLOT.YREL(A_N2)._AINSERT_AFTER
  If YLOT.YREL(A_N2).ASTALIN=[V]CST_ADEL or YLOT.YREL(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLOT.YREL(A_N2)
   YLOT.YREL(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YLOT.YLNL)
 If YLOT.YLNL(A_N2) <> null
  [L]ASTATUS2 = fmet YLOT.YLNL(A_N2)._AINSERT_AFTER
  If YLOT.YLNL(A_N2).ASTALIN=[V]CST_ADEL or YLOT.YLNL(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YLOT.YLNL(A_N2)
   YLOT.YLNL(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



