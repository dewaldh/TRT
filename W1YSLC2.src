$ON_YDATSTA
Onevent INIT_AREA Call D_YDATSTA(zc) From W1YSLC2
Onevent BEFORE_INPUT Call AS_YDATSTA(zc) From W1YSLC2
Onevent CONTROL Call C_YDATSTA(zc) From W1YSLC2
Return

Subprog D_YDATSTA(VALEUR)
Variable Integer VALEUR
If varinit([M]YDATSTA,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_YDATSTA(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
If VALEUR<1
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog C_YDATSTA(VALEUR)
Variable Integer VALEUR
## Action %OBLIG
If VALEUR=0
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "YDATSTA" : GZNUM = 100509 : Endif
Gosub _AFF
End

