$ON_PRIPRO
Onevent BEFORE_AREA Gosub AV_PRIPRO From W1PPC2
Onevent BEFORE_INPUT Call AS_PRIPRO(zc) From W1PPC2
Onevent AFTER_AREA Call AP_PRIPRO(zc) From W1PPC2
Onevent INIT_AREA Call D_PRIPRO(zc) From W1PPC2
Onevent CONTROL Call C_PRIPRO(zc) From W1PPC2
Return

$AV_PRIPRO
## Action STD
Call AV_PRIPRO(zc) From SUBPPC
Gosub _AFF
Return

Subprog D_PRIPRO(VALEUR)
Variable Integer VALEUR
If varinit([M]PRIPRO,indice)=0
 VALEUR = 1
Endif
Gosub _AFF
End

Subprog AS_PRIPRO(VALEUR)
Variable Integer VALEUR
Local Integer INDIC : INDIC = indice
Raz mkstat
Call INICOMBO(270) From COMBOS
## Action STD
Call AS_PRIPRO(VALEUR) From SUBPPC
If mkstat=2
 [M]PRIPRO(INDIC) = VALEUR
 Affzo PRIPRO(INDIC) : mkstat = 2
Endif
If VALEUR<1 | GMENLOC(VALEUR)=1
 VALEUR = find(0,GMENLOC(1..len(mess(0,270,1))))
Endif
If !mkstat
 Discombo 0 From PRIPRO(INDIC)
 If sum(GMENLOC)<>0
  Gosub COMBOS From COMBOS
  For WI=1 to WN : Discombo WCOMBO(WI) From PRIPRO(INDIC) : Next WI
 Endif
Endif
Gosub _AFF
End

Subprog C_PRIPRO(VALEUR)
Variable Integer VALEUR
If VALEUR>0 & VALEUR<dim(GMENLOC) & GMENLOC(VALEUR)<>0
 mkstat = 1 : End
Endif
Gosub _AFF
End

Subprog AP_PRIPRO(VALEUR)
Variable Integer VALEUR
## Action STD
Call AP_PRIPRO(VALEUR) From SUBPPC
If mkstat : GERR = 1 : GZONE = "PRIPRO" : GZNUM = 3981 : Endif
Gosub _AFF
End

