Subprog AS_PRIFLD(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(1484,GBOUT1) From OBJDIV
If !GWEBSERV
 ## Action AFRIB
 Call IB_FORMULE From AFORMULE
 If mkstat=2
  [M]PRIFLD(INDIC) = VALEUR
  Affzo PRIFLD(INDIC) : mkstat = 2
 Endif
Endif
If !mkstat
 ## Action STD
 Call AS_PRIFLD(VALEUR) From SUBPPC
 If mkstat=2
  [M]PRIFLD(INDIC) = VALEUR
  Affzo PRIFLD(INDIC) : mkstat = 2
 Endif
 Onkey 129 Titled GBOUT1 Call B1_PRIFLD(zc) From W1PPC2
Endif
Gosub _AFF
End

Subprog C_PRIFLD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action AFRCONT
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "AFRCONT"
 PARMSK(1) = ""
 Call S_AFF_CHAR(VALEUR,PARMSK,"AFRCONT","AFORMULE","") From GACTION
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "PRIFLD" : GZNUM = 2672 : Endif
Endif
Gosub _AFF
End

Subprog B1_PRIFLD(VALEUR)
Variable Char    VALEUR()
If !GWEBSERV
 ## Action AFRB1F
 Call B1_FORMUL(VALEUR,[M]FLTTAB,[M]XXCONV,[M]XXVAR) From AFORMULE
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

