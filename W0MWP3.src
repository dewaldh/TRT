# Sage X3 supervisor
# Mask management MWP3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 SAVACT = GACTION : GACTION = "STD"
 Call AS_NBLIG From TRTMWIP
 GACTION = SAVACT
 If mkstat=1 : Raz [M]NBLIG : Endif
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone TXNACT
mkstat = 0
zoncou = "TXNACT"
IMPENT = [M]TXNACT(indice)
If varinit([M]TXNACT,indice)=0
 IMPENT = 1
Endif
[M]TXNACT(indice) = IMPENT

## Zone PRINAT
mkstat = 0
zoncou = "PRINAT"
IMPENT = [M]PRINAT(indice)
If varinit([M]PRINAT,indice)=0
 IMPENT = 1
Endif
[M]PRINAT(indice) = IMPENT

## Zone AMOUNT
mkstat = 0
zoncou = "AMOUNT"
SAVACT = GACTION : GACTION = "FMTFM3DEV"
Call FMTFM3DEV(GLOCALDEV) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]AMOUNT(indice) : Endif

## Zone CSTELM2
mkstat = 0
zoncou = "CSTELM2"
SAVACT = GACTION : GACTION = "FMTFM3DEV"
Call FMTFM3DEV(GLOCALDEV) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]CSTELM2(indice) : Endif

## Zone CSTELM3
mkstat = 0
zoncou = "CSTELM3"
SAVACT = GACTION : GACTION = "FMTFM3DEV"
Call FMTFM3DEV(GLOCALDEV) From TRTDIV
GACTION = SAVACT
If mkstat=1 : Raz [M]CSTELM3(indice) : Endif

## Zone GLPSTA
mkstat = 0
zoncou = "GLPSTA"
IMPENT = [M]GLPSTA(indice)
If varinit([M]GLPSTA,indice)=0
 IMPENT = 1
Endif
[M]GLPSTA(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "MWP3"
Return

