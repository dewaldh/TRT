  If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
A_N2=1
If BPCUSTOMER4.BPC.BPR(A_N2) <> null
& and BPCUSTOMER4.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
Endif
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Local Integer A_OK1
A_N2=1
If BPCUSTOMER4.BPC.BPR(A_N2).ASTALIN<>[V]CST_ADEL & BPCUSTOMER4.BPC.BPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
 [L]A_OK1 = 1
Endif
If [L]A_OK1=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(BPCUSTOMER4.BPC,"",0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,"BPARTNER:"-mess(70,126,1)) : # Mandatory link
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(BPCUSTOMER4.BPC.BPD) : # BPDLVCUST -- 3
 If BPCUSTOMER4.BPC.BPD(A_N2) <> null
& and BPCUSTOMER4.BPC.BPD(A_N2).ASTALIN<>[V]CST_ANEWDEL
