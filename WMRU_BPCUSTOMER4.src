#-----------------------------------------------------#
$AUPDATE_TR_BPCUSTOMER4
Trbegin [F:BPC]
 Gosub AUPDATE_BPCUSTOMER4
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(BPCUSTOMER4.BPC.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 BPCUSTOMER4.BPC.ASTALIN = [V]CST_ALL
Endif
For [L]A_I=1 To maxtab(BPCUSTOMER4.BPC.C_DAYMON)
 If BPCUSTOMER4.BPC.C_DAYMON([L]A_I)<>null
  If BPCUSTOMER4.BPC.C_DAYMON([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER4.BPC.C_DAYMON([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER4.BPC.C_DAYMON([L]A_I)
   BPCUSTOMER4.BPC.C_DAYMON([L]A_I) = null
  Elsif BPCUSTOMER4.BPC.C_DAYMON([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER4.BPC.C_DAYMON([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER4.BPC.C_DIE)
 If BPCUSTOMER4.BPC.C_DIE([L]A_I)<>null
  If BPCUSTOMER4.BPC.C_DIE([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER4.BPC.C_DIE([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER4.BPC.C_DIE([L]A_I)
   BPCUSTOMER4.BPC.C_DIE([L]A_I) = null
  Elsif BPCUSTOMER4.BPC.C_DIE([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER4.BPC.C_DIE([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER4.BPC.C_INVDTAAMT)
 If BPCUSTOMER4.BPC.C_INVDTAAMT([L]A_I)<>null
  If BPCUSTOMER4.BPC.C_INVDTAAMT([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER4.BPC.C_INVDTAAMT([L]A_I).ASTALIN=[V]CST_ANEWDEL
   FreeInstance BPCUSTOMER4.BPC.C_INVDTAAMT([L]A_I)
