############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(BPCUSTOMER4,ASTATUS)
Variable Instance BPCUSTOMER4 Using R_BPCUSTOMER4
Variable Integer ASTATUS

If BPCUSTOMER4 = null : End : Endif

Gosub AVARIABLES From WMR00BPCUSTOMER4
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If fmet BPCUSTOMER4.ACTX.AGETAFCRIGHT(BPCUSTOMER4,"GESBPP",[V]CST_ATRUE)>=[V]CST_AERROR : [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END : Endif
If fmet BPCUSTOMER4.ACTX.AGETAFCRIGHTU(BPCUSTOMER4,"GESBPP",[L]A_STRSITE,[V]CST_ATRUE)>=[V]CST_AERROR
 [L]ASTATUS=[V]CST_AERROR : Goto AUPDATE_END
Endif

Gosub AREPCONTROL_AUPDATE
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_BPCUSTOMER4
Else
 Gosub AUPDATE_BPCUSTOMER4
Endif
If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_END : Return : Endif

Gosub _AUPDATE_AFTER
[L]A_I = BPCUSTOMER4.freeSnapshot
BPCUSTOMER4.snapshotEnabled = 1
$AUPDATE_END
End

#-----------------------------------------------------#
$AREPCONTROL_AUPDATE
BPCUSTOMER4.ASTALIN = [V]CST_AUPD
BPCUSTOMER4.BPC.ASTALIN = [V]CST_AUPD
[L]A_ASTALIN = BPCUSTOMER4.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER4.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = BPCUSTOMER4.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet BPCUSTOMER4.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER4._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER4._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet BPCUSTOMER4._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

