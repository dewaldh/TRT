 If BPCUSTOMER4.BPC.BPR(A_N2).ASTALIN=[V]CST_ADEL or BPCUSTOMER4.BPC.BPR(A_N2).ASTALIN=[V]CST_ANEWDEL
  FreeInstance BPCUSTOMER4.BPC.BPR(A_N2)
  BPCUSTOMER4.BPC.BPR(A_N2) = null
 Endif
 [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(BPCUSTOMER4.BPC.BPD)
 If BPCUSTOMER4.BPC.BPD(A_N2) <> null
  [L]ASTATUS2 = fmet BPCUSTOMER4.BPC.BPD(A_N2)._AUPDATE_ABORT
