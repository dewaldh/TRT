 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER4.BPC.C_TSCCOD)
 If BPCUSTOMER4.BPC.C_TSCCOD([L]A_I)<>null
  If BPCUSTOMER4.BPC.C_TSCCOD([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER4.BPC.C_TSCCOD([L]A_I).ASTALIN=[V]CST_ANEWDEL
