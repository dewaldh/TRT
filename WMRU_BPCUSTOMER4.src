  Elsif BPCUSTOMER4.BPC.C_INVDTAAMT([L]A_I).ASTALIN <> [V]CST_ALL
   BPCUSTOMER4.BPC.C_INVDTAAMT([L]A_I).ASTALIN = [V]CST_ALL
  Endif
 Endif
Next
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif
For [L]A_I=1 To maxtab(BPCUSTOMER4.BPC.C_REP)
 If BPCUSTOMER4.BPC.C_REP([L]A_I)<>null
  If BPCUSTOMER4.BPC.C_REP([L]A_I).ASTALIN=[V]CST_ADEL or BPCUSTOMER4.BPC.C_REP([L]A_I).ASTALIN=[V]CST_ANEWDEL
