####################################################################
# Labels of the main class and methods 
####################################################################

####################################################################
# 1 : MACCST Machine cost ( Field MD8 0)
####################################################################

$SET_MACCST
If [L]_AOLDVAL<>this.MACCST
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_MACCST
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.MACCST
 Gosub PROPAGATE_MACCST
Endif
Return

$INIT_MACCST
Gosub DECLARE_VAR From WMC0ITMCOSTS_L_MACCST
[L]CURPRO = "MACCST"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_MACCST
Gosub DECLARE_VAR From WMC0ITMCOSTS_L_MACCST
[L]CURPRO = "MACCST"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_MACCST                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_MACCST                : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_MACCST
[L]CURPRO = "MACCST"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.MACCST=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_MACCST
[L]CURPRO = "MACCST"
Return

$_AOTHERS_MACCST
[L]CURPRO = "MACCST"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_MACCST
[L]CURPRO = "MACCST"
Return

$PROPAGATE_MACCST
this.isReadonly = 0
[L]CURPRO = "MACCST"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_MACCST
Gosub DECLARE_VAR From WMC0ITMCOSTS_L_MACCST
[L]CURPRO = "MACCST"
$_LNKPRO_MACCST
Return


