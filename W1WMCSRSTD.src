 SAVACT = GACTION : GACTION = "CONSPIV"
 PARMSK(1) = "SRH"
 PARMSK(2) = [M:CSR2]SRHNUM(nolign-1)
 PARMSK(3) = num$([M:CSR2]SRDLIN(nolign-1))
 Call CONSULT_NUM(BASPAG,"PIV",PARMSK) From GCONSULT
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
