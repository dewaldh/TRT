$OUVFIC_DERLU
Local Decimal  A_BUFI(0..GNBDERNIER)
Local Char     A_BUFC(200)(0..GNBDERNIER)
Local Char     A_BUF1(10)(0..GNBDERNIER)
Local Char     A_BUF2(30)(0..GNBDERNIER)
Local Libelle  A_BUF3(0..GNBDERNIER)
If clalev([F:AYD9])>0 : Close Local File [AYD9] : Endif
Local File (
& Decimal  A_TABI,
& Char     A_TABC(200),
& Char     A_TAB1(10),
& Char     A_TAB2(30),
& Libelle  A_TAB3
& ) From Variable A_BUFI,A_BUFC,A_BUF1,A_BUF2,A_BUF3
& As [AYD9]
Return

$LEC_DERLU
Read [AX3]AXX0="AYTPRF";"INTIT";GLANGUE;FCYCOD;PRFCOD
If fstat : Raz [F:AX3] : Endif
Return

$CHARGE_DERLU
If clalev([F:AYD9])>0
 Read [AYD9] = [F:AYD9]A_TABI
 [L]CLE = [F:AYD9]A_TABC
Endif
Return

$REMP_DERLU
If clalev([F:AYD9])>0
 Filter [AYD9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [AYD1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:AYD9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:AYD]PRFCOD+"~"
 A_CLEDERLU += [F:AYD]FCYCOD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:AYD9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:AYD9]
 Endif
Endif
Return

$OUVRE_LISTE
Local Integer NBSEL , NBTRI , SELCAR
