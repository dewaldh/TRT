# Supervisor
# Mask management STKSHTSEL 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone FLG
mkstat = 0
zoncou = "FLG"
IMPENT = [M]FLG(indice)
If varinit([M]FLG,indice)=0
 IMPENT = 1
Endif
If varmode([M]FLG,indice)=0
 [M]FLG(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "FLG" : Gosub ACTION From =IMPTRT
  IMPENT = [M]FLG(indice)
 Endif
 SAVACT = GACTION : GACTION = "STD"
 Call C_FLG(IMPENT) From TRTSHTS
 GACTION = SAVACT
 If mkstat : GERR = 1 : GZONE = "FLG" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif
[M]FLG(indice) = IMPENT

## Zone ITMREF
mkstat = 0
zoncou = "ITMREF"
SAVACT = GACTION : GACTION = "STD"
Call AV_ITMREF([M]ITMREF(indice)) From TRTSHTS
GACTION = SAVACT
If mkstat=1 : Raz [M]ITMREF(indice) : Endif

## Zone TYPREG
mkstat = 0
zoncou = "TYPREG"
IMPENT = [M]TYPREG(indice)
If varinit([M]TYPREG,indice)=0
 IMPENT = 1
Endif
[M]TYPREG(indice) = IMPENT

## Zone VCRTYP
mkstat = 0
zoncou = "VCRTYP"
IMPENT = [M]VCRTYP(indice)
If varinit([M]VCRTYP,indice)=0
 IMPENT = 1
Endif
[M]VCRTYP(indice) = IMPENT

## Zone VCRTYPORI
mkstat = 0
zoncou = "VCRTYPORI"
IMPENT = [M]VCRTYPORI(indice)
If varinit([M]VCRTYPORI,indice)=0
 IMPENT = 1
Endif
[M]VCRTYPORI(indice) = IMPENT

## Zone AVAFLG
mkstat = 0
zoncou = "AVAFLG"
IMPENT = [M]AVAFLG(indice)
If varinit([M]AVAFLG,indice)=0
 IMPENT = 1
Endif
[M]AVAFLG(indice) = IMPENT

## Zone ALLTYP
mkstat = 0
zoncou = "ALLTYP"
IMPENT = [M]ALLTYP(indice)
If varinit([M]ALLTYP,indice)=0
 IMPENT = 1
Endif
[M]ALLTYP(indice) = IMPENT

## Zone TRSCOD
mkstat = 0
zoncou = "TRSCOD"
If varinit([M]TRSCOD,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TRSCOD(indice),"14","") From SUBADI
 GACTION = SAVACT
Endif

## Zone TRSTYP
mkstat = 0
zoncou = "TRSTYP"
IMPENT = [M]TRSTYP(indice)
If varinit([M]TRSTYP,indice)=0
 IMPENT = 1
Endif
[M]TRSTYP(indice) = IMPENT

## Zone TRSFAM
mkstat = 0
zoncou = "TRSFAM"
If varinit([M]TRSFAM,indice)=0
 SAVACT = GACTION : GACTION = "INILEG"
 Call INILEG([M]TRSFAM(indice),"9","") From SUBADI
 GACTION = SAVACT
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "STKSHTSEL"
Return

