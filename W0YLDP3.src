# Supervisor
# Mask management YLDP3 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 1
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone YLIGID
mkstat = 0
zoncou = "YLIGID"
If varmode([M]YLIGID,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YLIGID" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YLIGID(indice)=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YLIGID" : GZNUM = 641 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YBUSPAR
mkstat = 0
zoncou = "YBUSPAR"
If varmode([M]YBUSPAR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YBUSPAR" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YBUSPAR(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YBUSPAR" : GZNUM = 19569 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YCLA
mkstat = 0
zoncou = "YCLA"
If varmode([M]YCLA,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YCLA" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]YCLA(indice),2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YCLA" : GZNUM = 11208 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YVALFRMR
mkstat = 0
zoncou = "YVALFRMR"
If varmode([M]YVALFRMR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YVALFRMR" : Gosub ACTION From =IMPTRT
 Endif
 If [M]YVALFRMR(indice)=[0/0/0]
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "YVALFRMR" : GZNUM = 100870 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
Endif

## Zone YVALTOR
mkstat = 0
zoncou = "YVALTOR"
If varmode([M]YVALTOR,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "YVALTOR" : Gosub ACTION From =IMPTRT
 Endif
Endif

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
