#<AdxTL>@(#)0.0.0.0 $Revision$
# Sage X3 supervisor
# Mask management VXINDO 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone WEMPNUM
mkstat = 0
zoncou = "WEMPNUM"
indice = 0
If varinit([M]WEMPNUM,indice)=0
 [M]WEMPNUM = mess(880,197,1)
Endif

## Zone EMPNUM
mkstat = 0
zoncou = "EMPNUM"
indice = 0
WNUM = [M]EMPNUM
If varmode([M]EMPNUM,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "EMPNUM" : Gosub ACTION From =IMPTRT
 Endif
 If [M]EMPNUM=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "EMPNUM" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If [M]EMPNUM<0
   mkstat = 1 : GMESSAGE = mess(180,141,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "EMPNUM" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_EMPNUM([M]EMPNUM) From VXINDO
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "EMPNUM" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WNUM<>[M]EMPNUM or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_EMPNUM([M]EMPNUM) From VXINDO
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "EMPNUM" : GZNUM = 0 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone WFCY
mkstat = 0
zoncou = "WFCY"
indice = 0
If varinit([M]WFCY,indice)=0
 [M]WFCY = mess(498,197,1)
Endif

## Zone FCY
mkstat = 0
zoncou = "FCY"
indice = 0
If varmode([M]FCY,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "FCY" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]FCY,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  SAVACT = GACTION : GACTION = "ACONTFCY"
  Call CONTSITE([M]FCY) From SUBAGF
  GACTION = SAVACT
  If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   Call CONTROLE("FCY","E",[M]FCY,"",mkstat) From CONTOBJ
   If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    SAVACT = GACTION : GACTION = "STD"
    Call C_FCY([M]FCY) From VXINDO
    GACTION = SAVACT
    If mkstat : GERR = 1 : GZONE = "FCY" : GZNUM = 0 : Endif
    Gosub AFF From GSAISIE
    If mkstat : End : Endif
   Endif
  Endif
 Endif
Endif

## Zone WINDREF
mkstat = 0
zoncou = "WINDREF"
indice = 0
If varinit([M]WINDREF,indice)=0
 [M]WINDREF = mess(878,197,1)
Endif

## Zone INDREF
mkstat = 0
zoncou = "INDREF"
indice = 0
WCHAR = [M]INDREF
If varmode([M]INDREF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "INDREF" : Gosub ACTION From =IMPTRT
 Endif
 If vireblc([M]INDREF,2)=""
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "INDREF" : GZNUM = 0 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  Call CONTROLE("INDREF","",[M]INDREF,"",mkstat) From CONTOBJ
  If mkstat : GERR = 1 : GZONE = "INDREF" : GZNUM = 0 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_INDREF([M]INDREF) From VXINDO
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "INDREF" : GZNUM = 0 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
   If !mkstat
    If WCHAR<>[M]INDREF or IMPMOD
     SAVACT = GACTION : GACTION = "STD"
     Call AM_INDREF([M]INDREF) From VXINDO
     GACTION = SAVACT
     If mkstat : GERR = 1 : GZONE = "INDREF" : GZNUM = 0 : Endif
     Gosub AFF From GSAISIE
     If mkstat : End : Endif
    Endif
   Endif
  Endif
 Endif
Endif

## Zone INDTYP
mkstat = 0
zoncou = "INDTYP"
indice = 0
IMPENT = [M]INDTYP
If varinit([M]INDTYP,indice)=0
 IMPENT = 1
Endif
[M]INDTYP = IMPENT

## Zone EMPTYP
mkstat = 0
zoncou = "EMPTYP"
indice = 0
IMPENT = [M]EMPTYP
If varinit([M]EMPTYP,indice)=0
 IMPENT = 1
Endif
[M]EMPTYP = IMPENT

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "VXINDO"
Return

