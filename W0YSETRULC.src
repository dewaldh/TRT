Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone YSEL
mkstat = 0
zoncou = "YSEL"
IMPENT = [M]YSEL(indice)
If varinit([M]YSEL,indice)=0
 IMPENT = 1
Endif
If varmode([M]YSEL,indice)=0
 [M]YSEL(indice) = IMPENT
 If !mkstat
  IMPMOD = 0 : IMPZON = "YSEL" : Gosub ACTION From =IMPTRT
  IMPENT = [M]YSEL(indice)
 Endif
Endif
[M]YSEL(indice) = IMPENT

## Zone YCALTYP
mkstat = 0
zoncou = "YCALTYP"
IMPENT = [M]YCALTYP(indice)
If varinit([M]YCALTYP,indice)=0
 IMPENT = 1
Endif
[M]YCALTYP(indice) = IMPENT

## Zone YALLOW
mkstat = 0
zoncou = "YALLOW"
IMPENT = [M]YALLOW(indice)
If varinit([M]YALLOW,indice)=0
 IMPENT = 1
Endif
[M]YALLOW(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

