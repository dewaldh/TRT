Subprog AS_LOCDES(VALEUR)
Variable Char    VALEUR()
Local Integer INDIC : INDIC = indice
Raz mkstat
## Action STKMODREMP
Call STKMODREMP([M]WLOCSEQ,[M]STOFCY,[M]ITMREF,[M]PCU,[M]QTYPCU,[M]STA,[M]LOC,""
&,mkstat,string$([L]VALEUR=[M]LOC,"QU")) From STKLOC
If mkstat=2
 [M]LOCDES(INDIC) = VALEUR
 Affzo LOCDES(INDIC) : mkstat = 2
Endif
If !mkstat
 ## Action STKSUGEMP
 Call STKSUGEMP([M]WLOCSEQ,[M:SCS1]TRSTYP,[M:SCS1]TRSCOD,[M]FCYDES,[M]ITMREF,[M]PCU,[M]QTYPCU,[M]STADES
& ,[M]LOCTYPDES,VALEUR,"",string$([L]VALEUR=[M]LOC,"QU"),GFILLOCENT,"") From STKLOC
 If mkstat=2
  [M]LOCDES(INDIC) = VALEUR
  Affzo LOCDES(INDIC) : mkstat = 2
 Endif
Endif
Gosub _AFF
End

Subprog C_LOCDES(VALEUR)
Variable Char    VALEUR()
## Action CTLEMPFMT
Call CTLEMPFMT(VALEUR,"") From STKACT
If mkstat : GERR = 1 : GZONE = "LOCDES" : GZNUM = 0 : Endif
If !mkstat
 ## Action STKCTREMP
 Call STKCTREMP([M]WLOCSEQ,[M:SCS1]TRSTYP,[M:SCS1]TRSCOD,[M]FCYDES,[M]ITMREF,[M]PCU,[M]QTYPCU,[M]STADES
& ,[M]LOCTYPDES,VALEUR,"",0,mkstat,GBIDD1,string$([L]VALEUR=[M]LOC,"QU"),GFILLOCENT
& ,"") From STKLOC
 If mkstat : GERR = 1 : GZONE = "LOCDES" : GZNUM = 0 : Endif
 If !mkstat
  ## Action STD
  Call C_LOCDES(VALEUR) From VXAIT
  If mkstat : GERR = 1 : GZONE = "LOCDES" : GZNUM = 0 : Endif
 Endif
Endif
Gosub _AFF
End

