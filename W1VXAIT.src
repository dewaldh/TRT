Subprog C_FCYDES(VALEUR)
Variable Char    VALEUR()
## Action ACONTFCY
Call CONTSITE(VALEUR) From SUBAGF
If mkstat : GERR = 1 : GZONE = "FCYDES" : GZNUM = 0 : Endif
If !mkstat
 ## Action %CNTOBJ
 Call CONTSEL("FCY","!D",VALEUR,"",mkstat) From CONTOBJ
 If mkstat : GERR = 1 : GZONE = "FCYDES" : GZNUM = 0 : Endif
 If !mkstat
  ## Action STKCTRFCY
  Call STKCTRFCY(VALEUR) From STKACT
  If mkstat : GERR = 1 : GZONE = "FCYDES" : GZNUM = 0 : Endif
  If !GWEBSERV
   If !mkstat
    ## Action STD
    Call C_FCYDES(VALEUR) From VXAIT
    If mkstat : GERR = 1 : GZONE = "FCYDES" : GZNUM = 0 : Endif
   Endif
  Endif
 Endif
Endif
Gosub _AFF
End

