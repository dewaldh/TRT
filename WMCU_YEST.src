############################################################################################
### Subprog _AUPDATE
############################################################################################
Subprog _AUPDATE(YEST,ASTATUS)
Variable Instance YEST Using C_YEST
Variable Integer ASTATUS

If YEST = null : End : Endif

Gosub _AVARIABLES From WMC00YEST

[L]A_STRSITE = ""
[L]A_STRACC = ""
Gosub AUPDATE_YEST

End

#-----------------------------------------------------#

############################################################################################
### Subprog AUPDATE
############################################################################################
Subprog AUPDATE(YEST,ASTATUS)
Variable Instance YEST Using C_YEST
Variable Integer ASTATUS

If YEST = null : End : Endif

Gosub AVARIABLES From WMC00YEST
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

[L]A_STRSITE = ""
[L]A_STRACC = ""
YEST.ASTALIN = [V]CST_AUPD
Gosub AUPDATE_CONTROL
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

If !adxlog
 Gosub AUPDATE_TR_YEST
Else
 Gosub AUPDATE_YEST
Endif
If [L]ASTATUS>=[V]CST_AERROR : Goto AUPDATE_END : Endif

Gosub _AUPDATE_AFTER
[L]A_I = YEST.freeSnapshot
YEST.snapshotEnabled = 1
$AUPDATE_END
End


$AUPDATE_CONTROL
Gosub _AUPDATE_CONTROL_BEFORE
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
# Control mandatory
[L]ASTATUS =  max([L]ASTATUS,fmet YEST._AMANDATORY)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control format
[L]ASTATUS =  max([L]ASTATUS,fmet YEST._AFORMAT)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

# Control other
[L]ASTATUS =  max([L]ASTATUS,fmet YEST._AOTHERS)
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AUPDATE_CONTROL_AFTER
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_BEFORE
If YEST.ASTALIN = [V]CST_ANEWDEL : Return : Endif
YEST.ASTALIN = [V]CST_AUPD
If [V]CST_ASTALIN(YEST.ASTALIN)<>""
 [L]A_ASTALIN = YEST.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_BEFORE"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
For A_N2=1 To maxtab(YEST.YSTAGE) : # YSTA -- 2
 If YEST.YSTAGE(A_N2) <> null
& and YEST.YSTAGE(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YEST.YSTAGE(A_N2).ASTALIN<>[V]CST_ANEW & YEST.ASTALIN=[V]CST_ADEL
   YEST.YSTAGE(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ANEW & find(YEST.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YEST.YSTAGE(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YEST.YSTAGE(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YEST.YSTAGE(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YLDP) : # YLNDPUR -- 3
 If YEST.YLDP(A_N2) <> null
& and YEST.YLDP(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEPR) : # YPRE -- 4
 If YEST.YEPR(A_N2) <> null
& and YEST.YEPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YEST.YEPR(A_N2).ASTALIN<>[V]CST_ANEW & YEST.ASTALIN=[V]CST_ADEL
   YEST.YEPR(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YEST.YEPR(A_N2).ASTALIN=[V]CST_ANEW & find(YEST.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YEST.YEPR(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YEST.YEPR(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YEST.YEPR(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YREL) : # YREL -- 5
 If YEST.YREL(A_N2) <> null
& and YEST.YREL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If YEST.YREL(A_N2).ASTALIN<>[V]CST_ANEW & YEST.ASTALIN=[V]CST_ADEL
   YEST.YREL(A_N2).ASTALIN = [V]CST_ADEL
  Endif
  If YEST.YREL(A_N2).ASTALIN=[V]CST_ANEW & find(YEST.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL)>0
   YEST.YREL(A_N2).ASTALIN = [V]CST_ANEWDEL
  Endif
  If [V]CST_ASTALIN(YEST.YREL(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YEST.YREL(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._AINSERT_CONTROL_BEFORE)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._AUPDATE_CONTROL_BEFORE)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._ADELETE_CONTROL_BEFORE)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

Return

#-----------------------------------------------------#
$_AUPDATE_CONTROL_AFTER
For A_N2=1 To maxtab(YEST.YSTAGE) : # YSTA -- 2
 If YEST.YSTAGE(A_N2) <> null
& and YEST.YSTAGE(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YEST.YSTAGE(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YEST.YSTAGE(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._ADELETE_CONTROL_AFTER)
