For A_N2=1 To maxtab(YEST.YLDP) : # YLNDPUR -- 3
 If YEST.YLDP(A_N2) <> null
& and YEST.YLDP(A_N2).ASTALIN<>[V]CST_ANEWDEL
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YEPR) : # YPRE -- 4
 If YEST.YEPR(A_N2) <> null
& and YEST.YEPR(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YEST.YEPR(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YEST.YEPR(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
For A_N2=1 To maxtab(YEST.YREL) : # YREL -- 5
 If YEST.YREL(A_N2) <> null
& and YEST.YREL(A_N2).ASTALIN<>[V]CST_ANEWDEL
  If [V]CST_ASTALIN(YEST.YREL(A_N2).ASTALIN)<>""
   Case [V]CST_ASTALIN(YEST.YREL(A_N2).ASTALIN)
    When [V]CST_APRGAINSERT  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._AINSERT_CONTROL_AFTER)
    When [V]CST_APRGAUPDATE : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._AUPDATE_CONTROL_AFTER)
    When [V]CST_APRGADELETE  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._ADELETE_CONTROL_AFTER)
   Endcase
   If [L]ASTATUS>=[V]CST_AERROR : Break  : Endif
  Endif
 Endif
Next A_N2
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

If [V]CST_ASTALIN(YEST.ASTALIN)<>""
 [L]A_ASTALIN = YEST.ASTALIN
 [L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_CONTROL_AFTER"))
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Endif
Return

#-----------------------------------------------------#
$AUPDATE_TR_YEST
Trbegin [F:YEST]
 Gosub AUPDATE_YEST
 If [L]ASTATUS>=[V]CST_AERROR : Gosub AUPDATE_ABORT : Return : Endif
Commit
Return

#-----------------------------------------------------#
#-----------------------------------------------------#
$AUPDATE_YEST
[L]A_ASTALIN = YEST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_BEFORE"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]ASTATUS = max([L]ASTATUS,func WMC00YEST._MAJSQL_YESTA(YEST))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

For [L]A_TYPMAJ=[V]CST_ADEL,[V]CST_AUPD,[V]CST_ANEW
 #-----------------------#
 For A_N2=1 To maxtab(YEST.YSTAGE)
  If YEST.YSTAGE(A_N2) <> null and
& (YEST.YSTAGE(A_N2).ASTALIN=[L]A_TYPMAJ or
& YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ALL)
   If YEST.YSTAGE(A_N2).ASTALIN=[L]A_TYPMAJ
    Case YEST.YSTAGE(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YSTAGE(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(YEST.YLDP)
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(YEST.YEPR)
  If YEST.YEPR(A_N2) <> null and
& (YEST.YEPR(A_N2).ASTALIN=[L]A_TYPMAJ or
& YEST.YEPR(A_N2).ASTALIN=[V]CST_ALL)
   If YEST.YEPR(A_N2).ASTALIN=[L]A_TYPMAJ
    Case YEST.YEPR(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YEPR(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
 #-----------------------#
 For A_N2=1 To maxtab(YEST.YREL)
  If YEST.YREL(A_N2) <> null and
& (YEST.YREL(A_N2).ASTALIN=[L]A_TYPMAJ or
& YEST.YREL(A_N2).ASTALIN=[V]CST_ALL)
   If YEST.YREL(A_N2).ASTALIN=[L]A_TYPMAJ
    Case YEST.YREL(A_N2).ASTALIN
     When [V]CST_AUPD  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._AUPDATE)
     When [V]CST_ANEW : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._AINSERT)
     When [V]CST_ADEL  : [L]ASTATUS = max([L]ASTATUS,fmet YEST.YREL(A_N2)._ADELETE)
    Endcase
    If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
   Endif
  Endif
  If [L]ASTATUS>=[V]CST_AERROR : Break 2 : Endif
 Next A_N2
 If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
 #-----------------------#
Next
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif

[L]A_ASTALIN = YEST.ASTALIN
[L]ASTATUS = max([L]ASTATUS,fmet YEST.ACTION([V]CST_ASTALIN([L]A_ASTALIN)+"_AFTER"))
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

#-----------------------------------------------------#
$AUPDATE_ABORT
Rollback
$_AUPDATE_ABORT
If YEST.snapshotEnabled=1
 [L]A_RETIK = YEST.revertUpdTick
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YSTAGE)
 If YEST.YSTAGE(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YSTAGE(A_N2)._AUPDATE_ABORT
  If YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ADEL or YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YSTAGE(A_N2)
   YEST.YSTAGE(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YLDP)
 If YEST.YLDP(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YLDP(A_N2)._AUPDATE_ABORT
  If YEST.YLDP(A_N2).ASTALIN=[V]CST_ADEL or YEST.YLDP(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YLDP(A_N2)
   YEST.YLDP(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEPR)
 If YEST.YEPR(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEPR(A_N2)._AUPDATE_ABORT
  If YEST.YEPR(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEPR(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEPR(A_N2)
   YEST.YEPR(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YREL)
 If YEST.YREL(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YREL(A_N2)._AUPDATE_ABORT
  If YEST.YREL(A_N2).ASTALIN=[V]CST_ADEL or YEST.YREL(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YREL(A_N2)
   YEST.YREL(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2

[L]ASTATUS = max(fmet YEST.ACTION("AUPDATE_ABORT"),[L]ASTATUS)
Return
#-----------------------------------------------------#


#-----------------------------------------------------#
$_AUPDATE_AFTER
If find(YEST.ASTALIN,[V]CST_ADEL,[V]CST_ANEWDEL,[V]CST_ALL)=0
 YEST.ASTALIN = [V]CST_ALL
Endif
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YSTAGE)
 If YEST.YSTAGE(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YSTAGE(A_N2)._AUPDATE_AFTER
  If YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ADEL or YEST.YSTAGE(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YSTAGE(A_N2)
   YEST.YSTAGE(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YLDP)
 If YEST.YLDP(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YLDP(A_N2)._AUPDATE_AFTER
  If YEST.YLDP(A_N2).ASTALIN=[V]CST_ADEL or YEST.YLDP(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YLDP(A_N2)
   YEST.YLDP(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YEPR)
 If YEST.YEPR(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YEPR(A_N2)._AUPDATE_AFTER
  If YEST.YEPR(A_N2).ASTALIN=[V]CST_ADEL or YEST.YEPR(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YEPR(A_N2)
   YEST.YEPR(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Local Integer ASTATUS2
For A_N2=1 To maxtab(YEST.YREL)
 If YEST.YREL(A_N2) <> null
  [L]ASTATUS2 = fmet YEST.YREL(A_N2)._AUPDATE_AFTER
  If YEST.YREL(A_N2).ASTALIN=[V]CST_ADEL or YEST.YREL(A_N2).ASTALIN=[V]CST_ANEWDEL
   FreeInstance YEST.YREL(A_N2)
   YEST.YREL(A_N2) = null
  Endif
  [L]ASTATUS = max([L]ASTATUS2,[L]ASTATUS)
 Endif
Next A_N2
Return



