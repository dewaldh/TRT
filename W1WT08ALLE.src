Subprog AM_RENSMP(VALEUR)
Variable Char    VALEUR()
If !mkstat & evalue("dim("+clanam([M])+"ZRENSMP)")>indice
 [M]RENSMP(indice) = VALEUR
 Affzo ZRENSMP(indice)
Endif
Gosub _AFF
End

Subprog S_RENSMP(VALEUR)
