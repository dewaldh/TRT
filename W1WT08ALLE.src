Subprog C_IPTDAT(VALEUR)
Variable Date    VALEUR
## Action %OBLIG
If VALEUR=[0/0/0]
 mkstat = 1 : GMESSAGE = mess(10,123,1)
Endif
If mkstat : GERR = 1 : GZONE = "IPTDAT" : GZNUM = 3205 : Endif
If !mkstat
 ## Action STKCTRIPT
 Call STKCTRIPT(VALEUR,[M:SMP]STOFCY,"1") From STKLIB
 If mkstat : GERR = 1 : GZONE = "IPTDAT" : GZNUM = 3205 : Endif
Endif
Gosub _AFF
End

