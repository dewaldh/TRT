Endif
Return

$REMP_DERLU
If clalev([F:SER9])>0
 Filter [SER9]
& Where A_TABI<>0
& Order by A_TABI
 Fillbox [SER1] GAU_CHE9
Endif
Return

$CLE_DERLU
If clalev([F:SER9])>0
 Local Integer A_IND
 Local Char    A_CLEDERLU(200)
 A_CLEDERLU = ""
 A_CLEDERLU = [F:SER]PRCCOD+"~"
 A_IND = find(A_CLEDERLU,A_BUFC)
 If A_IND
  [F:SER9]A_TABI = A_BUFI(A_IND-1)
 Else
  Raz [F:SER9]
 Endif
Endif
Return

