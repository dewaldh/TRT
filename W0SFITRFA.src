Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc tableau 1
####################################
IMPTBL = 1

## Zone NBSFI
mkstat = 0
zoncou = "NBSFI"
indice = 0
status = 1
zoncou = "NBSFI"
If varmode([M]NBSFI,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBSFI" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBSFI
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBSFI = nolign
   status = 75
   Call SAI_NBSFI(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBSFIT
mkstat = 0
zoncou = "NBSFIT"
indice = 0
status = 1
zoncou = "NBSFIT"
If varmode([M]NBSFIT,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBSFIT" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBSFIT
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBSFIT = nolign
   status = 75
   Call SAI_NBSFIT(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc tableau 3
####################################
IMPTBL = 1

## Zone NBTRF
mkstat = 0
zoncou = "NBTRF"
indice = 0
status = 1
zoncou = "NBTRF"
If varmode([M]NBTRF,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBTRF" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBTRF
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBTRF = nolign
   status = 75
   Call SAI_NBTRF(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

####################################
## Bloc liste 4
####################################
IMPTBL = 0

## Zone C_ORIVCR
mkstat = 0
zoncou = "C_ORIVCR"
indice = 0
IMPENT = [M]C_ORIVCR
If varinit([M]C_ORIVCR,indice)=0
 IMPENT = 1
Endif
[M]C_ORIVCR = IMPENT

## Zone C_DESVCR
mkstat = 0
zoncou = "C_DESVCR"
indice = 0
IMPENT = [M]C_DESVCR
If varinit([M]C_DESVCR,indice)=0
