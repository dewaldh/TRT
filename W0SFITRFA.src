Endif
[M]C_DESVCR = IMPENT

## Zone C_BRDRUL
mkstat = 0
zoncou = "C_BRDRUL"
indice = 0
IMPENT = [M]C_BRDRUL
If varinit([M]C_BRDRUL,indice)=0
 IMPENT = 1
Endif
[M]C_BRDRUL = IMPENT

## Zone C_GRUFLG
mkstat = 0
zoncou = "C_GRUFLG"
indice = 0
IMPENT = [M]C_GRUFLG
If varinit([M]C_GRUFLG,indice)=0
 IMPENT = 1
Endif
[M]C_GRUFLG = IMPENT

## Zone C_GRURUL
mkstat = 0
zoncou = "C_GRURUL"
indice = 0
IMPENT = [M]C_GRURUL
If varinit([M]C_GRURUL,indice)=0
 IMPENT = 1
Endif
[M]C_GRURUL = IMPENT

mkstat = 0

End

