# Supervisor
# Mask management SOHDNVX 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone ORDSTA
mkstat = 0
zoncou = "ORDSTA"
indice = 0
IMPENT = [M]ORDSTA
If varinit([M]ORDSTA,indice)=0
 IMPENT = 1
Endif
[M]ORDSTA = IMPENT

## Zone ALLSTA
mkstat = 0
zoncou = "ALLSTA"
indice = 0
IMPENT = [M]ALLSTA
If varinit([M]ALLSTA,indice)=0
 IMPENT = 1
Endif
[M]ALLSTA = IMPENT

## Zone INVSTA
mkstat = 0
zoncou = "INVSTA"
indice = 0
IMPENT = [M]INVSTA
If varinit([M]INVSTA,indice)=0
 IMPENT = 1
Endif
[M]INVSTA = IMPENT

## Zone CDTSTA
mkstat = 0
zoncou = "CDTSTA"
indice = 0
IMPENT = [M]CDTSTA
If varinit([M]CDTSTA,indice)=0
 IMPENT = 1
Endif
[M]CDTSTA = IMPENT

## Zone PRESTA
mkstat = 0
zoncou = "PRESTA"
indice = 0
IMPENT = [M]PRESTA
If varinit([M]PRESTA,indice)=0
 IMPENT = 1
Endif
[M]PRESTA = IMPENT

## Zone DLVSTA
mkstat = 0
zoncou = "DLVSTA"
indice = 0
IMPENT = [M]DLVSTA
If varinit([M]DLVSTA,indice)=0
 IMPENT = 1
Endif
[M]DLVSTA = IMPENT

####################################
## Bloc tableau 2
####################################
IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"
indice = 0
status = 1
zoncou = "NBLIG"
If varmode([M]NBLIG,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NBLIG" : Gosub ACTION From =IMPTRT
 Endif
 If !mkstat
  NBLIG = [M]NBLIG
  For nolign=1 To NBLIG
   indice = nolign-1
   [M]NBLIG = nolign
   status = 75
   Call SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
   If mkstat : Break : Endif
  Next nolign
  If mkstat : End : Endif
  indice = 0
 Endif
Endif

mkstat = 0

End

####################################
## Bloc tableau 2
####################################
Subprog SAI_NBLIG(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

IMPTBL = 1

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

## Zone NUMLIG
mkstat = 0
zoncou = "NUMLIG"
[M]NUMLIG(indice) = nolign

## Zone SOQSTA
mkstat = 0
zoncou = "SOQSTA"
IMPENT = [M]SOQSTA(indice)
If varinit([M]SOQSTA,indice)=0
 IMPENT = 1
Endif
[M]SOQSTA(indice) = IMPENT

## Zone FMI
mkstat = 0
zoncou = "FMI"
IMPENT = [M]FMI(indice)
If varinit([M]FMI,indice)=0
 IMPENT = 1
Endif
[M]FMI(indice) = IMPENT

## Zone NBLIG
mkstat = 0
zoncou = "NBLIG"

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "SOHDNVX"
Return

