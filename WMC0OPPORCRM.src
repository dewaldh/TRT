$ADELLINE
Case _ACOLLECTION
 When "STPSTA"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"STPSTA",this)
 When "STPSTB"
  [L]_ALINE =  func ASYRSUBTAB.ADELLINE([L]ALINE,"STPSTB",this)
 When Default :
  [L]_ALINE = [V]CST_ANOTDEFINED
Endcase
Return

