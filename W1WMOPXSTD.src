# Sage X3 supervisor
# Mask management WMOPXSTD 

$_AFF
If GIMPORT
 Gosub AFF From GSAISIE
Else
 If GMESSAGE <>""
  Gosub ERR From GSAISIE
 Else
  Raz GERR , GZNUM
 Endif
Endif
Return

$HORODAT
Local Date    A_DATE
Local Char    A_HEURE(10) , A_USER(10)
Local Char    A_STAMP(15)
A_DATE  = [26/03/2018]
A_HEURE = "13:13:06"
A_USER  = "ADMIN"
A_STAMP = "20180326131306"
Return

$TITRES
Return

$CHG_FMT
Case zoncou
 When "NBLIG"
  If [M]NBLIG=0
  Endif
Endcase
Return

$VISU
Local Integer A_REPONSE
Global Integer GPRE_VIEW
If dim(A_WINDPREV)<=0 : Local Char A_WINDPREV(30) : Endif
Local Mask WMOPXSTD [OPX2]
Gosub HORODAT
Local Inpbox "WMOPXSTD" From GFONCTION At A_STAMP With A_WINDPREV Mask [OPX2] As [SIM_]
Grizo [OPX2]20
Boxact [SIM_]
Gosub TITRES
Boxinp [SIM_] Using A_REPONSE
Close Local Inpbox [SIM_]
Close Local Mask [OPX2]
Kill GPRE_VIEW
Return

$HARDCOP
Local Integer X1(0..14),X2(0..14),Y1(0..14),Y2(0..14)
X1(0)=1 : X2(0)=112 : Y1(0)=1 : Y2(0)=20
Return

$ON_NBLIG
Onevent BEFORE_INPUT Call AS_NBLIG From W1WMOPXSTD
Return

Subprog IB_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action STD
 Call IB_NBLIG From CNSOPXSTD
Endif
Gosub _AFF
End

Subprog AS_NBLIG
Local Decimal BASPAG
Local Integer INDIC : INDIC = indice
Raz mkstat
Call TEXTE(16134,GBOUT1) From OBJDIV
Call TEXTE(8035,GBOUT2) From OBJDIV
Call TEXTE(454,GBOUT3) From OBJDIV
Call TEXTE(12439,GBOUT4) From OBJDIV
Call TEXTE(12372,GBOUT5) From OBJDIV
Call TEXTE(454,GBOUT6) From OBJDIV
Call TEXTE(18245,GBOUT8) From OBJDIV
Call TEXTE(735,GBOUT9) From OBJDIV
Call IB_NBLIG
Onkey 129 Titled GBOUT1 Call B1_NBLIG From W1WMOPXSTD
Onkey 130 Titled GBOUT2 Call B2_NBLIG From W1WMOPXSTD
Onkey 131 Titled GBOUT3 Call B3_NBLIG From W1WMOPXSTD
Onkey 132 Titled GBOUT4 Call B4_NBLIG From W1WMOPXSTD
Onkey 133 Titled GBOUT5 Call B5_NBLIG From W1WMOPXSTD
Onkey 134 Titled GBOUT6 Call B6_NBLIG From W1WMOPXSTD
Onkey 136 Titled GBOUT8 Call B8_NBLIG From W1WMOPXSTD
Onkey 137 Titled GBOUT9 Call B9_NBLIG From W1WMOPXSTD
Gosub _AFF
End

Subprog B1_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action STD
 Call B1_NBLIG From CNSOPXSTD
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

Subprog B2_NBLIG
Local Decimal BASPAG
If !GWEBSERV
 ## Action CONSORD
 Local Char    PARMSK(250)(1..20) , SAVACT(20)
 SAVACT = GACTION : GACTION = "CONSORD"
 Local Char ACTION(20), PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
 ACTION = "AB2_NBLIG" : PROGSUB = "CNSOPXSTD" : PROGSPE = "SPECNSOPX" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 PARMSK(1) = "0"
 PARMSK(2) = [M:OPX1]FCY
 PARMSK(3) = ""
 PARMSK(4) = ""
 PARMSK(5) = ""
 PARMSK(6) = ""
 PARMSK(7) = ""
 PARMSK(8) = [M:OPX2]MFGNUM(nolign-1)
 PARMSK(9) = "2"
 PARMSK(10) = ""
 PARMSK(11) = ""
 PARMSK(12) = ""
 PARMSK(13) = ""
 PARMSK(14) = ""
 PARMSK(15) = ""
 Call CONSULT_NUM(BASPAG,"ORD",PARMSK) From GCONSULT
 ACTION = "BB2_NBLIG" : PROGSUB = "CNSOPXSTD" : PROGSPE = "SPECNSOPX" : PROGSPV = ""
 Gosub ACTSUB From GOBJSUB
 GACTION = SAVACT
Endif
If mkstat=99 : mkstat = 0 : Elsif mkstat=0 : mkstat = 2 : Endif
Gosub _AFF
End

