####################################################################
# 31 : GPLINBR Number ( Field C 2)
####################################################################

$SET_GPLINBR
If [L]_AOLDVAL<>this.GPLINBR
 Call _ASTATUSPARENT(this) From ASYRSUB
Endif
Gosub CONTROL_GPLINBR
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
If [L]_AOLDVAL<>this.GPLINBR
 Gosub PROPAGATE_GPLINBR
Endif
Return

$INIT_GPLINBR
Gosub DECLARE_VAR From WMC0SFINDPRICE
[L]CURPRO = "GPLINBR"
[L]ARULE = "INIT" : [L]ACTION = "INIT" : Gosub ACTION From ASYRSUB
If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return
 
$CONTROL_GPLINBR
Gosub DECLARE_VAR From WMC0SFINDPRICE
[L]CURPRO = "GPLINBR"
[L]ASTATUS = fmet this.ADELETEERROR([L]CURPRO)
Gosub _AFORMAT_GPLINBR               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Gosub _AOTHERS_GPLINBR               : If [L]ASTATUS>=[V]CST_AERROR : Return : Endif
Return

$_AMANDATORY_GPLINBR
[L]CURPRO = "GPLINBR"
If fmet this.AGETATTRIBUTE([L]CURPRO,"$isMandatory")="true" & this.GPLINBR=0
 [L]ASTATUS = func ASYRSUB._ASETSTATUS(this,[L]CURPRO,0,[V]CST_AERROR,[V]CST_AHTTP_BAD_REQUEST,mess(10,123,1)) :  Return : # Mandatory field
Endif
Return

$_AFORMAT_GPLINBR
[L]CURPRO = "GPLINBR"
Return

$_AOTHERS_GPLINBR
[L]CURPRO = "GPLINBR"
[L]ARULE = "CONTROL" : [L]ACTION = "CONTROL" : Gosub ACTION From ASYRSUB
Return

$_AOTHERS_REF_GPLINBR
[L]CURPRO = "GPLINBR"
Return

$PROPAGATE_GPLINBR
this.isReadonly = 0
[L]CURPRO = "GPLINBR"
[L]ARULE = "PROPAGATE" : [L]ACTION = "PROPAGATE" : Gosub ACTION From ASYRSUB
this.isReadonly = 1
Return

$LNKPRO_GPLINBR
Gosub DECLARE_VAR From WMC0SFINDPRICE
[L]CURPRO = "GPLINBR"
$_LNKPRO_GPLINBR
Return


