# Supervisor
# Mask management BODRPL0 

Subprog SAIMSK(IMPFIC,ACTION,IMPTRT)
Value Char IMPFIC
Value Char ACTION
Value Char IMPTRT

Gosub DEFVAR

####################################
## Bloc liste 1
####################################
IMPTBL = 0

## Zone NEWSEQ
mkstat = 0
zoncou = "NEWSEQ"
indice = 0
If varmode([M]NEWSEQ,indice)=0
 If !mkstat
  IMPMOD = 0 : IMPZON = "NEWSEQ" : Gosub ACTION From =IMPTRT
 Endif
 If [M]NEWSEQ=0
  mkstat = 1 : GMESSAGE = mess(10,123,1)
 Endif
 If mkstat : GERR = 1 : GZONE = "NEWSEQ" : GZNUM = 14017 : Endif
 Gosub AFF From GSAISIE
 If mkstat : End : Endif
 If !mkstat
  If [M]NEWSEQ<0
   mkstat = 1 : GMESSAGE = mess(180,141,1)
  Endif
  If mkstat : GERR = 1 : GZONE = "NEWSEQ" : GZNUM = 14017 : Endif
  Gosub AFF From GSAISIE
  If mkstat : End : Endif
  If !mkstat
   SAVACT = GACTION : GACTION = "STD"
   Call C_NEWSEQ([M]NEWSEQ) From SUBBRP
   GACTION = SAVACT
   If mkstat : GERR = 1 : GZONE = "NEWSEQ" : GZNUM = 14017 : Endif
   Gosub AFF From GSAISIE
   If mkstat : End : Endif
  Endif
 Endif
Endif

mkstat = 0

End

$DEFVAR
Local Integer I , J , K , XOK , NOL , IMPENT , IMPMOD , IMPTBL , NBLIG
Local Char    IMPMSK(GLONAMK) , IMPZON(GLONAVA) , NOMZONE(40)
Local Char    PROGSUB(GLONADC), PROGSPE(GLONADC), PROGSPV(GLONADC)
Local Char    PARAM(250)(1..20) , SAVACT(20)
Local Char    WCHAR(255)
Local Decimal WNUM
Local Date    WDATE
Local Clbfile WCLOB(9)

IMPMSK = "BODRPL0"
Return

